package lk.informatics.ntc.model.dto;

import java.util.Date;
import java.io.Serializable;

public class InvestigationReportDTO implements Serializable {

	private static final long serialVersionUID = 8344243187243931903L;


	private String offenceCode;
	private String complteOffenceCodeCount;
	private String closedOffenceCodeCount;
	private int receiptAmmount;
	public String getOffenceCode() {
		return offenceCode;
	}
	public void setOffenceCode(String offenceCode) {
		this.offenceCode = offenceCode;
	}
	public String getComplteOffenceCodeCount() {
		return complteOffenceCodeCount;
	}
	public void setComplteOffenceCodeCount(String complteOffenceCodeCount) {
		this.complteOffenceCodeCount = complteOffenceCodeCount;
	}
	public String getClosedOffenceCodeCount() {
		return closedOffenceCodeCount;
	}
	public void setClosedOffenceCodeCount(String closedOffenceCodeCount) {
		this.closedOffenceCodeCount = closedOffenceCodeCount;
	}
	public int getReceiptAmmount() {
		return receiptAmmount;
	}
	public void setReceiptAmmount(int receiptAmmount) {
		this.receiptAmmount = receiptAmmount;
	}

	

}
