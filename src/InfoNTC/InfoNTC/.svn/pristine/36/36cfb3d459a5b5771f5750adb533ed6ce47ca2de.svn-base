<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 65% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}
</style>

		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Approve Elected Bidder</h1>
			<div style="width: 95%;" class="subDiv">

				<h:form id="frm_approveElectedBidder">


					<!-- Panel 01 -->
					<p:panel id="pnl_Search">
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<!-- _Tender reference number -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Tender Reference No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:selectOneMenu id="drpd_RefearanceNo" style="width:80%;"
								effect="fold"
								value="#{approveElectedBidderBackingBean.tenderDTO.tenderRefNo}">
								<f:selectItem itemLabel="-- Select --" itemValue="" />
								<f:selectItems
									value="#{approveElectedBidderBackingBean.referenceNoList}"
									var="referenceNoList" itemValue="#{referenceNoList}"
									itemLabel="#{costCodeList}" />
								<p:ajax
									listener="#{approveElectedBidderBackingBean.onReferenceNoChange}"
									process="@form"
									update="@form,addApplicantBtnId,clearBtnIdAddApp,approveBtnId" />
							</p:selectOneMenu>

							<p:panelGrid></p:panelGrid>

							<!-- Tender description -->
							<p:panelGrid></p:panelGrid>
							<p:outputLabel>Tender Description</p:outputLabel>
							<p:inputText id="txt_TenderDescription" readonly="true"
								style="width:89%;height:18px;" maxlength="50"
								value="#{approveElectedBidderBackingBean.tenderDTO.tenderDes}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
							layout="grid">

							<p:panelGrid></p:panelGrid>

							<p:panelGrid>
								<!-- View Tender  Button -->
								<p:commandButton styleClass="mainLinkButton" style="width:100%"
									value="View Tender Details" ajax="true" update="@form"
									action="#{approveElectedBidderBackingBean.viewTenderDetails}"></p:commandButton>
								<p:outputLabel></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<!-- Clear 01 Button -->
								<p:commandButton styleClass="mainLinkButton" style="width:100%"
									value="Clear" ajax="true" update="@form"
									action="#{approveElectedBidderBackingBean.clear}"></p:commandButton>
								<p:outputLabel></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

					</p:panel>


					<!-- panel 02 -->

					<p:panel id="pnl_AddApplications" style="width:100%">

						<p:panelGrid columns="6"
							columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-3,ui-grid-col-2,ui-grid-col-3,ui-grid-col-1"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Route No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:selectOneMenu id="drpd_RouteNo" style="width:80%;"
								effect="fold" filter="true" filterMatchMode="contains"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.routeSeqNo}">
								<p:ajax
									listener="#{approveElectedBidderBackingBean.onRouteNoChange}"
									process="@form" update="@form" />

								<f:selectItem itemLabel="-- Select --" itemValue="" />
								<f:selectItems
									value="#{approveElectedBidderBackingBean.routeNoList}"
									var="applicationNoList"
									itemLabel="#{applicationNoList.routeNo}"
									itemValue="#{applicationNoList.routeSeqNo}" />
							</p:selectOneMenu>

							<p:outputLabel>Departure</p:outputLabel>
							<p:inputText id="txt_Departure" style="width:89%;height:18px;"
								maxlength="50" readonly="true"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.departure}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:outputLabel>Serial No.</p:outputLabel>
							<p:inputText id="txt_SerialNo" style="width:89%;height:18px;"
								maxlength="50" readonly="true"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.serialNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:outputLabel>Arrival</p:outputLabel>
							<p:inputText id="txt_Arrival" style="width:89%;height:18px;"
								maxlength="50" readonly="true"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.arrival}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:outputLabel>TH Price Rs.</p:outputLabel>
							<p:inputText id="txt_ThPrice" style="width:89%;height:18px;"
								maxlength="50" readonly="true"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.thBidPriceAnnum}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:outputLabel>No. of permits</p:outputLabel>
							<p:inputText id="txt_NoOfPermits" style="width:89%;height:18px;"
								maxlength="50" readonly="true"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.noOfPermits}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

						</p:panelGrid>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-8,ui-grid-col-1"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:outputLabel>Effective Routes</p:outputLabel>
							<p:inputText id="txt_EffectiveRoutes"
								style="width:96%;height:18px;" maxlength="50" readonly="true"
								value="#{approveElectedBidderBackingBean.tenderApplicantDTO.effectiveRoutes}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

						</p:panelGrid>


						<br></br>

						<p:panelGrid columns="3"
							columnClasses="ui-grid-col-3,ui-grid-col-6,ui-grid-col-3"
							layout="grid">

							<p:outputLabel></p:outputLabel>
							<p:commandButton styleClass="mainLinkButton" style="width:100%"
								value="Clear" ajax="true"
								update="pnl_AddApplications,tbl_SelectApplication"
								action="#{approveElectedBidderBackingBean.clearRouteDetails}"></p:commandButton>
							<p:outputLabel></p:outputLabel>


						</p:panelGrid>

						<br></br>

						<!-- Table 01 -->
						<p:dataTable id="tbl_SelectApplication"
							value="#{approveElectedBidderBackingBean.selectingApplicationList}"
							var="selectingApplicationList"
							selection="#{approveElectedBidderBackingBean.temptSelectedApplicationList}"
							rowKey="#{selectingApplicationList.applicationNo}"
							resizableColumns="false" liveResize="true" rowIndexVar="index"
							emptyMessage="No Records Available.">

							<p:column headerText="Application No."
								style="text-align:left;width:10%">
								<h:outputText value="#{selectingApplicationList.applicationNo}"></h:outputText>
							</p:column>
							<p:column headerText="Bidder Name"
								style="text-align:center;width:25%">
								<h:outputText value="#{selectingApplicationList.bidderName}"></h:outputText>
							</p:column>
							<p:column headerText="Bidder Price Rs."
								style="text-align:center;width:15%">
								<h:outputText value="#{selectingApplicationList.bidPrice}"></h:outputText>
							</p:column>
							<p:column headerText="TH Price %"
								style="text-align:center;width:15%">
								<h:outputText
									value="#{selectingApplicationList.thPricePercentage}"></h:outputText>
							</p:column>


							<p:column headerText="Special Remarks"
								style="text-align:center;width:30%;word-wrap: break-word;">
								<h:outputText
									value="#{selectingApplicationList.secondEnvRemark}"></h:outputText>
							</p:column>

							<p:column headerText="Select" selectionMode="multiple"
								style="text-align:center;width:10%" />

						</p:dataTable>

						<br></br>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<p:outputLabel></p:outputLabel>

							<!-- Add applications Button -->
							<p:commandButton styleClass="mainLinkButton" style="width:100%"
								id="addApplicantBtnId" value="Add Applications" ajax="true"
								update="@form"
								disabled="#{approveElectedBidderBackingBean.disabledBtnsAfterApproved}"
								action="#{approveElectedBidderBackingBean.addApplcations}"></p:commandButton>
							<!-- Clear 03 Button -->
							<p:commandButton styleClass="mainLinkButton" style="width:100%"
								id="clearBtnIdAddApp" value="Clear" ajax="true"
								disabled="#{approveElectedBidderBackingBean.disabledBtnsAfterApproved}"
								update="pnl_AddApplications,tbl_SelectApplication"
								action="#{approveElectedBidderBackingBean.clearSelectingApplications}"></p:commandButton>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>

						</p:panelGrid>

						<br></br>
						<!-- Table 02 -->
						<p:dataTable id="tbl_SelectedApplications"
							var="selectedApplications"
							value="#{approveElectedBidderBackingBean.selectedApplicationList}"
							scrollable="true" width="100%" draggableColumns="false"
							scrollRows="10" styleClass="tblBody"
							emptyMessage="No Records Available.">
							<p:column headerText="Route No." draggable="false"
								style="text-align:center;width:90px">
								<h:outputText value="#{selectedApplications.routeNo}"></h:outputText>
							</p:column>
							<p:column headerText="Application No."
								style="text-align:left;width:90px">
								<h:outputText value="#{selectedApplications.applicationNo}"></h:outputText>
							</p:column>
							<p:column headerText="Bidder Name"
								style="text-align:left;width:150px">
								<h:outputText value="#{selectedApplications.bidderName}"></h:outputText>
							</p:column>
							<p:column headerText="Bidder Price Rs."
								style="text-align:center;width:90px">
								<h:outputText value="#{selectedApplications.bidPrice}"></h:outputText>
							</p:column>
							<p:column headerText="TH Price %"
								style="text-align:center;width:90px">
								<h:outputText value="#{selectedApplications.thPricePercentage}"></h:outputText>
							</p:column>
							<p:column headerText="TH Price Rs." style="width:90px">
								<h:outputText value="#{selectedApplications.thBidPriceAnnum}"></h:outputText>
							</p:column>
							<p:column headerText="Serial Number" style="width:90px">
								<h:outputText value="#{selectedApplications.serialNo}"></h:outputText>
							</p:column>
							<p:column headerText="Arrival" style="width:90px">
								<h:outputText value="#{selectedApplications.arrival}"></h:outputText>
							</p:column>
							<p:column headerText="Departure" style="width:90px">
								<h:outputText value="#{selectedApplications.departure}"></h:outputText>
							</p:column>
							<p:column headerText="Effective Routes" style="width:100px">
								<h:outputText value="#{selectedApplications.effectiveRoutes}"></h:outputText>
							</p:column>

							<p:column headerText="No. of permits" style="width:60px">
								<h:outputText value="#{selectedApplications.noOfPermits}"></h:outputText>
							</p:column>
							<p:column headerText="Special Remarks"
								style="width:120px;word-wrap: break-word;">
								<h:outputText value="#{selectedApplications.secondEnvRemark}"></h:outputText>
							</p:column>
							<p:column headerText="Remove" style="width:90px">
								<center>
									<p:commandLink
										action="${approveElectedBidderBackingBean.removeSelectedApplication(selectedApplications.applicationNo,selectedApplications.serialNo)}"
										disabled="#{approveElectedBidderBackingBean.disabledBtnsAfterApproved}"
										update="@form">
										<p:graphicImage library="images" name="delete.png"
											style="text-align:center;width:24px;height:24px;"
											title="Remove"></p:graphicImage>
									</p:commandLink>
								</center>
							</p:column>
						</p:dataTable>

						<br></br>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<p:outputLabel></p:outputLabel>

							<!-- Complete Tender Button -->
							<p:commandButton styleClass="mainLinkButton" style="width:100%"
								id="approveBtnId" value="Approve" ajax="true" update="@form"
								disabled="#{approveElectedBidderBackingBean.disabledBtnsAfterApproved}"
								action="#{approveElectedBidderBackingBean.approveElectedBidder}"></p:commandButton>
							<!-- Clear 04 Button -->
							<p:commandButton styleClass="mainLinkButton" style="width:100%"
								value="Clear" ajax="true" update="@form"
								action="#{approveElectedBidderBackingBean.clear}"></p:commandButton>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>

						</p:panelGrid>


					</p:panel>
				</h:form>
			</div>
		</div>

		<p:dialog id="errorMSG" global="true" modal="true" header="Error"
			width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{approveElectedBidderBackingBean.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog id="successMSG" global="true" modal="true" header="Success"
			width="30%" severity="info" widgetVar="successMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{approveElectedBidderBackingBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog id="infoMSG" global="true" modal="true" header="Alert"
			width="30%" severity="info" widgetVar="infoMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmInfo">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning_24.png"></p:graphicImage>

						<p:outputLabel
							value="#{approveElectedBidderBackingBean.infoMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmInfo" onclick="PF('infoMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>


	</ui:define>
</ui:composition>