<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

#Dob {
	width: 80% !important;
}
</style>


		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Draft Survey Form</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmGenSurvey">
					<p:panel>
						<!--First part panelgrid start  -->
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4,ui-grid-col-3"
							layout="grid" id="frmIdPG">

							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Form ID<p:outputLabel value="*"
									style="color:red;"></p:outputLabel>
							</p:outputLabel>
							<p:selectOneMenu style="width:185px"
								value="#{generateDraftServeyBackingBean.selectedFormID}"
								effect="fold" filter="true" filterMatchMode="contains"
								tabindex="1">
								<p:ajax
									listener="#{generateDraftServeyBackingBean.onFormIdClick}"
									update="frmIdPG" />
								<f:selectItem itemLabel="Select Form Id" itemValue=""
									noSelectionOption="true" />
								<f:selectItems
									value="#{generateDraftServeyBackingBean.formList}"
									var="formList" itemLabel="#{formList.formId}"
									itemValue="#{formList.formId}" />
							</p:selectOneMenu>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Form Description<p:outputLabel
									value="*" style="color:red;"></p:outputLabel>
							</p:outputLabel>
							<p:inputText style="width:200px" id="formDesc"
								value="#{generateDraftServeyBackingBean.formDTO.formDescription}"
								readonly="true"></p:inputText>


							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Survey No.</p:outputLabel>
							<p:inputText style="width:200px" id="surveyNo"
								value="#{generateDraftServeyBackingBean.formDTO.surveyNo}"
								readonly="true"></p:inputText>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Survey Type</p:outputLabel>
							<p:inputText style="width:200px" id="surveyType"
								value="#{generateDraftServeyBackingBean.formDTO.surveyType}"
								readonly="true"></p:inputText>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Survey Method</p:outputLabel>
							<p:inputText style="width:200px" id="surveyMethod"
								value="#{generateDraftServeyBackingBean.formDTO.surveyMethod}"
								readonly="true"></p:inputText>

						</p:panelGrid>
						<!--First part panelgrid end  -->
						<p:panel style="border-bottom:5px solid #404d6f;" />
						<br />
						<!--Mid point survey panelgrid start  -->
						<h3 style="margin-top: 0%; text-align: center; margin-bottom: 2%;">National
							Transport Commission , Sri Lanka Mid Point Survey</h3>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
							layout="grid">

							<p:outputLabel>Survey Form No.</p:outputLabel>
							<p:inputText style="width:200px" id="surveyFormNo"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.surveyNo}"
								readonly="true"></p:inputText>

							<p:outputLabel>Location</p:outputLabel>
							<p:inputText style="width:200px"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.location}"
								readonly="true"></p:inputText>
							<p:outputLabel>Direction Form</p:outputLabel>
							<p:inputText style="width:200px" id="directionFrm"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.directinFrom}"
								readonly="true"></p:inputText>

							<p:outputLabel>Date</p:outputLabel>
							<!-- <p:inputText style="width:200px" id="date" value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.date}"  readonly="true"></p:inputText>
							 -->
							<p:calendar inputStyle="position: absolute;width: 200px;"
								id="date"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.date}"
								readonly="true" pattern="dd/MM/yyyy" showOn="none" />

							<p:outputLabel>Direction To</p:outputLabel>
							<p:inputText style="width:200px" id="directionTo"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.directionTo}"
								readonly="true"></p:inputText>

							<p:outputLabel>Time</p:outputLabel>
							<p:inputText style="width:200px" id="time"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.time}"
								readonly="true">
								<f:convertDateTime pattern="HH:mm" />
							</p:inputText>

							<p:outputLabel>Name of the Recorder</p:outputLabel>
							<p:inputText style="width:200px" id="nameOfRecorder"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.nameOfRecorder}"
								readonly="true"></p:inputText>

							<p:outputLabel>Remarks</p:outputLabel>
							<p:inputText style="width:200px"
								value="#{generateDraftServeyBackingBean.formDTO.midpointSurvey.remarks}"
								readonly="true"></p:inputText>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>

						</p:panelGrid>
						<!--Mid point survey panelgrid end  -->
						<p:panel style="border-bottom:5px solid #404d6f;" />
						<br />
						<!--third panelgrid start  -->

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-2,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid" style="margin-bottom:2%;">
							<p:outputLabel>No of Rows(Per one page)</p:outputLabel>

							<p:selectOneMenu id="noOfRows" style="width:80%;"
								disabled="#{!generateDraftServeyBackingBean.printCopies}"
								effect="fold" value="#{generateDraftServeyBackingBean.noOfRows}">
								<f:selectItem itemLabel="-- Select --" itemValue="" />
								<f:selectItems
									value="#{generateDraftServeyBackingBean.noOfRowsList}"
									var="noOfRowsList" itemValue="#{noOfRowsList}"
									itemLabel="#{noOfRowsList}" />

							</p:selectOneMenu>
							<p:commandButton value="Add" class="mainActionbtn"
								style="width:50%;margin-left:25%;"
								actionListener="#{generateDraftServeyBackingBean.addRowsAction}"
								update="@form"
								disabled="#{!generateDraftServeyBackingBean.printCopies}"></p:commandButton>
						</p:panelGrid>

						<div style="overflow-x: auto;">
							<table>
								<tr>
									<c:forEach
										items="#{generateDraftServeyBackingBean.tableIndicatorList}"
										var="indicatorDTO">

										<th><h:outputLabel
												style="margin-right:40px;margin-bottom:15px;"
												value="#{indicatorDTO.fieldNameEn}" /></th>

									</c:forEach>
								</tr>
								<!--third panelgrid end  -->



								<c:forEach begin="1"
									end="#{generateDraftServeyBackingBean.rowCount}"
									items="#{generateDraftServeyBackingBean.integerIndicatorList}"
									var="intVar">
									<tr>
										<c:forEach
											items="#{generateDraftServeyBackingBean.inputDataIndicatorList}"
											var="indicatorDTO">
											<td><p:inputText value="#{indicatorDTO.fieldNameEn}"
													style="margin-right:40px;" readonly="true" /></td>

										</c:forEach>
										<td><p:commandButton value="Delete"
												actionListener="#{generateDraftServeyBackingBean.deleteRowAction(intVar)}"
												style="margin-right:40px;" /></td>
									</tr>
								</c:forEach>

							</table>
						</div>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-2,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid" style="margin-top:2%;">


							<p:commandButton value="Print" class="mainActionbtn"
								style="width:50%;margin-left:25%;"
								actionListener="#{generateDraftServeyBackingBean.printCopiesAction}"
								onclick="PrimeFaces.monitorDownload(start, stop);"
								oncomplete="PF('genReport').hide();" process="@this"
								update="@form"
								disabled="#{!generateDraftServeyBackingBean.printCopies}">
								<p:fileDownload value="#{generateDraftServeyBackingBean.files}" />
							</p:commandButton>

						</p:panelGrid>


						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-2,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid" style="margin-top:3%;">

							<p:commandButton
								value="#{msg['generate.survey.management.back.btn']}"
								class="mainActionbtn" style="width:50%;margin-left:25%;"
								actionListener="#{generateDraftServeyBackingBean.backButonAction}"
								update="@form"
								rendered="#{generateDraftServeyBackingBean.backBtnRender}"></p:commandButton>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>

					</p:panel>

					<p:dialog styleClass="" id="successSve" global="true" modal="true"
						header="#{msg['um.employeeprofile.dlsuccess']}" width="30%"
						severity="info" widgetVar="successSve" closeOnEscape="true"
						resizable="false">
						<h:form id="frmsuccessSve">
							<center>
								<p:panelGrid columns="2" styleClass="ui-noborder">
									<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
									<p:outputLabel value="Succesfully Printed"></p:outputLabel>

								</p:panelGrid>
								<br></br>
								<p:commandButton style="margin-top:10px;margin-bottom:10px"
									process="frmsuccessSve" value="Ok"
									onclick="PF('successSve').hide()">
								</p:commandButton>

							</center>
						</h:form>
					</p:dialog>

				</h:form>

			</div>
		</div>
	</ui:define>
</ui:composition>