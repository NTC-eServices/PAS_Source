<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.calendarClass input {
	width: 73% !important
}
</style>

		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Create Tender Process</h1>
			<div style="width: 95%;" class="subDiv">
				<h:form id="tender">

					<!-- Panel 01 -->
					<p:panel>
						<p:panelGrid columns="3"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-8"
							layout="grid">
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Traffic Proposal No.</p:outputLabel>
								<p:outputLabel rendered="#{!createTenderBackingBean.viewMode}"
									value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="trafficProposalNo" style="width:33%;"
									rendered="#{!createTenderBackingBean.viewMode}" effect="fold"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{createTenderBackingBean.tenderDTO.trafficProposalNo}">
									<f:selectItem itemLabel="-- All --" itemValue="" />
									<f:selectItems
										value="#{createTenderBackingBean.getTrafficProposalList}"
										var="d" itemLabel="#{d.trafficProposalNo}"
										itemValue="#{d.trafficProposalNo}" />
								</p:selectOneMenu>
								<p:inputText rendered="#{createTenderBackingBean.viewMode}"
									style="width:33%;height:18px;" maxlength="50"
									id="trafficProposal_View"
									value="#{createTenderBackingBean.tenderDTO.trafficProposalNo}"
									onkeyup="this.value=this.value.toUpperCase();" readonly="true">
								</p:inputText>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Tender Reference No.</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText rendered="#{!createTenderBackingBean.viewMode}"
									style="width:33%;height:18px;color:red;font-weight: bold;"
									maxlength="50" id="tenderRef"
									value="#{createTenderBackingBean.tenderRefNo}"
									onkeyup="this.value=this.value.toUpperCase();" readonly="true">
								</p:inputText>
								<p:inputText rendered="#{createTenderBackingBean.viewMode}"
									style="width:33%;height:18px;color:red;font-weight: bold;"
									maxlength="50" id="tenderRef_View"
									value="#{createTenderBackingBean.tenderDTO.tenderRefNo}"
									onkeyup="this.value=this.value.toUpperCase();" readonly="true">
								</p:inputText>
							</p:panelGrid>

						</p:panelGrid>
					</p:panel>


					<!-- Panel 01 : Buttons -->
					<center>
						<p:panel>
							<!-- Search button -->
							<p:commandButton styleClass="mainActionbtn" value="Search"
								update="@form,frmError,frmAlert,frmsuccess,routePanel"
								action="#{createTenderBackingBean.search}"
								rendered="#{!createTenderBackingBean.viewMode}"></p:commandButton>
							<!-- Clear button -->
							<p:commandButton styleClass="mainActionbtn" value="Clear"
								update="@form,frmError,frmAlert,frmsuccess"
								action="#{createTenderBackingBean.clearOne}"
								rendered="#{!createTenderBackingBean.viewMode}"></p:commandButton>
						</p:panel>
					</center>

					<p:panel>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">



							<p:panelGrid>
								<p:outputLabel>Closing Date</p:outputLabel>
								<p:outputLabel rendered="#{!createTenderBackingBean.viewMode}"
									value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:calendar id="closeDate" styleClass="calendarClass"
									rendered="#{!createTenderBackingBean.viewMode}"
									navigator="true" inputStyle="width:100%%"
									value="#{createTenderBackingBean.closeDate}" showOn="button"
									pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
									yearRange="c-100:c+100"></p:calendar>
								<p:inputText rendered="#{createTenderBackingBean.viewMode}"
									style="width:100%;height:18px"
									value="#{createTenderBackingBean.tenderDTO.tenClosingDate}"
									onkeyup="this.value=this.value.toUpperCase();" readonly="true">
								</p:inputText>
							</p:panelGrid>


							<p:panelGrid>
								<h:outputText value="Time" />
								<p:outputLabel rendered="#{!createTenderBackingBean.viewMode}"
									value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="time"
									rendered="#{!createTenderBackingBean.viewMode}"
									value="#{createTenderBackingBean.closeTime}"
									inputStyle="width:100%" pattern="HH:mm" timeOnly="true" />
								<p:inputText rendered="#{createTenderBackingBean.viewMode}"
									style="width:100%;height:18px" pattern="HH:mm"
									value="#{createTenderBackingBean.tenderDTO.tenClosingTime}"
									onkeyup="this.value=this.value.toUpperCase();" readonly="true">
								</p:inputText>
							</p:panelGrid>


						</p:panelGrid>

						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">
							<p:panelGrid>
								<p:outputLabel>Tender Description</p:outputLabel>
								<p:outputLabel rendered="#{!createTenderBackingBean.viewMode}"
									value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputTextarea rows="3" style="width:100%"
									value="#{createTenderBackingBean.tenderDTO.tenderDes}"
									readonly="#{createTenderBackingBean.viewMode}" />
							</p:panelGrid>
						</p:panelGrid>

					</p:panel>
					<br />


					<p:panel>
						<p:dataTable id="dataTableOne" var="p" rows="5" paginator="true"
							value="#{createTenderBackingBean.routeDetailList}"
							selection="#{createTenderBackingBean.selectDTO}"
							selectionMode="single" rowKey="#{p.routeNo}"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="10,20" resizableColumns="false"
							liveResize="true" tableStyle="width:100%;font-size:12px;"
							rowIndexVar="index" styleClass="myTable"
							emptyMessage="No route details are available">


							<p:ajax event="rowSelect" update="@form"
								listener="#{createTenderBackingBean.selectRow}" process="@form" />

							<p:column headerText="Departure" visible="false"
								style="width:100px;">
								<h:outputText value="#{p.departure}" />
							</p:column>
							<p:column headerText="arrival" visible="false"
								style="width:100px;">
								<h:outputText value="#{p.arrival}" />
							</p:column>

							<p:column headerText="survey No" visible="false">
								<h:outputText value="#{p.surveyNo}" />
							</p:column>


							<p:column headerText="No. of permits" visible="false">
								<h:outputText value="#{p.noOfPermits}" />
							</p:column>

							<p:column headerText="Route No." width="10%">
								<h:outputText value="#{p.routeNo}" />
							</p:column>
							<p:column headerText="Via">
								<h:outputText value="#{p.via}" />
							</p:column>

							<p:column headerText="Type of Service" width="15%">
								<h:outputText value="#{p.typeOfServices}" />
							</p:column>

							<p:column headerText="One Way Fare Rs." width="15%">
								<h:outputText value="#{p.oneWayBusFare}" />
							</p:column>


							<p:column headerText="Effective routes" visible="true">
								<h:outputText value="#{p.effectiveRoutes}" />
							</p:column>

						</p:dataTable>
					</p:panel>
					<br />



					<!-- Panel 02 start-->
					<p:panel id="routePanel">
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:panelGrid>
								<p:outputLabel>Route No.</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:100%;height:18px;" readonly="true"
									value="#{createTenderBackingBean.tenderDTO.routeNo}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>

							<!-- Panel 02 : row 02 -->
							<p:panelGrid>
								<p:outputLabel>Via</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:100%;height:18px;" readonly="true"
									value="#{createTenderBackingBean.tenderDTO.via}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Type of Service</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:100%;height:18px;" readonly="true"
									value="#{createTenderBackingBean.tenderDTO.typeOfServices}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>


							<!-- Panel 02 : row 03 -->
							<p:panelGrid>
								<p:outputLabel>Effective Routes for Requesting </p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:100%;height:18px;" readonly="true"
									value="#{createTenderBackingBean.tenderDTO.effectiveRoutes}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>

							<!-- Panel 02 : row 04 -->

							<p:panelGrid>
								<p:outputLabel>One Way Fare Rs.</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputNumber style="height:18px;" readonly="true"
									value="#{createTenderBackingBean.tenderDTO.oneWayBusFare}"
									onkeyup="this.value=this.value.toUpperCase();"
									inputStyle="text-align: right;width:100%">
								</p:inputNumber>
							</p:panelGrid>



							<p:panelGrid>
								<p:outputLabel>No. of Turns Per Bus Per Day</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>

								<p:inputText style="width:100%;height:18px;"
									value="#{createTenderBackingBean.advertisementObjectDTO.turnsPerDay}"
									minValue="0" maxlength="2">
									<p:keyFilter regEx="/[\d\.]/" />
								</p:inputText>
							</p:panelGrid>

							<!-- Panel 02 : row 04 -->

							<p:panelGrid>
								<p:outputLabel>Treasure holder price</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputNumber style="height:18px;"
									value="#{createTenderBackingBean.advertisementObjectDTO.treasureHolderPrice}"
									onkeyup="this.value=this.value.toUpperCase();" minValue="0"
									inputStyle="text-align: right;width:100%" maxlength="12">
								</p:inputNumber>
							</p:panelGrid>

							<p:panelGrid>
								<p:outputLabel>Parallel Roads (Impact Prone)</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:100%;height:18px;"
									value="#{createTenderBackingBean.advertisementObjectDTO.parallelRoads}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>

							</p:panelGrid>

						</p:panelGrid>

						<!-- Panel 02 : Buttons -->

						<center>
							<p:panel>
								<!-- Add button -->
								<p:commandButton styleClass="mainActionbtn" value="Add"
									disabled="#{createTenderBackingBean.disabledAdd}"
									update="@form,frmError,frmAlert,frmsuccess,routePanel,dataTableTwo"
									action="#{createTenderBackingBean.addTender}"
									rendered="#{!createTenderBackingBean.viewMode}"></p:commandButton>
								<!-- Clear button -->
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									update="@form,frmError,frmAlert,frmsuccess"
									disabled="#{createTenderBackingBean.disabledClearTwo}"
									action="#{createTenderBackingBean.clearTwo}"></p:commandButton>
							</p:panel>
						</center>
					</p:panel>
					<br />
					<!-- Panel 02 end-->
					<!-- Panel 03-->

					<p:panel>

						<p:dataTable id="dataTableTwo" var="x" rows="5" paginator="true"
							value="#{createTenderBackingBean.advertisementList}"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="10,20" resizableColumns="false"
							liveResize="true" tableStyle="width:auto;font-size:12px;"
							rowIndexVar="index" styleClass="myTable" sortBy="#{x.slNumber}"
							sortOrder="ascending"
							emptyMessage="No tender details are available">


							<p:column headerText="SL No." width="7%">
								<h:outputText value="#{x.slNumber}" />
							</p:column>


							<p:column headerText="Route No." width="7%">
								<h:outputText value="#{x.routeNo}" />
							</p:column>

							<p:column headerText="Departure" width="10%">
								<h:outputText value="#{x.departure}" />
							</p:column>

							<p:column headerText="Arrival" width="10%">
								<h:outputText value="#{x.arrival}" />
							</p:column>

							<p:column headerText="No. of Permits" width="7%">
								<h:outputText value="#{x.noOfPermits}" />
							</p:column>

							<p:column headerText="Via" width="14%">
								<h:outputText value="#{x.via}" />
							</p:column>

							<p:column headerText="Type of Service">
								<h:outputText value="#{x.typeOfServices}" />
							</p:column>

							<p:column headerText="One Way Fare (Rs.)"
								style="text-align:right">
								<h:outputText value="#{x.oneWayBusFare}">
									<f:convertNumber pattern="##,###.00" />
								</h:outputText>
							</p:column>

							<p:column headerText="No. of Turns Per Day Per Bus" width="13%">
								<h:outputText value="#{x.turnsPerDay}">

								</h:outputText>
							</p:column>


							<p:column headerText="Parallel Roads (Impact Prone)" width="13%">
								<h:outputText value="#{x.parallelRoads}" />
							</p:column>

							<p:column headerText="Effective" visible="false">
								<h:outputText value="#{x.effectiveRoutes}" />
							</p:column>

							<p:column headerText="teasure holder price" visible="false">
								<h:outputText value="#{x.treasureHolderPrice}" />
							</p:column>

							<p:column headerText="" width="4%"
								rendered="#{!createTenderBackingBean.viewMode}">
								<p:commandLink action="#{createTenderBackingBean.editAction()}"
									update="@form">

									<f:setPropertyActionListener value="#{x}"
										target="#{createTenderBackingBean.viewSelect}" />

									<p:graphicImage library="images" name="edit.png"
										style="width:24px;height:24px;" title="Edit"></p:graphicImage>
								</p:commandLink>
							</p:column>

						</p:dataTable>
					</p:panel>

					<!-- Panel 04-->
					<p:panel>
						<center>
							<p:panel>
								<!-- Back button -->
								<p:commandButton
									value="#{msg['generate.survey.management.back.btn']}"
									class="mainActionbtn" style="width:auto;"
									actionListener="#{createTenderBackingBean.backButonAction}"
									update="@form" rendered="#{createTenderBackingBean.viewMode}"></p:commandButton>
								<!-- Search button -->
								<p:commandButton styleClass="mainLinkButton" value="Save"
									update="@form,frmError,frmAlert,frmsuccess"
									disabled="#{createTenderBackingBean.disabledSave}"
									action="#{createTenderBackingBean.save}"
									rendered="#{!createTenderBackingBean.viewMode}"></p:commandButton>
								<!-- Clear button -->
								<p:commandButton styleClass="mainLinkButton" value="Cancel"
									update="@form,frmError,frmAlert,frmsuccess"
									disabled="#{createTenderBackingBean.disabledCancel}"
									action="#{createTenderBackingBean.clearThree}"
									rendered="#{!createTenderBackingBean.viewMode}"></p:commandButton>
								<!-- Document Management button -->
								<p:commandButton styleClass="mainLinkButton"
									value="Documents Management"
									disabled="#{createTenderBackingBean.disabledDoc}"
									update="@form,frmError,frmAlert,frmsuccess,uploadTenderDocumentsDialog"
									action="#{createTenderBackingBean.uploadDoc}"
									rendered="#{!createTenderBackingBean.viewMode}"></p:commandButton>
							</p:panel>
						</center>
					</p:panel>


				</h:form>
			</div>
		</div>


		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel value="#{createTenderBackingBean.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Success" width="30%" severity="info"
			widgetVar="successMessage" closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{createTenderBackingBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
