		<h:outputStylesheet name="css/layout.css" library="theme-layout" />
	</f:facet>

	<p:idleMonitor timeout="7200000">
		<!-- 7200000 = 2 HRS -->
		<!-- 30 mins -->
		<!-- 900000 = 15 minutes -->
		<p:ajax event="idle" listener="#{loginBean.onIdle}" />

	</p:idleMonitor>
	<h:form>
		<p:dialog id="dlgSessionTimeout" widgetVar="dlgSessionTimeout"
			dynamic="true" hideEffect="fade" modal="true" showEffect="fade"
			closeOnEscape="false" closable="false" resizable="false">
			<f:facet name="header">
				<h:outputText value="Session Expired" />
			</f:facet>
			<p:panelGrid columns="1" styleClass="ui-noborder">
				<h:panelGrid columns="2"
					columnClasses="msg-box-icon-col msg-box-msg-col">
					<h:outputText id="txt_count"
						value="Your session will expire in #{loginBean.number} seconds. Do you want to extend the session?"
						styleClass="common_lable_h3" />
					<p:poll interval="3" listener="#{loginBean.increment}"
						update="txt_count" />
				</h:panelGrid>

				<h:panelGroup style="display:block; text-align:center">
					<p:commandButton value="Yes" action="#{loginBean.increaseNumber}"
						style="margin:0 auto !important;" />
					<p:commandButton value="No" action="#{loginBean.goToLoginPage}"
						oncomplete="PF('dlgSessionTimeout').hide()"
						style="margin:0 auto !important;" />
				</h:panelGroup>
			</p:panelGrid>
		</p:dialog>



	</h:form>


