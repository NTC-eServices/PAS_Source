package lk.informatics.ntc.model.dto;

import java.io.Serializable;

/**
 * Created for combine control sheet
 * Created by: gayathra.m
 * */
public class RouteScheduleMidPointDTO implements Serializable {

	private static final long serialVersionUID = 7160305929723247867L;

	private String mpOrigin;
	private String mpStartTime;
	private String mpMidpointTime;
	private String mpMidpoint;
	private String mpDestination;
	private String mpEndTime;
	private String isFixed;
	private String mpBusNo;

	public RouteScheduleMidPointDTO() {
	}

	public String getMpOrigin() {
		return mpOrigin;
	}

	public void setMpOrigin(String mpOrigin) {
		this.mpOrigin = mpOrigin;
	}

	public String getMpStartTime() {
		return mpStartTime;
	}

	public void setMpStartTime(String mpStartTime) {
		this.mpStartTime = mpStartTime;
	}

	public String getMpMidpointTime() {
		return mpMidpointTime;
	}

	public void setMpMidpointTime(String mpMidpointTime) {
		this.mpMidpointTime = mpMidpointTime;
	}

	public String getMpMidpoint() {
		return mpMidpoint;
	}

	public void setMpMidpoint(String mpMidpoint) {
		this.mpMidpoint = mpMidpoint;
	}

	public String getMpDestination() {
		return mpDestination;
	}

	public void setMpDestination(String mpDestination) {
		this.mpDestination = mpDestination;
	}

	public String getMpEndTime() {
		return mpEndTime;
	}

	public void setMpEndTime(String mpEndTime) {
		this.mpEndTime = mpEndTime;
	}

	public String getIsFixed() {
		return isFixed;
	}

	public void setIsFixed(String isFixed) {
		this.isFixed = isFixed;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	public String getMpBusNo() {
		return mpBusNo;
	}

	public void setMpBusNo(String mpBusNo) {
		this.mpBusNo = mpBusNo;
	}

}
