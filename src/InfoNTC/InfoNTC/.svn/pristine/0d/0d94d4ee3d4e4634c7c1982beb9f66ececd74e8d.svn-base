<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn1 {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 180px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

.calendarClass input {
	width: 62% !important
}

.ui-datepicker-calendar {
	display: none;
	width: 100%;
}
</style>

		<div class="mainDiv">

			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Running Sheet</h1>
			<h:form id="mainxx">
				<div style="width: 95%;" class="subDiv">

					<!-- Main Panel -->

					<p:panelGrid columns="4"
						columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-4,ui-grid-col-2"
						layout="grid">

						<!-- Route Number -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Route No. "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:selectOneMenu id="routeNoId" style="width:75%"
								value="#{runningSheetBackingBean.route}" filter="true"
								filterMatchMode="contains" tabindex="1" effect="fold">
								<p:ajax listener="#{runningSheetBackingBean.onRouteChange}"
									update="originId,destinationId,depSideId"></p:ajax>
								<f:selectItem itemLabel=" --Select Route No.-- " itemValue="" />
								<f:selectItems value="#{runningSheetBackingBean.routeList}"
									var="routeList" itemValue="#{routeList}"
									itemLabel="#{routeList}" />
							</p:selectOneMenu>
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Service Type -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Service Type "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:selectOneMenu id="serviceTypeId" style="width:75%"
								value="#{runningSheetBackingBean.serviceType}"
								onkeyup="this.value=this.value.toUpperCase();"
								filterMatchMode="contains" tabindex="1" effect="fold">
								<f:selectItem itemLabel=" --Select Service Type-- " itemValue="" />
								<f:selectItems value="#{runningSheetBackingBean.serviceList}"
									var="serviceList" itemValue="#{serviceList.serviceCode}"
									itemLabel="#{serviceList.serviceDescription}" />
							</p:selectOneMenu>
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Origin -->
						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Origin"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText id="originId" tabindex="2" style="width:80%; "
								value="#{runningSheetBackingBean.origin}" readonly="true"
								effect="fold"></p:inputText>
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Destination"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText id="destinationId" tabindex="2" style="width:80%; "
								readonly="true" value="#{runningSheetBackingBean.destination}"
								effect="fold"></p:inputText>
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Departure Side -->
						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Departure Side "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:selectOneMenu id="depSideId" style="width:75%"
								value="#{runningSheetBackingBean.depSide}"
								onkeyup="this.value=this.value.toUpperCase();" filter="true"
								filterMatchMode="contains" tabindex="1" effect="fold">
								<f:selectItem itemLabel=" --Select Departure Side-- "
									itemValue="" />
								<f:selectItem itemLabel="Colombo" itemValue="Colombo" />
								<f:selectItem itemLabel="Galle" itemValue="Galle" />

								<f:selectItems value="#{runningSheetBackingBean.departureList}"
									var="departureList" itemValue="#{departureList}"
									itemLabel="#{departureList}" />
							</p:selectOneMenu>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Year -->
						<p:panelGrid>
							<p:outputLabel value="Year "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:inputText id="yearId" style="width:80%;"
								value="#{runningSheetBackingBean.year}" effect="fold"
								styleClass="calendarClass" maxlength="4" size="4" locale="en_US"
								placeholder="YYYY" type="number" min="2018" />
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Start Month -->
						<p:panelGrid>
							<p:outputLabel value="Start Month "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:selectOneMenu id="startMonthId" style="width:75%"
								value="#{runningSheetBackingBean.startMonth}"
								onkeyup="this.value=this.value.toUpperCase();"
								filterMatchMode="contains" effect="fold">

								<f:selectItem itemLabel=" --Select Start Month-- "
									itemValue="-1" />
								<f:selectItems value="#{runningSheetBackingBean.monthList}"
									var="monthlist" itemValue="#{monthlist.non}"
									itemLabel="#{monthlist.description}" />

							</p:selectOneMenu>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>

						<p:panelGrid></p:panelGrid>

						<!-- End Month -->
						<p:panelGrid>
							<p:outputLabel value="End Month "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:selectOneMenu id="endMonthId" style="width:75%"
								value="#{runningSheetBackingBean.endMonth}"
								onkeyup="this.value=this.value.toUpperCase();"
								filterMatchMode="contains" tabindex="1" effect="fold">
								<f:selectItem itemLabel=" --Select End Month-- " itemValue="-1" />
								<f:selectItems value="#{runningSheetBackingBean.monthList}"
									var="monthlist" itemValue="#{monthlist.non}"
									itemLabel="#{monthlist.description}" />
							</p:selectOneMenu>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>

						<p:panelGrid></p:panelGrid>

      					<!-- Page Size -->
						<p:panelGrid>
							<p:outputLabel value="Page Size "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:selectOneMenu id="pageFormatId" style="width:75%"
								value="#{runningSheetBackingBean.pageFormat}"
								onkeyup="this.value=this.value.toUpperCase();"
								filterMatchMode="contains" tabindex="1" effect="fold">
								<f:selectItem itemLabel=" --Select Page Size-- " itemValue="" />
								<f:selectItems value="#{runningSheetBackingBean.pageFormatList}"
									var="pageFormatList" itemValue="#{pageFormatList.format}"
									itemLabel="#{pageFormatList.format}" />
							</p:selectOneMenu>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Page Orientation -->
						<p:panelGrid>
							<p:outputLabel value="Page Orientation "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:selectOneMenu id="pageOrientationId" style="width:75%"
								value="#{runningSheetBackingBean.pageOrientation}"
								onkeyup="this.value=this.value.toUpperCase();"
								filterMatchMode="contains" tabindex="1" effect="fold">
								<f:selectItem itemLabel=" --Select Page Orientation-- "
									itemValue="" />
								<f:selectItems
									value="#{runningSheetBackingBean.pageOrientationList}"
									var="pageOrientationList" itemValue="#{pageOrientationList}"
									itemLabel="#{pageOrientationList}" />
							</p:selectOneMenu>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Number of Columns per Page -->
						<p:panelGrid>
							<p:outputLabel value="No.of Columns per Page "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:inputText id="ColumnsCountId" tabindex="1" style="width:80%; "
								value="#{runningSheetBackingBean.columnsCount}"
								placeholder="Insert No. of Columns per Page" effect="fold"
								maxlength="2" min="1" type="number"></p:inputText>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>


						<!--month Column Width -->
						<p:panelGrid>
							<p:outputLabel value="Month Column Width "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:inputText id="monthWidthId" tabindex="1" style="width:80%; "
								value="#{runningSheetBackingBean.monthColumnsWidth}"
								placeholder="Insert Month Column Width" effect="fold"
								maxlength="3" min="5" type="number"></p:inputText>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!--data Column Width -->
						<p:panelGrid>
							<p:outputLabel value="Data Column Width "></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:inputText id="ColumnWidthId" tabindex="1" style="width:80%; "
								value="#{runningSheetBackingBean.dataColumnsWidth}"
								placeholder="Insert Data Column Width" effect="fold"
								maxlength="3" min="5" type="number"></p:inputText>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Row height -->
						<!-- Font Size -->
						<p:panelGrid>
							<p:outputLabel value="Font Size"></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid>
							<p:inputText id="fontSizeId" tabindex="1" style="width:80%; "
								value="#{runningSheetBackingBean.fontSize}"
								placeholder="Insert Font Size" effect="fold" maxlength="2"
								min="5" type="number"></p:inputText>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- End of the form -->
					</p:panelGrid>
				</div>
				<br></br>
				<center>

					<p:commandButton styleClass="mainActionbtn" value="Print"
						process="@form" ajax="false"
						actionListener="#{runningSheetBackingBean.printSheet}"
						id="ptintID" update="@form">
						<p:fileDownload value="#{runningSheetBackingBean.files}" />
					</p:commandButton>

					<p:commandButton styleClass="mainActionbtn" value="Clear"
						process="@all" action="#{runningSheetBackingBean.clearSheet}"
						id="clear" update="@form"></p:commandButton>

				</center>

			</h:form>
		</div>

		<!-- Dialog boxes -->

		<!-- Error Message -->

		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{runningSheetBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField" value="OK"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<!-- Success Message -->

		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="Success" width="30%" severity="info" widgetVar="successSve"
			closeOnEscape="true" resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel value="#{runningSheetBackingBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton tabindex="1"
						style="margin-top:10px;margin-bottom:10px" process="frmsuccessSve"
						update="@form" value="OK" onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>


	</ui:define>
</ui:composition>
