
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.calendarClass input {
	width: 73% !important
}
</style>

		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Grant Approval for (Owner Change/Bus
				Change/Service Change)</h1>
			<div style="width: 95%;" class="subDiv">
				<h:form id="amendments">
					<p:panel id="panelOne">

						<p:panelGrid columns="3"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-8"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Transaction Type"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="tranType" style="width:50%"
									value="#{grantApprovalAmendments.amendmentDTO.tranCode}"
									filter="true" filterMatchMode="contains" tabindex="1">
									<f:selectItem itemLabel="-- All --" itemValue=""
										noSelectionOption="true" />

									<f:selectItems
										value="#{grantApprovalAmendments.transactionList}" var="p"
										itemLabel="#{p.transactionType}" itemValue="#{p.tranCode}" />

									<p:ajax
										listener="#{grantApprovalAmendments.ajaxFilterApplicationNoANDPermitNo}"
										update="@form" />

								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Application No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="applicationNo" style="width:50%"
									disabled="#{grantApprovalAmendments.disabledApplicationNo}"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{grantApprovalAmendments.amendmentDTO.applicationNo}"
									filter="true" filterMatchMode="contains" tabindex="1">

									<f:selectItem itemLabel="-- All --" itemValue=""
										noSelectionOption="true" />

									<f:selectItems
										value="#{grantApprovalAmendments.applicationNoList}" var="d"
										itemLabel="#{d.applicationNo}" itemValue="#{d.applicationNo}" />

									<p:ajax listener="#{grantApprovalAmendments.ajaxFillData}"
										update="@form" />

								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Permit No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="permitNo" style="width:50%"
									disabled="#{grantApprovalAmendments.disabledPermitNo}"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{grantApprovalAmendments.amendmentDTO.permitNo}"
									filter="true" filterMatchMode="contains" tabindex="1">
									<f:selectItem itemLabel="-- All --" itemValue=""
										noSelectionOption="true" />

									<f:selectItems value="#{grantApprovalAmendments.permitNoList}"
										var="p" itemLabel="#{p.permitNo}" itemValue="#{p.permitNo}" />

								</p:selectOneMenu>
							</p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Exisiting Bus No" rendered="false"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText id="exisitingBusdate" readonly="true"
									rendered="false"
									value="#{grantApprovalAmendments.amendmentDTO.exisitingBusNo}"
									onkeyup="this.value=this.value.toUpperCase();"
									style="width:70%" />
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="New Bus No" rendered="false"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText id="newBusdate" readonly="true" rendered="false"
									value="#{grantApprovalAmendments.amendmentDTO.newBusNo}"
									onkeyup="this.value=this.value.toUpperCase();"
									style="width:70%" />
							</p:panelGrid>
						</p:panelGrid>
					</p:panel>

					<br />
					<p:panel>
						<center>
							<!-- Search button -->
							<p:commandButton styleClass="mainActionbtn" value="Search"
								update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.search}"></p:commandButton>
							<!-- Clear button -->
							<p:commandButton styleClass="mainActionbtn" value="Clear"
								update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.clearOne}"></p:commandButton>
						</center>
					</p:panel>


					<p:panel>
						<p:dataTable id="dataTableOne" var="p" rows="5" paginator="true"
							value="#{grantApprovalAmendments.amendmentsDetailList}"
							selection="#{grantApprovalAmendments.selectDTO}"
							selectionMode="single" rowKey="#{p.applicationNo}"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="10,20" resizableColumns="false"
							liveResize="true" tableStyle="width:100%;font-size:12px;"
							rowIndexVar="index" styleClass="myTable"
							emptyMessage="No Amendments details are available">

							<p:ajax event="rowSelect" update="@form"
								listener="#{grantApprovalAmendments.selectRow}" process="@form" />

							<p:column headerText="Application No." style="text-align: left;"
								width="9%">
								<h:outputText value="#{p.applicationNo}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="Permit No." style="text-align: left;"
								width="9%">
								<h:outputText value="#{p.permitNo}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="Transaction Type" width="20%"
								style="text-align: left;">
								<h:outputText value="#{p.transactionType}"
									style="font-size:90%;" />
							</p:column>

							<p:column headerText="Status" width="15%"
								style="text-align: left;">
								<h:outputText value="#{p.status}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="Transaction Code" width="20%"
								style="text-align: left;" visible="false">
								<h:outputText value="#{p.tranCode}" style="font-size:90%;" />
							</p:column>


							<p:column headerText="Service Type" style="text-align: left;"
								visible="false">
								<h:outputText value="#{p.serviceType}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="Ex. Bus No." style="text-align: left;"
								width="8%">
								<h:outputText value="#{p.exisitingBusNo}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="New Bus No." style="text-align: left;"
								width="9%">
								<h:outputText value="#{p.newBusNo}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="Permit Owner" width="22%"
								style="text-align: left;">
								<h:outputText value="#{p.ownerName}" style="font-size:90%;" />
							</p:column>

							<p:column headerText="Created / Modified Date" width="12%"
								style="text-align: left;">
								<h:outputText value="#{p.createdDateString}"
									style="font-size:90%;" />
							</p:column>

							<p:column headerText="Time Approval For Service Changes" width="15%"
								style="text-align: left;">
								<h:outputText value="#{p.timeApprovalForServiceChange}"
									style="font-size:90%;" />
							</p:column>

							<p:column headerText="" width="5%">
								<p:commandLink action="#{grantApprovalAmendments.view()}"
									update="@form">

									<f:setPropertyActionListener value="#{p}"
										target="#{grantApprovalAmendments.viewSelectDTO}" />

									<p:graphicImage library="images" name="view1.png"
										style="width:24px;height:24px;" title="view"></p:graphicImage>
								</p:commandLink>
							</p:column>

						</p:dataTable>

					</p:panel>

					<br />

					<p:panel>

						<center>

							<!-- Committee Approve -->
							<p:commandButton styleClass="mainActionbtn"
								value="Committee Approve"
								disabled="#{grantApprovalAmendments.disabledCommitteeApprove}"
								style="width:18%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.committeeApprove}"></p:commandButton>

							<!-- REQ PTA -->
							<p:commandButton styleClass="mainActionbtn" value="Request PTA"
								rendered="#{grantApprovalAmendments.renderOne}"
								disabled="#{grantApprovalAmendments.disabledReqPTA}"
								style="width:14%" update="@form,frmError,frmAlert,frmsuccess"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=RequestPTA')" ajax="true"
								action="#{grantApprovalAmendments.requestPTA}">

							</p:commandButton>

							<!-- Slip PTA -->
							<p:commandButton styleClass="mainActionbtn" value="Skip PTA"
								rendered="#{grantApprovalAmendments.renderOne}"
								disabled="#{grantApprovalAmendments.disabledSkipPTA}"
								style="width:13%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.skipPTA}"></p:commandButton>

							<!-- REQ PTA -->
							<p:commandButton styleClass="mainActionbtn" value="Approve PTA"
								rendered="#{grantApprovalAmendments.renderOne}"
								disabled="#{grantApprovalAmendments.disabledApprovePTA}"
								style="width:14%" update="@form,frmError,frmAlert,frmsuccess"
								ajax="true" action="#{grantApprovalAmendments.approvePTA}"></p:commandButton>

							<!-- Slip PTA -->
							<p:commandButton styleClass="mainActionbtn" value="Reject PTA"
								rendered="#{grantApprovalAmendments.renderOne}"
								disabled="#{grantApprovalAmendments.disabledRejectPTA}"
								style="width:13%" update="@form,frmError,frmAlert,frmsuccess"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=Reject PTA')" ajax="true"
								action="#{grantApprovalAmendments.rejectPTA}"></p:commandButton>

							<!-- Request Time Approval -->
							<p:commandButton styleClass="mainActionbtn"
								rendered="#{grantApprovalAmendments.renderOne}"
								value="Request Time Approval"
								disabled="#{grantApprovalAmendments.disabledTimeApproval}"
								style="width:19%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.requestTimeApproval}"></p:commandButton>

						</center>
					</p:panel>
					<p:panel>
						<center>

							<!-- Board Approve -->
							<p:commandButton styleClass="mainActionbtn" value="Board Approve"
								disabled="#{grantApprovalAmendments.disabledBoardApprove}"
								style="width:18%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.boardApprove}"></p:commandButton>

							<!-- Request New Route -->
							<p:commandButton styleClass="mainActionbtn"
								value="Request New Route"
								rendered="#{grantApprovalAmendments.renderOne}"
								disabled="#{grantApprovalAmendments.disabledNewRoute}"
								style="width:20%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.requestNewRoute}"></p:commandButton>


							<!-- Generate Response Letters -->
							<p:commandButton styleClass="mainActionbtn"
								value="Generate Response Letter"
								
								style="width:20%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.generateResponseLetters}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=Response Letter')" process="@this"
								ajax="true"></p:commandButton>

						</center>

					</p:panel>
					<p:panel>
						<center>
							<!-- approve 1 -->
							<p:commandButton styleClass="mainActionbtn"
								value="Approval for Amendment Payments"
								disabled="#{grantApprovalAmendments.disabledApproveOne}"
								style="width:28%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.approveOne}"></p:commandButton>
							<!-- approve 2 -->
							<p:commandButton styleClass="mainActionbtn"
								value="Request [DG / Chairman] Approval"
								disabled="#{grantApprovalAmendments.disabledApproveTwo}"
								style="width:25%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.approveTwo}"></p:commandButton>

							<!-- Print Permit -->
							<p:commandButton styleClass="mainActionbtn" value="Print Permit"
								disabled="#{grantApprovalAmendments.disabledPrint}"
								style="width:12%"
								update="@form,frmError,frmAlert,frmsuccess,frmConfirmOne,frmConfirmTwo"
								action="#{grantApprovalAmendments.printPermit}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=permit')" process="@this"
								ajax="true">

							</p:commandButton>
							<!-- Re-Print Permit -->
							<p:commandButton styleClass="mainActionbtn"
								value="Reprint Permit"
								disabled="#{grantApprovalAmendments.disabledRePrint}"
								style="width:12%"
								update="@form,frmError,frmAlert,frmsuccess,frmConfirmOne,frmConfirmTwo"
								action="#{grantApprovalAmendments.ReprintPermit}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=Re-print permit')" process="@this"
								ajax="true">

							</p:commandButton>

							<!-- Reject -->
							<p:commandButton styleClass="mainActionbtn" value="Reject"
								disabled="#{grantApprovalAmendments.disabledReject}"
								style="width:12%" update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.preReject}"></p:commandButton>



							<!-- Clear button -->
							<p:commandButton styleClass="mainActionbtn" value="Cancel"
								disabled="#{grantApprovalAmendments.disabledCancel}"
								update="@form,frmError,frmAlert,frmsuccess"
								action="#{grantApprovalAmendments.clearTwo}"></p:commandButton>

						</center>
					</p:panel>
					<p:panel>
						<center>

							<!-- Generate Report Added by tharushi.e -->
							<p:commandButton styleClass="mainActionbtn"
								value="Generate Report" style="width:18%" update="@form"
								disabled="#{grantApprovalAmendments.disablGenRepo}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=Committe Report')"
								action="#{grantApprovalAmendments.generateReport}"></p:commandButton>

						</center>

					</p:panel>


				</h:form>
			</div>
		</div>

		<p:dialog id="rejectDialog" global="true" modal="true" width="439px"
			header="Reject Grant Approval" severity="info" widgetVar="dlg1"
			closeOnEscape="true" resizable="false" style=" border-radius: 3px;">
			<h:form id="frmReject">

				<h:panelGroup id="reject">
					<center>
						<p:outputPanel id="MainHead">
							<h:outputLabel value="Reject Grant Approval"
								style="font-size: 15px !important;color: black;font-weight:bold" />
						</p:outputPanel>
						<br />
					</center>

					<h:panelGrid id="rejectPanel" columns="1"
						style="margin-top:20px;width:100%; border: 0px solid blue; border-radius: 12px;"
						styleClass="loginpanel">

						<p:inputTextarea placeholder="Enter the comment here"
							style="width:400px; height: 200px;" autoResize="false"
							value="#{grantApprovalAmendments.rejectReason}" />

						<center>
							<p:row>

								<p:commandButton id="btnReject" type="submit"
									class="btn btn-primary btn-lg btn-block"
									style="width:80px; margin-top:10px;height:40px;float: left;font-size: 12px !important; border: 1px solid #FFFFFF; 
										color: white;background:#6A9DED;text-shadow: 2px 2px 5px blue !important;letter-spacing: 1px !important;border-radius:7px;"
									value="Reject" action="#{grantApprovalAmendments.reject}"
									ajax="false">
								</p:commandButton>

								<p:commandButton id="btnClear"
									class="btn btn-primary btn-lg btn-block"
									style="width:80px; margin-top:10px;height:40px;float: left;font-size: 12px !important; border: 1px solid #FFFFFF; 
										color: white;background:#6A9DED;text-shadow: 2px 2px 5px blue !important;letter-spacing: 1px !important;border-radius:7px;"
									value="Cancel" oncomplete="PF('dlg1').hide();">
								</p:commandButton>
							</p:row>
						</center>
						<p:spacer height="30"></p:spacer>

					</h:panelGrid>
				</h:panelGroup>
			</h:form>
		</p:dialog>


		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel value="#{grantApprovalAmendments.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Success" width="30%" severity="info"
			widgetVar="successMessage" closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{grantApprovalAmendments.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMSG"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="#{grantApprovalAmendments.alertMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
