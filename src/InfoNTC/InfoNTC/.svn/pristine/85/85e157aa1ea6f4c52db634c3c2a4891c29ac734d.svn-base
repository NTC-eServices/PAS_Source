package lk.informatics.ntc.model.dto;

import java.io.Serializable;
import java.util.Date;

public class UserLoginHistoryDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1199911470141205621L;
	private String userName;
	private Date loginDate;
	private Date logoutDate;
	private String machineIp;
	private String logOffUser;

	private long loginHisSeq;

	public Date getLoginDate() {
		return loginDate;
	}

	public void setLoginDate(Date loginDate) {
		this.loginDate = loginDate;
	}

	public Date getLogoutDate() {
		return logoutDate;
	}

	public void setLogoutDate(Date logoutDate) {
		this.logoutDate = logoutDate;
	}

	public String getMachineIp() {
		return machineIp;
	}

	public void setMachineIp(String machineIp) {
		this.machineIp = machineIp;
	}

	public String getLogOffUser() {
		return logOffUser;
	}

	public void setLogOffUser(String logOffUser) {
		this.logOffUser = logOffUser;
	}

	public long getLoginHisSeq() {
		return loginHisSeq;
	}

	public void setLoginHisSeq(long loginHisSeq) {
		this.loginHisSeq = loginHisSeq;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

}
