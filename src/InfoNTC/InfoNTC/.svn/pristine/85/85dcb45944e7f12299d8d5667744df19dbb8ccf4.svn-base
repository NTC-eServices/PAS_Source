	<queryString>
		<![CDATA[select b.tfc_stage, b.tfc_normal_round_fee,b.tfc_half_adjuested,b.tfc_quarter_adjuested,
substring(a.mfc_re_ammendment_date,1,4) as year
from nt_t_fee_circle b inner join nt_m_fee_circle_master a on a.mfc_fare_reference_no=b.tfc_fare_reference_no
and a.mfc_status = 'A'
and b.tfc_stage !=0
where substring(a.mfc_re_ammendment_date,1,4) =  $P{P_year} 
order by tfc_stage]]>
