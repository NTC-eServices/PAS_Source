<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 79% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}

.ui-datatable-scrollable .ui-datatable-scrollable-header {
	min-height: 20px;
}

.ui-datatable-scrollable-body {
	height: 125px !important;
}

.genReqNumButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	text-decoration: none !important;
	color: white !important;
	border-radius: 5px !important;
}

.divStyle {
	width: 95%;
	height: 60px;
	background-color: #6BB7C3;
	border-radius: 7px;
}

.buttonDiv {
	text-align: center;
	font-weight: bold;
	color: black;
	display: block;
	margin: auto;
}

/* .ui-datatable table {
    border-collapse: collapse;
    width: auto;
    table-layout: fixed;
    height: auto;
} */
</style>

		<div class="mainDiv">

			<!-- Header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Combine Panel Generator</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="mainform">
					<p:panel id="pnl-search">
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4,ui-grid-col-3"
							layout="grid">

							<!-- Row 1 Start -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Route No"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="routeNo" style="width:70%" effect="fold"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{combinePanelGenaratorBean.combinePanelDTO.routeNo}">
									<f:selectItem itemLabel="-- Select the Route --" itemValue="" />
									<f:selectItems
										value="#{combinePanelGenaratorBean.busRouteList}" var="route"
										itemLabel="#{route.routeNo} - #{route.routeDes}"
										itemValue="#{route.routeNo}" />

									<p:ajax update="origin destination"
										listener="#{combinePanelGenaratorBean.ajaxFillOriginDestination}" />

								</p:selectOneMenu>
								<h:panelGroup id="origin" layout="block">
									<h:outputLabel
										value="Origin: #{combinePanelGenaratorBean.combinePanelDTO.origin}"
										rendered="#{combinePanelGenaratorBean.visible}" />
								</h:panelGroup>
								<h:panelGroup id="destination" layout="block">
									<h:outputLabel
										value="Destination: #{combinePanelGenaratorBean.combinePanelDTO.destination}"
										rendered="#{combinePanelGenaratorBean.visible}" />
								</h:panelGroup>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<!-- Row 1 End -->

							<!-- Row 2 Start -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Type"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="drpd-serviceType" style="width:70%;"
									effect="fold" filter="true" filterMatchMode="contains"
									value="#{combinePanelGenaratorBean.combinePanelDTO.serviceType}">
									<f:selectItem itemLabel="-- Select Service Type. --"
										itemValue="" />
									<f:selectItems
										value="#{combinePanelGenaratorBean.drpdServiceTypeList}"
										var="requestNoList" itemLabel="#{requestNoList.description}"
										itemValue="#{requestNoList.code}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<!-- Row 2 End -->

							<!-- Row 3 Start -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Group No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="groupId" style="width:70%" effect="fold"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{combinePanelGenaratorBean.combinePanelDTO.groupNo}">
									<!-- Should create Bean & change value-->
									<f:selectItem itemLabel="-- Select Group --" itemValue="" />
									<f:selectItem itemLabel="Group 01" itemValue="1" />
									<f:selectItem itemLabel="Group 02" itemValue="2" />
									<f:selectItem itemLabel="Group 03" itemValue="3" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<!-- Row 3 End -->

							<!-- Row 4 Start -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Define the Side"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="side" style="width:70%" effect="fold"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{combinePanelGenaratorBean.combinePanelDTO.defineSide}">
									<!-- Should create Bean & change value-->
									<f:selectItem itemLabel="-- Select the Side --" itemValue="" />
									<f:selectItem itemLabel="Origin to Destination"
										itemValue="Origin to Destination" />
									<f:selectItem itemLabel="Destination to Origin"
										itemValue="Destination to Origin" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<!-- Row 4 End -->
						</p:panelGrid>

						<!-- Search & Clear button -->
						<p:panelGrid columns="5"
							columnClasses="ui-grid-col-6,ui-grid-col-6,ui-grid-col-6,ui-grid-col-6,ui-grid-col-6"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:commandButton id="btn-search" styleClass="mainActionbtn"
									value="Search" update="subpanel,errorMSG"
									actionListener="#{combinePanelGenaratorBean.searchAction}" />


								<p:commandButton value="Clear" styleClass="mainActionbtn"
									action="#{combinePanelGenaratorBean.clearOne}" update="@form" />

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>

						<!-- Start mid point table row  -->
						<p:panel id="subpanel">
							<center>
								<p:dataTable id="routeTable" editable="true"
									value="#{combinePanelGenaratorBean.midPointList}"
									rendered="#{combinePanelGenaratorBean.visibleDiv}" var="data"
									rowKey="#{data.seq}"
									selection="#{combinePanelGenaratorBean.selectedList}"
									tableStyle="width: 40%;" styleClass="myTable"
									emptyMessage="No records available.">

									<p:column headerText="Mid-Point" style="text-align:center;"
										visible="true">
										<h:outputText style="text-align:center;width:80%;"
											visible="true" value="#{data.midPointName}" />
									</p:column>

									<p:column selectionMode="multiple"
										style="text-align:center;width:20%;"
										headerText="Applicable for Time Table" visible="true" />

								</p:dataTable>
							</center>
							<!-- End mid point table row  -->

							<!-- Save button & div -->
							<p:panelGrid columns="3"
								columnClasses="ui-grid-col-6,ui-grid-col-6,ui-grid-col-6"
								layout="grid" id="shwTableDetail">
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<center>
										<p:commandButton id="btn-save" styleClass="mainActionbtn"
											disabled="#{combinePanelGenaratorBean.disableSave}"
											value="Save" action="#{combinePanelGenaratorBean.saveAction}"
											update="searchedData,frmError"
											style="display: #{combinePanelGenaratorBean.visibleDiv ? 'block' : 'none'};" />
									</center>
								</p:panelGrid>
								<p:panelGrid>

									<!--Add div start-->
									<div class="divStyle"
										style="display: #{combinePanelGenaratorBean.visibleDiv ? 'block' : 'none'};"
										id="showDiv">
										<!-- Content within the div -->
										<h:outputLabel value="Add another Route or Service Type"
											class="buttonDiv"></h:outputLabel>
										<p:commandButton value="Add" styleClass="OthActionbtn"
											action="#{combinePanelGenaratorBean.addAction}"
											update="routeNo drpd-serviceType groupId side origin destination routeTable"
											style="display: block; margin: auto; margin-top:10px" />
									</div>
									<!--Add div end -->

								</p:panelGrid>
							</p:panelGrid>
						</p:panel>
					</p:panel>

					<!-- Searched Data table start -->
					<p:panel id="searchedData">
						<center>
							<p:dataTable id="DataTable" editable="true"
								value="#{combinePanelGenaratorBean.tableDataList}"
								rendered="#{combinePanelGenaratorBean.visibleDiv}"
								var="dataTable" tableStyle="width: 80%;" styleClass="myTable"
								selection="#{combinePanelGenaratorBean.busTimeDetailsList}"
								rowKey="#{dataTable.seq}" emptyMessage="No records available.">


								<p:column headerText="Route" style="text-align:center;"
									visible="true">
									<h:outputText style="text-align:center;width:80%"
										visible="true" value="#{dataTable.routeNo}" />
									<!-- should add value -->
								</p:column>

								<p:column headerText="Origin" style="text-align:center;"
									visible="true">
									<h:outputText style="text-align:center;width:80%"
										visible="true" value="#{dataTable.origin}" />
									<!-- should add value -->
								</p:column>

								<p:column headerText="Destination" style="text-align:center;"
									visible="true">
									<h:outputText style="text-align:center;width:80%"
										visible="true" value="#{dataTable.destination}" />
									<!-- should add value -->
								</p:column>

								<p:column headerText="Service Type" style="text-align:center;"
									visible="true">
									<h:outputText style="text-align:center;width:80%"
										visible="true" value="#{dataTable.serviceType}" />
									<!-- should add value -->
								</p:column>

								<p:column headerText="Group No." style="text-align:center;"
									visible="true">
									<h:outputText style="text-align:center;width:80%"
										visible="true" value="#{dataTable.groupNo}" />
									<!-- should add value -->
								</p:column>

								<p:column selectionMode="multiple" style="text-align:center"
									headerText="Applicable for control sheet" visible="true" />


								<p:column headerText="View mid-points"
									style="text-align:center;" visible="true">
									<p:commandLink update="midPointList" process="@form"
										oncomplete="PF('midPointList').show()" title="View">
										<p:graphicImage library="images" name="view.png"
											style="width:24px;height:24px;" title="View"></p:graphicImage>
										<f:setPropertyActionListener value="#{dataTable}"
											target="#{combinePanelGenaratorBean.tableDetails}" />
									</p:commandLink>
								</p:column>

							</p:dataTable>
							<!-- Searched Data table start -->
						</center>
						<br></br>

						<!-- Generate button start -->
						<center>
							<p:commandButton id="btn-generate" styleClass="mainLinkButton"
								value="Generate Data" update="genarateTimeSlote,errorMSG"
								action="#{combinePanelGenaratorBean.genarateData}"
								style="display: #{combinePanelGenaratorBean.visibleDiv ? 'block' : 'none'};" />
						</center>
						<!-- Generate button end -->
					</p:panel>
				</h:form>
			</div>
		</div>

		<!-- Combine Panel Generator – Time slots grid including the mid points -->
		<p:dialog id="genarateTimeSlote" global="true" modal="true"
			closable="true" width="98%" height="90vh" severity="info"
			widgetVar="genarateTimeSlote" closeOnEscape="true" resizable="false">

			<h:form id="frmtempfarerate">
				<p:panel>
					<ui:repeat value="#{combinePanelGenaratorBean.runningNoDetailsList}" var="item">
						<p:outputLabel value="#{item}" />
						<br />
						<br />
					</ui:repeat>
				</p:panel>
				<center>
					<p:panelGrid columns="1" styleClass="ui-noborder">
						<p:panel id="ratePanel">
							<p:dataTable id="table" var="data" styleClass="myTable-two"
								value="#{combinePanelGenaratorBean.selectedDataList}"
								tableStyle="width:auto;font-size:12px;text-align:center;"
								resizableColumns="false" liveResize="true" rowIndexVar="index"
								emptyMessage="No records are available" editable="true"
								editMode="cell" widgetVar="tempCellRate">

								<p:column headerText="Service Type"
									style="padding: 10px 20px 10px 20px;" id="tableService">
									<h:outputText value="#{data.serviceType}" />
								</p:column>
								
								<p:column headerText="Running No"
									style="padding: 10px 25px 10px 25px;">
									<p:selectOneMenu style="width:70%" effect="fold" filter="true"
										filterMatchMode="contains" tabindex="1"
										value="#{data.abbreviation}"
										disabled="#{combinePanelGenaratorBean.disableSelectMenu}">
										<f:selectItems
											value="#{combinePanelGenaratorBean.runningNoList}" var="route"
											itemLabel="#{route}" itemValue="#{route}" />
									</p:selectOneMenu>
								</p:column>

								<p:column headerText="Route No"
									style="padding: 10px 20px 10px 20px;">
									<h:outputText value="#{data.routeNo}" />
								</p:column>

								<p:column headerText="Start From"
									style="padding: 10px 20px 10px 20px;">
									<h:outputText value="#{data.origin}" />
								</p:column>

								<p:columns
									value="#{combinePanelGenaratorBean.combinedStartTimes}"
									var="column" columnIndexVar="colIndex">
									<f:facet name="header">
										<h:outputText value="#{column}" />
									</f:facet>

									<p:cellEditor>
										<f:facet name="output">
											<h:outputText value="#{data.startTimes[column]}" />
										</f:facet>

										<f:facet name="input">
											<p:inputText value="#{data.startTimes[column]}"
												style="width:100%" placeholder="HH:mm">
												<f:validateRegex pattern="([01]?[0-9]|2[0-3]):[0-5][0-9]" />
												<p:ajax event="change"
													listener="#{combinePanelGenaratorBean.changeStartTime(data, 's')}"
													partialSubmit="true" />
											</p:inputText>
										</f:facet>
									</p:cellEditor>
								</p:columns>

								<p:columns value="#{combinePanelGenaratorBean.combinedMidtimes}"
									var="column" columnIndexVar="colIndex">
									<f:facet name="header">
										<h:outputText value="#{column}" />
									</f:facet>

									<p:cellEditor>
										<f:facet name="output">
											<h:outputText value="#{data.midpoints[column]}" />
										</f:facet>

										<f:facet name="input">
											<p:inputText value="#{data.midpoints[column]}"
												style="width:100%" placeholder="HH:mm">
												<f:validateRegex pattern="([01]?[0-9]|2[0-3]):[0-5][0-9]" />
												<p:ajax event="change"
													listener="#{combinePanelGenaratorBean.changeStartTime(data, 'm')}"
													partialSubmit="true" />
											</p:inputText>
										</f:facet>
									</p:cellEditor>
								</p:columns>

								<p:columns value="#{combinePanelGenaratorBean.combinedEndTimes}"
									var="column" columnIndexVar="colIndex">
									<f:facet name="header">
										<h:outputText value="#{column}" />
									</f:facet>

									<p:cellEditor>
										<f:facet name="output">
											<h:outputText value="#{data.endTimes[column]}" />
										</f:facet>

										<f:facet name="input">
											<p:inputText value="#{data.endTimes[column]}"
												style="width:100%" placeholder="HH:mm">
												<f:validateRegex pattern="([01]?[0-9]|2[0-3]):[0-5][0-9]" />
												<p:ajax event="change"
													listener="#{combinePanelGenaratorBean.changeStartTime(data, 'e')}"
													partialSubmit="true" />
											</p:inputText>
										</f:facet>
									</p:cellEditor>
								</p:columns>

								<p:column headerText="Route End"
									style="padding: 10px 20px 10px 20px;">
									<h:outputText value="#{data.destination}" />
								</p:column>
							</p:dataTable>

						</p:panel>
					</p:panelGrid>

					<br></br>
					<p:commandButton id="btnsave" styleClass="mainLinkButton"
						value="Save" update="successMSG"
						action="#{combinePanelGenaratorBean.saveActionTimeGenerator}" />

					<p:commandButton value="Generate Report"
						ajax="false"
						actionListener="#{combinePanelGenaratorBean.generateExcel()}"
						 style="width:20%"
						styleClass="mainLinkButton">
						<p:fileDownload
							value="#{combinePanelGenaratorBean.excelSheet}" />
					</p:commandButton>


				</center>
			</h:form>

		</p:dialog>


		<!-- mid points -->
		<p:dialog id="midPointList" global="true" modal="true" closable="true"
			width="15%" height="auto" widgetVar="midPointList"
			closeOnEscape="true" resizable="false">

			<h:form id="frmmidPoint">
				<p:panel id="midPointListPanel">
					<center>
						<p:dataTable id="midPointtableData"
							value="#{combinePanelGenaratorBean.tableDetails.selectedMidName}"
							var="data">

							<p:column headerText="Mid - Point" style="text-align:center;"
								visible="true">
								<h:outputText style="text-align:center;width:100%"
									visible="true" escape="false" value="#{data}" />
							</p:column>
						</p:dataTable>
					</center>
				</p:panel>
			</h:form>
		</p:dialog>

		<!-- BusNo time details -->
		<p:dialog id="busMidTime" global="true" modal="true" closable="true"
			width="70%" height="60vh" widgetVar="busMidTime" closeOnEscape="true"
			resizable="false" severity="info">

			<h:form id="midPointTimeFrm">
				<p:panel id="midPointTimeListPanel">
					<center>
						<p:dataTable id="a"
							value="#{combinePanelGenaratorBean.busTimeDetailsList}"
							var="details" editable="true" editMode="cell" rowIndexVar="index"
							styleClass="myTable-two" emptyMessage="No records available"
							lazy="true" paginator="true" rows="100"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="100,200,300">

							<f:facet name="header">
								<h:outputLabel
									value="#{combinePanelGenaratorBean.origin} - #{combinePanelGenaratorBean.destination}"></h:outputLabel>
							</f:facet>

							<p:column headerText="Permit No." exportable="false"
								id="permitOri">
								<h:outputText style="width:25%" value="#{details.permitNo}" />
							</p:column>

							<p:column headerText="Bus No." exportable="false" id="busOri">
								<h:outputText style="width:25%" value="#{details.busNo}" />
							</p:column>

							<p:column headerText="Start Time." exportable="false"
								id="startTimeOri">
								<h:outputText style="width:25%" value="#{details.startTime}" />
							</p:column>

							<p:columns
								value="#{combinePanelGenaratorBean.tableDetails.selectedMidName}"
								var="genCol" columnIndexVar="colIndex" id="midOri">
								<f:facet name="header">
									<h:outputText value="#{genCol}" />
								</f:facet>
								<h:outputText value="#{details.takenTime[colIndex]}" />
							</p:columns>

							<p:column headerText="End Time." exportable="false"
								id="endTimeOri">
								<h:outputText style="width:25%" value="#{details.endTime}" />
							</p:column>

						</p:dataTable>
					</center>
				</p:panel>
			</h:form>
		</p:dialog>

		<!-- mid points time selector -->
		<p:dialog id="timeSelector" global="true" modal="true" closable="true"
			width="50%" height="50vh" widgetVar="timeSelector"
			closeOnEscape="true" resizable="false" severity="info"
			header="Select times of midpoints">

			<h:form id="frmTimeSelector">
				<p:panel id="timeSelectorPanel">
					<center>
						<h1
							style="margin-top: 0%; margin-bottom: 3%; text-align: center; color: #000800; font-weight: bold; background-color: #6FFEBB !important;">#{combinePanelGenaratorBean.midPointName}</h1>
					</center>

					<h:panelGrid columns="2">

						<p:dataTable var="entry"
							value="#{combinePanelGenaratorBean.times.entrySet()}">

							<f:facet name="header">
								<h:outputLabel value="Origin to Destination"></h:outputLabel>
							</f:facet>

							<p:column headerText="Route" style="width:20%">
								<h:outputText value="#{entry.key}" />
							</p:column>

							<p:column headerText="Time">
								<p:selectOneRadio
									value="#{combinePanelGenaratorBean.selectedTimes[entry.key]}">
									<f:selectItems value="#{entry.value}" var="time"
										itemValue="#{time}" itemLabel="#{time}" />
								</p:selectOneRadio>
							</p:column>
						</p:dataTable>

						<p:dataTable var="entry"
							value="#{combinePanelGenaratorBean.timesDes.entrySet()}">

							<f:facet name="header">
								<h:outputLabel value="Destination to Origin"></h:outputLabel>
							</f:facet>

							<p:column headerText="Route" style="width:20%">
								<h:outputText value="#{entry.key}" />
							</p:column>

							<p:column headerText="Time">
								<p:selectOneRadio
									value="#{combinePanelGenaratorBean.selectedTimes[entry.key]}">
									<f:selectItems value="#{entry.value}" var="time"
										itemValue="#{time}" itemLabel="#{time}" />
								</p:selectOneRadio>
							</p:column>
						</p:dataTable>
					</h:panelGrid>
					<br></br>
					<br></br>
					<center>
						<p:commandButton styleClass="mainActionbtn" value="Submit"
							action="#{combinePanelGenaratorBean.submit(combinePanelGenaratorBean.midPointName)}"
							update="frmtempfarerate" />
					</center>
				</p:panel>
			</h:form>
		</p:dialog>

		<!-- Error msg -->
		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel value="#{combinePanelGenaratorBean.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Info" width="30%" severity="info" widgetVar="successMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{combinePanelGenaratorBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMSG"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="#{combinePanelGenaratorBean.alertMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmAlert" onclick="PF('alertMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

	</ui:define>
</ui:composition>