		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Generate Traffic Proposal</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="frm-generateTrafficProposal">

					<p:panel id="pnl-search">
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-3,ui-grid-col-4"
							layout="grid">

							<!-- _Survey No.-->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Survey No.</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="drpd-surveyNo" style="width:80%;"
									filter="true" filterMatchMode="contains"
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.surveyNo}">
									<p:ajax
										listener="#{gsTrafficProposalBackingBean.onSurveyNoChange}"
										update="@form" />
									<f:selectItem itemLabel="-- Select Survey No. --" itemValue="" />
									<f:selectItems
										value="#{gsTrafficProposalBackingBean.drpdSurveyNoList}"
										var="surveyNoList" itemLabel="#{surveyNoList.surveyNo}"
										itemValue="#{surveyNoList.surveyNo}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<!-- _Survey Type -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Survey Type</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="drpd-serviceType" style="width:80%;"
									disabled="true" effect="fold" filter="true"
									filterMatchMode="contains"
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.surveyTypeDes}">
									<f:selectItem itemLabel="-- Select Service Type. --"
										itemValue="" />
									<f:selectItems
										value="#{gsTrafficProposalBackingBean.drpdServiceTypeList}"
										var="drpdServiceTypeList"
										itemLabel="#{drpdServiceTypeList.surviceTypeDesc}"
										itemValue="#{drpdServiceTypeList.surviceTypeCode}" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<!-- _Survey Method -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Survey Method</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="drpd-surviceMethod" style="width:80%;"
									effect="fold" filter="true" filterMatchMode="contains"
									disabled="true"
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.surveyMethodDes}">
									<f:selectItem itemLabel="-- Select Service Method. --"
										itemValue="" />
									<f:selectItems
										value="#{gsTrafficProposalBackingBean.drpdServiceMethodList}"
										var="drpdServiceMethodList"
										itemLabel="#{drpdServiceMethodList.surviceMethodDesc}"
										itemValue="#{drpdServiceMethodList.surviceMethodCode}" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Traffic Proposal No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>

								<p:inputText style="width:89%;height:18px; color:red;"
									readonly="true" id="requestNoId"
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.trafficProposalNo}"></p:inputText>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

						<center>
							<p:panel>
								<!-- Search button -->
								<p:commandButton styleClass="mainActionbtn" value="Search"
									id="btn-search" process="@form" update="@form"
									action="#{gsTrafficProposalBackingBean.btnSearch}"></p:commandButton>
								<!-- Clear button -->
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									id="btn-clear" process="@form" update="@form"
									action="#{gsTrafficProposalBackingBean.btnClear}">
								</p:commandButton>
							</p:panel>
						</center>
					</p:panel>
					<p:panel id="pnl-routeInfo"
						rendered="#{gsTrafficProposalBackingBean.renderPnlRouteInfo}">

						<p:dataTable id="tbl-trafficProRouteInfo" rendered="true"
							value="#{gsTrafficProposalBackingBean.tblTrafficProposalRouteInfo}"
							var="tblTrafficProposalRouteInfo" selectionMode="single"
							rows="10" paginator="true"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							tableStyle="width:auto" rowsPerPageTemplate="10,20"
							liveResize="true" rowIndexVar="index" styleClass="myTable"
							emptyMessage="No records are available"
							rowKey="#{tblTrafficProposalRouteInfo.seqNo}">

							<p:ajax event="rowSelect"
								listener="#{gsTrafficProposalBackingBean.onRouteRowSelect}"
								process="@form" update="@form" />

							<p:column headerText="Sequence No."
								style="text-align:left;width:10%">
								<h:outputText value="#{tblTrafficProposalRouteInfo.sequenceNo}"></h:outputText>
							</p:column>
							<p:column headerText="Origin" style="text-align:left;width:15%">
								<h:outputText value="#{tblTrafficProposalRouteInfo.originDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Destination"
								style="text-align:left;width:15%">
								<h:outputText
									value="#{tblTrafficProposalRouteInfo.destinationDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Via" style="text-align:left;width:15%">
								<h:outputText value="#{tblTrafficProposalRouteInfo.viaDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Province" style="text-align:left;width:15%">
								<h:outputText
									value="#{tblTrafficProposalRouteInfo.provinceDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Total Length (KM)"
								style="text-align:right;width:10%">
								<h:outputText value="#{tblTrafficProposalRouteInfo.totalLength}"></h:outputText>
							</p:column>
							<p:column headerText="Unserved Portion (KM)"
								style="text-align:right;width:10%">
								<h:outputText
									value="#{tblTrafficProposalRouteInfo.unservedPortion}"></h:outputText>
							</p:column>
							<p:column headerText="No. of Permits Req. to Issue"
								style="text-align:right;width:10%">
								<h:outputText
									value="#{tblTrafficProposalRouteInfo.noOfPermitsRequired}"></h:outputText>
							</p:column>

						</p:dataTable>
						<br></br>
						<p:panelGrid columns="6"
							columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-3, ui-grid-col-2,ui-grid-col-3,ui-grid-col-1"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:outputLabel value="Sequence No."></p:outputLabel>
							<p:inputText style="width:100%;height:18px;" id="txt-sequenceNo"
								maxlength="50" disabled="true"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.sequenceNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:outputLabel value="Province"></p:outputLabel>
							<p:inputText style="width:67%;height:18px;" id="txt-provinceNo"
								maxlength="50" disabled="true"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.provinceDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Origin"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:inputText style="width:100%;height:18px;" id="txt-origin"
								maxlength="50" disabled="true"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.originDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:outputLabel value="Total Length(KM)"></p:outputLabel>
							<p:inputNumber style="width:100%;height:18px;"
								id="txt-totalLength" maxlength="50" disabled="true"
								inputStyle="text-align: right"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.totalLength}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputNumber>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Destination"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:inputText style="width:100%;height:18px;" id="txt-destination"
								maxlength="50" disabled="true"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.destinationDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:panelGrid>
								<p:outputLabel value="Unserved Portion (KM)"></p:outputLabel>
							</p:panelGrid>
							<p:inputNumber style="width:100%;height:18px;"
								inputStyle="text-align: right" id="txt-unservedPortion"
								maxlength="50" disabled="true"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.unservedPortion}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputNumber>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Via"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:inputText style="width:100%;height:18px;" id="txt-via"
								maxlength="50" disabled="true"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.viaDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:panelGrid>
								<p:outputLabel value="No. Of Permits Req. To Issue"></p:outputLabel>
							</p:panelGrid>
							<p:inputNumber style="width:100%;height:18px;"
								id="txt-noOfPermits" maxlength="50"
								inputStyle="text-align: right"
								value="#{gsTrafficProposalBackingBean.gamiRouteDTO.noOfPermitsRequired}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputNumber>
							<p:panelGrid></p:panelGrid>


						</p:panelGrid>

						<br></br>
						<center>
							<p:panel>
								<!-- Add button -->
								<p:commandButton styleClass="mainActionbtn" value="Add"
									id="btn-add" process="@form"
									disabled="#{gsTrafficProposalBackingBean.disableByFlow}"
									update="@form,frmsuccess,frmrequiredField"
									action="#{gsTrafficProposalBackingBean.btnAdd}"></p:commandButton>
								<!-- Clear button -->
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									id="btn-clearRouteInfo" process="@form"
									update="@form,frmsuccess,frmrequiredField"
									action="#{gsTrafficProposalBackingBean.btnClearRouteInfo}">
								</p:commandButton>
							</p:panel>
						</center>
						<br></br>
						<p:dataTable id="tbl-trafficProSelectedRouteInfo" rendered="true"
							value="#{gsTrafficProposalBackingBean.tblTrafficProposalSelectedRouteInfo}"
							var="tblRouteInfo" rows="10" paginator="true"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							tableStyle="width:auto" rowsPerPageTemplate="10,20"
							liveResize="true" rowIndexVar="index" styleClass="myTable"
							emptyMessage="No records are available"
							rowKey="#{tblRouteInfo.sequenceNo}">

							<p:column headerText="Sequence No."
								style="text-align:left;width:10%">
								<h:outputText value="#{tblRouteInfo.sequenceNo}"></h:outputText>
							</p:column>
							<p:column headerText="Origin" style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.originDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Destination"
								style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.destinationDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Via" style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.viaDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Province" style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.provinceDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Total Length (KM)"
								style="text-align:right;width:10%">
								<h:outputText value="#{tblRouteInfo.totalLength}"></h:outputText>
							</p:column>

							<p:column headerText="No. of Permits Req. to Issue"
								style="text-align:right;width:10%">
								<h:outputText value="#{tblRouteInfo.noOfPermitsRequired}"></h:outputText>
							</p:column>

							<p:column headerText="Delete" style="text-align:center;width:10%">
								<p:commandLink
									disabled="#{gsTrafficProposalBackingBean.disableByFlow}"
									action="${gsTrafficProposalBackingBean.deleteTblTrafficProSelectedRouteRaw(tblRouteInfo)}"
									update="@form,frmsuccess,frmrequiredField">
									<p:graphicImage library="images" name="delete.png"
										style="width:24px;height:24px;" title="Delete"></p:graphicImage>
								</p:commandLink>
							</p:column>
						</p:dataTable>

						<br></br>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-8,ui-grid-col-1"
							layout="grid">

							<p:panel></p:panel>
							<p:panelGrid>
								<p:outputLabel>Special Remarks</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputTextarea style="width:98%" maxlength="200" rows="3"
									counter="displayA" counterTemplate="{0} characters remaining."
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.specialRemark}" />
								<br></br>

								<h:outputText id="displayA" />
							</p:panelGrid>
							<p:panel></p:panel>

							<p:panel></p:panel>
							<p:panelGrid>
								<p:outputLabel>Special Print Note</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputTextarea style="width:98%" maxlength="200" rows="3"
									counter="displayB" counterTemplate="{0} characters remaining."
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.specialPrintNote}" />
								<br></br>

								<h:outputText id="displayB" />
							</p:panelGrid>
							<p:panel></p:panel>

							<p:panel></p:panel>
							<p:panelGrid>
								<p:outputLabel>Suggestions</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputTextarea style="width:98%" maxlength="200" rows="3"
									counter="displayC" counterTemplate="{0} characters remaining."
									value="#{gsTrafficProposalBackingBean.trafficProposalDTO.suggestions}" />
								<br></br>

								<h:outputText id="displayC" />
							</p:panelGrid>
							<p:panel></p:panel>

						</p:panelGrid>


						<br></br>
