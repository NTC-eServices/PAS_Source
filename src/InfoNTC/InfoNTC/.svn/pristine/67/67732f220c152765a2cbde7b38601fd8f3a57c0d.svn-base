									action="#{gsTrafficProposalBackingBean.btnClearTrafficProposalInfo}">
								</p:commandButton>
								<p:commandButton id="btn-generateTenderRequest"
									styleClass="mainLinkButton" style="width:100%"
									value="Generate Tender Request"
									action="#{gsTrafficProposalBackingBean.btnClearTrafficProposalInfo}">
								</p:commandButton>
								<p:panelGrid></p:panelGrid>


								<p:commandButton id="btn-committeeApproval"
									styleClass="mainLinkButton" style="width:100%"
