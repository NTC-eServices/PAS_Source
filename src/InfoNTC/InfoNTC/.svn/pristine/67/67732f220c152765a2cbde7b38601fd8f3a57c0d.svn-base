								columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
								layout="grid">

								<p:panelGrid>
									<p:commandButton id="btn-uploadDocuments"
										styleClass="mainLinkButton" style="width:100%"
										value="Upload Documents" update="uploadDocumentsDialog"
										action="#{gsTrafficProposalBackingBean.btnDocumentMangement}">
									</p:commandButton>
								</p:panelGrid>
								<p:commandButton id="btn-update" styleClass="mainLinkButton"
									style="width:100%" value="Update"
									disabled="#{gsTrafficProposalBackingBean.disableByFlow}"
									update="@form,frmsuccess,frmrequiredField"
									rendered="#{gsTrafficProposalBackingBean.renderBtnUpdate}"
									action="#{gsTrafficProposalBackingBean.btnUpdate}"></p:commandButton>
								<p:commandButton id="btn-save" styleClass="mainLinkButton"
									style="width:100%" value="Save"
									disabled="#{gsTrafficProposalBackingBean.disableByFlow}"
									update="@form,frmsuccess,frmrequiredField"
									rendered="#{!gsTrafficProposalBackingBean.renderBtnUpdate}"
									action="#{gsTrafficProposalBackingBean.btnSave}"></p:commandButton>

								<p:commandButton id="btn-printTrafficPropsal"
									styleClass="mainLinkButton" style="width:100%"
