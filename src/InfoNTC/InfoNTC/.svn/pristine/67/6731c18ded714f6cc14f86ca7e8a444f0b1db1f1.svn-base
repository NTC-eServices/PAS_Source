	<queryString>
		<![CDATA[select right (a.pm_permit_no,5) as permitno, a.pm_valid_to,a.pm_route_no,b.rou_service_origine as origin ,b.rou_service_destination as destination ,b.rou_via as via,
b.rou_service_origine_sin as originSinhala,b.rou_service_destination_sin as detinationSinhala,b.rou_via_sin as viasinhala,
a.pm_service_type,c.description as servicetype,c.description_si as serviceTypeSinhala ,a.pm_permit_expire_date,a.pm_application_no,pm_route_flag
from public.nt_t_pm_application a
inner join nt_r_route b on a.pm_route_no=b.rou_number
inner join public.nt_r_service_types c on a.pm_service_type=c.code
