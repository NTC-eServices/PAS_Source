								selection="#{sisuAuthorizationTeamBackingBean.selectedRow}"
								rowKey="#{showData1.serviceRefNo}" selectionMode="single"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:100%" rowIndexVar="index"
								styleClass="myTable" emptyMessage="No data available.">

								<p:column headerText="" visible="false">
									<h:outputText value="" />
								</p:column>



								<p:column headerText="Service Reference No." style="width:20%">
									<h:outputText value="#{showData1.serviceRefNo}" />
								</p:column>

								<p:column headerText="Service No.">
									<h:outputText value="#{showData1.serviceNo}" />
								</p:column>

								<p:column headerText="Operator/Depo Name">
									<h:outputText value="#{showData1.nameOfOperator}" />
								</p:column>

								<p:column headerText="Service Start Date">
									<h:outputText value="#{showData1.serviceStartDateVal}" />
								</p:column>
								<p:column headerText="Service End Date">
									<h:outputText value="#{showData1.serviceEndDateVal}" />
								</p:column>
								<p:column headerText="Status" visible="false">
									<h:outputText value="#{showData1.status}" />
								</p:column>

							</p:dataTable>
						</center>
					</p:panel>

					<p:panel id="panelThree">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Name of the Authorized Person"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:68%;height:18px;"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.authPerson}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Designation"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu style="width:68%"
									onkeyup="this.value=this.value.toUpperCase();" filter="true"
									filterMatchMode="contains" tabindex="1"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.designationCode}">
									<f:selectItem itemLabel="Select Designation" itemValue="" />
									<f:selectItem itemLabel="Principle" itemValue="Principle" />
									<f:selectItem itemLabel="Vice-Principle" itemValue="Principle" />
									<f:selectItem itemLabel="Students" itemValue="Students" />

									<f:selectItem itemLabel="Other" itemValue="Other" />

								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="NIC No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:68%;height:18px;" maxlength="12"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.nicNo}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Telephone No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:68%;height:18px;"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.telNo}">
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Active"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectBooleanCheckbox
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.isActive}"></p:selectBooleanCheckbox>
							</p:panelGrid>

						</p:panelGrid>

						<br />

						<center>
							<p:commandButton value="Add" styleClass="mainActionbtn"
								update="@form"
								rendered="#{sisuAuthorizationTeamBackingBean.renderButton}"
								action="#{sisuAuthorizationTeamBackingBean.addButton}" />

						</center>
					</p:panel>


					<!--Data Table -->
					<p:panel id="panelFour">
						<center>
							<p:dataTable id="dataTable2" rows="5" paginator="true"
								var="showData"
								value="#{sisuAuthorizationTeamBackingBean.showDataOnGrid}"
								selection="#{sisuAuthorizationTeamBackingBean.selectedRow1}"
								rowKey="#{showData.nicNo}" selectionMode="single"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:100%" rowIndexVar="index"
								styleClass="myTable" emptyMessage="No data available.">

								<p:column headerText="" visible="false">
									<h:outputText value="" />
								</p:column>



								<p:column headerText="Name of the Authorized Person"
									style="width:20%">
									<h:outputText value="#{showData.authPerson}" />
								</p:column>

								<p:column headerText="Designation">
									<h:outputText value="#{showData.designationCode}" />
								</p:column>

								<p:column headerText="NIC No.">
									<h:outputText value="#{showData.nicNo}" />
								</p:column>

								<p:column headerText="Telephone No.">
									<h:outputText value="#{showData.telNo}" />
								</p:column>
								<p:column headerText="Active/Inactive">
									<h:outputText value="#{showData.isActive}" />
								</p:column>

								<p:column style=""
									visible="${sisuAuthorizationTeamBackingBean.renderButton}">
									<center>
										<p:commandLink
											action="${sisuAuthorizationTeamBackingBean.deleteButton()}"
											update="@form" process="@form">

											<f:setPropertyActionListener value="#{showData}"
												target="#{sisuAuthorizationTeamBackingBean.selectedRow1}" />


											<p:graphicImage library="images" name="delete.png"
												style="width:24px;height:24px;" title="delete"></p:graphicImage>
										</p:commandLink>
									</center>

								</p:column>
								<p:column style=""
									visible="${sisuAuthorizationTeamBackingBean.renderButton}">
									<p:commandLink
										action="#{sisuAuthorizationTeamBackingBean.editButton()}"
										update="@form">

										<f:setPropertyActionListener value="#{showData}"
											target="#{sisuAuthorizationTeamBackingBean.selectedRow1}" />

										<p:graphicImage library="images" name="edit.png"
											style="width:24px;height:24px;" title="edit"></p:graphicImage>
									</p:commandLink>


								</p:column>


							</p:dataTable>
						</center>
					</p:panel>

					<p:panel id="btnPanel">
						<center>
							<p:commandButton value="Save" styleClass="mainActionbtn"
								rendered="false" style="width:20%" />
							<p:commandButton value="Clear" styleClass="mainActionbtn"
								update="@form" style="width:20%"
								action="#{sisuAuthorizationTeamBackingBean.clearFields2}" />
							<p:commandButton value="Document Management"
								action="#{sisuAuthorizationTeamBackingBean.documentManagement}"
								update="uploadDocumentsDialog" styleClass="mainActionbtn"
								style="width:20%" />
						</center>
					</p:panel>

				</div>
			</div>

		</h:form>

		<p:dialog id="errorMSG" global="true" modal="true" header="Error"
			width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{sisuAuthorizationTeamBackingBean.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog id="successMSG" global="true" modal="true" header="Success"
			width="30%" severity="info" widgetVar="successMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{sisuAuthorizationTeamBackingBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog
			styleClass="#{editPermitIssuanceBackingBean.strSelectedTitle}"
			id="uploadDocumentsDialog" global="true" modal="true"
			header="Upload Documents" width="80%" height="400px" severity="info"
			widgetVar="uploadDocument" closeOnEscape="true" resizable="false"
			style="postion: absolute;">


			<h:form id="frmdoc">

				<p:tabView>
					<p:tab title="Upload Documents">

						<p:accordionPanel>
							<p:growl id="msgs" showDetail="false" />
							<p:tab title="Mandatory Documents">


								<h:panelGrid>
									<p:dataTable id="abc" tableStyle="width:auto"
										var="docMandatory"
										value="#{sisuAuthorizationTeamBackingBean.mandatoryList}"
										rows="5" paginator="true" sortOrder="ascending"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,10,15">

										<p:column headerText="Code" style="width: 100px;">
											<h:outputText value="#{docMandatory.doc_Code}" />
										</p:column>
										<p:column headerText="Description" style="width: 300px;">
											<h:outputText value="#{docMandatory.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path" style="width: 300px;">

											<h:outputText value="#{docMandatory.uploadFilePath}" />
										</p:column>

										<p:column
											headerText="#{editPermitIssuanceBackingBean.strSelectedTitle}"
											style="width: 150px !important;">

											<p:fileUpload
												fileUploadListener="#{documentManagementBean.saveSisuSariyaAgreementRenewalsMandatoryUploadDocuments}"
												mode="advanced" dragDropSupport="false" multiple="true"
												auto="true" showButtons="false" update="@form" fileLimit="1"
												accept=".pdf,.jpg,.jpeg,.png"
												allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
												

												<f:attribute name="uploadedObj" value="#{docMandatory}" />

											</p:fileUpload>

										</p:column>



									</p:dataTable>
								</h:panelGrid>

							</p:tab>
						</p:accordionPanel>

						<br></br>
						<br></br>

						<p:accordionPanel>
							<p:growl id="msgse" showDetail="false" />
							<p:tab title="Optional Documents">


								<h:panelGrid>
									<p:dataTable id="abcd" tableStyle="width:auto"
										var="docOptional"
										value="#{sisuAuthorizationTeamBackingBean.optionalList}"
										rows="5" paginator="true" sortOrder="ascending"
										scrollable="true" scrollHeight="100%" liveResize="true"
										liveScroll="true"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,10,15">



										<p:column headerText="Code" style="width: 100px;">
											<h:outputText value="#{docOptional.doc_Code}" />
										</p:column>
										<p:column headerText="Description" style="width: 300px;">
											<h:outputText value="#{docOptional.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path" style="width: 300px;">
											<h:outputText value="#{docOptional.uploadOptionalFilePath}" />
										</p:column>

										<p:column
											headerText="#{editPermitIssuanceBackingBean.strSelectedTitle}"
											style="width: 150px;">
											<p:fileUpload
												fileUploadListener="#{documentManagementBean.saveSisuSariyaAgreementRenewalsOptionalUploadDocuments}"
												mode="advanced" dragDropSupport="false" multiple="true"
												auto="true" showButtons="false" update="@form" fileLimit="1"
