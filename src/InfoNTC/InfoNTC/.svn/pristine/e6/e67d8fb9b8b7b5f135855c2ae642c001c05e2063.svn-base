		<h:form id="form">
			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Sisu Sariya Authorization Team
					Maintenance</h1>
				<div style="width: 90%;" class="subDiv">


					<p:panel id="panelOne">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Start Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar styleClass="dateTimeField" navigator="true"
									showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
									style="width: 72% ;"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.serviceStartDateObj}">
									<p:ajax event="dateSelect"
										listener="#{sisuAuthorizationTeamBackingBean.onServicetStartDateChange}"
										update="serRefNo" />


								</p:calendar>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="End Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar styleClass="dateTimeField" navigator="true"
									showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
									style="width: 68% !important;"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.serviceEndDateObj}">
									<p:ajax event="dateSelect"
										listener="#{sisuAuthorizationTeamBackingBean.onRequestEndDateChange}"
										update="@form" />


								</p:calendar>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Reference No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu style="width:68%" id="serRefNo"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.serviceRefNo}"
									filter="true" filterMatchMode="contains" tabindex="1">

									<p:ajax
										listener="#{sisuAuthorizationTeamBackingBean.fillServiceNo}"
										update="serNo" />

									<f:selectItem itemLabel="--Select Service Reference No.--"
										itemValue="" />
									<f:selectItems
										value="#{sisuAuthorizationTeamBackingBean.serviceRefNOList}"
										var="serviceRefNOList"
										itemLabel="#{serviceRefNOList.serviceRefNo}"
										itemValue="#{serviceRefNOList.serviceRefNo}"></f:selectItems>

								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu style="width:68%" id="serNo"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.serviceNo}"
									disabled="#{sisuAuthorizationTeamBackingBean.disableServiceNo}"
									filter="true" filterMatchMode="contains" tabindex="1">

									<f:selectItem itemLabel="---Select Service No.--" itemValue="" />
									<f:selectItems
										value="#{sisuAuthorizationTeamBackingBean.serviceNOList}"
										var="serviceNOList" itemLabel="#{serviceNOList.serviceNo}"
										itemValue="#{serviceNOList.serviceNo}"></f:selectItems>

								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<!-- Operator/Depo Name Start -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Operator/Depo Name</p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="operatorDepoName" style="width:68%;"
									effect="fold"
									value="#{sisuAuthorizationTeamBackingBean.sisuSariyaTeamAuthDTO.nameOfOperator}"
									filter="true" filterMatchMode="contains">
									<f:selectItem itemLabel="-- Select Operator/Depo Name --"
										itemValue="" />
									<f:selectItems
										value="#{sisuAuthorizationTeamBackingBean.operatorDepoNameList}"
										var="list" itemValue="#{list.nameOfOperator}"
										itemLabel="#{list.nameOfOperator}" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<!-- Operator/Depo Name End -->

						</p:panelGrid>

						<br />

						<center>
