							<p:commandButton value="Clear" styleClass="mainActionbtn"
								update="@form"
								action="#{sisuAuthorizationTeamBackingBean.clearFields}" />

						</center>
					</p:panel>


					<!--Data Table -->
					<p:panel id="panelTwo">
