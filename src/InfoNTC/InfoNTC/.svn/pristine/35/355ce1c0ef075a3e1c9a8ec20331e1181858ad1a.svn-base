package lk.informatics.ntc.model.dto;

import java.io.Serializable;

public class TerminalArrivalDepartureTimeDTO implements Serializable {

	private static final long serialVersionUID = 2578785460L;
	
	private String tripType; //O-Arriving from origin //D-Arriving from destination
	private String tripId;
	private String endTime; //Estimated Arrival //end-time of O, end-time of D
	private String startTime; //start-time of O, start-time of D
	private String actualArrivalDeparture;
	private String authorized; //Y, N
	private String authorizationFailReasons;
	private Long sequence;
	
	public String getTripType() {
		return tripType;
	}
	public void setTripType(String tripType) {
		this.tripType = tripType;
	}
	public String getTripId() {
		return tripId;
	}
	public void setTripId(String tripId) {
		this.tripId = tripId;
	}
	public String getEndTime() {
		return endTime;
	}
	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}
	public String getStartTime() {
		return startTime;
	}
	public void setStartTime(String startTime) {
		this.startTime = startTime;
	}
	public String getAuthorized() {
		return authorized;
	}
	public String getActualArrivalDeparture() {
		return actualArrivalDeparture;
	}
	public void setActualArrivalDeparture(String actualArrivalDeparture) {
		this.actualArrivalDeparture = actualArrivalDeparture;
	}
	public void setAuthorized(String authorized) {
		this.authorized = authorized;
	}
	public String getAuthorizationFailReasons() {
		return authorizationFailReasons;
	}
	public void setAuthorizationFailReasons(String authorizationFailReasons) {
		this.authorizationFailReasons = authorizationFailReasons;
	}
	public Long getSequence() {
		return sequence;
	}
	public void setSequence(Long sequence) {
		this.sequence = sequence;
	}
	
}
