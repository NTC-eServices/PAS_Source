	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}
/* padding: 7px 25px 7px 25px !important; */
.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #404d6f !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
	align-self: center;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 79% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}

.ui-datatable-scrollable .ui-datatable-scrollable-header {
	min-height: 20px;
}

.ui-datatable-scrollable-body {
	height: 125px !important;
}

.genReqNumButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	text-decoration: none !important;
	color: white !important;
	border-radius: 5px !important;
}

.ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	text-shadow: none;
}

.ui-accordion .ui-accordion-header {
	cursor: pointer;
	position: relative;
	margin-top: 1px;
	min-height: 0;
	padding: .5em .5em .5em 2.2em;
	background-color: cornflowerblue;
}

.ui-autocomplete-itemtip {
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box;
}
</style>

		<div class="mainDiv">

			<!-- header -->

			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">SIM Transfer</h1>

			<div style="width: 95%;" class="subDiv">

				<p:panel id="pnl-sim-reg">

					<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
						layout="grid">


						<h:form id="simRegistrationForm">

							<!-- Search area -->

							<p:panel
								style="border: #404d6f solid 1px !important;border-radius: 25px;background-color: #bdcafc;
								box-shadow: 3px 3px 3px grey; margin-bottom: 15px;">

								<!-- SIM REG No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="SIM Registration No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="simRegSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simTransferBean.simSearchDTO.simRegNo}">

											<f:selectItem itemLabel=" -- Select SIM Reg No. -- "
												itemValue="" />
											<f:selectItems value="#{simTransferBean.simDetailsList}"
												var="dto" itemValue="#{dto.simRegNo}"
												itemLabel="#{dto.simRegNo}" />


										</p:selectOneMenu>
									</div>
									<div class="ui-g-3"></div>
								</div>

								<!-- Permit No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="Permit No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="permitNoSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simTransferBean.simSearchDTO.permitNo}">

											<f:selectItem itemLabel=" -- Select Permit No. -- "
												itemValue="" />
											<f:selectItems value="#{simTransferBean.simDetailsList}"
												var="dto" itemValue="#{dto.permitNo}"
												itemLabel="#{dto.permitNo}" />

										</p:selectOneMenu>
									</div>
									<div class="ui-g-3"></div>
								</div>

								<!-- Bus No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="Bus No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="busNoSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simTransferBean.simSearchDTO.busNo}">

											<f:selectItem itemLabel=" -- Select Bus No. -- " itemValue="" />
											<f:selectItems value="#{simTransferBean.simDetailsList}"
												var="dto" itemValue="#{dto.busNo}" itemLabel="#{dto.busNo}" />

										</p:selectOneMenu>
									</div>

									<!-- Search Button -->
									<div class="ui-g-3">
										<p:commandButton id="searchBtn" styleClass="otherLinkButton"
											style="width:80%;" value="Search" process="@all"
											update="@form" action="#{simTransferBean.searchAction}">
										</p:commandButton>
									</div>
								</div>

							</p:panel>

							<!-- Input Details part -->

							<p:panelGrid columns="4" style="margin-bottom: 15px;"
								columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
								layout="grid">

								<!-- SIM Registration Number -->

								<p:panelGrid>
									<p:outputLabel value="SIM Registration No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="simRegNoId" style="width:81%" filter="true"
											placeholder="Search SIM Details" filterMatchMode="contains"
											tabindex="1" effect="fold" readonly="true"
											value="#{simTransferBean.simRegistrationDTO.simRegNo}" />
									</span>
								</p:panelGrid>

								<!-- Service Category -->

								<p:panelGrid>
									<p:outputLabel value="Service Category "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:selectOneMenu id="serviceCategoryId" style="width:73%;"
										effect="fold" filter="true" filterMatchMode="contains"
										disabled="true"
										value="#{simTransferBean.simRegistrationDTO.serviceCategory}">

										<p:ajax listener="#{simTransferBean.onServiceCategoryChange}"
											update="@form"></p:ajax>

										<f:selectItem itemLabel=" -- Select Service Category -- "
											itemValue="" />
										<f:selectItem itemLabel="NTC" itemValue="ntc" />
										<f:selectItem itemLabel="Sisu Sariya" itemValue="sisu" />
										<f:selectItem itemLabel="Gami Sariya" itemValue="gami" />
										<f:selectItem itemLabel="Nisi Sariya" itemValue="nisi" />
										<f:selectItem itemLabel="Other" itemValue="other" />

									</p:selectOneMenu>
								</p:panelGrid>

								<!-- Permit No. -->

								<p:panelGrid>
									<p:outputLabel value="Permit No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="permitNoId" style="width:81%" filter="true"
											readonly="true" filterMatchMode="contains" tabindex="1"
											effect="fold" placeholder="Permit Number"
											value="#{simTransferBean.simRegistrationDTO.permitNo}"
											onkeyup="this.value=this.value.toUpperCase();"
											rendered="#{simTransferBean.simRegistrationDTO.serviceCategory == 'other'}" />

										<p:autoComplete id="permitNoId2"
											value="#{simTransferBean.simRegistrationDTO.permitNo}"
											completeMethod="#{simTransferBean.completePermitNo}"
											size="29" readonly="true"
											rendered="#{simTransferBean.simRegistrationDTO.serviceCategory != 'other'}"
											onkeyup="this.value=this.value.toUpperCase();"
											forceSelection="true" scrollHeight="500"
											placeholder="Permit Number">
											<p:ajax event="itemSelect"
												listener="#{simTransferBean.onPermitNoSelect}"
												process="@all" update="@form" />
										</p:autoComplete>
									</span>
								</p:panelGrid>

								<!-- Bus No. -->

								<p:panelGrid>
									<p:outputLabel value="Bus No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="BusNoId"
											style="width:81%" filter="true" readonly="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="Bus Number"
											value="#{simTransferBean.simRegistrationDTO.busNo}"
											onkeyup="this.value=this.value.toUpperCase();"
											rendered="#{simTransferBean.simRegistrationDTO.serviceCategory == 'other'}" />

										<p:autoComplete id="BusNoId2"
											value="#{simTransferBean.simRegistrationDTO.busNo}"
											completeMethod="#{simTransferBean.completeBusNo}" size="29"
											readonly="true"
											rendered="#{simTransferBean.simRegistrationDTO.serviceCategory != 'other'}"
											onkeyup="this.value=this.value.toUpperCase();"
											forceSelection="true" scrollHeight="500"
											placeholder="Bus Number">
											<p:ajax event="itemSelect"
												listener="#{simTransferBean.onBusNoSelect}" process="@all"
												update="@form" />
										</p:autoComplete>
									</span>
								</p:panelGrid>


								<!-- SIM No. -->

								<p:panelGrid>
									<p:outputLabel value="SIM No. (Mobile No.) "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="simNoId"
											style="width:81%" filter="true" readonly="true"
											filterMatchMode="contains" tabindex="1" maxlength="10"
											placeholder="SIM Number"
											value="#{simTransferBean.simRegistrationDTO.simNo}">
											<p:keyFilter regEx="/[0-9-]/i" />
										</p:inputText>
									</span>
								</p:panelGrid>

							</p:panelGrid>

							<!-- SIM Transfer -->
							<p:panelGrid columns="4" style="margin-bottom: 15px;"
								columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
								layout="grid">
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<center>
										<p:commandButton id="simTransferBtnId"
											styleClass="otherLinkButton" style="width:80%;"
											value="SIM Transfer" process="@all" update="@form"
											disabled="#{simTransferBean.disableButton}"
											action="#{simTransferBean.simTransferBtnAction}">
										</p:commandButton>
									</center>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>
							</p:panelGrid>

							<!-- EMI Number Panel -->


							<p:panel
								style="border: #404d6f solid 1px !important;border-radius: 25px;background-color: #e4eeff;
						box-shadow: 3px 3px 3px grey; margin-bottom: 15px;">

								<p:panelGrid columns="4" style="margin-bottom: 15px;"
									columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
									layout="grid">

									<!-- EMI Number -->

									<p:panelGrid>
										<p:outputLabel value="EMI Number "></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>
									<p:panelGrid>
										<p:inputText id="emiNoId" style="width:81%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="EMI Number" maxlength="20"
											value="#{simTransferBean.simRegistrationDTO.emiNo}"
											disabled="#{simTransferBean.disableEmiNo}">
											<p:keyFilter regEx="/[0-9-]/i" />
										</p:inputText>

									</p:panelGrid>

								</p:panelGrid>


								<p:panelGrid columns="7"
									columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-1,ui-grid-col-2,ui-grid-col-2,ui-grid-col-2,ui-grid-col-2"
									layout="grid">

									<!-- Bus Number -->

									<p:panelGrid>
										<center>
											<p:outputLabel value="Bus No."></p:outputLabel>
											<p:outputLabel value="*" style="color:red;"></p:outputLabel>
										</center>
									</p:panelGrid>
									<p:panelGrid>
										<span class="ui-float-label"> <p:inputText
												id="emiBusNoId" style="width:100%" filter="true"
												filterMatchMode="contains" tabindex="1" effect="fold"
												placeholder="Bus Number"
												onkeyup="this.value=this.value.toUpperCase();"
												value="#{simTransferBean.emiDTO.emiBusNo}" />
										</span>
									</p:panelGrid>

									<!-- Status -->

									<p:panelGrid>
										<center>
											<p:outputLabel value="Status"></p:outputLabel>
										</center>
									</p:panelGrid>
									<p:panelGrid>
										<span class="ui-float-label"> <p:selectOneMenu
												id="emiStatusId" value="#{simTransferBean.emiDTO.emiStatus}"
												style="width:70%;" filterMatchMode="contains" tabindex="1"
												effect="fold">
												<f:selectItem itemLabel="Active" itemValue="Active" />
												<f:selectItem itemLabel="In-active" itemValue="Inactive" />
											</p:selectOneMenu>
										</span>
									</p:panelGrid>

									<!-- Transaction Date -->

									<p:panelGrid>
										<center>
											<p:outputLabel value="Transaction Date"></p:outputLabel>
										</center>
									</p:panelGrid>
									<p:panelGrid>
										<p:calendar id="reIssueDate" style="height:18px;"
											maxdate="today" inputStyle="width:50%;" navigator="true"
											showOn="button" locale="en_US" pattern="dd/MM/yyyy"
											placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
											readonlyInput="true" disabled="false"
											value="#{simTransferBean.emiDTO.emiReIssueDate}">
										</p:calendar>
									</p:panelGrid>

									<!-- Add Button -->
									<p:commandButton id="addBtn" styleClass="otherLinkButton"
										style="width:100%;" value="Add" process="@all" update="@form"
										disabled="#{simTransferBean.disableEmiNo}"
										action="#{simTransferBean.addEmiBtnAction}">

									</p:commandButton>

								</p:panelGrid>

								<!-- EMI Data Grid -->

								<p:dataTable id="emiDT" styleClass="myTable-two" var="emiDto"
									style="margin-top:2%; margin-bottom:3%;" rowIndexVar="index"
									rowsPerPageTemplate="2,3" resizableColumns="false"
									liveResize="true" emptyMessage="No records are available"
									value="#{simTransferBean.emiDTOList}"
									sortBy="#{emiDto.emiStatus}">

									<!-- Index -->
									<p:column width="8%" headerText="Index"
										style="text-align:left;">
										<h:outputText value="#{index + 1}" />
									</p:column>

									<!-- Bus No. -->
									<p:column headerText="Bus No." style="text-align:left;">
										<h:outputText value="#{emiDto.emiBusNo}" />
									</p:column>

									<!-- Status -->
									<p:column headerText="Status" style="text-align:left;">
										<h:outputText value="#{emiDto.emiStatus}" />
									</p:column>

									<!-- Re Issue Date -->
									<p:column headerText="Transaction Date"
										style="text-align:left;">
										<h:outputText value="#{emiDto.emiReIssueDateString}">
										</h:outputText>
									</p:column>

									<!-- delete -->

									<p:column width="9%">
										<center>
											<p:commandLink action="${simTransferBean.emiDelete()}"
												disabled="${simTransferBean.disableEmiDelete}"
												process="@all" update="emiDT">

												<f:setPropertyActionListener value="#{emiDto}"
													target="${simTransferBean.selectEmiDTO}" />

												<p:graphicImage library="images" name="delete.png"
													style="width:24px;height:24px;" title="delete"></p:graphicImage>
											</p:commandLink>
										</center>

									</p:column>

								</p:dataTable>

							</p:panel>

							<!-- Data -->

							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4,ui-grid-col-3"
								layout="grid">

								<!-- Receivers Name -->
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Receivers Name "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="receiversNameId" style="width:62%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="Receivers Name" readonly="true"
											value="#{simTransferBean.simRegistrationDTO.receiversName}" />
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- NIC Number -->
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="NIC Number "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="nicNoId"
											style="width:62%" filter="true" filterMatchMode="contains"
											tabindex="1" effect="fold" readonly="true"
											placeholder="NIC Number" maxlength="15"
											onkeyup="this.value=this.value.toUpperCase();"
											value="#{simTransferBean.simRegistrationDTO.nicNo}">
											<p:keyFilter regEx="/[a-z0-9-]/i" />
										</p:inputText>
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Issue Date -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Issue Date "></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:calendar id="issueDate" style="height:18px;" maxdate="today"
										navigator="true" showOn="button" locale="en_US"
										pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
										yearRange="c-100:c+100" readonlyInput="true" disabled="true"
										value="#{simTransferBean.simRegistrationDTO.issueDate}">
										<p:ajax event="dateSelect" update="validUntilDate"
											process="@all" />
									</p:calendar>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Valid Until -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Valid Until "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:calendar id="validUntilDate" style="height:18px;"
										mindate="#{simTransferBean.simRegistrationDTO.issueDate}"
										navigator="true" showOn="button" locale="en_US"
										pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
										yearRange="c-100:c+100" readonlyInput="true" disabled="true"
										value="#{simTransferBean.simRegistrationDTO.validUntilDate}">
									</p:calendar>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>


								<!-- Payment Receipt Number -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Payment Receipt No."></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="paymentReceiptNoId" style="width:62%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											readonly="true" placeholder="Enter Receipt Number"
											onkeyup="this.value=this.value.toUpperCase();"
											value="#{simTransferBean.simRegistrationDTO.paymentReceiptNo}" />
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Remarks -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Remarks"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputTextarea style="width:80%;height:60px;" id="remarkId"
										value="#{simTransferBean.simRegistrationDTO.remarks}"
										placeholder="Enter Message" maxlength="300" rows="5"
										readonly="true">
									</p:inputTextarea>
									<br />
									<h:outputText id="display" />
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

							</p:panelGrid>

							<!-- Main Button Panel -->

							<p:panelGrid columns="3"
								columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
								layout="grid" style="margin-top:2%;margin-bottom:2%;">


								<!-- Update Button -->

								<p:commandButton id="updateBtn" styleClass="mainLinkButton"
									style="width:100%" value="Update" process="@all" update="@form"
									action="#{simTransferBean.updateBtnAction}"
									disabled="#{simTransferBean.disableButtonUp}">

								</p:commandButton>

								<!-- Clear Button -->

								<p:commandButton id="clearBtn" styleClass="mainLinkButton"
									style="width:100%" value="Clear" process="@all" update="@form"
									action="#{simTransferBean.clearBtnAction}">

								</p:commandButton>


								<!-- View button -->
								<p:commandButton id="viewBtn" styleClass="mainLinkButton"
									style="width:100%" value="View" process="@all"
									update="hisDetForm"
									disabled="#{simTransferBean.disableViewBtn}"
									action="#{simTransferBean.viewBtnAction}">

								</p:commandButton>



							</p:panelGrid>

						</h:form>


					</p:panelGrid>

				</p:panel>



			</div>
		</div>
		<p:dialog header="View Details" widgetVar="hisDet" modal="true"
			height="50%" width="80%">
			<h:form id="hisDetForm">
				<br />


				<center>
					<p:panel>
						<br />

						<p:dataTable var="a" value="#{simTransferBean.oldPermitDetails}"
							id="permitDataTable" tableStyle="text-align:left; width:100%"
							paginator="true" rows="10" rowsPerPageTemplate="10,20"
							resizableColumns="false" liveResize="true" rowIndexVar="index"
							styleClass="myTable" emptyMessage="No data are available">

							<p:column headerText="Permit No." style="text-align:left">
								<h:outputText value="#{a.permitNo}" />
							</p:column>
							<p:column headerText="Vehicle No." style="text-align:left">
								<h:outputText value="#{a.emiBusNo}" />
							</p:column>
							<p:column headerText="SIM No." style="text-align:left;">
								<h:outputText value="#{a.simNo}" />
							</p:column>

							<p:column headerText="Emi No." style="text-align:left;">
								<h:outputText value="#{a.emiNo}" />
							</p:column>

							<p:column headerText="Owner Name" style="text-align:left;">
								<h:outputText value="#{a.receiversName}" />
							</p:column>
							<p:column headerText="Issue Date" style="text-align:left;">
								<h:outputText value="#{a.issueDateN}" />
							</p:column>



						</p:dataTable>
					</p:panel>
				</center>



			</h:form>
		</p:dialog>

	</ui:define>
</ui:composition>