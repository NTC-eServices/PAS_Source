<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>

	<ui:define name="content">

		<style>
.dateTimeField input {
	width: 100px;
}

.dateTimeField img {
	vertical-align: right;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.ui-input {
	margin-top: -20px;
}
</style>



		<h:form id="form">
			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Log Sheet Received Maintenance
					(Sisu Sariya/Gami Sariya/Nisi Seriya)</h1>
				<div style="width: 90%;" class="subDiv">


					<p:panel id="panelOne">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Type">
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>


								<p:selectOneMenu id="serviceType" style="width:68%"
									value="#{logSheetRecievBackingBean.sisuSeriyaDto.serviceTypeCode}"
									effect="fold" filter="true" filterMatchMode="contains"
									tabindex="1">

									<p:ajax
										listener="#{logSheetRecievBackingBean.ajaxDisableBeforeSearch}"
										update="@form" />

									<f:selectItem itemLabel="--Select Service Type--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems value="#{logSheetRecievBackingBean.serviceType}"
										var="serviceType" itemValue="#{serviceType.serviceTypeCode}"
										itemLabel="#{serviceType.serviceTypeDes}" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service  No./Service Agreement No.">

								</p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="serviceAgt" style="width:68%"
									value="#{logSheetRecievBackingBean.sisuSeriyaDto.serviceNo}"
									effect="fold" filter="true" filterMatchMode="contains"
									tabindex="1"
									disabled="#{logSheetRecievBackingBean.disabledServiceAgreementNo}">

									<f:selectItem itemLabel="--Service Agreement No.--"
										itemValue="" noSelectionOption="true" />
									<f:selectItems
										value="#{logSheetRecievBackingBean.serviceAgreementNo}"
										var="serviceAgreementNo"
										itemValue="#{serviceAgreementNo.serviceNo}"
										itemLabel="#{serviceAgreementNo.serviceNo}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Reference No.">
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="serviceRef" style="width:68%"
									value="#{logSheetRecievBackingBean.sisuSeriyaDto.serviceRefNo}"
									effect="fold" filter="true" filterMatchMode="contains"
									tabindex="1"
									disabled="#{logSheetRecievBackingBean.disabledServiceRefNo}">

									<p:ajax
										listener="#{logSheetRecievBackingBean.ajaxFilterAgrementNo}"
										update="serviceAgt" />

									<f:selectItem itemLabel="--Service Reference No.--"
										itemValue="" noSelectionOption="true" />
									<f:selectItems
										value="#{logSheetRecievBackingBean.serviceReferenceNo}"
										var="serviceReferenceNoVar"
										itemValue="#{serviceReferenceNoVar.serviceRefNo}"
										itemLabel="#{serviceReferenceNoVar.serviceRefNo}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>



							<p:panelGrid>
								<p:outputLabel value="Year"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:68%;height:18px;" id="logYear"
									value="#{logSheetRecievBackingBean.logSheetYear}"
									padControl="false" maxlength="4" maxValue="9999"
									onkeyup="this.value=this.value.toUpperCase();"
									disabled="#{logSheetRecievBackingBean.disabledYear}">
									<p:keyFilter regEx="/[\d]/i" />
									<p:ajax update="logYear,frmsuccess,frmError,frmAlert,@form"
										listener="#{logSheetRecievBackingBean.yearValidator}" />
								</p:inputText>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Month">

								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu effect="fold" style="width:68%;"
									value="#{logSheetRecievBackingBean.sisuSeriyaDto.stringMonth}"
									filter="true" filterMatchMode="contains" tabindex="1"
									disabled="#{logSheetRecievBackingBean.disabledMonth}">
									<f:selectItem itemLabel="-- Select Month --" itemValue="" />
									<f:selectItem itemLabel="JANUARY" itemValue="JANUARY" />
									<f:selectItem itemLabel="FEBRUARY" itemValue="FEBRUARY" />
									<f:selectItem itemLabel="MARCH" itemValue="MARCH" />
									<f:selectItem itemLabel="APRIL" itemValue="APRIL" />
									<f:selectItem itemLabel="MAY" itemValue="MAY" />
									<f:selectItem itemLabel="JUNE" itemValue="JUNE" />
									<f:selectItem itemLabel="JULY" itemValue="JULY" />
									<f:selectItem itemLabel="AUGUST" itemValue="AUGUST" />
									<f:selectItem itemLabel="SEPTEMBER" itemValue="SEPTEMBER" />
									<f:selectItem itemLabel="OCTOBER" itemValue="OCTOBER" />
									<f:selectItem itemLabel="NOVEMBER" itemValue="NOVEMBER" />
									<f:selectItem itemLabel="DECEMBER" itemValue="DECEMBER" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>

							<p:panelGrid>
								<p:outputLabel value="Bus No.">

								</p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="busNo" style="width:68%"
									value="#{logSheetRecievBackingBean.sisuSeriyaDto.busRegNo}"
									effect="fold" filter="true" filterMatchMode="contains"
									tabindex="1"
									disabled="#{logSheetRecievBackingBean.disabledServiceAgreementNo}">

									<f:selectItem itemLabel="--Bus No.--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems value="#{logSheetRecievBackingBean.busNoList}"
										var="busNoList" itemValue="#{busNoList}"
										itemLabel="#{busNoList}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>



							<p:panelGrid></p:panelGrid>



							<p:panelGrid>
								<p:outputLabel value="Operator Name">

								</p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="opName" style="width:68%"
									value="#{logSheetRecievBackingBean.sisuSeriyaDto.nameOfOperator}"
									effect="fold" filter="true" filterMatchMode="contains"
									tabindex="1"
									disabled="#{logSheetRecievBackingBean.disabledServiceAgreementNo}">

									<f:selectItem itemLabel="--Operator Name--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems
										value="#{logSheetRecievBackingBean.operatorNameList}"
										var="operatorNameList" itemValue="#{operatorNameList}"
										itemLabel="#{operatorNameList}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>



						</p:panelGrid>

						<br />

						<center>
							<!-- Search button -->
							<p:commandButton styleClass="mainActionbtn" value="Search"
								update="@form"
								action="#{logSheetRecievBackingBean.searchAction}"></p:commandButton>
							<!-- Clear button -->
							<p:commandButton styleClass="mainActionbtn" value="Clear"
								update="@form" action="#{logSheetRecievBackingBean.clearData}"></p:commandButton>

						</center>
					</p:panel>





					<!--Data Table -->
					<p:panel id="panelTwo">
						<center>

							<p:dataTable id="dataTable" rows="5" paginator="true" var="log"
								rowKey="#{log.logRefNo}" selectionMode="single"
								selection="#{logSheetRecievBackingBean.selectDTO}"
								value="#{logSheetRecievBackingBean.showSearchData}"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:100%;font-size:12px;"
								rowIndexVar="index" sortBy="#{log.serviceRefNo}"
								sortOrder="ascending" styleClass="myTable"
								emptyMessage="No records available">

								<p:column headerText="Service Reference No."
									style="text-align:left;">
									<h:outputText style="text-align:left;"
										value="#{log.serviceRefNo}" />
								</p:column>
								

								<p:column headerText="Year" style="width:40px">
									<h:outputText style="text-align:left;" value="#{log.year}" />
								</p:column>

								<!-- For the Database, should pass as a number, where as item value should be number,
								 Month and Received Date should be editable -->

								<p:column headerText="Month" style="text-align:left;">
									<p:selectOneMenu effect="fold" style="width:68%;" id="monthId"
										disabled="#{!log.logRefCheck}" value="#{log.stringMonth}"
										filter="true" filterMatchMode="contains" tabindex="1">
										<f:selectItem itemLabel=" Select Month " itemValue="" />
										<f:selectItem itemLabel="JANUARY" itemValue="JANUARY" />
										<f:selectItem itemLabel="FEBRUARY" itemValue="FEBRUARY" />
										<f:selectItem itemLabel="MARCH" itemValue="MARCH" />
										<f:selectItem itemLabel="APRIL" itemValue="APRIL" />
										<f:selectItem itemLabel="MAY" itemValue="MAY" />
										<f:selectItem itemLabel="JUNE" itemValue="JUNE" />
										<f:selectItem itemLabel="JULY" itemValue="JULY" />
										<f:selectItem itemLabel="AUGUST" itemValue="AUGUST" />
										<f:selectItem itemLabel="SEPTEMBER" itemValue="SEPTEMBER" />
										<f:selectItem itemLabel="OCTOBER" itemValue="OCTOBER" />
										<f:selectItem itemLabel="NOVEMBER" itemValue="NOVEMBER" />
										<f:selectItem itemLabel="DECEMBER" itemValue="DECEMBER" />
										
										<p:ajax event="change" />
									</p:selectOneMenu>
								</p:column>

								<p:column headerText="Log Reference No."
									style="text-align:left;">
									<h:outputText value="#{log.logRefNo}" />
								</p:column>

								<p:column headerText="" style="width:38px;">
									<p:selectBooleanCheckbox value="#{log.logRefCheck}" id="box">
										<p:ajax event="change" update="modelInput,monthId" />
									</p:selectBooleanCheckbox>
								</p:column>


								<p:column headerText="Received Date" style="text-align:left;">

									<p:calendar id="modelInput" styleClass="dateTimeField"
										navigator="true" disabled="#{!log.logRefCheck}"
										value="#{log.receivedDate}" showOn="button"
										pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY">
										<p:ajax event="dateSelect" />
									</p:calendar>

								</p:column>

							</p:dataTable>
						</center>
					</p:panel>

					<p:panel id="btnPanel">
						<center>

							<!-- Save button -->
							<p:commandButton styleClass="mainActionbtn" value="Save"
								update="@form, dataTable"
								rendered="#{logSheetRecievBackingBean.renderButton}"
								action="#{logSheetRecievBackingBean.saveLogSheetRecievedData}"></p:commandButton>
							<!-- Cancel button -->
							<p:commandButton styleClass="mainActionbtn" value="Cancel"
								update="@form" action="#{logSheetRecievBackingBean.clearData}"></p:commandButton>

						</center>
					</p:panel>

				</div>
			</div>

		</h:form>

		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel value="#{logSheetRecievBackingBean.errorMessage}" ></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Info" width="30%" severity="info" widgetVar="successMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{logSheetRecievBackingBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMSG"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="#{logSheetRecievBackingBean.alertMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmAlert" onclick="PF('alertMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>


		<p:dialog styleClass="" id="comfirmMSG" global="true" modal="true"
			header="Confirmation" width="30%" severity="info"
			widgetVar="comfirmMSG" closeOnEscape="true" resizable="false">

			<h:form id="frmcomfirm">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="Do you want to update?"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="Yes" process="frmcomfirm" update="frmsuccess,frmError"
						action="#{logSheetRecievBackingBean.saveDate}">
					</p:commandButton>

					<p:commandButton
						style="margin-top:10px;margin-bottom:10px;margin-left:10px"
						value="No" process="frmcomfirm" onclick="PF('comfirmMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

	<!-- Added by dhananjika.d 29/02/2024 -->
	
	<p:dialog styleClass="" id="comfirmMSGNew" global="true" modal="true"
			header="Confirmation" width="30%" severity="info"
			widgetVar="comfirmMSGNew" closeOnEscape="true" resizable="false">

			<h:form id="frmcomfirmNew">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="#{logSheetRecievBackingBean.warningMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="Yes" process="frmcomfirmNew" update="frmsuccess,frmError"
						action="#{logSheetRecievBackingBean.saveDate}">
					</p:commandButton>

					<p:commandButton
						style="margin-top:10px;margin-bottom:10px;margin-left:10px"
						value="No" process="frmcomfirmNew" onclick="PF('comfirmMSGNew').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>
	
	</ui:define>
</ui:composition>