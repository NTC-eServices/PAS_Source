		<script type="text/javascript">
			
		</script>

		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Upload Owner / Vehicle Photos</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmUploadPhotos">
					<br></br>
					<!--  new design start -->
					<div class="ui-g">
						<div class="ui-lg-12" style="text-align: center;">
							<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
								layout="grid">


								<p:outputLabel value="Permit Owner Name"
									style="font-weight: bold;"></p:outputLabel>

								<p:inputText
									value="#{uploadPhotosBackingBean.uploadImageDTO.vehicleOwnerName}"
									style="width:75%;text-align: center;" readonly="true"></p:inputText>


								<p:outputLabel value="Vehicle No." style="font-weight: bold;"></p:outputLabel>

								<p:inputText
									value="#{uploadPhotosBackingBean.uploadImageDTO.vehicleNo}"
									style="width:75%;text-align: center;" readonly="true"></p:inputText>


							</p:panelGrid>
						</div>

						<div class="ui-lg-12" style="text-align: center;">
							<h1 style="margin-bottom: 2%;">Permit Owner</h1>
							<center>
								<p:panel id="imageOwner">
									<div class="ui-lg-4" style="text-align: center;">
										<img id="img"
											src="data:image/png;base64,#{uploadPhotosBackingBean.imageOfPermitOwner}"
											style="width: 60%; height: 60%;" />

										<div class="ui-g-12 ui-md-6 ui-lg-12">
											<div class="ui-g-12 ui-md-6 ui-lg-6">
												<p:fileUpload
													fileUploadListener="#{uploadPhotosBackingBean.handleImageUpload}"
													mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
													allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
													update="imageOwner" style="width:60%">
													
												</p:fileUpload>
											</div>
											<div class="ui-g-12 ui-md-6 ui-lg-6">
												<p:commandButton value="#{msg['upload.photo.delete.btn']}"
													class="mainActionbtn"
													actionListener="#{uploadPhotosBackingBean.deleteVehicleOwnerImage}"
													update="@form"></p:commandButton>
											</div>
										</div>
									</div>
								</p:panel>


							</center>
						</div>


						<div class="ui-lg-12" style="text-align: center;">
							<h1 style="">Vehicle Photos</h1>
							<p:panel id="vehicleImgs">
								<div class="ui-g-12 ui-md-6 ui-lg-4" style="text-align: center;">

									<img id="img"
										src="data:image/png;base64,#{uploadPhotosBackingBean.firstImageOfVehicle}"
										style="width: 100%; height: 100%;" />
									<div class="ui-g-12 ui-md-6 ui-lg-12">
										<p:fileUpload
											fileUploadListener="#{uploadPhotosBackingBean.handleVehicleImagesUpload}"
											mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
											update="vehicleImgs">
											
											<f:attribute name="imageId" value="1" />
										</p:fileUpload>
										<p:commandButton class="mainActionbtn" style="width:25%"
											icon="fa fa-fw fa-trash"
											action="#{uploadPhotosBackingBean.deleteVehicleImages(1)}"
											update="@form"></p:commandButton>
									</div>
								</div>
								<div class="ui-g-12 ui-md-6 ui-lg-4" style="text-align: center;">

									<img id="img"
										src="data:image/png;base64,#{uploadPhotosBackingBean.secondImageOfVehicle}"
										style="width: 100%; height: 100%;" />
									<div class="ui-g-12 ui-md-6 ui-lg-12">
										<p:fileUpload
											fileUploadListener="#{uploadPhotosBackingBean.handleVehicleImagesUpload}"
											mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
											update="vehicleImgs">
											
											<f:attribute name="imageId" value="2" />
										</p:fileUpload>
										<p:commandButton class="mainActionbtn" style="width:25%"
											icon="fa fa-fw fa-trash"
											action="#{uploadPhotosBackingBean.deleteVehicleImages(2)}"
											update="@form"></p:commandButton>
									</div>
								</div>
								<div class="ui-g-12 ui-md-6 ui-lg-4" style="text-align: center;">

									<img id="img"
										src="data:image/png;base64,#{uploadPhotosBackingBean.thirdImageOfVehicle}"
										style="width: 100%; height: 100%;" />
									<div class="ui-g-12 ui-md-6 ui-lg-12">
										<p:fileUpload
											fileUploadListener="#{uploadPhotosBackingBean.handleVehicleImagesUpload}"
											mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
											update="vehicleImgs">
											
											<f:attribute name="imageId" value="3" />
										</p:fileUpload>
										<p:commandButton class="mainActionbtn" style="width:25%"
											icon="fa fa-fw fa-trash"
											action="#{uploadPhotosBackingBean.deleteVehicleImages(3)}"
											update="@form"></p:commandButton>
									</div>
								</div>

								<div class="ui-g-12 ui-md-6 ui-lg-4" style="text-align: center;">

									<img id="img"
										src="data:image/png;base64,#{uploadPhotosBackingBean.fourthImageOfVehicle}"
										style="width: 100%; height: 100%;" />
									<div class="ui-g-12 ui-md-6 ui-lg-12">
										<p:fileUpload
											fileUploadListener="#{uploadPhotosBackingBean.handleVehicleImagesUpload}"
											mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
											update="vehicleImgs">
											
											<f:attribute name="imageId" value="4" />
										</p:fileUpload>
										<p:commandButton class="mainActionbtn" style="width:25%"
											icon="fa fa-fw fa-trash"
											action="#{uploadPhotosBackingBean.deleteVehicleImages(4)}"
											update="@form"></p:commandButton>
									</div>
								</div>

								<div class="ui-g-12 ui-md-6 ui-lg-4" style="text-align: center;">

									<img id="img"
										src="data:image/png;base64,#{uploadPhotosBackingBean.fifthImageOfVehicle}"
										style="width: 100%; height: 100%;" />
									<div class="ui-g-12 ui-md-6 ui-lg-12">
										<p:fileUpload
											fileUploadListener="#{uploadPhotosBackingBean.handleVehicleImagesUpload}"
											mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
											update="vehicleImgs">
											
											<f:attribute name="imageId" value="5" />
										</p:fileUpload>
										<p:commandButton class="mainActionbtn" style="width:25%"
											icon="fa fa-fw fa-trash"
											action="#{uploadPhotosBackingBean.deleteVehicleImages(5)}"
											update="@form"></p:commandButton>
									</div>
								</div>
								<div class="ui-g-12 ui-md-6 ui-lg-4" style="text-align: center;">

									<img id="img"
										src="data:image/png;base64,#{uploadPhotosBackingBean.sixthImageOfVehicle}"
										style="width: 100%; height: 100%;" />
									<div class="ui-g-12 ui-md-6 ui-lg-12">
										<p:fileUpload
											fileUploadListener="#{uploadPhotosBackingBean.handleVehicleImagesUpload}"
											mode="advanced" auto="true" accept=".jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png)$/" sizeLimit="15728640"
											update="vehicleImgs">
											
											<f:attribute name="imageId" value="6" />
										</p:fileUpload>
										<p:commandButton class="mainActionbtn" style="width:25%"
											icon="fa fa-fw fa-trash"
											action="#{uploadPhotosBackingBean.deleteVehicleImages(6)}"
											update="@form"></p:commandButton>
									</div>
								</div>
							</p:panel>
						</div>

						<div class="ui-lg-12" style="text-align: center;">

							<div class="ui-g-12 ui-md-6 ui-lg-12">

								<div>
									<!-- main inspection cancel start -->
									<p:commandButton value="#{msg['upload.photo.cancel.btn']}"
										class="mainActionbtn"
										rendered="#{uploadPhotosBackingBean.mainInsCancel}"
										action="#{uploadPhotosBackingBean.cancelPhotos}"
										update="@form">
									</p:commandButton>
									<!-- main inspection cancel end -->
									<!-- view edit inspection cancel start -->
									<p:commandButton value="#{msg['upload.photo.cancel.btn']}"
										class="mainActionbtn"
										rendered="#{uploadPhotosBackingBean.viewInsCancel}"
										action="#{uploadPhotosBackingBean.cancelPhotos2}"
										update="@form">
									</p:commandButton>
									<!-- view edit inspection cancel end -->

									<p:commandButton value="#{msg['upload.photo.save.btn']}"
										class="mainActionbtn"
										action="#{uploadPhotosBackingBean.savePhotos}" update="@form">
									</p:commandButton>

									<p:commandButton value="Back"
										rendered="#{uploadPhotosBackingBean.backBtn}"
										class="mainActionbtn" id="btnBack"
										action="#{uploadPhotosBackingBean.backToRenewals}"
										update="@form">
									</p:commandButton>
								</div>
							</div>



						</div>
				</h:form>

				<p:dialog id="saveSuccess" header="Success" global="true"
					modal="true" resizable="false" showEffect="fade" appendTo="@(body)"
					width="400" severity="info" widgetVar="saveSuccessWv"
					closeOnEscape="true">
					<center>
						<p:panelGrid columns="2">
							<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
							<p:outputLabel value="Successfully saved"></p:outputLabel>

						</p:panelGrid>
						<p:commandButton value="OK" style="margin-top:20px"
							action="#{uploadPhotosBackingBean.photoUploadSuccessAction}" />

					</center>
				</p:dialog>

				<p:dialog id="saveSuccess1" header="Error" global="true"
					modal="true" resizable="false" showEffect="fade" appendTo="@(body)"
					width="400" severity="info" widgetVar="saveSuccessWv1"
					closeOnEscape="true">
					<center>
						<p:panelGrid columns="2">
							<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
							<p:outputLabel value="Please Contact Adminstration."></p:outputLabel>

						</p:panelGrid>
						<p:commandButton value="OK" style="margin-top:20px"
							action="#{uploadPhotosBackingBean.closeWarningMsg}" />

					</center>
				</p:dialog>


				<p:dialog id="saveSuccess2" header="Error" global="true"
					modal="true" resizable="false" showEffect="fade" appendTo="@(body)"
					width="400" severity="info" widgetVar="saveSuccessWv2"
					closeOnEscape="true">
					<center>
						<p:panelGrid columns="2">
							<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
							<p:outputLabel
								value="Image has been corrupted .Please Contact Adminstration."></p:outputLabel>

						</p:panelGrid>
						<p:commandButton value="OK" style="margin-top:20px"
							action="#{uploadPhotosBackingBean.closeWarningMsg2}" />

					</center>
				</p:dialog>




				<p:dialog id="cancelPhotoUpload" header="Confirm" global="true"
					modal="true" resizable="false" showEffect="fade" appendTo="@(body)"
					width="400" severity="info" widgetVar="calcelPhotoSccss"
					closeOnEscape="true">
					<center>
						<p:panelGrid columns="2">

							<p:outputLabel value="Are you sure to remove uploaded photos?"></p:outputLabel>

						</p:panelGrid>
						<p:commandButton value="Yes" style="margin-top:20px"
							action="#{uploadPhotosBackingBean.photoUploadCancelAction}" />

						<p:commandButton value="No" style="margin-top:20px"
							onclick="PF('calcelPhotoSccss').hide()" />

					</center>
				</p:dialog>





			</div>
		</div>

	</ui:define>
</ui:composition>
