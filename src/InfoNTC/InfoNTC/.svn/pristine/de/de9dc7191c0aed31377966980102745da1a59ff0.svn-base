.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.transTypeBtn {
	margin-bottom: 10%;
	border-radius: 10px;
	width: 90%;
	color: white;
}

.btn-selected {
	color: #ffffff !important;
	font-size: 20px !important;
	font-weight: bold;
	text-shadow: 1px 1px 5px #000000;
	background: -webkit-gradient(linear, center top, center bottom, from(#00d2ff),
		to(#3a7bd5));
	background-image: linear-gradient(#00d2ff, #3a7bd5);
	border-color: #ffffff;
	border-width: 2px;
	border-radius: 5px !important;
	-webkit-box-shadow: 0px 0px 2px 2px rgba(0, 0, 0, 0.75);
	-moz-box-shadow: 0px 0px 2px 2px rgba(0, 0, 0, 0.75);
	box-shadow: 0px 0px 2px 2px rgba(0, 0, 0, 0.75);
	border-color: red !important;
}

.selectedBtn {
	border: 2px solid red
}

.vehiclePhoto {
	width: 100%;
}

/*Hide the advanced buttons and progress*/
.ui-fileupload-upload, .ui-fileupload-cancel, .ui-fileupload-progress {
	display: none;
}

/*Move the file name and size of file onto same line*/
.ui-fileupload-buttonbar, .ui-fileupload-content {
	float: left;
}

/*Remove some of the margin from the file name to align correctly*/
.ui-fileupload-files {
	margin: 0;
}

.ui-fileupload-progress {
	display: none;
}

.ui-fileupload-preview {
	display: none;
}

.ui-fileupload-files {
	display: none;
