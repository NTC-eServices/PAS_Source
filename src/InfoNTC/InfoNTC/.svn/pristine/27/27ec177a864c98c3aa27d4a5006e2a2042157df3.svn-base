		<h:panelGroup layout="block" id="page">

			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Vehicle Inspection</h1>

				<div style="width: 95%;" class="subDiv">

					<h:form id="field">

						<p:dialog closable="false" header="Select Counter"
							visible="#{vehicleBackingBean.localcheckcounter}"
							widgetVar="dlg2" modal="true" height="120" width="420"
							resizable="false" update="@all">

							<br> </br>

							<p:panelGrid columns="2" id="dialoggrid"
								columnClasses="ui-grid-col-5,ui-grid-col-5">

								<p:panelGrid>

									<p:outputLabel value="Select Counter No. :- "></p:outputLabel>

								</p:panelGrid>


								<p:panelGrid>
									<p:selectOneMenu style="width:171px" id="counter"
										value="#{vehicleBackingBean.commonDTO.counterId}">
										<p:ajax listener="#{vehicleBackingBean.onCounterSelect}"
											onClose="#{vehicleBackingBean.handleClose}">
										</p:ajax>
										<f:selectItem itemLabel="--Select Counter--" itemValue=""
											noSelectionOption="true" />
										<f:selectItems value="#{vehicleBackingBean.counterList}"
											var="counterList" itemLabel="#{counterList.counter}"
											itemValue="#{counterList.counterId}" />

									</p:selectOneMenu>

								</p:panelGrid>

								<center>
									<p:panel>
										<p:commandButton id="Exit" value="Exit"
											action="#{vehicleBackingBean.handleClose}"
											class="mainActionbtn" ajax="true" update="@all">
										</p:commandButton>
									</p:panel>
								</center>

							</p:panelGrid>

						</p:dialog>

						<p:panel>
							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
								layout="grid">


								<p:outputLabel value="Permit No."></p:outputLabel>

								<p:inputText id="permitno" style="width:100%"
									onkeyup="this.value=this.value.toUpperCase();" readonly="true"
									value="#{vehicleBackingBean.vehicleDTO.permitNo}">
								</p:inputText>



								<p:panelGrid>
									<p:outputLabel value="Queue No."></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:inputText value="#{vehicleBackingBean.vehicleDTO.queueNo}"
									readonly="#{vehicleBackingBean.disableQueueNo || vehicleBackingBean.vehicleDTO.backlogApp}"
									onkeyup="this.value=this.value.toUpperCase();" id="queue"
									style="width:100%">
								</p:inputText>
								<p:outputLabel value="Counter No.">
								</p:outputLabel>
								<p:inputText style="width:100%;height:18px;" maxlength="50"
									value="#{sessionBackingBean.counterId}" readonly="true"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>

								<p:panelGrid>
									<p:outputLabel value="Vehicle No."></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="vehicle" style="width:100%" readonly="true"
										maxlength="10" onkeyup="this.value=this.value.toUpperCase();"
										value="#{vehicleBackingBean.vehicleDTO.vehicleNo}">
									</p:inputText>
								</p:panelGrid>
							</p:panelGrid>

							<center>
								<p:panel>
									<p:commandButton id="cmdbtnSearch" value="Search"
										action="#{vehicleBackingBean.searchAction}"
										disabled="#{vehicleBackingBean.callnext}"
										class="mainActionbtn" ajax="true" update="page,save">
									</p:commandButton>


									<p:commandButton id="cmdbtnNext" value="Call Next"
										class="mainActionbtn"
										action="#{vehicleBackingBean.nextAction}"
										update="page,save,cmdbtnSkip"
										disabled="#{vehicleBackingBean.callnext}">
									</p:commandButton>

									<p:commandButton id="cmdbtnSkip" value="Skip"
										class="mainActionbtn" ajax="false" update="save,frmsuccess"
										action="#{vehicleBackingBean.skipAction}"
										disabled="#{vehicleBackingBean.skip}">
									</p:commandButton>

									<p:commandButton value="Cancel"
										action="#{vehicleBackingBean.clearAction}"
										class="mainActionbtn" ajax="true"
										update="page,save,cmdbtnNext">

									</p:commandButton>
								</p:panel>
							</center>
						</p:panel>


						<div style="width: 90%;" class="subDiv">
							<br> </br>
							<p:panelGrid columns="2" id="bla2"
