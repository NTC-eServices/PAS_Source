								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="nic" style="width:100%" maxlength="12"
										readonly="#{vehicleBackingBean.vehicleDTO.backlogApp}"
										onkeyup="this.value=this.value.toUpperCase();"
										value="#{vehicleBackingBean.vehicleDTO.nicreg}">
									</p:inputText>
								</p:panelGrid>







								<p:panelGrid>
									<p:outputLabel value="Make"></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>



								<p:panelGrid>

									<p:selectOneMenu id="make" style="width:95%"
