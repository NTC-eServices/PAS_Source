										update="page" class="mainActionbtn">
									</p:commandButton>


									<p:commandButton id="cancel" value="Cancel"
										action="#{vehicleBackingBean.clearAction}"
										class="mainActionbtn" ajax="true"
										update="page,save,cmdbtnNext">

									</p:commandButton>
									<p:commandButton id="uploadphotos" value="Upload Photos"
										action="#{vehicleBackingBean.uploadAction}"
