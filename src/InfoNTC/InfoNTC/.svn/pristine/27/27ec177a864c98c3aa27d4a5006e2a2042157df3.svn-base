									disabled="#{vehicleBackingBean.vehicleDTO.backlogApp}">
									<p:ajax listener="#{vehicleBackingBean.routeFlagListener}"
										update="txtOrigin,txtDestination" />
								</p:selectBooleanButton>

								<p:outputLabel
									value="#{msg['common.backlog.tbpermitinfo.origin']}"></p:outputLabel>
								<p:inputText style="width:100%;height:18px;" id="txtOrigin"
									value="#{vehicleBackingBean.permitDTO.origin}" readonly="true"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>

								<p:outputLabel
									value="#{msg['common.backlog.tbpermitinfo.destination']}"></p:outputLabel>
								<p:inputText style="width:100%;height:18px;" id="txtDestination"
