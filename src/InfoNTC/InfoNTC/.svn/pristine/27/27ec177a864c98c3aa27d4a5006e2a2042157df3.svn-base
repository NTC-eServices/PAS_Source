											update="route,txtOrigin,txtDestination,txtVia,routeFlag" />
										<f:selectItem itemLabel="--Select Route No.--" itemValue=""
											noSelectionOption="true" />
										<f:selectItems value="#{vehicleBackingBean.routelist}"
											var="routelist" itemLabel="#{routelist.routeNo}"
											itemValue="#{routelist.routeNo}" />
									</p:selectOneMenu>

								</p:panelGrid>

								<p:outputLabel value="Swap Origin and Destination"></p:outputLabel>


								<p:selectBooleanButton id="routeFlag"
