									readonly="true" onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>

								<p:outputLabel value="#{msg['common.backlog.tbpermitinfo.via']}"></p:outputLabel>
								<p:panelGrid>
									<p:inputTextarea style="width:100%;height:18px;" id="txtVia"
										readonly="true" rows="3"
										value="#{vehicleBackingBean.permitDTO.via}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputTextarea>
								</p:panelGrid>




								<p:panelGrid>
									<p:outputLabel value="Route Details"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="route" style="width:100%"
										onkeyup="this.value=this.value.toUpperCase();"
										value="#{vehicleBackingBean.vehicleDTO.routeDetails}"
										readonly="true">
									</p:inputText>

								</p:panelGrid>



								<p:panelGrid>
									<p:outputLabel value="Manufacture Year"></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="Man" style="width:100%"
										onkeyup="this.value=this.value.toUpperCase();"
										value="#{vehicleBackingBean.vehicleDTO.manyear}"
										readonly="#{not empty vehicleBackingBean.vehicleDTO.manyear}"
										padControl="false" maxlength="4" maxValue="9999">
										<p:keyFilter regEx="/[\d]/i" />
										<p:ajax update="Man"
											listener="#{vehicleBackingBean.manufactureYearRegistrationYearValidator}" />

									</p:inputText>
								</p:panelGrid>

								<p:panelGrid>

									<p:outputLabel value="Service Type"></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:selectOneMenu id="service" style="width:95%"
										onkeyup="this.value=this.value.toUpperCase();"
										disabled="#{vehicleBackingBean.vehicleDTO.backlogApp}"
										value="#{vehicleBackingBean.vehicleDTO.serviceTypeCode}">

										<f:selectItem itemLabel="--Select Service Type--" itemValue=""
											noSelectionOption="true" />
										<f:selectItems value="#{vehicleBackingBean.servicelist}"
											var="servicelist"
											itemLabel="#{servicelist.serviceTypeDescription}"
											itemValue="#{servicelist.serviceTypeCode}" />
									</p:selectOneMenu>
								</p:panelGrid>



								<p:panelGrid>
									<p:outputLabel value="Chassis No."></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="chass" style="width:100%" maxlength="20"
										onkeyup="this.value=this.value.toUpperCase();"
										readonly="#{not empty vehicleBackingBean.vehicleDTO.chassisNo}"
										value="#{vehicleBackingBean.vehicleDTO.chassisNo}">
									</p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Application No."></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="application"
										style="width:100%;color:red;font-weight: bold;"
										onkeyup="this.value=this.value.toUpperCase();" readonly="true"
										value="#{vehicleBackingBean.vehicleDTO.applicationNo}">
									</p:inputText>
								</p:panelGrid>


							</p:panelGrid>
							<p:panel>


								<p:dataTable id="abc" var="vehicle"
									value="#{vehicleBackingBean.dataList}" tableStyle="100%">
									<p:column headerText="#">
										<h:outputText value="#{vehicle.actionPointSeqNo}" />
									</p:column>

									<p:column headerText="Section">
										<h:outputText value="#{vehicle.section}" />
									</p:column>

									<p:column headerText="Description"
										style="width:30%;word-break: break-word">
										<h:outputText value="#{vehicle.description}" />
									</p:column>

									<p:column headerText="Remark"
										style="width:30%;vertical-align: top;">
										<p:inputTextarea style="width:90%;margin: auto;"
											maxlength="100" value="#{vehicle.remark}" />
									</p:column>


									<p:column headerText="Exists"
										style="width:8%;vertical-align: top;">

										<h:selectBooleanCheckbox value="#{vehicle.exists}" />

									</p:column>
								</p:dataTable>

							</p:panel>

						</div>



						<br> </br>
						<div style="width: 90%;" class="subDiv">
							<p:panelGrid columns="2"
								columnClasses="ui-grid-col-6,ui-grid-col-6" layout="grid">


								<p:panelGrid>
									<h:outputText
										value="If any issue in the Inspection Report on section 1 and 2 require correct
			and next inspection date on or before">
									</h:outputText>
								</p:panelGrid>

								<p:panelGrid>

									<p:calendar id="cal1_2"
										value="#{vehicleBackingBean.vehicleDTO.calender1}"
