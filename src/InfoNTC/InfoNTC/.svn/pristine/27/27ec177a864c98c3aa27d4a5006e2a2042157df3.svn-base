										<f:selectItem itemLabel="--Select Model--" itemValue=""
											noSelectionOption="true" />
										<f:selectItems value="#{vehicleBackingBean.modellist}"
											var="modellist" itemLabel="#{modellist.modelDescription}"
											itemValue="#{modellist.modelTypeCode}" />
									</p:selectOneMenu>
								</p:panelGrid>



								<p:panelGrid>
									<p:outputLabel value="Route No."></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>

								</p:panelGrid>
								<p:panelGrid>

									<p:selectOneMenu id="routeNo" style="width:95%"
										onkeyup="this.value=this.value.toUpperCase();"
