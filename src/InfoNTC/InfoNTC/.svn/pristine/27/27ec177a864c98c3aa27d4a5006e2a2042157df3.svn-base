										<f:attribute name="appNo"
											value="#{vehicleBackingBean.vehicleDTO.applicationNo}"></f:attribute>
									</p:commandButton>

								</p:panel>


								<p:panel rendered="#{vehicleBackingBean.inspectionForAmendment}">
									<p:commandButton id="save2"
										rendered="#{!vehicleBackingBean.reinspection}"
										value="#{msg['vehicle.inspection.save.btn']}"
										actionListener="#{vehicleBackingBean.saveInspectionForAmendment}"
										disabled="#{vehicleBackingBean.disableSave}" ajax="true"
										update="page" class="mainActionbtn">
									</p:commandButton>
									<p:commandButton id="reinspection2"
										rendered="#{vehicleBackingBean.reinspection}"
										value="#{msg['vehicle.inspection.save.btn']}"
										actionListener="#{vehicleBackingBean.saveReinspection}"
										disabled="#{vehicleBackingBean.disableSave}" ajax="true"
										update="page" class="mainActionbtn">
									</p:commandButton>


									<p:commandButton id="cancel2" value="Cancel"
										action="#{vehicleBackingBean.clearAction}"
										class="mainActionbtn" ajax="true"
										update="page,save,cmdbtnNext">

									</p:commandButton>
									<p:commandButton id="uploadphotos2" value="Upload Photos"
										action="#{vehicleBackingBean.uploadAction}"
										class="mainActionbtn" ajax="true" update="field"
										disabled="#{vehicleBackingBean.upload}" style="width:115px">
									</p:commandButton>

									<p:commandButton value="Print"
										disabled="#{vehicleBackingBean.printReport}"
										style="width:115px" class="mainActionbtn"
										actionListener="#{printInspectionReportBackingBean.printActButtonAction}"
										oncomplete="window.open('/InfoNTC/showUpload?reportName=VehicleInspection')">
										<f:attribute name="appNo"
											value="#{vehicleBackingBean.vehicleDTO.applicationNo}"></f:attribute>
									</p:commandButton>

								</p:panel>

							</center>
						</div>

					</h:form>
