package lk.informatics.ntc.model.dto;

import java.io.Serializable;

public class StationDetailsDTO implements Serializable {

	private static final long serialVersionUID = 1L;

	private String stationCode;
	private String stationNameEn;
	private String stationNameSin;
	private String stationNameTam;
	private String status;
	private String Stage;
	private String serviceTypeCode;
	private String serviceTypeDes;
	private String exampleFee;

	private String destination;
	private String origin;

	private boolean normal;
	private boolean luxury;
	private boolean semiLuxury;
	private boolean superLuxury;
	private boolean express;

	private boolean edit;
	private String semifare;

	private String normalRep;
	private String luxuryRep;
	private String semiLuxuryRep;
	private String superLuxuryRep;
	private String expressrep;
	
	private String stationType; // used for combine control sheets

	public StationDetailsDTO() {
	}

	public StationDetailsDTO(String stationCode, String stationNameEn, String stationType) {
		this.stationCode = stationCode;
		this.stationNameEn = stationNameEn;
		this.stationType = stationType;
	}

	public String getStationCode() {
		return stationCode;
	}

	public void setStationCode(String stationCode) {
		this.stationCode = stationCode;
	}

	public String getStationNameEn() {
		return stationNameEn;
	}

	public void setStationNameEn(String stationNameEn) {
		this.stationNameEn = stationNameEn;
	}

	public String getStationNameSin() {
		return stationNameSin;
	}

	public void setStationNameSin(String stationNameSin) {
		this.stationNameSin = stationNameSin;
	}

	public String getStationNameTam() {
		return stationNameTam;
	}

	public void setStationNameTam(String stationNameTam) {
		this.stationNameTam = stationNameTam;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getStage() {
		return Stage;
	}

	public void setStage(String stage) {
		Stage = stage;
	}

	public String getServiceTypeCode() {
		return serviceTypeCode;
	}

	public void setServiceTypeCode(String serviceTypeCode) {
		this.serviceTypeCode = serviceTypeCode;
	}

	public String getServiceTypeDes() {
		return serviceTypeDes;
	}

	public void setServiceTypeDes(String serviceTypeDes) {
		this.serviceTypeDes = serviceTypeDes;
	}

	public String getExampleFee() {
		return exampleFee;
	}

	public void setExampleFee(String exampleFee) {
		this.exampleFee = exampleFee;
	}

	public String getDestination() {
		return destination;
	}

	public void setDestination(String destination) {
		this.destination = destination;
	}

	public String getOrigin() {
		return origin;
	}

	public void setOrigin(String origin) {
		this.origin = origin;
	}

	public boolean isLuxury() {
		return luxury;
	}

	public void setLuxury(boolean luxury) {
		this.luxury = luxury;
	}

	public boolean isSemiLuxury() {
		return semiLuxury;
	}

	public void setSemiLuxury(boolean semiLuxury) {
		this.semiLuxury = semiLuxury;
	}

	public boolean isSuperLuxury() {
		return superLuxury;
	}

	public void setSuperLuxury(boolean superLuxury) {
		this.superLuxury = superLuxury;
	}

	public boolean isExpress() {
		return express;
	}

	public void setExpress(boolean express) {
		this.express = express;
	}

	public boolean isNormal() {
		return normal;
	}

	public void setNormal(boolean normal) {
		this.normal = normal;
	}

	public boolean isEdit() {
		return edit;
	}

	public void setEdit(boolean edit) {
		this.edit = edit;
	}

	public String getSemifare() {
		return semifare;
	}

	public void setSemifare(String semifare) {
		this.semifare = semifare;
	}

	public String getNormalRep() {
		return normalRep;
	}

	public void setNormalRep(String normalRep) {
		this.normalRep = normalRep;
	}

	public String getLuxuryRep() {
		return luxuryRep;
	}

	public void setLuxuryRep(String luxuryRep) {
		this.luxuryRep = luxuryRep;
	}

	public String getSemiLuxuryRep() {
		return semiLuxuryRep;
	}

	public void setSemiLuxuryRep(String semiLuxuryRep) {
		this.semiLuxuryRep = semiLuxuryRep;
	}

	public String getSuperLuxuryRep() {
		return superLuxuryRep;
	}

	public void setSuperLuxuryRep(String superLuxuryRep) {
		this.superLuxuryRep = superLuxuryRep;
	}

	public String getExpressrep() {
		return expressrep;
	}

	public void setExpressrep(String expressrep) {
		this.expressrep = expressrep;
	}

	public String getStationType() {
		return stationType;
	}

	public void setStationType(String stationType) {
		this.stationType = stationType;
	}

}
