				closeOnEscape="true" resizable="false">

				<h:form id="xx">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="warning.png"></p:graphicImage>

							<p:outputLabel value="#{viewOtherVehicleBackingBean.errorMsg}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="OK" process="xx" onclick="PF('fill').hide()">
						</p:commandButton>
					</center>
				</h:form>

			</p:dialog>

			<p:dialog styleClass="" id="proceedSuccess" global="true"
				modal="true" header="Success" width="30%" severity="info"
				closable="false" widgetVar="proceedSuccess" closeOnEscape="false"
				resizable="false">

				<h:form id="frmProceedSuccess">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

							<p:outputLabel value="#{viewOtherVehicleBackingBean.successMSG}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="OK" process="proceedSuccess"
							update="field,frmProceedIncompleteAppConfirm,frmProceedSuccess,frmrequiredField"
							action="#{viewOtherVehicleBackingBean.reloadInit}">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>

			<p:dialog id="proceedIncompleteAppConfirm" global="true" modal="true"
				header="Proceed Incomplete Application" width="30%" severity="info"
				widgetVar="proceedIncompleteAppConfirm" resizable="false">
				<h:form id="frmProceedIncompleteAppConfirm">
					<p:panelGrid>
						<br></br>
						<p:panelGrid>
							<p:outputLabel value="Special Remark"></p:outputLabel>
							<p:outputLabel value=" *" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<br></br>
						<p:panelGrid>
							<p:inputTextarea style="width:415px" id="proceedIncompleteRemark"
								rows="5" maxlength="150"
								value="#{viewOtherVehicleBackingBean.proceedIncompleteApplicationDTO.proceedRemark}">
							</p:inputTextarea>
						</p:panelGrid>
					</p:panelGrid>
					<center>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmProceedIncompleteAppConfirm" value="Proceed"
							update="frmProceedIncompleteAppConfirm,frmProceedSuccess,frmrequiredField"
							action="#{viewOtherVehicleBackingBean.proceedIncompleteApplication}">
						</p:commandButton>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="Back" process="frmProceedIncompleteAppConfirm"
							onclick="PF('proceedIncompleteAppConfirm').hide()">
