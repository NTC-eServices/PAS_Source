				closeOnEscape="true" resizable="false">
				<h:form id="queueField">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
							<p:outputLabel value="#{viewOtherVehicleBackingBean.errorMsg}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="queueField" value="OK" onclick="PF('queueError').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>




			<p:dialog styleClass="" id="requiredField" global="true" modal="true"
				header="Error" width="30%" severity="info" widgetVar="requiredField"
				closeOnEscape="true" resizable="false">
				<h:form id="frmrequiredField">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
							<p:outputLabel value="#{viewOtherVehicleBackingBean.errorMsg}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmrequiredField" value="OK"
							onclick="PF('requiredField').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>



			<p:dialog styleClass="" id="successMSG" global="true" modal="true"
				header="Success" width="30%" severity="info"
				widgetVar="successMessage" closeOnEscape="true" resizable="false">

				<h:form id="frmsuccess">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

							<p:outputLabel value="#{viewOtherVehicleBackingBean.successMSG}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="OK" process="frmsuccess"
							onclick="PF('successMessage').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>

			<p:dialog styleClass="" id="validator" global="true" modal="true"
				header="Alert" width="30%" severity="info" widgetVar="valid"
				closeOnEscape="true" resizable="false">

				<h:form id="frmValidator">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="warning.png"></p:graphicImage>

							<p:outputLabel value="#{viewOtherVehicleBackingBean.errorMsg}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="OK" process="frmValidator" onclick="PF('valid').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>



			<p:dialog styleClass="" id="fillfields" global="true" modal="true"
