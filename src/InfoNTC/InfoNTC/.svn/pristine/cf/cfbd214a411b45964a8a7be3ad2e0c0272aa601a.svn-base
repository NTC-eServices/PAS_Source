										disabled="#{viewOtherVehicleBackingBean.renderBackIncompleteApproval}"
										class="mainActionbtn" ajax="true"
										update="page">
									</p:commandButton>

									<p:commandButton id="uploadphotos" value="Upload Photos"
										action="#{viewOtherVehicleBackingBean.uploadAction}"
										class="mainActionbtn" ajax="true" update="field"
										disabled="#{viewOtherVehicleBackingBean.upload}"
										style="width:115px">
									</p:commandButton>

									<p:commandButton value="Print"
										disabled="#{viewOtherVehicleBackingBean.printReport}"
										style="width:115px" class="mainActionbtn"
										actionListener="#{printInspectionReportBackingBean.printActButtonAction}"
									oncomplete="window.open('/InfoNTC/showUpload?reportName=VehicleInspection')">
										<f:attribute name="appNo"
											value="#{viewOtherVehicleBackingBean.vehicleDTO.applicationNo}"></f:attribute>
									</p:commandButton>

									<p:commandButton id="backOther" value="Back"
										action="#{viewOtherVehicleBackingBean.backToOtherInspection}"
										rendered="#{viewOtherVehicleBackingBean.renderBackOtherInspection}"
										class="mainActionbtn" ajax="true"
										update="page">
									</p:commandButton>


									<!-- for incomplete application view start -->
									<p:commandButton id="proceedIncompleteApplication"
										value="Proceed Incomplete Application"
										actionListener="#{viewOtherVehicleBackingBean.proceedIncompleteApplicationBtnAction}"
										disabled="#{viewOtherVehicleBackingBean.disableProceedIncompleteApplicationBtn}"
										rendered="#{viewOtherVehicleBackingBean.renderBackIncompleteApproval}"
										ajax="true" update="page" class="mainActionbtn"
										style="width:250px">
									</p:commandButton>

									<p:commandButton id="backIncompleteApproval" value="Back"
										action="#{viewOtherVehicleBackingBean.backToIncompleteApproval}"
										rendered="#{viewOtherVehicleBackingBean.renderBackIncompleteApproval}"
										class="mainActionbtn" ajax="true"
										update="page">
									</p:commandButton>

									<!-- for incomplete application view end -->

								</p:panel>

							</center>


						</div>

					</h:form>
				</div>
				<br> </br>
			</div>

			<p:dialog styleClass="" id="queueError" global="true" modal="true"
