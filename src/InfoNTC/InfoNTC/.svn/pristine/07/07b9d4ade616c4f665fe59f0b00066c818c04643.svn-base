								onmouseout="this.style.color='white';"
								style="margin-bottom: 5%;height:150px;width:150px;font-size:20px;background-color: #008000;background-image:none;color:#ffffff;"
								actionListener="#{queueManagementBackingBean.onTransTypeButtonClick(17)}"
								disabled="false" ajax="false"
								oncomplete="PF('statusDialog').hide()"
								rendered="#{queueManagementBackingBean.queueServeiceVisible}">
							</p:commandButton>
						</p:panelGrid>
					</p:panelGrid>
				</h:form>
				<!-- inspection  end-->
			</div>

			<p:dialog modal="true" widgetVar="statusDialog"
				header="Generating Token..." width="150" appendTo="@(body)"
				draggable="false" closable="false" resizable="false">
				<center>
					<p:graphicImage library="images" name="ajax-loader.gif"
						style="margin: 0 auto;" />
				</center>
			</p:dialog>

		</div>
	</ui:define>
</ui:composition>
