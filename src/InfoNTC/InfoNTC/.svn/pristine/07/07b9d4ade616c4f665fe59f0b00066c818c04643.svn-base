									action="#{queueManagementBackingBean.onTransTypeButtonClick(1)}"
									ajax="false"
									style="height:150px;width:150px;font-size:20px;background-color: #9400D3;background-image:none;"
									disabled="false" onmouseout="this.style.color='white';"></p:commandButton>
							</div>
							<div class="ui-grid-col-4">

								<p:commandButton
									value="#{msg['queue.transaction.type.code.inspection.btn']}"
									styleClass="transTypeBtn #{queueManagementBackingBean.inspection eq 'true' ? 'selectedBtn' : null}"
									action="#{queueManagementBackingBean.onTransTypeButtonClick(112)}"
									ajax="false"
									style="height:150px;width:150px;font-size:20px;background-color: #4B0082;background-image:none;"
									onmouseout="this.style.color='white';"></p:commandButton>
							</div>
