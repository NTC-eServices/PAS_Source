		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Unblock User</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmUserSearch">

					<div class="ui-g-12">

						<p:panel id="pnlEmpDet" style="width:80%;margin-left:10%">
							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
								layout="grid">
								
								<p:panelGrid>
									<p:outputLabel value="User ID" for="userId"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:selectOneMenu id="userId" style="width:200px"
										value="#{unblockUserBackingBean.manageUserDTO.userId}"
										effect="fold">

										<f:selectItem itemLabel="-- Select User ID --" itemValue=""
											noSelectionOption="true" />
										<f:selectItems value="#{unblockUserBackingBean.userIdList}"
											var="userIdList" itemValue="#{userIdList.userId}"
											itemLabel="#{userIdList.userId}" />
									</p:selectOneMenu>
								</p:panelGrid>

								<br></br>
							</p:panelGrid>
						</p:panel>

					</div>

					<center>
						<p:panelGrid></p:panelGrid>

						<p:panelGrid columns="2">

							<p:column>
								<p:commandButton id="cmdbtnSearchx" value="Search"
									class="mainActionbtn"
									action="#{unblockUserBackingBean.searchUser}" update="@form,frmuserTable">
								</p:commandButton>
								<p:commandButton id="cmdbtnClearPagex" value="Clear"
									class="mainActionbtn" action="#{unblockUserBackingBean.clearSearch}"
									update="@form,frmuserTable">
								</p:commandButton>
							</p:column>

						</p:panelGrid>
					</center>

					<br> </br>

				</h:form>

				<h:form id="frmuserTable">
					<br></br>
					<center>
						<p:dataTable var="user" paginator="true" rows="10"
							id="userTable" rowIndexVar="rowIndex"
							value="#{unblockUserBackingBean.userList}"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="10,20" resizableColumns="false"
							liveResize="true" rowKey="#{userList.empNo}"
							tableStyle="text-align:left;"
							styleClass="myTable" style="width:80%;">

							<p:column headerText="User ID" visible="true" width="20%"
								style="text-align:left;">
								<h:outputText style="text-align:left;" value="#{user.userId}"
									visible="true" />
							</p:column>
							
							<p:column headerText="Employee No." visible="true" width="30%"
								style="text-align:left;">
								<h:outputText style="text-align:left;" value="#{user.empNo}"
									visible="true" />
							</p:column>
							<p:column headerText="Blocked Date/Time" visible="true" width="20%"
								style="text-align:left;">
								<h:outputText style="text-align:left;" value="#{user.blockedDate}"
									visible="true" />
							</p:column>

							<p:column width="30%">
								<center>
									<p:commandButton id="cmdbtnUnblock" value="Unblock"
										action="#{unblockUserBackingBean.unblockUser(user)}"
										update="@form">
									</p:commandButton>
								</center>

							</p:column>

						</p:dataTable>
					</center>
					<br></br>

				</h:form>

			</div>

		</div>

		<p:dialog styleClass="" id="successMessage" global="true" modal="true"
			header="#{msg['um.employeeprofile.dlsuccess']}" width="25%"
			severity="info" widgetVar="successMessage" closeOnEscape="true"
			resizable="false">
			<h:form id="frmsuccessMsge">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel value="#{unblockUserBackingBean.successMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmsuccessMsge"
						value="#{msg['um.employeeprofile.dlsuccessclose']}"
						onclick="PF('successMessage').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="errorMessage" global="true" modal="true"
			header="#{msg['um.employeeprofile.dlcommerror']}" width="25%"
			severity="info" widgetVar="errorMessage" closeOnEscape="true"
			resizable="false">
			<h:form id="frmerrorMsge">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{unblockUserBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmerrorMsge"
						value="#{msg['um.employeeprofile.dlcommerrorclose']}"
						onclick="PF('errorMessage').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>


	</ui:define>
</ui:composition>