	<queryString>
		<![CDATA[select distinct  d.permit_no,d.vehicle_no,details_of_complain,(TO_CHAR(d.event_datetime,'dd-MM-yyyy')) as event_datetime,
d.origin,d.destination,d.seq,d.other_commited_offences,d.other_commited_offences,
a.pm_service_type,b.description_si,c.pmo_full_name_sinhala,c.pmo_address1_sinhala,c.pmo_address2_sinhala,c.pmo_address3_sinhala,
case when  $P{P_covid_txt} ='P' then '"ෙසෟඛ්‍ය උපදෙස් පිළිපදිමු - කොවිඩ් 19 පරදමු."' else ' ' end,c.pmo_city_sinhala
from public.nt_m_complain_request d
inner join  public.nt_t_pm_application a on a.pm_permit_no=d.permit_no
inner join public.nt_r_service_types b on b.code=a.pm_service_type
inner join public.nt_t_pm_vehi_owner c on c.pmo_permit_no=d.permit_no
inner join public.nt_t_grievance_commited_offence x on d.seq=x.complain_seq
inner join public.nt_m_offence_management y on y.ofm_offence_code=x.offence_type_code
inner join  public.nt_t_pm_vehi_owner e on a.pm_application_no = c.pmo_application_no  
where b.active='A'
and a.pm_status in ('A','O')
and d.complain_no= $P{P_Complain_no}]]>
	</queryString>
