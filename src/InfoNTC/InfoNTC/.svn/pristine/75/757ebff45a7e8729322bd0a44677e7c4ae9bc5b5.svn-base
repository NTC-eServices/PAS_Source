<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>

	<ui:define name="content">

		<style>
.dateTimeField input {
	width: 100px;
}

.dateTimeField img {
	vertical-align: right;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.ui-input {
	margin-top: -20px;
}
</style>



		<h:form id="form">
			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Subsidy Bulk Payment</h1>
				<div style="width: 90%;" class="subDiv">


					<p:panel id="panelOne">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Type"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="serType" style="width:68%"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{sisuSariyaBulkPaymentBackingBean.sisuSeriyaDTO.serviceTypeCode}"
									filter="true" filterMatchMode="contains" tabindex="1">

									<f:selectItem itemLabel="-- Select Service Type --"
										itemValue="" />
									<f:selectItems
										value="#{sisuSariyaBulkPaymentBackingBean.getServiceTypeList}"
										var="d" itemLabel="#{d.serviceTypeDes}"
										itemValue="#{d.serviceTypeCode}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Start Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="startDate" styleClass="dateTimeField"
									navigator="true" showOn="button"
									value="#{sisuSariyaBulkPaymentBackingBean.sisuSeriyaDTO.requestStartDate}"
									pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY">
								</p:calendar>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="End Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="endDate" styleClass="dateTimeField"
									navigator="true" showOn="button"
									value="#{sisuSariyaBulkPaymentBackingBean.sisuSeriyaDTO.requestEndDate}"
									pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY">
								</p:calendar>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Year"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:68%;height:18px;" id="logYear"
									value="#{sisuSariyaBulkPaymentBackingBean.year}"
									padControl="false" maxlength="4" maxValue="9999"
									onkeyup="this.value=this.value.toUpperCase();">
									<p:keyFilter regEx="/[\d]/i" />
									<p:ajax update="logYear,frmsuccess,frmError,frmAlert"
										listener="#{sisuSariyaBulkPaymentBackingBean.yearValidator}" />
								</p:inputText>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

						<br />

						<center>
							<p:commandButton value="Search" styleClass="mainActionbtn"
								id="cmdBtnSearch"
								action="#{sisuSariyaBulkPaymentBackingBean.search}"
								update="frmsuccess,frmError,frmAlert,@form,dataTable" />

							<p:commandButton value="Clear" styleClass="mainActionbtn"
								action="#{sisuSariyaBulkPaymentBackingBean.clearOne}"
								update="frmsuccess,frmError,frmAlert,@form,dataTable" />

						</center>
					</p:panel>


					<!--Data Table -->
					<p:panel id="panelTwo">
						<center>
							<p:dataTable id="dataTable" var="p" rows="5" paginator="true"
								value="#{sisuSariyaBulkPaymentBackingBean.approvedLogSheetList}"
								selection="#{sisuSariyaBulkPaymentBackingBean.selectedLogSheetList}"
								rowKey="#{p.logRefNo}" 
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:100%;font-size:12px;"
								rowIndexVar="index" styleClass="myTable"
								emptyMessage="No data available">

								<p:ajax event="toggleSelect"
									listener="#{sisuSariyaBulkPaymentBackingBean.selectRow}"
									update="@form" process="@form" />

								<p:ajax event="rowSelectCheckbox"
									listener="#{sisuSariyaBulkPaymentBackingBean.selectRow}"
									update="@form" process="@form" /> 

								 <p:ajax event="rowUnselectCheckbox"
									listener="#{sisuSariyaBulkPaymentBackingBean.selectRow}"
									update="@form" process="@form" /> 

								<p:ajax event="rowSelect"
									listener="#{sisuSariyaBulkPaymentBackingBean.selectRow}"
									update="@form" process="@form" />

								<p:column selectionMode="Multiple" style="text-align:center;">
							
								</p:column>

								<p:column headerText="Log Reference No.">
									<h:outputText value="#{p.logRefNo}" />
								</p:column>

								<p:column headerText="Service Ref. No.">
									<h:outputText value="#{p.referenceNo}" />
								</p:column>

								<p:column headerText="Year">
									<h:outputText value="#{p.year}" />
								</p:column>

								<p:column headerText="Month">
									<h:outputText value="#{p.month}" />
								</p:column>

								<p:column headerText="Received Date">
									<h:outputText value="#{p.receivedDate}" />
								</p:column>

								<p:column headerText="Turns">
									<h:outputText value="#{p.noOfTurns}" />
								</p:column>

								<p:column headerText="GPS Turns">
									<h:outputText value="#{p.noOfTurnsinGPS}" />
								</p:column>

								<p:column headerText="Amount Rs." style="text-align:right;">
									<h:outputText value="#{p.payment}">
										<f:convertNumber pattern="##,###.00" />
									</h:outputText>
								</p:column>

								<p:column headerText="Remarks">
									<h:outputText value="#{p.specialRemark}" />
								</p:column>

							</p:dataTable>
						</center>
					</p:panel>


					<p:panel id="panelThree">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Owner Type"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu style="width:68%"
									value="#{sisuSariyaBulkPaymentBackingBean.subSidyDTO.ownerType}"
									onkeyup="this.value=this.value.toUpperCase();" filter="true"
									filterMatchMode="contains" tabindex="1">

									<f:selectItem itemLabel="-- Select Owner Type --" itemValue="" />

									<f:selectItem itemLabel="Private" itemValue="PRIVATE" />
									<f:selectItem itemLabel="CTB" itemValue="CTB" />

								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Holding %"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputNumber style="width:68%;height:18px;"
									value="#{sisuSariyaBulkPaymentBackingBean.subSidyDTO.holdingPercentage}"
									maxlength="3" maxValue="100" padControl="false"
									onkeyup="this.value=this.value.toUpperCase();">
									<p:keyFilter regEx="/[\d]/i" />
									<p:ajax
										listener="#{sisuSariyaBulkPaymentBackingBean.selectRow}" />
										
								</p:inputNumber>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Total No. of Logs"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:65%;height:18px;text-align:left;"
									readonly="true"
									value="#{sisuSariyaBulkPaymentBackingBean.subSidyDTO.totalNoOfLogs}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Total Amount Rs."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:65%;height:18px;text-align:right;"
									readonly="true"
									value="#{sisuSariyaBulkPaymentBackingBean.subSidyDTO.totalBalanceAmount}"
									onkeyup="this.value=this.value.toUpperCase();">

									<f:convertNumber pattern="##,###.00" />


								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Voucher Amount Rs."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText style="width:65%;height:18px;text-align:right;"
									readonly="true"
									value="#{sisuSariyaBulkPaymentBackingBean.subSidyDTO.voucherAmount}"
									onkeyup="this.value=this.value.toUpperCase();">

									<f:convertNumber pattern="##,###.00" />

								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Voucher No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText
									style="width:65%;height:18px;color:red;font-weight: bold;text-align:left;"
									readonly="true"
									value="#{sisuSariyaBulkPaymentBackingBean.subSidyDTO.voucherNo}"
									onkeyup="this.value=this.value.toUpperCase();">
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>



						</p:panelGrid>

					</p:panel>



					<p:panel id="btnPanel">
						<center>

							<p:commandButton value="Generate Voucher"
								rendered="#{sisuSariyaBulkPaymentBackingBean.createMode}"
								disabled="#{sisuSariyaBulkPaymentBackingBean.disabledMode}"
								styleClass="mainActionbtn" style="width:20%"
								action="#{sisuSariyaBulkPaymentBackingBean.generateVoucher}"
								update="frmsuccess,frmError,frmAlert,@form,dataTable" />

							<p:commandButton value="Clear" styleClass="mainActionbtn"
								action="#{sisuSariyaBulkPaymentBackingBean.clearTwo}"
								style="width:20%"
								update="frmsuccess,frmError,frmAlert,@form,dataTable" />

						</center>
					</p:panel>

				</div>
			</div>

		</h:form>


		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{sisuSariyaBulkPaymentBackingBean.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Info" width="30%" severity="info" widgetVar="successMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{sisuSariyaBulkPaymentBackingBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMSG"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel
							value="#{sisuSariyaBulkPaymentBackingBean.alertMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmAlert" onclick="PF('alertMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>



	</ui:define>
</ui:composition>