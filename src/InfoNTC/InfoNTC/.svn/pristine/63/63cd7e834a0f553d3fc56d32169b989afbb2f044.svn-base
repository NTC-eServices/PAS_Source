			<div style="width: 95%;" class="subDiv">
				<h:form id="mainfrm">

					<p:panel>

						<p:panelGrid id="shwDetail" columns="5" style="margin-top:5px"
							columnClasses="ui-grid-col-1,ui-grid-col-4,ui-grid-col-6,ui-grid-col-4,ui-grid-col-6"
							layout="grid">

							<!-- 01 -->

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Route No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="routeNo" style="width:70%" effect="fold"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{noOfTripsGeneratorBackingBean.timeTableDTO.routeNo}">


									<f:selectItem itemLabel="-- All --" itemValue="" />
									<f:selectItems
										value="#{noOfTripsGeneratorBackingBean.busRouteList}" var="d"
										itemLabel="#{d.routeNo}" itemValue="#{d.routeNo}" />

									<p:ajax update="@form"
										listener="#{noOfTripsGeneratorBackingBean.ajaxFillBusCategory}" />

								</p:selectOneMenu>
							</p:panelGrid>



							<p:panelGrid>
								<p:outputLabel value="Bus Category"></p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="buscategory" style="width:70%"
									effect="fold" filter="true" filterMatchMode="contains"
									tabindex="1"
									disabled="#{noOfTripsGeneratorBackingBean.disableBusCategory}"
									value="#{noOfTripsGeneratorBackingBean.timeTableDTO.busCategory}">


									<f:selectItem itemLabel="-- All --" itemValue="" />
									<f:selectItems
										value="#{noOfTripsGeneratorBackingBean.busCategoryList}"
										var="d" itemLabel="#{d.busCategoryDes}"
										itemValue="#{d.busCategory}" />
										
										
									<p:ajax update="@form"
										listener="#{noOfTripsGeneratorBackingBean.ajaxFillRouteDetails}" />

								</p:selectOneMenu>

							</p:panelGrid>

							<!-- 02 -->

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Origin"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText onkeyup="this.value=this.value.toUpperCase();"
									style="width:70%; " readonly="true"
									value="#{noOfTripsGeneratorBackingBean.timeTableDTO.origin}" />

							</p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Destination"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText onkeyup="this.value=this.value.toUpperCase();"
									style="width:70%; " readonly="true"
									value="#{noOfTripsGeneratorBackingBean.timeTableDTO.destination}" />
							</p:panelGrid>


