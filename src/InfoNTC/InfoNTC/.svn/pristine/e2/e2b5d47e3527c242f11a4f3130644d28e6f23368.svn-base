<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">
		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.right .ui-inputfield {
	text-align: right;
}

.numberinput {
	text-align: right;
}

.numberinput .ui-inputfield {
	text-align: right;
	border: 0px;
	box-shadow: none;
	background-color: #f1f1f1;
	font-weight: bold;
	font-size: 15px;
	width: 80%;
	opacity: 1;
}

.large .ui-inputfield {
	font-size: 18px;
}

.ui-fluid .ui-spinner .ui-spinner-input {
	height: 28px
}
</style>

		<div class="mainDiv">
			<div style="margin-top: 0%;" class="common_form_header">
				<h1 style="color: #ffffff; text-align: center; font-weight: bold">Terminal
					Payment View</h1>

			</div>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmAcceptPaymentsView"
					style="background-color :#f1f1f1;">
					<p:spacer height="10px"></p:spacer>
					<p:panelGrid columns="4"
						columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-3,ui-grid-col-3"
						layout="grid">
						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Payment Type"></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:selectOneMenu id="paymentTypeId" effect="fold" filter="true"
								filterMatchMode="contains" tabindex="1"
								style="width:calc(95% - 20px)"
								value="#{acceptPaymentViewBackingBean.selectedPayTypeCode}"
								valueChangeListener="#{acceptPaymentViewBackingBean.onPaymentTypeChange}">
								<f:selectItem itemLabel="-- Select Payment Type --" />
								<f:selectItems
									value="#{acceptPaymentViewBackingBean.payTypeList}" var="d"
									itemLabel="#{d.description}" itemValue="#{d.code}" />
								<p:ajax update="frmAcceptPaymentsView" />

							</p:selectOneMenu>
						</p:panelGrid>
						
						
					
						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Receipt Reference No"></p:outputLabel>
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:selectOneMenu id="receiptRefId" effect="fold" filter="true"
								filterMatchMode="contains" tabindex="1"
								style="width:calc(95% - 20px)"
								value="#{acceptPaymentViewBackingBean.selectedRecieptRefNO}"
								valueChangeListener="#{acceptPaymentViewBackingBean.onReceiptChange}">

								<f:selectItem itemLabel="-- Select Reference No. --" />
								<f:selectItems
									value="#{acceptPaymentViewBackingBean.receiptRefNo}" var="d"
									itemLabel="#{d.receiptRefNo}" itemValue="#{d.receiptRefNo}" />
								<p:ajax update="frmAcceptPaymentsView" />
							</p:selectOneMenu>

						</p:panelGrid>
						<br></br>
					</p:panelGrid>


					<center>
						<br></br>
						<p:commandButton id="cmdbtnAdd" value="Search"
							class="mainActionbtn" update="frmAcceptPaymentsView,frmError"
							process="@this"
							actionListener="#{acceptPaymentViewBackingBean.searchDet}">
						</p:commandButton>

						<p:commandButton id="cmdbtnAddClear" value="Clear"
							styleClass="mainActionbtn" update="frmAcceptPaymentsView"
							process="frmAcceptPaymentsView"
							actionListener="#{acceptPaymentViewBackingBean.clearFields}">
						</p:commandButton>
					</center>
					<br></br>

					<p:panelGrid id="shwDetail" columns="4" style="margin-top:5px"
						columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
						layout="grid">
						<!-- 01 -->
							<p:panelGrid>
							<p:outputLabel value="Terminal Location."></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" readonly="true" id="locationId"
								value="#{acceptPaymentViewBackingBean.terminalLocation}"
								style="width:95%; text-transform: uppercase;">
							</p:inputText>
						</p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="NTC No./ Permit No."></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" readonly="true" id="PermitId"
								value="#{acceptPaymentViewBackingBean.permitNo}"
								style="width:95%; text-transform: uppercase;">
							</p:inputText>
						</p:panelGrid>

						<!-- 02 -->

						<p:panelGrid>
							<p:outputLabel value="Vehicle Reg.No."></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="vehicleRegNoId" readonly="true"
								value="#{acceptPaymentViewBackingBean.vehicleRegNo}"
								style="width:95%; text-transform: uppercase;">

							</p:inputText>
						</p:panelGrid>

						<p:panelGrid>
							<p:outputLabel value="Route Origin"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="fromRouteId"
								value="#{acceptPaymentViewBackingBean.routeOrigin}"
								style="width:95%" readonly="true">
							</p:inputText>
						</p:panelGrid>

						<!-- 03 -->
						<p:panelGrid>
							<p:outputLabel value="Payment Due Date"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="dueDate"
								value="#{acceptPaymentViewBackingBean.dueDate}" readonly="true"
								style="width:95%">
							</p:inputText>

						</p:panelGrid>

						<p:panelGrid>
							<p:outputLabel value="Route Destination"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4"
								value="#{acceptPaymentViewBackingBean.routeDestination}"
								style="width:95%" readonly="true">
							</p:inputText>

						</p:panelGrid>

						<!-- 04 -->

						<p:panelGrid>
							<p:outputLabel value="Paid Date"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<!-- have to change -->


							<p:inputText tabindex="4" id="paidDate"
								value="#{acceptPaymentViewBackingBean.paidDate}"
								style="width:95%" readonly="true">
							</p:inputText>
						</p:panelGrid>

						<p:panelGrid>
							<p:outputLabel value="Owner Name "></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="ownerNameId"
								value="#{acceptPaymentViewBackingBean.owner}" style="width:95%"
								readonly="true">
							</p:inputText>
						</p:panelGrid>

						<!-- 05 -->

						<p:panelGrid>
							<p:outputLabel value="Voucher No."></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="receiptRepNoId1"
								value="#{acceptPaymentViewBackingBean.voucherNo}"
								style="width:95%" readonly="true">
							</p:inputText>
						</p:panelGrid>

						<p:panelGrid>
							<p:outputLabel value="Gender "></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="genderId"
								value="#{acceptPaymentViewBackingBean.gender}" style="width:95%"
								readonly="true">
							</p:inputText>

						</p:panelGrid>

						<!-- 06 -->

						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.issuedDateShow}">
							<p:outputLabel value="Issued Date."></p:outputLabel>

						</p:panelGrid>

						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.issuedDateShow}">

							<p:inputText tabindex="4" id="issueDate"
								value="#{acceptPaymentViewBackingBean.issuedDate}"
								style="width:95%" readonly="true">
							</p:inputText>

						</p:panelGrid>

						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.validFromShow}">
							<p:outputLabel value="Valid From "></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.validFromShow}">


							<p:inputText tabindex="4" id="fromDate"
								value="#{acceptPaymentViewBackingBean.validFromDate}"
								style="width:95%" readonly="true">
							</p:inputText>
						</p:panelGrid>

						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.validToShow}">
							<p:outputLabel value="Valid To "></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.validToShow}">


							<p:inputText tabindex="4" id="toDate"
								value="#{acceptPaymentViewBackingBean.validToDate}"
								readonly="true" style="width:95%">

							</p:inputText>
						</p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Contact Number"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText tabindex="4" id="contactNoId"
								value="#{acceptPaymentViewBackingBean.contactNo}"
								style="width:95%" readonly="true">
							</p:inputText>
						</p:panelGrid>

						<!-- 07 -->
						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.issuedMonthShow}">
							<p:outputLabel value="Issued Month."></p:outputLabel>

						</p:panelGrid>
						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.issuedMonthShow}">
							<p:inputText tabindex="4" id="issueMonth"
								value="#{acceptPaymentViewBackingBean.issuedMonth}"
								readonly="true" style="width:95%">

							</p:inputText>
						</p:panelGrid>

						<!-- 08 -->
						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.noOfTurnShow}">
							<p:outputLabel value="No. of Turns "></p:outputLabel>
						</p:panelGrid>

						<p:panelGrid
							rendered="#{acceptPaymentViewBackingBean.noOfTurnShow}">
							<p:inputText tabindex="4" id="noOfTurns"
								value="#{acceptPaymentViewBackingBean.noOfTurn}" readonly="true"
								style="width:95%">

							</p:inputText>
						</p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Penalty Amount "></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>

							<p:inputText tabindex="4" id="penAmount"
								value="#{acceptPaymentViewBackingBean.penalty}" readonly="true"
								style="width:95%">

							</p:inputText>
						</p:panelGrid>


						<p:panelGrid>
							<p:outputLabel value="Total Amount Paid "></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>


							<p:inputText tabindex="4" id="idTotalPaid"
								value="#{acceptPaymentViewBackingBean.amountPaid}"
								readonly="true" style="width:95%">
							</p:inputText>
						</p:panelGrid>

















					</p:panelGrid>



					<center>


						<p:commandButton id="cmdbtnGenerateReceipt"
							style="width:20%;height:18px;" value="Generate Receipt"
							class="mainActionbtn" update="frmAcceptPaymentsView"
							process="frmAcceptPaymentsView"
							disabled="#{acceptPaymentViewBackingBean.showPrintButt}"
							oncomplete="window.open('/InfoNTC/showUpload?reportName=Receipt')"
							actionListener="#{acceptPaymentViewBackingBean.generateReceipt}">
						</p:commandButton>


						<p:commandButton id="cmdbtnGeneratePermit"
							style="width:20%;height:18px;" value="Print Permit"
							class="mainActionbtn" update="frmAcceptPaymentsView"
							process="frmAcceptPaymentsView"
							oncomplete="window.open('/InfoNTC/showUpload?reportName=Permit')"
							disabled="#{acceptPaymentViewBackingBean.enablePermit}"
							actionListener="#{acceptPaymentViewBackingBean.genratePermit}">
						</p:commandButton>

					</center>
					<br></br>

				</h:form>


				<p:dialog id="dialogError" global="true" modal="true" header="Error"
					width="30%" severity="info" widgetVar="errorDialog"
					closeOnEscape="true" resizable="false">
					<h:form id="frmError">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
								<p:outputLabel value="#{acceptPaymentViewBackingBean.errorMsg}"></p:outputLabel>
							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmError" value="OK" onclick="PF('errorDialog').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>

				<p:dialog id="dialogSucces" global="true" modal="true"
					header="Success" width="30%" severity="info"
					widgetVar="successDialog" closeOnEscape="true" resizable="false">
					<h:form id="frmSuccess">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
								<p:outputLabel value="#{acceptPaymentViewBackingBean.sucessMsg}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmSuccess" value="OK"
								onclick="PF('successDialog').hide()">
							</p:commandButton>
						</center>
					</h:form>
				</p:dialog>

			</div>
		</div>

	</ui:define>
</ui:composition>