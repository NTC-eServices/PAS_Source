	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>

	<ui:define name="content">

		<style>
.dateTimeField input {
	width: 100px;
}

.dateTimeField img {
	vertical-align: right;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.ui-input {
	margin-top: -20px;
}
</style>
		<h:form id="form">

			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Permit Cancellations</h1>
				<div style="width: 90%;" class="subDiv">

					<p:panel id="panelOne">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-3,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
							layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Permit No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="permitNo" style="width:68%"
									value="#{permitCancellationsBackingBean.permitDTO.permitNo}"
									filter="true" filterMatchMode="contains" tabindex="1"
									placeholder="--Select Permit No.--">
									<p:ajax
										listener="#{permitCancellationsBackingBean.completevehicleNO}"
										update="registration" />
									<f:selectItem itemLabel="-- Select Permit No. --" itemValue="" />
									<f:selectItems
										value="#{permitCancellationsBackingBean.permitNoList}" var="p"
										itemLabel="#{p.permitNo}" itemValue="#{p.permitNo}" />
								</p:selectOneMenu>
							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Expiry Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="expiryDate" style="width:68%"
									value="#{permitCancellationsBackingBean.date}" showOn="button"
									pattern="dd/MM/yyyy" placeholder="--Select Expiry Date--">
								</p:calendar>
							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Registration No. of the Bus"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="registration" style="width:68%"
									value="#{permitCancellationsBackingBean.permitDTO.busRegNo}"
									filter="true" filterMatchMode="contains" tabindex="1"
									placeholder="--Select Registration No.--">
									<p:ajax
										listener="#{permitCancellationsBackingBean.completePermitNo}"
										update="permitNo" />
									<f:selectItem itemLabel="-- Select Registration No. --"
										itemValue="" />
									<f:selectItems
										value="#{permitCancellationsBackingBean.busRegNoList}" var="p"
										itemLabel="#{p.busRegNo}" itemValue="#{p.busRegNo}" />
								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<br />

						</p:panelGrid>
						<center>
							<p:commandButton value="Search" styleClass="mainActionbtn"
								update="dataTable,frmError,frmAlert, @form" id="cmdBtnSearch"
								action="#{permitCancellationsBackingBean.search}" />
							<p:commandButton value="Clear" styleClass="mainActionbtn"
								update="dataTable,panelOne"
								action="#{permitCancellationsBackingBean.clearOne}" />
						</center>
					</p:panel>

					<p:panel id="panelTwo">

						<center>
							<p:dataTable widgetVar="dataTableWidgetVar" id="dataTable"
								var="p" rows="10" paginator="true"
								selection="#{permitCancellationsBackingBean.selectDTO}"
								selectionMode="single" rowKey="#{p.applicationNo}"
								value="#{permitCancellationsBackingBean.cancellationList}"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:auto" rowIndexVar="index"
								styleClass="myTable" emptyMessage="No Permits are available">

								<p:ajax event="rowSelect"
									listener="#{permitCancellationsBackingBean.selectRow}"
									process="@form" update="@form">
								</p:ajax>
								<p:ajax event="rowUnselect"
									listener="#{permitCancellationsBackingBean.unSelectRow}"
									update="dataTable" process="@form" />

								<p:column headerText="Application No." visible="true"
									style=" text-align:left;">
									<h:outputText value="#{p.applicationNo}" />
								</p:column>

								<p:column headerText="" visible="false" style="text-align:left;">
									<h:outputText value="#{p.temporary}" />
								</p:column>

								<p:column headerText="Permit No." style=" text-align:left;">
									<h:outputText value="#{p.permitNo}" />
								</p:column>

								<p:column headerText="Vehicle No."
									style="font-size:100%; text-align:left;">
									<h:outputText value="#{p.busRegNo}" />
								</p:column>

								<p:column headerText="Expiry Date"
									style="font-size:100%; text-align:left;">
									<h:outputText value="#{p.expirDate}" />
								</p:column>

								<p:column headerText="Status"
									style="font-size:100%; text-align:left;">
									<h:outputText value="#{p.cancelStatus}" />
								</p:column>

								<p:column headerText="Cancel Reason" ajax="true"
									style="font-size:100%; text-align:left;">
									<h:outputText value="#{p.cancelReason}" />
								</p:column>

								<p:column headerText="Permit Owner"
									style="font-size:100%; text-align:left;">
									<h:outputText value="#{p.vehicleOwner}" />
								</p:column>

								<p:column headerText="Temporary Cancellation" width="6%"
									style="font-size:100%;padding-top :1px !important;padding-bottom :1px !important;text-align:center;">
									<h:selectBooleanCheckbox
										readonly="#{permitCancellationsBackingBean.readOnly}" id="box"
										disabled="#{p.temporary}" value="#{p.checkBoxClicked}"
										style="margin-bottom:6px; width 25px; height 25px;"
										onclick="PF('temporaryCancellationDB').show()">
									</h:selectBooleanCheckbox>

								</p:column>

								<p:column width="5%"
									style="font-size:100%;padding-top :1px !important;padding-bottom :1px !important;">
									<p:commandLink update="@form,frmError, frmRenewalPermitDetails"
										process="@form"
										action="#{permitCancellationsBackingBean.view()}">
										<f:setPropertyActionListener value="#{p}"
											target="#{permitCancellationsBackingBean.viewSelect}" />
										<p:graphicImage library="images" name="view1.png"
											style="width:24px;height:24px;" title="view"></p:graphicImage>
									</p:commandLink>
								</p:column>

							</p:dataTable>
						</center>
					</p:panel>

					<p:panel id="btnPanel">
						<center>

							<p:commandButton value="Save" styleClass="mainActionbtn"
								update="@form, frmError,frmsuccess,btnPrint,frmTemporaryCancellation"
								id="btnApprove"
								action="#{permitCancellationsBackingBean.approve}"
								disabled="#{permitCancellationsBackingBean.diableONETWO}" />

							<p:commandButton value="Reject" styleClass="mainActionbtn"
								update="@form,frmError" rendered="false"
								action="#{permitCancellationsBackingBean.preReject}"
								id="btnReject"
								disabled="#{permitCancellationsBackingBean.diableONETWO}" />

							<p:commandButton value="Print Cancellation Letter"
								style="width:20%" styleClass="mainActionbtn" id="btnPrint"
								disabled="#{permitCancellationsBackingBean.disablePrint}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=PermitCancellationLetter')" update="@form"
								actionListener="#{permitCancellationsBackingBean.printCancellationLetter}" />

							<p:commandButton value="Clear" styleClass="mainActionbtn"
								id="btnClearTwo"
								update="dataTable,panelOne,@form,btnReject,btnApprove"
								action="#{permitCancellationsBackingBean.clearTwo}" />

						</center>
					</p:panel>
				</div>
			</div>

		</h:form>


		<p:dialog styleClass="" id="temporaryCancellation" closable="false"
			global="true" height="250" modal="true"
			header="Temporary Cancellation" width="35%" severity="info"
			widgetVar="temporaryCancellationDB" closeOnEscape="true"
			resizable="false">

			<h:form id="frmTemporaryCancellation">
				<center>
					<br />

					<p:panelGrid columns="2" styleClass="ui-noborder"
						columnClasses="ui-grid-col-6,ui-grid-col-9" layout="grid">

						<p:panelGrid>
							<p:outputLabel value="Cancel Reason"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputTextarea rows="5" counter="display"
								style="height:80px;width:200px" id="cancelReason"
								value="#{permitCancellationsBackingBean.permitDTO.tempcancelReason}"
								maxlength="200" counterTemplate="{0} characters remaining."
								autoResize="false" />
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel id="display" style="margin-left:-25%"></p:outputLabel>
						</p:panelGrid>


						<p:panelGrid>
							<p:outputLabel value="Cancel From Date" style="margin-left:10%"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:calendar id="cancelFrom" style="width:40%"
								value="#{permitCancellationsBackingBean.cancelFrom}"
								showOn="button" pattern="dd/MM/yyyy" />
						</p:panelGrid>

						<p:panelGrid>
							<p:outputLabel value="Cancel To Date"></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:calendar id="cancelTo" style="width:40%"
								value="#{permitCancellationsBackingBean.cancelTo}"
								showOn="button" pattern="dd/MM/yyyy" />
						</p:panelGrid>
					</p:panelGrid>

					<br />

					<p:commandButton value="Save" update="frmError"
						action="#{permitCancellationsBackingBean.saveTemporaryCancellation}"
						styleClass="mainActionbtn">
					</p:commandButton>

					<p:commandButton value="Cancel" styleClass="mainActionbtn"
						action="#{permitCancellationsBackingBean.closeTemporaryCancellation}">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<p:dialog id="rejectDialog" global="true" modal="true" width="439px"
			closable="false" header="Reject Permit Cancellation" severity="info"
			widgetVar="dlg1" closeOnEscape="true" resizable="false"
			style=" border-radius: 3px;">
			<h:form id="frmReject">

				<h:panelGroup id="reject">
					<center>
						<p:outputPanel id="MainHead">
							<h:outputLabel value="Reject Permit Cancellation"
								style="font-size: 15px !important;color: black;font-weight:bold" />
						</p:outputPanel>
						<br />
					</center>

					<h:panelGrid id="rejectPanel" columns="1"
						style="margin-top:20px;width:100%; border: 0px solid blue; border-radius: 12px;"
						styleClass="loginpanel">

						<p:inputTextarea placeholder="Enter the reject reason here"
							style="width:400px; height: 200px;" autoResize="false"
							id="rejectreason"
							value="#{permitCancellationsBackingBean.rejectReason}" />

						<center>
							<p:row>
								<p:commandButton id="btnReject" type="submit"
									class="btn btn-primary btn-lg btn-block"
									style="width:80px; margin-top:10px;height:30px;float: left;font-size: 12px !important; border: 1px solid #FFFFFF;  color: white;background:#6A9DED;text-shadow: 2px 2px 5px blue !important;letter-spacing: 1px !important;border-radius:7px;"
									value="Reject" update="frmError,frmsuccess"
									oncomplete="PF('dlg1').hide();" ajax="false"
									action="#{permitCancellationsBackingBean.reject}">
								</p:commandButton>
								<p:commandButton id="btnClear"
									class="btn btn-primary btn-lg btn-block"
									style="width:80px; margin-top:10px;height:30px;float: left;font-size: 12px !important; border: 1px solid #FFFFFF;  color: white;background:#6A9DED;text-shadow: 2px 2px 5px blue !important;letter-spacing: 1px !important;border-radius:7px;"
									value="Cancel"
									action="#{permitCancellationsBackingBean.rejectCancel}"
									update="rejectreason">
								</p:commandButton>
							</p:row>
						</center>
						<p:spacer height="30"></p:spacer>

					</h:panelGrid>
				</h:panelGroup>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="renewalPermitDetails" global="true"
			height="525px" modal="true" header="Renewal Permit Details"
			width="80%" severity="info" widgetVar="renewalPermitDetailsDB"
			closeOnEscape="true" resizable="false">

			<h:form id="frmRenewalPermitDetails">
				<div>
					<div style="width: 100%;" class="subDiv">

						<p:panel id="panelOne">

							<p:panelGrid columns="4" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-4,ui-grid-col-3,ui-grid-col-4"
								layout="grid">

								<p:panelGrid>
									<p:outputLabel value="Service Type"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="serviceType"
										value="#{permitCancellationsBackingBean.viewDTO.serviceType}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Place of Origin of the Services"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="placeOfOrigin"
										value="#{permitCancellationsBackingBean.viewDTO.origin}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Route No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="routeNo"
										value="#{permitCancellationsBackingBean.viewDTO.routeNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Place of Destination"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="destination"
										value="#{permitCancellationsBackingBean.viewDTO.destination}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Permit Expiry Date"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="expiryDate"
										value="#{permitCancellationsBackingBean.viewDTO.expirDate}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Reminder Letter Status"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="letterStatus" disabled="true"
										value="#{permitCancellationsBackingBean.viewDTO.letterStatus}"></p:inputText>
								</p:panelGrid>

							</p:panelGrid>

							<p:panelGrid columns="2" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-11" layout="grid">

								<p:panelGrid>
									<p:outputLabel value="Expired Reason"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputTextarea id="expiredReason" style="width:88%;"
										disabled="true"
										value="#{permitCancellationsBackingBean.viewDTO.expireReason}"></p:inputTextarea>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Special Remark"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputTextarea id="specialRemark" style="width:88%;"
										value="#{permitCancellationsBackingBean.viewDTO.specialRemark}"></p:inputTextarea>
								</p:panelGrid>

							</p:panelGrid>

							<br />
							<br />
							<p:panelGrid columns="4" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-4,ui-grid-col-3,ui-grid-col-4"
								layout="grid">


								<p:panelGrid>
									<p:outputLabel value="Owner/Organization Name"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="ownerOrganizationName" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.organizationName}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Telephone No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="telephoneNo" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.telephoneNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="NIC/ Org Reg No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="nicorgNo" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.nicNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Mobile No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="mobileNo" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.mobileNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Address 1."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="addOne" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.addressOne}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Province"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="province" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.province}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Address 2."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="addTwo" readonly="true"
										value="#{permitCancellationsBackingBean.viewDTO.addressTwo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
