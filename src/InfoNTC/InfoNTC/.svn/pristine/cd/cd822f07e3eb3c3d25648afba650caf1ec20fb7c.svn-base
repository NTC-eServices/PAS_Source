	<summary>
		<band height="370">
			<staticText>
				<reportElement x="0" y="70" width="552" height="20" uuid="629fab08-fa4b-4d84-b2ca-d8dabdcf507b"/>
				<box>
					<topPen lineWidth="0.0"/>
