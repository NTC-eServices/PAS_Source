<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 65% !important;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}
</style>


		<div class="mainDiv">

			<!-- header -->


			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Generate Advertisement</h1>

			<br> </br>

			<div style="width: 95%;" class="subDiv">
				<h:form id="formOne">
					<center>
						<p:panelGrid columns="2" id="bla2"
							columnClasses="ui-grid-col-5,ui-grid-col-5" layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Tender Reference No."
									style=" margin-left: 19px;"></p:outputLabel>

							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu style="width:200px" id="refNo"
									value="#{advertisementBackingBean.advertisementDTO.tenderNo}"
									filter="true" filterMatchMode="contains">
									<f:selectItem itemLabel="--Select No.--" itemValue="" />
									<f:selectItems value="#{advertisementBackingBean.dataList}"
										var="routeList" itemValue="#{routeList.tenderNo}"
										itemLabel="#{routeList.tenderNo}" />
									<p:ajax listener="#{advertisementBackingBean.onrefNoChange}"
										update="@form" />
								</p:selectOneMenu>

							</p:panelGrid>

							<p:panelGrid>
								<p:outputLabel value="Tender Description    "></p:outputLabel>

							</p:panelGrid>

							<p:panelGrid>
								<p:inputText id="description" style="width:220px;"
									maxlength="50"
									value="#{advertisementBackingBean.advertisementDTO.description}"
									readonly="true">
								</p:inputText>

							</p:panelGrid>


							<p:panelGrid>
								<br></br>
							</p:panelGrid>

							<p:panelGrid>
								<br></br>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>

							</p:panelGrid>


						</p:panelGrid>
					</center>
					<center>
						<p:panel>
							<p:commandButton class="mainActionbtn" value="Search"
								action="#{advertisementBackingBean.search}" update="@form"
								disabled="#{advertisementBackingBean.searchcheck}">

							</p:commandButton>
							<p:commandButton value="Clear" class="mainActionbtn" ajax="true"
								action="#{advertisementBackingBean.clear}" update="@form">
							</p:commandButton>

						</p:panel>
					</center>
					<br></br>



					<div align="center" class="subDiv">
						<p:commandButton value="English" class="mainActionbtn" ajax="true"
							rendered="false" update="@form"
							action="#{advertisementBackingBean.english}">
						</p:commandButton>

						<p:commandButton value="Sinhala" class="mainActionbtn" ajax="true"
							rendered="false" update="@form"
							action="#{advertisementBackingBean.sinhala}">
						</p:commandButton>

						<p:commandButton value="Tamil" class="mainActionbtn" ajax="true"
							rendered="false" update="@form"
							action="#{advertisementBackingBean.tamil}">
						</p:commandButton>
						<br></br> <br></br>
						<p:outputLabel
							style="font-size:20px; display:block; text-align:center"
							value="National Transport Commisson"></p:outputLabel>
						<br></br>
						<p:outputLabel
							style="font-size:15px; display:block; text-align:center"
							value="Invitation of Quoatations for the Issuance of Permanent Passenger Service Permits">
						</p:outputLabel>
						<p:outputLabel
							style="font-size:15px; display:block; text-align:center"
							value="for the Inter-Provincial Passenger Transport via the Southern Expressway">
						</p:outputLabel>

						<br> </br>


						<p:panelGrid columns="2" id="panel"
							columnClasses="ui-grid-col-4,ui-grid-col-6" layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Header Content"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:inputTextarea
								value="#{advertisementBackingBean.advertisementDTO.header}"
								rows="4" cols="60">

								<p:ajax listener="#{advertisementBackingBean.onHeaderChange}"
									update="@form" />
							</p:inputTextarea>
						</p:panelGrid>
						<br> </br>

						<p:dataTable id="abc" scrollable="true" scrollWidth="900"
							value="#{advertisementBackingBean.dataTableList}" var="data"
							tableStyle="100%" emptyMessage="No tender details are available"
							editable="#{advertisementBackingBean.editable}" liveResize="true"
							resizableColumns="false" sortOrder="ascending"
							sortBy="#{data.ssiNo}">

							<p:ajax event="rowEdit"
								listener="#{advertisementBackingBean.onRowEdit}"
								update=":formOne:abc" />
							<p:ajax event="rowEditCancel"
								listener="#{advertisementBackingBean.onRowCancel}"
								update=":formOne:abc" />

							<p:column visible="#{advertisementBackingBean.editable}"
								style="width:15px;">
								<p:rowEditor />
							</p:column>

							<p:column headerText="SL No." style="width:45px;">
								<p:cellEditor disabled="true">
									<f:facet name="output">
										<h:outputText value="#{data.ssiNo}" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="ssiNo" value="#{data.ssiNo}"
											style="width:100%" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Route No." style="width:45px;">

								<p:cellEditor disabled="true">
									<f:facet name="output">
										<h:outputText value="#{data.routeNo}" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="routeNo" value="#{data.routeNo}"
											style="width:100%" />
									</f:facet>
								</p:cellEditor>

							</p:column>

							<p:column headerText="Arrival " style="width:100px;">
								<p:cellEditor disabled="true">
									<f:facet name="output" style="right">
										<h:outputText value="#{data.arrival}"
											style="text-align:center" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="arrival" value="#{data.arrival}"
											style="width:100% text-align:center" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Departure" style="width:100px;">

								<p:cellEditor disabled="true">
									<f:facet name="output">
										<h:outputText value="#{data.departure}"
											style="text-align:center" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="departure" value="#{data.departure}"
											style="width:100% float:center" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="No. of Permits" style="width:45px;">

								<p:cellEditor disabled="true">
									<f:facet name="output">
										<h:outputText value="#{data.noofpermits}"
											style="margin-left: 50%;" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="noofpermits" value="#{data.noofpermits}"
											style="width:100%" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Via" style="width:100px;">

								<p:cellEditor disabled="true">
									<f:facet name="output">
										<h:outputText value="#{data.via}" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="via" value="#{data.via}" style="width:100%" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Type of Service" style="width:45px;">

								<p:cellEditor disabled="true">
									<f:facet name="output">
										<h:outputText value="#{data.typeofService}" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="typeofService" value="#{data.typeofService}"
											style="width:100%" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="One Way Fare (Rs.)"
								style="width:75px; text-align:right">

								<p:cellEditor style="margin-left: 190px;" disabled="true">
									<f:facet name="output" style="text-align:right">
										<h:outputText value="#{data.onewayfare}"
											style="text-align:right" />
									</f:facet>
									<f:facet name="input" style="margin-left: 190px;">
										<p:inputText id="onewayfare" value="#{data.onewayfare}"
											style="width:100% text-align:right" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Treasure holder price (Rs.)"
								style="width:65px; text-align:right">

								<p:cellEditor>
									<f:facet name="output" style="text-align:right">
										<h:outputText value="#{data.chargersoftechevaluation}"
											style="text-align:right" />
									</f:facet>
									<f:facet name="input" style="text-align:right">
										<p:inputText id="chargesofevaluation"
											value="#{data.chargersoftechevaluation}"
											style="width:100% text-align:right" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="No. of Turns per day"
								style="width:45px; text-align:center">

								<p:cellEditor>
									<f:facet name="output">
										<h:outputText value="#{data.noofTurns}"
											style="text-align:center" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="noofTurns" value="#{data.noofTurns}"
											style="width:100%; text-align:center" />
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Parallel Roads"
								style="width:90px; text-align:center">

								<p:cellEditor>
									<f:facet name="output">
										<h:outputText value="#{data.parallelRoads}"
											style="text-align:center" />
									</f:facet>
									<f:facet name="input">
										<p:inputText id="parallelRoads" value="#{data.parallelRoads}"
											style="width:100% text-align:center" />
									</f:facet>
								</p:cellEditor>
							</p:column>



						</p:dataTable>
						<br> </br>
						<div align="right">

							<p:commandButton value="Edit" class="mainActionbtn" ajax="true"
								disabled="#{advertisementBackingBean.editCheck}" update="@form"
								action="#{advertisementBackingBean.edit}">

								<f:attribute name="edit" value="#{data}" />
							</p:commandButton>

						</div>
						<br> </br>

						<p:panelGrid columns="2" id="panel2"
							columnClasses="ui-grid-col-4,ui-grid-col-6" layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Footer Content"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:inputTextarea ajax="true"
								value="#{advertisementBackingBean.advertisementDTO.footer}"
								rows="4" cols="60">
								<p:ajax listener="#{advertisementBackingBean.onFooterchange}"
									update="@form" />
							</p:inputTextarea>
						</p:panelGrid>
						<br> </br>

						<div align="center">
							<p:commandButton value="Save" class="mainActionbtn" ajax="true"
								update="@form" action="#{advertisementBackingBean.save}"
								disabled="#{advertisementBackingBean.savecheck}">
							</p:commandButton>

							<p:commandButton value="Cancel" class="mainActionbtn" ajax="true"
								update="@form" action="#{advertisementBackingBean.clear}">
							</p:commandButton>

							<p:commandButton value="Print" class="mainActionbtn" ajax="true"
								actionListener="#{advertisementBackingBean.print}"
								disabled="#{advertisementBackingBean.printCheck}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=Tender Ad Report')" update="@form">


								<!-- <f:attribute name="vehicelNo" value="#{generateOminiBusReqBackingBean.vehicelNo}"></f:attribute> -->

							</p:commandButton>
						</div>
					</div>

					<br />
				</h:form>
			</div>


			<p:dialog styleClass="" id="successMSG" global="true" modal="true"
				header="Success" width="30%" severity="info"
				widgetVar="successMessage" closeOnEscape="true" resizable="false">

				<h:form id="frmsuccess">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

							<p:outputLabel value="#{advertisementBackingBean.successMSG}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="OK" process="frmsuccess"
							onclick="PF('successMessage').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>

			<p:dialog styleClass="" id="fillfields" global="true" modal="true"
				header="Alert" width="30%" severity="info" widgetVar="fill"
				closeOnEscape="true" resizable="false">

				<h:form id="xx">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="warning.png"></p:graphicImage>

							<p:outputLabel value="#{advertisementBackingBean.errorMsg}"></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							value="OK" process="xx" onclick="PF('fill').hide()">
						</p:commandButton>
					</center>
				</h:form>

			</p:dialog>


		</div>

	</ui:define>
</ui:composition>