		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Verify Survey Information</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="xx">
					<p:panel id="pnl-search">

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-3, ui-grid-col-4"
							layout="grid">

							<!-- _Survey No.-->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Survey No.</p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="drpd-surveyNo" style="width:80%;"
									filter="true" filterMatchMode="contains"
									value="#{gsVerifySurveyInfoBackingBean.surveyDTO.surveyNo}">
									<p:ajax
										listener="#{gsVerifySurveyInfoBackingBean.onSurveyNoChange}"
										process="@form" update="@form" />

									<f:selectItem itemLabel="-- Select Survey No. --" itemValue="" />
									<f:selectItems
										value="#{gsVerifySurveyInfoBackingBean.drpdSurveyNoList}"
										var="surveyNoList" itemLabel="#{surveyNoList.surveyNo}"
										itemValue="#{surveyNoList.surveyNo}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Survey Type</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:panelGrid>

								</p:panelGrid>

								<p:selectOneMenu id="drpd-serviceType" style="width:80%;"
									disabled="true" effect="fold" filter="true"
									filterMatchMode="contains"
									value="#{gsVerifySurveyInfoBackingBean.surveyDTO.surveyTypeDescription}">
									<f:selectItem itemLabel="-- Select Service Type. --"
										itemValue="" />
									<f:selectItems
										value="#{gsVerifySurveyInfoBackingBean.drpdServiceTypeList}"
										var="drpdServiceTypeList"
										itemLabel="#{drpdServiceTypeList.surviceTypeDesc}"
										itemValue="#{drpdServiceTypeList.surviceTypeCode}" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<!-- _Survey Method -->
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel>Survey Method</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>

								<p:selectOneMenu id="drpd-surviceMethod" style="width:80%;"
									effect="fold" filter="true" filterMatchMode="contains"
									disabled="true"
									value="#{gsVerifySurveyInfoBackingBean.surveyDTO.surveyMethodDescription}">
									<f:selectItem itemLabel="-- Select Service Method. --"
										itemValue="" />
									<f:selectItems
										value="#{gsVerifySurveyInfoBackingBean.drpdServiceMethodList}"
										var="drpdServiceMethodList"
										itemLabel="#{drpdServiceMethodList.surviceMethodDesc}"
										itemValue="#{drpdServiceMethodList.surviceMethodCode}" />
								</p:selectOneMenu>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>
						<center>
							<p:panel>
								<!-- Search button -->
								<p:commandButton styleClass="mainActionbtn" value="Search"
									process="@form" update="@form"
									action="#{gsVerifySurveyInfoBackingBean.btnSearch}"></p:commandButton>
								<!-- Clear button -->
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									process="@form" update="@form"
									action="#{gsVerifySurveyInfoBackingBean.btnClear}">
								</p:commandButton>
							</p:panel>
						</center>
					</p:panel>
					<p:panel id="pnl-routeInfo"
						rendered="#{gsVerifySurveyInfoBackingBean.renderPnlRouteInfo}">

						<p:panelGrid columns="6"
							columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-3, ui-grid-col-2,ui-grid-col-3,ui-grid-col-1"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:outputLabel value="Sequence No."></p:outputLabel>
							<p:inputText style="width:81%;height:18px;" id="txt-sequenceNo"
								maxlength="50" readonly="true"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.sequenceNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:panelGrid>
								<p:outputLabel value="Province"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:inputText style="width:67%;height:18px;" id="txt-provinceNo"
								maxlength="50"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.provinceDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Origin"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:inputText style="width:81%;height:18px;" id="txt-origin"
								maxlength="50"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.originDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:panelGrid>
								<p:outputLabel value="Total Length(KM)"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:inputNumber style="text-align:right;width:100%;height:18px;"
								inputStyle="text-align: right" id="txt-totalLength"
								maxlength="50"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.totalLength}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputNumber>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Destination"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>

							<p:inputText style="width:81%;height:18px;" id="txt-destination"
								maxlength="50"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.destinationDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:panelGrid>
								<p:outputLabel value="Unserved Portion (KM)"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:inputNumber
								style="width:100%;height:18px;text-align:right !important"
								inputStyle="text-align: right" id="txt-unservedPortion"
								maxlength="50"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.unservedPortion}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputNumber>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Via"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:inputText style="width:81%;height:18px;" id="txt-via"
								maxlength="50"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.viaDesc}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<p:panelGrid>
								<p:outputLabel value="No. Of Permits Req. To Issue"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:inputNumber
								style="width:100%;height:18px;text-align:right !important"
								id="txt-noOfPermits" maxlength="50"
								inputStyle="text-align: right"
								value="#{gsVerifySurveyInfoBackingBean.gamiRouteDTO.noOfPermitsRequired}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputNumber>
							<p:panelGrid></p:panelGrid>


						</p:panelGrid>

						<center>
							<p:panel>
								<!-- add button -->
								<p:commandButton styleClass="mainActionbtn" value="Add"
									rendered="#{!gsVerifySurveyInfoBackingBean.renderBtnUpdateRouteInfo}"
									id="btn-add" process="@form"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnApprove}"
									update="@form,requiredField,successMessage"
									action="#{gsVerifySurveyInfoBackingBean.btnAdd}"></p:commandButton>

								<!-- Update button -->
								<p:commandButton styleClass="mainActionbtn" value="Update"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnApprove}"
									rendered="#{gsVerifySurveyInfoBackingBean.renderBtnUpdateRouteInfo}"
									id="btn-update" process="@form"
									update="@form,requiredField,successMessage"
									action="#{gsVerifySurveyInfoBackingBean.btnAdd}"></p:commandButton>

								<!-- Clear button -->
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnApprove}"
									id="btn-clearRouteInfo" process="@form" update="@form"
									action="#{gsVerifySurveyInfoBackingBean.btnClearRouteInfo}">
								</p:commandButton>
							</p:panel>
						</center>

						<br></br>

						<p:dataTable id="tbl-routeInfo"
							rendered="#{gsVerifySurveyInfoBackingBean.renderTblrouteInfo}"
							value="#{gsVerifySurveyInfoBackingBean.tblRouteInfo}"
							var="tblRouteInfo" rows="10" paginator="true"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							tableStyle="width:auto" rowsPerPageTemplate="10,20"
							liveResize="true" rowIndexVar="index" styleClass="myTable"
							emptyMessage="No records are available"
							rowKey="#{tblRouteInfo.sequenceNo}">

							<p:column headerText="Sequence No."
								style="text-align:left;width:10%">
								<h:outputText value="#{tblRouteInfo.sequenceNo}"></h:outputText>
							</p:column>
							<p:column headerText="Origin" style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.originDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Destination"
								style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.destinationDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Via" style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.viaDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Province" style="text-align:left;width:15%">
								<h:outputText value="#{tblRouteInfo.provinceDesc}"></h:outputText>
							</p:column>
							<p:column headerText="Total Length (KM)"
								style="text-align:right;width:10%">
								<h:outputText value="#{tblRouteInfo.totalLength}"></h:outputText>
							</p:column>

							<p:column headerText="No. of Permits Req. to Issue"
								style="text-align:right;width:10%">
								<h:outputText value="#{tblRouteInfo.noOfPermitsRequired}"></h:outputText>
							</p:column>
							<p:column headerText="Edit" style="text-align:center;width:10%">
								<p:commandLink
									action="${gsVerifySurveyInfoBackingBean.editTblGamiRouteInfoRaw(tblRouteInfo)}"
									process="@form" update="@form"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnApprove}">
									<p:graphicImage library="images" name="edit.png"
										style="width:24px;height:24px;" title="Edit"></p:graphicImage>
								</p:commandLink>
							</p:column>

							<p:column headerText="Delete" style="text-align:center;width:10%">
								<p:commandLink
									action="${gsVerifySurveyInfoBackingBean.deleteTblGamiRouteInfoRaw(tblRouteInfo)}"
									process="@form" update="@form,successMessage,requiredField"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnApprove}">
									<p:graphicImage library="images" name="delete.png"
										style="width:24px;height:24px;" title="Delete"></p:graphicImage>
								</p:commandLink>
							</p:column>
						</p:dataTable>


						<br></br>
						<p:panel id="pnl-saveRoutes">
							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-8,ui-grid-col-1"
								layout="grid">
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Special Remarks"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputTextarea style="width:98%" maxlength="200" rows="3"
										counter="displayC" counterTemplate="{0} characters remaining."
										value="#{gsVerifySurveyInfoBackingBean.surveyDTO.remarks}" />
									<br></br>

									<h:outputText id="displayC" />
								</p:panelGrid>

							</p:panelGrid>
							<br></br>
							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
								layout="grid">

								<p:panelGrid></p:panelGrid>
								<p:commandButton id="tab02-btn-save" styleClass="mainLinkButton"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnSave}"
									style="width:100%" value="Save"
									update="@form,requiredField,successMessage"
									action="#{gsVerifySurveyInfoBackingBean.btnSave}"></p:commandButton>

								<p:commandButton id="tab02-btn-documentManagement"
									styleClass="mainLinkButton" style="width:100%"
									update="uploadDocumentsDialog" value="Document Management"
									action="#{gsVerifySurveyInfoBackingBean.btnDocumentManagement}">
								</p:commandButton>
								<p:panelGrid></p:panelGrid>
							</p:panelGrid>

							<br></br>
							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-8,ui-grid-col-1"
								layout="grid">


								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Special Remarks"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputTextarea style="width:98%" maxlength="200" rows="3"
										counter="displayD" counterTemplate="{0} characters remaining."
										value="#{gsVerifySurveyInfoBackingBean.surveyDTO.specialRemarks}" />
									<br></br>

									<h:outputText id="displayD" />
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<p:panelGrid>
									<br></br>
									<br></br>
								</p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Tender Process Require"></p:outputLabel>
								</p:panelGrid>
								<p:selectBooleanCheckbox
									value="#{gsVerifySurveyInfoBackingBean.surveyDTO.tenderRequire}"></p:selectBooleanCheckbox>
								<p:panelGrid></p:panelGrid>

							</p:panelGrid>

							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
								layout="grid">

								<p:panelGrid></p:panelGrid>
								<p:commandButton id="btn-approve" styleClass="mainLinkButton"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnApprove}"
									style="width:100%" value="Approve"
									update="@form,requiredField,successMessage"
									action="#{gsVerifySurveyInfoBackingBean.btnApprove}"></p:commandButton>
								<p:commandButton id="btn-reject" styleClass="mainLinkButton"
									disabled="#{gsVerifySurveyInfoBackingBean.disableBtnReject}"
									style="width:100%" value="Reject"
									update="@form,requiredField,successMessage"
									action="#{gsVerifySurveyInfoBackingBean.btnReject}">
								</p:commandButton>
								<p:panelGrid></p:panelGrid>
							</p:panelGrid>

						</p:panel>

					</p:panel>

				</h:form>
			</div>
		</div>



		<!-- Error massage  -->
		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{gsVerifySurveyInfoBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField" value="OK"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<!-- success massage  -->

		<p:dialog styleClass="" id="successMessage" global="true" modal="true"
			header="Success" width="30%" severity="info"
			widgetVar="successMessage" closeOnEscape="true" resizable="false">
			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{gsVerifySurveyInfoBackingBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
