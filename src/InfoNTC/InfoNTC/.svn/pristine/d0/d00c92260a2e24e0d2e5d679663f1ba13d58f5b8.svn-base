										value="#{panelGeneratorWithFixedBusesBean.destinationGroupThreeList}"
										rowKey="#{p.stationCode}" resizableColumns="false"
										tableStyle="width:100%" rowIndexVar="index"
										styleClass="myTable" emptyMessage="No records available.">

										<f:facet name="header">
											<h:outputLabel
												value="#{panelGeneratorWithFixedBusesBean.timeTableDTO.groupThreeDays}" />
										</f:facet>

										<p:columnGroup type="header">
											<p:row>
												<p:column headerText="" visible="false" />
												<p:column headerText="Bus No." width="35%" style="text-align:left"/>
												<p:column headerText="Abbreviation" style="text-align:left"/>
												<p:column headerText="Start" width="15%" style="text-align:left"/>
												<p:column headerText="End" width="15%" style="text-align:left"/>
											</p:row>
										</p:columnGroup>

										<p:column visible="false">
											<h:outputText value="#{p.id}" />
										</p:column>

										<p:column width="35%"
											style="#{p.ctbBus ? 'background-color:#6f98d4e8;':'background-color:none;'}">
											<p:selectOneMenu value="#{p.assigneBusNo}" style="width:85%;"
												effect="fold" filter="true" filterMatchMode="contains"
												tabindex="1">
												<f:selectItem itemLabel="-- Bus No. --" itemValue="" />
												<f:selectItems
													value="#{panelGeneratorWithFixedBusesBean.busNoListDestination}"
													var="e" itemLabel="#{e.assigneBusNo}"
													itemValue="#{e.assigneBusNo}" />
											</p:selectOneMenu>
										</p:column>

										<p:column
											style="#{p.ctbBus ? 'background-color:#6f98d4e8;':'background-color:none;'}">
											<h:outputText value="#{p.busNo}" />
										</p:column>

										<p:column
											style="#{p.ctbBus ? 'background-color:#6f98d4e8;':'background-color:none;'}">
											<h:outputText value="#{p.startTime}" />
										</p:column>

										<p:column styleClass="colNoOfBuses"
											style="#{p.ctbBus ? 'background-color:#9599b3eb;':'background-color:none;'}">
											<h:outputText value="#{p.endTime}" />
										</p:column>


									</p:dataTable>
									<!-- destination table end -->

								</p:panelGrid>
							</p:panel>



						</p:tab>

					</p:tabView>


					<p:panel id="buttonPanelTwo">
						<center>
							<p:commandButton value="Save" styleClass="mainActionbtn"
								action="#{panelGeneratorWithFixedBusesBean.saveAction}"
								update="@form,frmError,frmsuccess,frmAlert"></p:commandButton>

							<p:commandButton value="Clear" styleClass="mainActionbtn"
								action="#{panelGeneratorWithFixedBusesBean.clearTwo}"
								update="@form,frmError,frmsuccess,frmAlert"></p:commandButton>
						</center>
					</p:panel>

				</h:form>
			</div>
		</div>
		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{panelGeneratorWithFixedBusesBean.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Success" width="30%" severity="info"
			widgetVar="successMessage" closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{panelGeneratorWithFixedBusesBean.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMSG"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel
							value="#{panelGeneratorWithFixedBusesBean.alertMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmAlert" onclick="PF('alertMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>



	</ui:define>
</ui:composition>