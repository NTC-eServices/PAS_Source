		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Grant Permission To Proceed Active
				Application</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="permissionForm">
					<p:panel id="pnl-search">
						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-9,ui-grid-col-3" layout="grid">
							<p:panelGrid columns="3"
								columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
								layout="grid">




								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Complaint No."></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:autoComplete
										value="#{grantPermissionToProceedActiveApplicationBackingBean.complaintNo}"
										completeMethod="#{grantPermissionToProceedActiveApplicationBackingBean.completeComplaintNo}"
										dropdown="true" size="15"
										onkeyup="this.value=this.value.toUpperCase();"
										forceSelection="true" scrollHeight="500" />

								</p:panelGrid>


								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Vehicle No."></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:autoComplete id="idVehicle"
										value="#{grantPermissionToProceedActiveApplicationBackingBean.vehicleNo}"
										completeMethod="#{grantPermissionToProceedActiveApplicationBackingBean.completeVehicleNo}"
										size="18" onkeyup="this.value=this.value.toUpperCase();"
										forceSelection="true" scrollHeight="500" >
										
										</p:autoComplete>
								</p:panelGrid>


								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Permit No."></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:autoComplete id="idPermit"
										value="#{grantPermissionToProceedActiveApplicationBackingBean.permitNo}"
										completeMethod="#{grantPermissionToProceedActiveApplicationBackingBean.completePermitNo}"
										size="18" onkeyup="this.value=this.value.toUpperCase();"
										forceSelection="true" scrollHeight="500" >
										
										</p:autoComplete>
								</p:panelGrid>




							</p:panelGrid>

						</p:panelGrid>

						<p:panelGrid columns="4" style="margin-top:2%;"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:panelGrid></p:panelGrid>

							<p:commandButton id="searchBtn" styleClass="mainLinkButton"
								style="width: 100%" value="Search" update="@form"
								action="#{grantPermissionToProceedActiveApplicationBackingBean.searchButtonAction}">

							</p:commandButton>

							<p:commandButton id="clearBtn" styleClass="mainLinkButton"
								style="width: 100%" value="Clear" update="@form"
								action="#{grantPermissionToProceedActiveApplicationBackingBean.resetSearchFilters}">
							</p:commandButton>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>

						<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
							layout="grid" style="margin-top:2%;">

							<p:dataTable id="actions" var="action"
								value="#{grantPermissionToProceedActiveApplicationBackingBean.complaintDetailDTOList}"
								rows="5" width="100%" paginator="true"
								selection="#{grantPermissionToProceedActiveApplicationBackingBean.selectedComplaint}"
								selectionMode="single" rowIndexVar="index"
								rowKey="#{action.complaintNo}"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="5,10,15">

								<p:ajax event="rowSelect"
									listener="#{grantPermissionToProceedActiveApplicationBackingBean.onRowClick}"
									process="@form" update="@form" />


								<p:column headerText="Complain No." width="20%;text-align:left;">
									<h:outputText value="#{action.complaintNo}" />
								</p:column>
								<p:column headerText="Vehicle No." width="20%;text-align:left;">
									<h:outputText value="#{action.vehicleNo}" />
								</p:column>

								<p:column headerText="Permit No." width="20%;text-align:left;">
									<h:outputText value="#{action.permitNo}" />
								</p:column>

								<p:column headerText="Status" width="20%;text-align:left;">
									<h:outputText value="#{action.process_status_des}" />
								</p:column>

								<p:column width="20%">
									<p:commandLink id="cmdBtnViewComplaint"
										action="#{grantPermissionToProceedActiveApplicationBackingBean.viewComplaint}">
										<p:graphicImage library="images" name="complaint.png"
											style="margin:2px;width:25px;height:25px;border-radius: 50%;"
											title="View Complaint"></p:graphicImage>
										<f:setPropertyActionListener value="#{action}"
											target="#{grantPermissionToProceedActiveApplicationBackingBean.selectedComplaint}" />
									</p:commandLink>
								</p:column>

							</p:dataTable>
						</p:panelGrid>
						<br />
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4,ui-grid-col-3"
							layout="grid">
							<p:panelGrid></p:panelGrid>

							<p:outputLabel value="Special Remark"></p:outputLabel>

							<p:inputTextarea style="width:100%;height:150px"
								autoResize="false" maxlength="100"
								value="#{grantPermissionToProceedActiveApplicationBackingBean.specialRemark}">
																
								</p:inputTextarea>
							<p:panelGrid></p:panelGrid>


						</p:panelGrid>
						<br />
						<center>

							<p:commandButton style="margin-top:5px;margin-bottom:10px"
								styleClass="mainActionbtn" value="Proceed" update="@form"
								action="#{grantPermissionToProceedActiveApplicationBackingBean.proceedButton}">
							</p:commandButton>


							<p:commandButton style="margin-top:5px;margin-bottom:10px"
								styleClass="mainActionbtn" value="Clear" update="@form"
								action="#{grantPermissionToProceedActiveApplicationBackingBean.clearButton}">
							</p:commandButton>
						</center>

					</p:panel>
				</h:form>

				<p:dialog header="Complaint" id="complaintDlg"
					widgetVar="complaintDlg" height="550" width="1000" modal="true"
					resizable="true" focus="complaintDlgFrm:dlg-complaint">
					<h:form id="complaintDlgFrm">
						<p:panel id="dlg-complaint">
							<p:panel
