}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}
</style>


		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">View Approve Survey Process Requests</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmSurveyProcess">
					<p:panel>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-3,ui-grid-col-4"
							layout="grid">

							<p:outputLabel></p:outputLabel>


							<p:outputLabel>Survey Request No.
							<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:outputLabel>


							<p:selectOneMenu id="surveyReqNo" style="width:100%"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.requestNo}"
								effect="fold" filter="true" filterMatchMode="contains"
								tabindex="1">

								<p:ajax
									listener="#{editApproveSurveyProcessRequestsBackingBean.onSurveyRequestNoChange}"
									update="surveyNo,requestDate" />

								<f:selectItem itemLabel="--Select Survey Request No.--"
									itemValue="" noSelectionOption="true" />
								<f:selectItems
									value="#{editApproveSurveyProcessRequestsBackingBean.surveyRequestEditNoList}"
									var="surveyRequestNoList"
									itemLabel="#{surveyRequestNoList.requestNo}"
									itemValue="#{surveyRequestNoList.requestNo}" />
							</p:selectOneMenu>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Survey No.</p:outputLabel>

							<p:inputText id="surveyNo" styleClass="caption_lbl"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.surveyNo}"
								readonly="true" style="width:100%;">
							</p:inputText>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>Request Date</p:outputLabel>

							<p:inputText style="width:100%;" id="requestDate"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.requestDate}"
								readonly="true"></p:inputText>
						</p:panelGrid>
						<br />

						<center>
							<p:commandButton styleClass="mainActionbtn" value="Search"
								update="@form"
								action="#{editApproveSurveyProcessRequestsBackingBean.searchAction}"></p:commandButton>

							<p:commandButton styleClass="mainActionbtn" value="Clear"
								update="@form"
								action="#{editApproveSurveyProcessRequestsBackingBean.clearSearch}"></p:commandButton>

						</center>
						<p:panel style="border-bottom:5px solid #404d6f;" />
					</p:panel>

					<p:panel>
						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-4" layout="grid">

							<p:outputLabel>Organization</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50" id="organization"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.organisationDescription}"
								readonly="true">
							</p:inputText>

							<p:outputLabel></p:outputLabel>

							<p:outputLabel></p:outputLabel>

							<p:outputLabel>Department</p:outputLabel>

							<p:inputText style="width:100%;" maxlength="50" id="department"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.departmentDescription}"
								readonly="true">
							</p:inputText>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>


							<p:outputLabel>Request Type</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50" id="requestType"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.requestTypeDescription}"
								readonly="true">
							</p:inputText>

							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>


							<p:outputLabel>Route Available</p:outputLabel>
							<p:selectBooleanCheckbox
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.routeAvailable}"
								disabled="true" readonly="true" />

							<p:outputPanel></p:outputPanel>
							<p:outputLabel></p:outputLabel>
							<br></br>
							<p:outputPanel></p:outputPanel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel></p:outputLabel>

						</p:panelGrid>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:outputLabel></p:outputLabel>
							<p:outputLabel>
								<b>Existing Route</b>
							</p:outputLabel>
							<p:outputLabel></p:outputLabel>
							<p:outputLabel>
								<b>New Route</b>
							</p:outputLabel>

							<p:outputLabel>Route No.</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.routeNo}"
								id="RouteNO" readonly="true">
							</p:inputText>

							<p:outputLabel>Route No.</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.routeNoNew}"
								id="RouteNONew" readonly="true">
							</p:inputText>

							<p:outputLabel>Origin</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50" id="origin"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.origin}"
								onkeyup="this.value=this.value.toUpperCase();" readonly="true">
							</p:inputText>

							<p:outputLabel>Origin</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50" id="newOrg"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.originNew}"
								onkeyup="this.value=this.value.toUpperCase();" readonly="true">
							</p:inputText>


							<p:outputLabel>Destination</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50"
								id="destinationNew"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.destination}"
								onkeyup="this.value=this.value.toUpperCase();" readonly="true">
							</p:inputText>

							<p:outputLabel>Destination</p:outputLabel>
							<p:inputText style="width:100%;" maxlength="50" id="destination"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.destinationNew}"
								onkeyup="this.value=this.value.toUpperCase();" readonly="true">
							</p:inputText>

							<p:outputLabel>Via</p:outputLabel>
							<p:inputTextarea rows="4" style="width:100%;"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.via}"
								id="viaIdNew" readonly="true" />

							<p:outputLabel>Via</p:outputLabel>
							<p:inputTextarea
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.viaNew}"
								id="viaId" rows="4" style="width:100%;" readonly="true" />

							<p:outputLabel>Service Type</p:outputLabel>
							<p:inputTextarea rows="4" style="width:100%;"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.serviceType}"
								id="serviceType" readonly="true" />

							<p:outputLabel>Total Bus Fare (Rs.)</p:outputLabel>
							<p:inputNumber maxlength="10" readonly="true"
								inputStyle="text-align: right;width:100%;"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.busFare}">
							</p:inputNumber>
						</p:panelGrid>
						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">
							<p:outputLabel>Reason for Survey</p:outputLabel>
							<p:inputTextarea rows="4" style="width:100%;" readonly="true"
								id="reason"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.surveyReason}" />
						</p:panelGrid>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<p:outputLabel>Survey Type</p:outputLabel>
							<p:selectOneMenu id="surveyType" style="width:90%;"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.surveyTypeCode}"
								disabled="true">


								<f:selectItem itemLabel="" itemValue=""
									noSelectionOption="false" />
								<f:selectItems
									value="#{editApproveSurveyProcessRequestsBackingBean.surveyTypeList}"
									var="surveyTypeList"
									itemValue="#{surveyTypeList.surveyTypeCode}"
									itemLabel="#{surveyTypeList.surveyTypeDescription}" />
							</p:selectOneMenu>
							<p:outputLabel>Survey Method </p:outputLabel>
							<p:selectOneMenu id="surveyMethod" style="width:90%"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.surveyMethodCode}"
								disabled="true">


								<f:selectItem itemLabel="" itemValue=""
									noSelectionOption="false" />
								<f:selectItems
									value="#{editApproveSurveyProcessRequestsBackingBean.surveyMethodList}"
									var="surveyMethodList"
									itemValue="#{surveyMethodList.surveyMethodCode}"
									itemLabel="#{surveyMethodList.surveyMethodDescription}" />
							</p:selectOneMenu>
						</p:panelGrid>
						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">
							<p:outputLabel>Remark</p:outputLabel>
							<p:inputTextarea rows="4" style="width:100%" id="remark"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.remarks}"
								readonly="true"></p:inputTextarea>
						</p:panelGrid>
						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">
							<p:outputLabel>Special Remark</p:outputLabel>
							<p:inputTextarea rows="4" style="width:100%" id="specialRemark"
								readonly="true"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.specialRemarks}"
								onkeyup="this.value = this.value.toUpperCase();"></p:inputTextarea>
							<p:outputLabel>Status</p:outputLabel>
							<p:inputText size="250" style="width:30%;" id="status"
								value="#{editApproveSurveyProcessRequestsBackingBean.surveyDTO.status}"
								onkeyup="this.value = this.value.toUpperCase();"></p:inputText>
						</p:panelGrid>
					</p:panel>

					<p:panel>

						<br></br>
						<center>


							<p:column>
								<p:commandButton id="cmdbtnDocumentManagement"
									disabled="#{editApproveSurveyProcessRequestsBackingBean.disablePanelThree}"
									styleClass="mainActionbtn" value="Document Management"
									class="mainActionbtn" ajax="false"
									action="#{editApproveSurveyProcessRequestsBackingBean.documentManagement}"
									update="@form" style="width:auto"></p:commandButton>
							</p:column>
							<p:column>
								<p:commandButton id="cmdbtnClearPage" value="Clear"
									class="mainActionbtn"
									actionListener="#{editApproveSurveyProcessRequestsBackingBean.clearPage}"
									update="@form" styleClass="mainActionbtn"></p:commandButton>
							</p:column>
						</center>
						<br></br>
					</p:panel>

				</h:form>

				<p:dialog id="errorMSG" global="true" modal="true" header="Error"
					width="30%" severity="info" widgetVar="errorMessage"
					closeOnEscape="true" resizable="false">

					<h:form id="frmError">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

								<p:outputLabel
									value="#{editApproveSurveyProcessRequestsBackingBean.errorMessage}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								value="OK" process="frmError"
								onclick="PF('errorMessage').hide()">
							</p:commandButton>
						</center>
					</h:form>
				</p:dialog>

				<p:dialog id="successMSG" global="true" modal="true"
					header="Success" width="30%" severity="info"
					widgetVar="successMessage" closeOnEscape="true" resizable="false">

					<h:form id="frmsuccess">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

								<p:outputLabel
									value="#{editApproveSurveyProcessRequestsBackingBean.successMessage}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								value="OK" process="frmsuccess"
								onclick="PF('successMessage').hide()">
							</p:commandButton>
						</center>
					</h:form>
				</p:dialog>

				<p:dialog id="infoMSG" global="true" modal="true" header="Alert"
					width="30%" severity="info" widgetVar="infoMessage"
					closeOnEscape="true" resizable="false">

					<h:form id="frmInfo">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="warning_24.png"></p:graphicImage>

								<p:outputLabel
									value="#{editApproveSurveyProcessRequestsBackingBean.infoMessage}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								value="OK" process="frmInfo" onclick="PF('infoMessage').hide()">
							</p:commandButton>
						</center>
					</h:form>
				</p:dialog>


				<p:dialog styleClass="" id="uploadDocumentsDialog" global="true"
					modal="true" header="View Documents" width="80%" height="400px"
					severity="info" widgetVar="uploadDocument" closeOnEscape="true"
					resizable="false" style="postion: absolute;">
					<p:ajax event="close"
						listener="#{newPermitIssuanceBackingBean.clearCheckDocumentsTable}" />
					<h:form id="frmdoc">



						<p:accordionPanel>
							<p:growl id="msgs" showDetail="false" />
							<p:tab title="Mandatory Documents">


								<h:panelGrid>
									<p:dataTable id="abc" tableStyle="width:auto"
										var="docMandatory"
										value="#{editApproveSurveyProcessRequestsBackingBean.mandatoryList}"
										rows="3" paginator="true" sortOrder="ascending"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,6,9">



										<p:column headerText="Code"
											style="width: 100px;text-align: left;">
											<h:outputText value="#{docMandatory.doc_Code}" />
										</p:column>
										<p:column headerText="Description"
											style="width: 300px;text-align: left;">
											<h:outputText value="#{docMandatory.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path"
											style="width: 300px;text-align: left;">

											<h:outputText value="#{docMandatory.uploadFilePath}" />
										</p:column>




										<p:column headerText="" style="width: 75px;">


											<p:commandLink
												actionListener="#{documentManagementBean.viewMandatoryUploadDocs}"
												oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
												<p:graphicImage library="images" name="view.png"
													style="width:20px;height:20px;" title="view"></p:graphicImage>
												<f:attribute name="documentMandatory"
													value="#{docMandatory}" />
											</p:commandLink>


										</p:column>


									</p:dataTable>
								</h:panelGrid>




							</p:tab>
						</p:accordionPanel>

						<br></br>
						<br></br>

						<p:accordionPanel>
							<p:growl id="msgse" showDetail="false" />
							<p:tab title="Optional Documents">


								<h:panelGrid>
									<p:dataTable id="abcd" tableStyle="width:auto"
										var="docOptional"
										value="#{editApproveSurveyProcessRequestsBackingBean.optionalList}"
										rows="3" paginator="true" sortOrder="ascending"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="3,6,9">



										<p:column headerText="Code"
											style="width: 100px;text-align: left;text-align: left;">
											<h:outputText value="#{docOptional.doc_Code}" />
										</p:column>
										<p:column headerText="Description"
											style="width: 300px;text-align: left;">
											<h:outputText value="#{docOptional.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path"
											style="width: 300px;text-align: left;">
											<h:outputText value="#{docOptional.uploadOptionalFilePath}" />
										</p:column>


										<p:column headerText="" style="width: 75px;">


											<p:commandLink
												actionListener="#{documentManagementBean.viewUploadedOptionalDocAction}"
												oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
												<p:graphicImage library="images" name="view.png"
													style="width:20px;height:20px;" title="view"></p:graphicImage>
												<f:attribute name="documentOptional" value="#{docOptional}" />
											</p:commandLink>
										</p:column>


									</p:dataTable>
								</h:panelGrid>
							</p:tab>
						</p:accordionPanel>





					</h:form>
				</p:dialog>


				<p:dialog styleClass="" id="successSveUploadDoc" global="true"
					modal="true" header="#{msg['um.employeeprofile.dlsuccess']}"
					width="30%" severity="info" widgetVar="successSveUploadDoc"
					closeOnEscape="true" resizable="false">
					<h:form id="frmsuccessSveUploadDoc">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
								<p:outputLabel value="#{documentManagementBean.successMsg}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								update="frmdoc" process="frmsuccessSveUploadDoc"
								value="#{msg['um.employeeprofile.dlsuccessclose']}"
								onclick="PF('successSveUploadDoc').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>


				<p:dialog styleClass="" id="requiredFieldUploadDoc" global="true"
					modal="true" header="#{msg['um.employeeprofile.dlmissing']}"
