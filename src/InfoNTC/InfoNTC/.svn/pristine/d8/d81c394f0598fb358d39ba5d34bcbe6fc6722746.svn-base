		<div class="mainDiv">

			<div style="margin-top: 0%;" class="common_form_header">
				<h1 style="color: #ffffff; text-align: center; font-weight: bold">Terminal
					Payments</h1>

				<div style="text-align: right; margin-top: -16px; margin-right: 2px">
					<p:clock pattern="EEE, d MMM yyyy HH:mm:ss" />
					
								<p:commandLink id="ajax"
									action="#{terminalPaymentNewBackingBean.openStationPopUp}"
									style="margin-left:5px;">
									<u><h:outputText value="Change Location" /></u>
								</p:commandLink>

				</div>
			</div>
			<div style="width: 95%;" class="subDiv">
				<h:form id="frmAcceptPayments" style="background-color :#f1f1f1;"
					styleClass="disableread">
					<p:spacer height="10px"></p:spacer>
					
					
					<p:outputPanel id="stationPanel" styleClass="center">
						<p:outputLabel value="#{terminalPaymentNewBackingBean.stationName}"
							styleClass="title5"></p:outputLabel>
					</p:outputPanel>
					<br></br>
					<p:dialog closable="false" header="Select Station"
						visible="#{terminalPaymentNewBackingBean.localcheckcounter}"
						widgetVar="dlg2" modal="true" height="120" width="420"
						resizable="false" update="@all">



						<p:panelGrid columns="2" id="dialoggrid"
							columnClasses="ui-grid-col-5,ui-grid-col-5">

							<p:panelGrid
								rendered="#{terminalPaymentNewBackingBean.showStationPopup}">

								<p:outputLabel value="Select Terminal Location : "></p:outputLabel>

							</p:panelGrid>


							<p:panelGrid
								rendered="#{terminalPaymentNewBackingBean.showStationPopup}">
								<p:selectOneMenu id="counter" style="width:171px"
									value="#{terminalPaymentNewBackingBean.selectedTerminalLocation}">
									<p:ajax
										listener="#{terminalPaymentNewBackingBean.onCounterSelect}"
										update="stationPanel"
										onClose="#{terminalPaymentNewBackingBean.handleClose}">
									</p:ajax>
									<f:selectItem itemLabel="-- Select Location --" itemValue="" />
									<f:selectItems
										value="#{assignBusRoutesToBlockBackingBean.mainStationDetailsList}"
										var="stationDesList" itemValue="#{stationDesList.code}"
										itemLabel="#{stationDesList.description}" />
								</p:selectOneMenu>

							</p:panelGrid>



						</p:panelGrid>
						<br></br>
						<center>


							<p:commandButton id="Exit" value="Exit"
								action="#{terminalPaymentNewBackingBean.handleClose}"
								class="mainActionbtn" ajax="true" update="@all">
							</p:commandButton>

							<p:commandButton class="mainActionbtn" ajax="true"
								update="frmAcceptPayments" value="Cancel"
								action="#{terminalPaymentNewBackingBean.cancelDialog}"
								onclick="PF('errorDialog').hide()">
							</p:commandButton>


						</center>

					</p:dialog>
					
					
					
					
					
					
					

					<div class="ui-widget-content ui-grid ui-grid-responsive">
						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-3"></div>
							<div class="ui-grid-col-2">
								<p:outputLabel value="Payment Type."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</div>
							<div class="ui-grid-col-3">
								<p:selectOneMenu id="paymentTypeId" effect="fold" filter="true"
									filterMatchMode="contains" style="width:calc(95% - 20px)"
									value="#{terminalPaymentNewBackingBean.selectedPayTypeCode}"
									valueChangeListener="#{terminalPaymentNewBackingBean.onPaymentTypeChange}">
									<f:selectItem itemLabel="--Select a Payment Type--" />
									<f:selectItems
										value="#{terminalPaymentNewBackingBean.paymentTypeList}"
										var="d" itemLabel="#{d.description}" itemValue="#{d.code}" />
									<p:ajax update="frmAcceptPayments" />
								</p:selectOneMenu>
							</div>
						</div>

				

						<h:panelGroup rendered="true">
							<div class="ui-grid-row">
								<div class="gridspadding ui-grid-col-6 sm-padding">
									<p:panel id="mainDetail" style="border:1px solid #b1cbef">
										<p:panelGrid columns="2" layout="grid"
											columnClasses="ui-grid-col-4, ui-grid-col-8">
											<p:panelGrid>
												<p:outputLabel value="Vehicle Reg.No."></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:inputText id="vehicleRegNoId" maxlength="8"
													value="#{terminalPaymentNewBackingBean.vehicleRegNo}"
													rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode != '001'}"
													style="width:95%; text-transform: uppercase;">
													<p:keyFilter regEx="/[a-zA-Z0-9-_]/i" />
													<p:ajax event="blur"
														listener="#{terminalPaymentNewBackingBean.onVehicleNoChange}"
														update="frmAcceptPayments" />
												</p:inputText>
												<p:autoComplete
													value="#{terminalPaymentNewBackingBean.vehicleRegNo}"
													completeMethod="#{complaintViewEditBackingBean.completeVehicleNo}"
													rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode eq '001'}"
													onkeyup="this.value=this.value.toUpperCase();"
													forceSelection="true" inputStyle="width:95%" size="30">
													<p:ajax event="itemSelect"
														listener="#{terminalPaymentNewBackingBean.onVehicleNoChange}"
														update="frmAcceptPayments" />
												</p:autoComplete>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Payment Due Date"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:calendar id="dueDate"
													value="#{terminalPaymentNewBackingBean.dueDate}"
													styleClass="calendarClass" navigator="true"
													inputStyle="width:calc(100% - 43px)" showOn="button"
													pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
													yearRange="c-100:c+100" mask="true" readonlyInput="true"
													validatorMessage="Invalid Date"
													valueChangeListener="#{terminalPaymentNewBackingBean.onDueDateChange}">
													<f:convertDateTime pattern="dd/MM/yyyy"></f:convertDateTime>
													<p:ajax event="dateSelect" immediate="true"
														update="frmAcceptPayments:idBasicAmt, frmAcceptPayments:idPenaltyAmt, frmAcceptPayments:idTotalPaid, frmAcceptPayments:idTotalCollection" />
													<p:ajax event="valueChange" immediate="true"
														update="frmAcceptPayments:idBasicAmt, frmAcceptPayments:idPenaltyAmt, frmAcceptPayments:idTotalPaid, frmAcceptPayments:idTotalCollection" />
												</p:calendar>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Paid Date"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:calendar id="paidDate"
													value="#{terminalPaymentNewBackingBean.paidDate}"
													styleClass="calendarClass" navigator="true"
													inputStyle="width:calc(100% - 43px)" showOn="button"
													pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
													yearRange="c-100:c+100" mask="true" readonlyInput="true"
													validatorMessage="Invalid Date"
													valueChangeListener="#{terminalPaymentNewBackingBean.onPaidDateChange}">
													<f:convertDateTime pattern="dd/MM/yyyy"></f:convertDateTime>
													<p:ajax event="dateSelect" immediate="true"
														update="frmAcceptPayments:idBasicAmt, frmAcceptPayments:idPenaltyAmt, frmAcceptPayments:idTotalPaid, frmAcceptPayments:idTotalCollection" />
													<p:ajax event="valueChange" immediate="true"
														update="frmAcceptPayments:idBasicAmt, frmAcceptPayments:idPenaltyAmt, frmAcceptPayments:idTotalPaid, frmAcceptPayments:idTotalCollection" />
												</p:calendar>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Payment Mode"></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:selectOneMenu
													value="#{terminalPaymentNewBackingBean.payMode}"
													style="width:calc(95% - 18px);" autoWidth="false"
													effect="fold" filter="true" filterMatchMode="contains">
													<f:selectItems
														value="#{terminalPaymentNewBackingBean.paymentModes}"
														var="pay" itemLabel="#{pay.description}"
														itemValue="#{pay.code}" />
													<p:ajax update="receiptRepNoId" />
												</p:selectOneMenu>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Receipt Reference No."></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:inputText id="receiptRepNoId"
													value="#{terminalPaymentNewBackingBean.payReciptRef}"
													style="width:95%;" maxlength="15"
													onkeyup="this.value=this.value.toUpperCase();">
													<p:keyFilter regEx="/[a-zA-Z0-9-_/]/i" />
												</p:inputText>
											</p:panelGrid>

											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode != '001'}">
												<p:outputLabel value="Valid From "></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode != '001'}">
												<p:calendar id="fromDate"
													value="#{terminalPaymentNewBackingBean.validFromDate}"
													styleClass="calendarClass" navigator="true"
													readonlyInput="true" inputStyle="width:calc(100% - 43px)"
													showOn="button" pattern="dd/MM/yyyy"
													placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
													mask="true"
													disabled="#{terminalPaymentNewBackingBean.selectedPayTypeCode eq '001'}">
												</p:calendar>
											</p:panelGrid>

											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode == '001'}">
												<p:outputLabel value="Issued Date"></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode == '001'}">
												<p:calendar id="issueDate"
													value="#{terminalPaymentNewBackingBean.issuedDate}"
													styleClass="calendarClass" navigator="true"
													readonlyInput="true" inputStyle="width:calc(100% - 43px)"
													showOn="button" pattern="dd/MM/yyyy"
													placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
													mask="true"></p:calendar>
											</p:panelGrid>

											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode == '001'}">
												<p:outputLabel value="Issued Month"></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"
													rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode eq '001'}"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode == '001'}">
												<p:inputText id="issueMonth"
													value="#{terminalPaymentNewBackingBean.issuedMonth}"
													valueChangeListener="#{terminalPaymentNewBackingBean.onIssuedMonthChange}"
													type="month" style="width:95%">
													<p:ajax update="@this"></p:ajax>
												</p:inputText>
											</p:panelGrid>

											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode != '001'}">
												<p:outputLabel value="Valid To "></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid
												rendered="#{terminalPaymentNewBackingBean.selectedPayTypeCode != '001'}">
												<p:calendar id="toDate"
													value="#{terminalPaymentNewBackingBean.validToDate}"
													styleClass="calendarClass" navigator="true"
													readonlyInput="true" inputStyle="width:calc(100% - 43px)"
													showOn="button" pattern="dd/MM/yyyy"
													placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
													disabled="#{terminalPaymentNewBackingBean.selectedPayTypeCode eq '001'}">
												</p:calendar>
											</p:panelGrid>

										</p:panelGrid>
									</p:panel>
									<br></br>

									<p:panelGrid id="shwAmount" columns="2" layout="grid"
										columnClasses="ui-grid-col-5, ui-grid-col-7">

										<p:panelGrid>
											<div style="margin-top: 10px">
												<p:outputLabel value="Basic Amount "></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</div>
										</p:panelGrid>
										<p:panelGrid columns="2">
											<p:outputLabel value="Rs."></p:outputLabel>
											<p:inputNumber id="idBasicAmt"
												valueChangeListener="#{terminalPaymentNewBackingBean.onBasicAmountChange}"
												value="#{terminalPaymentNewBackingBean.basicAmount}"
												decimalPlaces="2" disabled="false" styleClass="numberinput">
												<p:ajax update="idTotalPaid,idTotalCollection"></p:ajax>
											</p:inputNumber>
										</p:panelGrid>

										<p:panelGrid>
											<div style="margin-top: 10px">
												<p:outputLabel value="Penalty "></p:outputLabel>
											</div>
										</p:panelGrid>
										<p:panelGrid columns="2">
											<p:outputLabel value="Rs."></p:outputLabel>
											<p:inputNumber id="idPenaltyAmt"  
												value="#{terminalPaymentNewBackingBean.penalty}"
												valueChangeListener="#{terminalPaymentNewBackingBean.onPenaltyChange}"
												minValue="0" disabled="#{terminalPaymentNewBackingBean.penaltyDisable}" styleClass="numberinput"
												style="#{terminalPaymentNewBackingBean.selectedPayTypeCode == null or terminalPaymentNewBackingBean.selectedPayTypeCode == '' ?'display:none' :''};"
												inputStyle="width:80%">
												<p:ajax update="idTotalPaid,idTotalCollection,frmAcceptPayments:idPenaltyAmt"></p:ajax>
												<p:ajax event="valueChange" immediate="true" process="frmAcceptPayments:idPenaltyAmt"
														update=" frmAcceptPayments:idPenaltyAmt" />
											</p:inputNumber>
										</p:panelGrid>

										<p:panelGrid>
											<div style="margin-top: 10px">
												<p:outputLabel value="Total Amount Paid "></p:outputLabel>
											</div>
										</p:panelGrid>
										<p:panelGrid columns="2">
											<p:outputLabel value="Rs."></p:outputLabel>
											<p:inputNumber id="idTotalPaid"
												value="#{terminalPaymentNewBackingBean.amountPaid}"
												disabled="true" styleClass="numberinput">
											</p:inputNumber>
										</p:panelGrid>

										<p:panelGrid>
											<div style="margin-top: 10px">
												<p:outputLabel value="Total Daily Collection "
													style="color:#10b561; font-weight:bold"></p:outputLabel>
											</div>
										</p:panelGrid>

										<p:panelGrid columns="2">
											<p:outputLabel value="Rs."
												style="color:#10b561; font-weight:bold"></p:outputLabel>
											<p:inputNumber id="idTotalCollection"
												value="#{terminalPaymentNewBackingBean.totalCollection}"
												disabled="true" styleClass="numberinput large green">
											</p:inputNumber>
										</p:panelGrid>

									</p:panelGrid>

									<p:spacer height="10px"></p:spacer>

								</div>

								<div class="gridspadding ui-grid-col-6">
									<p:panel id="panelpermitDetail"
										header="Permanent Permit Details"
										style="border:1px solid #b1cbef;background-color: cadetblue;">
										<p:panelGrid columns="2" layout="grid"
											columnClasses="ui-grid-col-4, ui-grid-col-8">
											<p:panelGrid>
												<p:outputLabel value="Permit No."></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:inputText maxlength="20"
													value="#{terminalPaymentNewBackingBean.permitNo}"
													onkeyup="this.value=this.value.toUpperCase();"
													disabled="#{terminalPaymentNewBackingBean.withoutNTCPermit}"
													style="width:95%">
													<p:keyFilter regEx="/[a-zA-Z0-9-_]/i" />
												</p:inputText>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Route No."></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:inputText id="fromRouteNo" maxlength="20"
													value="#{terminalPaymentNewBackingBean.routeNo}"
													onkeyup="this.value=this.value.toUpperCase();"
													style="width:95%;"
													disabled="#{terminalPaymentNewBackingBean.withoutNTCPermit}">
													<p:keyFilter regEx="/[a-zA-Z0-9-_/]/i" />
												</p:inputText>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Route Origin"></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
											<p:panelGrid>
												<p:inputText id="fromRouteId" maxlength="50"
													value="#{terminalPaymentNewBackingBean.routeOrigin}"
													onkeyup="this.value=this.value.toUpperCase();"
													style="width:95%;"
													disabled="#{terminalPaymentNewBackingBean.withoutNTCPermit}">
													<p:keyFilter regEx="/[a-zA-Z0-9-_/]/i" />
												</p:inputText>
											</p:panelGrid>

											<p:panelGrid>
												<p:outputLabel value="Route Destination"></p:outputLabel>
												<p:outputLabel value="*" style="color:red;"></p:outputLabel>
											</p:panelGrid>
