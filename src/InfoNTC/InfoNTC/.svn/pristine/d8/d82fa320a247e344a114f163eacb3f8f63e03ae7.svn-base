package lk.informatics.ntc.model.dto;
import java.io.Serializable;
import java.util.Date;

import javax.print.DocFlavor.BYTE_ARRAY;

import java.sql.Timestamp;

import javafx.scene.chart.PieChart.Data;
import javassist.bytecode.ByteArray;


public class DivisionDTO implements Serializable{

	private static final long serialVersionUID = 8344243187243931903L;
	private String sectionCode;
	private String sectionDes;
	
	public DivisionDTO() {
		super();
	}

	public DivisionDTO(String sectionCode, String sectionDes) {
		super();
		this.sectionCode = sectionCode;
		this.sectionDes = sectionDes;
	}

	public String getSectionCode() {
		return sectionCode;
	}

	public void setSectionCode(String sectionCode) {
		this.sectionCode = sectionCode;
	}

	public String getSectionDes() {
		return sectionDes;
	}

	public void setSectionDes(String sectionDes) {
		this.sectionDes = sectionDes;
	}

}
