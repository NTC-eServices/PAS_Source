								<p:selectOneMenu id="servieAgreemntNums" style="width:90%;"
									disabled="#{!nisiSeriyaPermitHolderInfoEditBean.searchEditDisable}"
									effect="fold" filter="true" filterMatchMode="contains"
									value="#{nisiSeriyaPermitHolderInfoEditBean.serviceAgreementNo}">
									<p:ajax
										listener="#{nisiSeriyaPermitHolderInfoEditBean.onServiceAgreeNumChange}"
										process="@form" update="@form" />
									<f:selectItem itemLabel="-- Select Service Ref No. --"
										itemValue="" />
									<f:selectItems
										value="#{nisiSeriyaPermitHolderInfoEditBean.serviceAgreementNumList}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:panelGrid></p:panelGrid>

							<p:commandButton id="btn-search" styleClass="mainLinkButton"
								style="width: 100%" value="Search" update="@form,alertMessage"
								action="#{nisiSeriyaPermitHolderInfoEditBean.btnSearch}"
								rendered="#{!nisiSeriyaPermitHolderInfoEditBean.renderBtnSearch}">

							</p:commandButton>

							<p:commandButton id="btn-clear" styleClass="mainLinkButton"
								style="width: 100%"
								rendered="#{nisiSeriyaPermitHolderInfoEditBean.renderClear}"
								value="Clear" update="@form"
								action="#{nisiSeriyaPermitHolderInfoEditBean.btnClear}">
							</p:commandButton>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>

						<p:dataTable id="permitHolderInfoTbl"
							rendered="#{nisiSeriyaPermitHolderInfoEditBean.disableDataTable}"
							value="#{nisiSeriyaPermitHolderInfoEditBean.nisiSeriyaDTOList}"
							var="permitHolderInfo" rows="10" paginator="true"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							tableStyle="width:auto" rowsPerPageTemplate="10,20"
							liveResize="true" rowIndexVar="index" styleClass="myTable"
							emptyMessage="No. records are available"
							rowKey="#{permitHolderInfo.serviceRefNo}">

							<p:column headerText="Request No."
								style="text-align:left;width:15%">
								<h:outputText value="#{permitHolderInfo.requestNo}"></h:outputText>
							</p:column>
							<p:column headerText="Service Reference No."
								style="text-align:left;width:15%">
								<h:outputText value="#{permitHolderInfo.serviceRefNo}"></h:outputText>
							</p:column>
							<p:column headerText="Name Of Operator"
								style="text-align:left;width:30%">
								<h:outputText value="#{permitHolderInfo.nameOfOperator}"></h:outputText>
							</p:column>
							<p:column headerText="Status" style="text-align:left;width:10%">
								<h:outputText value="#{permitHolderInfo.statusDes}"></h:outputText>
