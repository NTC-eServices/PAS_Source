								</p:commandLink>
							</p:column>
							<p:column headerText="Edit" style="text-align:left;width:10%">
								<p:commandLink disabled="#{permitHolderInfo.approvedRecord}"
									action="#{nisiSeriyaPermitHolderInfoEditBean.editDataListener(permitHolderInfo)}"
									process="@form" update="@form">
									<p:graphicImage library="images" name="edit.png"
										style="width:24px;height:24px;" title="Edit"></p:graphicImage>
								</p:commandLink>
							</p:column>
						</p:dataTable>


					</p:panel>


					<p:tabView id="tabviewId"
						activeIndex="#{nisiSeriyaPermitHolderInfoEditBean.activeTabIndex}">

						<!-- Tab 01 start-->
						<p:tab id="tab-serviceInformation" title="Service Information"
