		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Nisi Seriya Permit Holder
				Information View/Edit</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="nisiSeriyaMainFrm">
					<p:panel id="pnl-search">

						<p:panelGrid columns="5"
							columnClasses="ui-grid-col-2,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Request No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="requestNo" style="width:90%;"
									disabled="false" effect="fold" filter="true"
									filterMatchMode="contains"
									value="#{nisiSeriyaPermitHolderInfoEditBean.requestedNo}">
									<p:ajax
										listener="#{nisiSeriyaPermitHolderInfoEditBean.onRequestNumChange}"
										process="@form" update="@form" />
									<f:selectItem itemLabel="-- Select Request No. --" itemValue="" />
									<f:selectItems
										value="#{nisiSeriyaPermitHolderInfoEditBean.requestNumList}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Reference No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="serviceRefNums" style="width:90%;"
									disabled="#{!nisiSeriyaPermitHolderInfoEditBean.searchEditDisable}"
									effect="fold" filter="true" filterMatchMode="contains"
									value="#{nisiSeriyaPermitHolderInfoEditBean.serviceRefNo}">
									<p:ajax
										listener="#{nisiSeriyaPermitHolderInfoEditBean.onServiceRefNumChange}"
										process="@form" update="@form" />
									<f:selectItem itemLabel="-- Select Service Ref No. --"
										itemValue="" />
									<f:selectItems
										value="#{nisiSeriyaPermitHolderInfoEditBean.serviceRefNumList}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Permit No. of PRFTA"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="permitNums" style="width:90%;"
									disabled="#{!nisiSeriyaPermitHolderInfoEditBean.searchEditDisable}"
									effect="fold" filter="true" filterMatchMode="contains"
									value="#{nisiSeriyaPermitHolderInfoEditBean.permitNo}">
									<p:ajax
										listener="#{nisiSeriyaPermitHolderInfoEditBean.onPermitNumChange}"
										process="@form" update="@form" />
									<f:selectItem itemLabel="-- Select Service Ref No. --"
										itemValue="" />
									<f:selectItems
										value="#{nisiSeriyaPermitHolderInfoEditBean.permitNumList}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Agreement No."></p:outputLabel>
							</p:panelGrid>
