		<p:dialog styleClass="" id="uploadDocumentsDialog" global="true"
			modal="true" header="Upload Documents" width="80%" height="400px"
			severity="info" widgetVar="uploadDocument" closeOnEscape="true"
			resizable="false" style="postion: absolute;">
			<p:ajax event="close"
				listener="#{newPermitIssuanceBackingBean.clearCheckDocumentsTable}" />
			<h:form id="frmdoc">

				<p:accordionPanel>
					<p:growl id="msgs" showDetail="false" />
					<p:tab title="Mandatory Documents">
						<h:panelGrid>
							<p:dataTable id="abc" tableStyle="width:auto" var="docMandatory"
								value="#{employeeProfileBackingBean.mandatoryList}" rows="10"
								paginator="true" sortOrder="ascending"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20,30,40,50">



								<p:column headerText="Code" style="width: 100px;">
									<h:outputText value="#{docMandatory.doc_Code}" />
								</p:column>
								<p:column headerText="Description" style="width: 300px;">
									<h:outputText value="#{docMandatory.add_Doc_Description}" />
								</p:column>
								<p:column headerText="File Path" style="width: 300px;">

									<h:outputText value="#{docMandatory.uploadFilePath}" />
								</p:column>

								<p:column headerText="" style="width: 150px !important;"
									rendered="#{employeeProfileBackingBean.checkUpload}">

									<p:fileUpload
										fileUploadListener="#{documentManagementBean.saveUserManagementMandatoryUploadDocuments}"
										mode="advanced" dragDropSupport="false" multiple="true"
										auto="true" showButtons="false" update="@form" fileLimit="1"
										accept=".pdf,.jpg,.jpeg,.png"
										allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
										

										<f:attribute name="uploadedObj" value="#{docMandatory}" />

									</p:fileUpload>

								</p:column>

								<p:column headerText="" style="width: 75px;"
									rendered="#{employeeProfileBackingBean.checkView}">

									<p:commandLink
										actionListener="#{documentManagementBean.viewAction}"
										oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
										<p:graphicImage library="images" name="view.png"
											style="width:20px;height:20px;" title="view"></p:graphicImage>
										<f:attribute name="documentPathMandatory"
											value="#{docMandatory}" />
									</p:commandLink>

								</p:column>
							</p:dataTable>
						</h:panelGrid>
					</p:tab>
				</p:accordionPanel>

				<br></br>
				<br></br>

				<p:accordionPanel>
					<p:growl id="msgse" showDetail="false" />
					<p:tab title="Optional Documents">
						<h:panelGrid>
							<p:dataTable id="abcd" tableStyle="width:auto" var="docOptional"
								value="#{employeeProfileBackingBean.optionalList}" rows="10"
								paginator="true" sortOrder="ascending"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20,30,40,50">



								<p:column headerText="Code"
									style="width: 100px;text-align:left;">
									<h:outputText value="#{docOptional.doc_Code}" />
								</p:column>
								<p:column headerText="Description"
									style="width: 300px;text-align:left;">
									<h:outputText value="#{docOptional.add_Doc_Description}" />
								</p:column>
								<p:column headerText="File Path"
									style="width: 300px;text-align:left;">
									<h:outputText value="#{docOptional.uploadOptionalFilePath}" />
								</p:column>

								<p:column headerText="" style="width: 150px;"
									rendered="#{employeeProfileBackingBean.checkUpload}">
									<p:fileUpload
										fileUploadListener="#{documentManagementBean.saveUserManagementOptionalUploadDocuments}"
										mode="advanced" dragDropSupport="false" multiple="true"
										update="@form" auto="true" showButtons="false" fileLimit="1"
										accept=".pdf,.doc,.docx,.jpg,.jpeg,.png,.xls,.xlsl,.xlsm"
										allowTypes="/(\.|\/)(jpe?g|png|pdf|doc|docx|xls|xlsx|xlsm)$/"
										sizeLimit="15728640"
										fileLimitMessage="Please Upload Only One Document">
										

										<f:attribute name="uploadedOptionalObj" value="#{docOptional}" />


									</p:fileUpload>

								</p:column>

								<p:column headerText="" style="width: 75px;"
									rendered="#{employeeProfileBackingBean.checkView}">
									<p:commandLink
										actionListener="#{documentManagementBean.viewOptionalDocAction}"
										oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
										<p:graphicImage library="images" name="view.png"
											style="width:20px;height:20px;" title="view"></p:graphicImage>
										<f:attribute name="documentPathOptional"
											value="#{docOptional}" />
									</p:commandLink>
								</p:column>
							</p:dataTable>
						</h:panelGrid>
					</p:tab>
				</p:accordionPanel>


