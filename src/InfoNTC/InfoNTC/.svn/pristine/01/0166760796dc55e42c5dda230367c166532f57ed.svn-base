package lk.informatics.ntc.model.dto;

import java.io.Serializable;

public class MainCounterDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -669140115609927184L;
	private String counterId;
	private String counterName;
	private String servingQueueNo;
	private String sectionCode;

	public MainCounterDTO() {
		super();
	}

	public MainCounterDTO(String counterId, String counterName, String servingQueueNo, String sectionCode) {
		super();
		this.counterId = counterId;
		this.counterName = counterName;
		this.servingQueueNo = servingQueueNo;
		this.sectionCode = sectionCode;
	}

	public String getCounterId() {
		return counterId;
	}

	public void setCounterId(String counterId) {
		this.counterId = counterId;
	}

	public String getCounterName() {
		return counterName;
	}

	public void setCounterName(String counterName) {
		this.counterName = counterName;
	}

	public String getServingQueueNo() {
		return servingQueueNo;
	}

	public void setServingQueueNo(String servingQueueNo) {
		this.servingQueueNo = servingQueueNo;
	}

	public String getSectionCode() {
		return sectionCode;
	}

	public void setSectionCode(String sectionCode) {
		this.sectionCode = sectionCode;
	}

}
