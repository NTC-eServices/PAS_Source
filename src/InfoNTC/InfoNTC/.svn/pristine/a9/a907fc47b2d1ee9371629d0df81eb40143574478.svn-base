		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Upload Driver/Conductor Photos</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="frmDriConducPhoto">
					<br />

					<p:panel>
						<p:panelGrid id="selectPanel" columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>

								<p:selectOneRadio id="console"
									value="#{uploadDriverConductorPhotoBackingBean.forVal}"
									unselectable="true">

									<p:ajax
										listener="#{uploadDriverConductorPhotoBackingBean.showPanelData}"
										update="idNoId,driveridId,conductoridId,nameFull,appNoId" />
									<f:selectItem itemLabel="Drivers" itemValue="Drivers" />

									<f:selectItem itemLabel="Conductors" itemValue="Conductors" />


								</p:selectOneRadio>

							</p:panelGrid>

							<br></br>
							<p:panelGrid>



							</p:panelGrid>


						</p:panelGrid>
						<p:panelGrid id="searchPanel" columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Application No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>




							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="appNoId" tabindex="1" style="width:84%; "
									value="#{uploadDriverConductorPhotoBackingBean.uploadDriConducPhotoDTO.appNo}"
									disabled="#{uploadDriverConductorPhotoBackingBean.disableApp}"
									effect="fold" filter="true" filterMatchMode="contains">
									<p:ajax
										listener="#{uploadDriverConductorPhotoBackingBean.showFilterdData}"
										update="idNoId,driveridId,conductoridId,nameFull" />
									<f:selectItem itemLabel="-- Select Application No. --"
										itemValue="" />
									<f:selectItems
										value="#{uploadDriverConductorPhotoBackingBean.appNoNicNoListDropDown}"
										var="appNoListVar" itemValue="#{appNoListVar.appNo}"
										itemLabel="#{appNoListVar.appNo}" />
								</p:selectOneMenu>







							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="ID No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>


								<p:inputText id="idNoId" readonly="true"
									value="#{uploadDriverConductorPhotoBackingBean.uploadDriConducPhotoDTO.nic}"
									style="width:90%; "></p:inputText>



							</p:panelGrid>

							<p:panelGrid>
								<p:outputLabel value="Driver ID "></p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>


								<p:inputText id="driveridId" readonly="true"
									value="#{uploadDriverConductorPhotoBackingBean.uploadDriConducPhotoDTO.driverId}"
									style="width:90%; "></p:inputText>


							</p:panelGrid>



							<p:panelGrid>
								<p:outputLabel value="Conductor ID"></p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="conductoridId" readonly="true"
									value="#{uploadDriverConductorPhotoBackingBean.uploadDriConducPhotoDTO.conductorId}"
									style="width:90%; "></p:inputText>



							</p:panelGrid>

						</p:panelGrid>


						<p:panelGrid id="nameFull" columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-3" layout="grid">
							<p:panelGrid>
								<p:outputLabel value="Full Name"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="fullNameId" readonly="true"
									value="#{uploadDriverConductorPhotoBackingBean.uploadDriConducPhotoDTO.fullName}"
									style="width:310%; "></p:inputText>


							</p:panelGrid>
						</p:panelGrid>
						<br></br>

						<center>


							<p:commandButton value="Search" styleClass="mainActionbtn"
								id="searchButtId"
								actionListener="#{uploadDriverConductorPhotoBackingBean.searchAction}"
								update="@form" process="@form">


							</p:commandButton>



							<p:commandButton styleClass="mainActionbtn" value="Clear"
								process="@all"
								action="#{uploadDriverConductorPhotoBackingBean.clearButAction}"
								id="clear" update="@form"></p:commandButton>

						</center>

						<br></br>
						<p:panel style="border-bottom:5px solid #404d6f;" />


						<br></br>
						<br></br>

						<center>



							<p:panelGrid
								rendered="#{uploadDriverConductorPhotoBackingBean.showUploadIcon}"
								id="pnlImage">

								<p:fileUpload tabindex="21" id="fuimage"
									accept=".jpg,.jpeg,.png" allowTypes="/(\.|\/)(jpe?g|png)$/"
									sizeLimit="15728640"
									fileUploadListener="#{uploadDriverConductorPhotoBackingBean.fileUploadListener}"
									auto="true" update="a">
								</p:fileUpload>
								<p:panel id="a"
									style="border-color: #b7c7e0;border-style: solid;border-width: initial; width: 260px;">
									<img id="img"
										src="data:image/png;base64,#{uploadDriverConductorPhotoBackingBean.imageContentsAsBase64}"
										style="width: 240px; height: 240px;" />
								</p:panel>

								<p:outputLabel value="Driver/Conductor" style=" color:#2e07b8;"></p:outputLabel>
							</p:panelGrid>
						</center>

						<br></br>

						<center>


							<p:commandButton
								rendered="#{uploadDriverConductorPhotoBackingBean.showUploadIcon}"
								disabled="#{uploadDriverConductorPhotoBackingBean.saveButtonShow}"
								value="Save" styleClass="mainActionbtn" id="saveButtId"
								actionListener="#{uploadDriverConductorPhotoBackingBean.saveAction}"
								update="@form" process="@form">


							</p:commandButton>



							<p:commandButton
								rendered="#{uploadDriverConductorPhotoBackingBean.showUploadIcon}"
								styleClass="mainActionbtn" value="Clear" process="@all"
								action="#{uploadDriverConductorPhotoBackingBean.clearButAction2}"
								id="clear2" update="@form"></p:commandButton>


							<p:commandButton
								rendered="#{uploadDriverConductorPhotoBackingBean.showUploadIcon}"
								styleClass="mainActionbtn" value="Delete" process="@form"
								action="#{uploadDriverConductorPhotoBackingBean.deleteButAction}"
								id="uploadDelete" update="@form"></p:commandButton>

						</center>

					</p:panel>

				</h:form>


			</div>

		</div>




		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel
							value="#{uploadDriverConductorPhotoBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField" value="OK"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>



		<p:dialog styleClass="" id="successSave" global="true" modal="true"
			header="Success" width="30%" severity="info" widgetVar="successSave"
			closeOnEscape="true" resizable="false">
			<h:form id="frmsuccessSave">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel
							value="#{uploadDriverConductorPhotoBackingBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmsuccessSave" value="OK"
						onclick="PF('successSave').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>




		<p:dialog styleClass="" id="dlgRejectConfirm" global="true"
			modal="true" header="Warning" width="30%" severity="info"
			widgetVar="dlgRejectConfirm" closeOnEscape="true" resizable="false">
			<h:form id="frmDriConducPhotoDel">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>
						<p:outputLabel value="Do you want to delete this photo ?"></p:outputLabel>
					</p:panelGrid>
					<br></br>
					<p:commandButton id="yes" value="Yes"
						style="margin-top:10px;margin-bottom:10px"
						onclick="PF('dlgRejectConfirm').hide() "
						actionListener="#{uploadDriverConductorPhotoBackingBean.deleteActionForYes}"
						update="@all"></p:commandButton>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="No" onclick="PF('dlgRejectConfirm').hide();"
						actionListener="#{uploadDriverConductorPhotoBackingBean.deleteActionForNo}"
						update="@all">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>











	</ui:define>
</ui:composition>
