<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 79% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}

.ui-datatable-scrollable .ui-datatable-scrollable-header {
	min-height: 20px;
}

.ui-datatable-scrollable-body {
	height: 125px !important;
}

.genReqNumButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	text-decoration: none !important;
	color: white !important;
	border-radius: 5px !important;
}

.high {
	background-color: #e4a6a6 !important;
	background-image: none !important;
	color: #000000 !important;
}

.middle {
	background-color: #f5ba76 !important;
	background-image: none !important;
	color: #000000 !important;
}

.low {
	background-color: #f8e7ac !important;
	background-image: none !important;
	color: #000000 !important;
}
</style>

		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Subsidy Management Information</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="manageInqFrm">
					<p:panel id="pnl-search">
						<p:panelGrid columns="3"
							columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Subsidy Type">
									<span style="color: red">*</span>
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="subsidyType" style="width:85%;"
									effect="fold" filter="true" filterMatchMode="contains"
									value="#{subsidyManagementInforBackingBean.searchDto.serviceTypeCode}">
									<f:selectItem itemLabel="--Select Subsidy Type--" itemValue="" />
									<f:selectItems
										value="#{subsidyManagementInforBackingBean.getServiceTypeList}"
										var="typeList" itemLabel="#{typeList.serviceTypeDes}"
										itemValue="#{typeList.serviceTypeCode}" />

									<p:ajax
										listener="#{subsidyManagementInforBackingBean.onsubsidyTypeChange}"
										update="vehicleNo,nameofOparator,requestNo,refNo,serviceNo" />
								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Vehicle No.">
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="vehicleNo" style="width:85%;" filter="true"
									filterMatchMode="contains"
									disabled="#{subsidyManagementInforBackingBean.disableFieldVN}"
									value="#{subsidyManagementInforBackingBean.searchDto.busRegNo}">
									<f:selectItem itemLabel="--Select Vehicle No.--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems
										value="#{subsidyManagementInforBackingBean.vehiDropDownListData}"
										var="vehiList" itemLabel="#{vehiList.busRegNo}"
										itemValue="#{vehiList.busRegNo}" />
									<p:ajax
										listener="#{subsidyManagementInforBackingBean.onvalChange(1)}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Name of Oparator">
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="nameofOparator" style="width:85%;"
									effect="fold" filter="true" filterMatchMode="contains"
									disabled="#{subsidyManagementInforBackingBean.disableFieldNP}"
									value="#{subsidyManagementInforBackingBean.searchDto.nameOfOperator}">
									<f:selectItem itemLabel="--Select Name of Oparator--"
										itemValue="" />
									<f:selectItems
										value="#{subsidyManagementInforBackingBean.nameDropDownListData}"
										var="operateList" itemLabel="#{operateList.nameOfOperator}"
										itemValue="#{operateList.nameOfOperator}" />
									<p:ajax
										listener="#{subsidyManagementInforBackingBean.onvalChange(2)}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Request No.">
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="requestNo" style="width:85%;" effect="fold"
									filter="true" filterMatchMode="contains"
									disabled="#{subsidyManagementInforBackingBean.disableFieldRN}"
									value="#{subsidyManagementInforBackingBean.searchDto.requestNo}">
									<f:selectItem itemLabel="--Select Request No.--" itemValue="" />
									<f:selectItems
										value="#{subsidyManagementInforBackingBean.reqDropDownListData}"
										var="reqNoList" itemLabel="#{reqNoList.requestNo}"
										itemValue="#{reqNoList.requestNo}" />
									<p:ajax
										listener="#{subsidyManagementInforBackingBean.onvalChange(3)}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Reference No.">
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="refNo" style="width:85%;" effect="fold"
									filter="true" filterMatchMode="contains"
									disabled="#{subsidyManagementInforBackingBean.disableFieldSR}"
									value="#{subsidyManagementInforBackingBean.searchDto.serviceRefNo}">
									<f:selectItem itemLabel="--Select Service Reference No.--"
										itemValue="" />
									<f:selectItems
										value="#{subsidyManagementInforBackingBean.refDropDownListData}"
										var="refNoList" itemLabel="#{refNoList.serviceRefNo}"
										itemValue="#{refNoList.serviceRefNo}" />
									<p:ajax
										listener="#{subsidyManagementInforBackingBean.onvalChange(4)}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Service Agreement No.">
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="serviceNo" style="width:85%;" effect="fold"
									filter="true" filterMatchMode="contains"
									disabled="#{subsidyManagementInforBackingBean.disableFieldSA}"
									value="#{subsidyManagementInforBackingBean.searchDto.serviceAgreementNo}">
									<f:selectItem itemLabel="--Select Service Agreement No.--"
										itemValue="" />
									<f:selectItems
										value="#{subsidyManagementInforBackingBean.agreDropDownListData}"
										var="agreeNoList"
										itemLabel="#{agreeNoList.serviceAgreementNo}"
										itemValue="#{agreeNoList.serviceAgreementNo}" />
									<p:ajax
										listener="#{subsidyManagementInforBackingBean.onvalChange(5)}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>

						</p:panelGrid>
					</p:panel>

					<p:panelGrid columns="4" style="margin-top:2%;"
						columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
						layout="grid">

						<p:panelGrid></p:panelGrid>

						<p:commandButton id="searchBtn" styleClass="mainLinkButton"
							style="width: 100%" value="Search" update="@form"
							action="#{subsidyManagementInforBackingBean.searchButtonAction}">

						</p:commandButton>

						<p:commandButton id="clearBtn" styleClass="mainLinkButton"
							style="width: 100%" value="Clear" update="@form"
							action="#{subsidyManagementInforBackingBean.clearButtonAction}">
						</p:commandButton>
						<p:panelGrid></p:panelGrid>
					</p:panelGrid>

					<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
						layout="grid" style="margin-top:2%;">
						<p:dataTable id="subsidyDTId" var="subsidyDT"
							value="#{subsidyManagementInforBackingBean.searchedDataList}"
							tableStyle="width:auto;font-size:12px;"
							rowsPerPageTemplate="5,10,15">

							<p:column headerText="Subsidy Type" style="text-align:left">
								<h:outputText value="#{subsidyDT.serviceTypeDes}" />
							</p:column>
							<p:column headerText="Vehicle No." style="text-align:left">
								<h:outputText value="#{subsidyDT.busRegNo}" />
							</p:column>
							<p:column headerText="Name of Oparator" style="text-align:left">
								<h:outputText value="#{subsidyDT.nameOfOperator}" />
							</p:column>
							<p:column headerText="Request No." style="text-align:left">
								<h:outputText value="#{subsidyDT.requestNo}" />
							</p:column>
							<p:column headerText="Service Reference No."
								style="text-align:left">
								<h:outputText value="#{subsidyDT.serviceRefNo}" />
							</p:column>
							<p:column headerText="Service No." style="text-align:left">
								<h:outputText value="#{subsidyDT.serviceNo}" />
							</p:column>
							<p:column headerText="Service Agreement No."
								style="text-align:left">
								<h:outputText value="#{subsidyDT.serviceAgreementNo}" />
							</p:column>
							<p:column headerText="Origin" style="text-align:left">
								<h:outputText value="#{subsidyDT.originDes}" />
							</p:column>
							<p:column headerText="Destination" style="text-align:left">
								<h:outputText value="#{subsidyDT.destinationDes}" />
							</p:column>
							<p:column headerText="School Name" style="text-align:left">
								<h:outputText value="#{subsidyDT.schoolName}" />
							</p:column>
							<p:column headerText="NIC No." style="text-align:left">
								<h:outputText value="#{subsidyDT.nicNo}" />
							</p:column>
							<p:column headerText="Address" style="text-align:left">
								<h:outputText value="#{subsidyDT.addressOne}" />
							</p:column>
							<p:column headerText="Province" style="text-align:left">
								<h:outputText value="#{subsidyDT.provinceDes}" />
							</p:column>
							<p:column headerText="Service Expire Date"
								style="text-align:left">
								<h:outputText value="#{subsidyDT.expiryDateObj}">
									<f:convertDateTime type="date" pattern="dd/MM/yyyy" />
								</h:outputText>
							</p:column>
							<p:column headerText="Status" width="5%">
								<h:outputText value="#{subsidyDT.status}" />
							</p:column>
						</p:dataTable>
					</p:panelGrid>
				</h:form>
			</div>
		</div>
		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">

			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{subsidyManagementInforBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmrequiredField"
						onclick="PF('requiredField').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="Success" width="30%" severity="info" widgetVar="successSve"
			closeOnEscape="true" resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel
							value="#{subsidyManagementInforBackingBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton tabindex="1"
						style="margin-top:10px;margin-bottom:10px" process="frmsuccessSve"
						value="OK" onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>
	</ui:define>
</ui:composition>