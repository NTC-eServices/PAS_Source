<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}
</style>

		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Bus Owner Change -Committee Approval
			</h1>
			<div style="width: 95%;" class="subDiv">




				<h:form id="frmCommitteeApproval">
					<br />
					<panel> <p:panelGrid columns="4"
						columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
						layout="grid">


						<p:outputLabel>Permit No.</p:outputLabel>

						<p:selectOneMenu id="permitNo" value="" style="width:150px">
							<f:selectItem itemLabel="--All--" itemValue=""
								noSelectionOption="true" />
							<f:selectItems value="" />
						</p:selectOneMenu>

						<p:outputLabel>Date</p:outputLabel>

						<p:inputText style="width:70%;height:18px;" maxlength="50"
							value="" onkeyup="this.value=this.value.toUpperCase();">
						</p:inputText>


						<p:outputLabel>Application No.</p:outputLabel>

						<p:selectOneMenu id="appNo" value="" style="width:150px">
							<f:selectItem itemLabel="--All--" itemValue=""
								noSelectionOption="true" />
							<f:selectItems value="" />
						</p:selectOneMenu>
						<p:outputLabel>Type of Owner Change</p:outputLabel>

						<p:selectOneMenu id="ownerChange" value="" style="width:150px">
							<f:selectItem itemLabel="--All--" itemValue=""
								noSelectionOption="true" />
							<f:selectItems value="" />
						</p:selectOneMenu>


						<p:outputLabel>Service Type</p:outputLabel>

						<p:selectOneMenu id="serviceType" value="" style="width:70%">
							<f:selectItem itemLabel="--All--" itemValue=""
								noSelectionOption="true" />
							<f:selectItems value="" />
						</p:selectOneMenu>

						<p:outputLabel>Bus change Require? yes</p:outputLabel>
						<p:selectBooleanCheckbox value="" />



						<p:outputLabel>Excisting Bus No.</p:outputLabel>
						<p:inputText style="width:80%;height:18px;" maxlength="50"
							value="" onkeyup="this.value=this.value.toUpperCase();">
						</p:inputText>


					</p:panelGrid> </panel>

					<center>
						<p:panel>
							<!-- Search button -->
							<p:commandButton styleClass="mainActionbtn" value="Search"
								ajax="false" update="@all"></p:commandButton>
							<!-- Clear button -->
							<p:commandButton styleClass="mainActionbtn" value="Clear"
								ajax="false" update="@all"></p:commandButton>
						</p:panel>
					</center>

					<br />

					<p:panel>
						<p:panelGrid columns="2">
							<p:panelGrid>
								<p:outputLabel value="Committee/Board reference No"></p:outputLabel>


							</p:panelGrid>
							<p:panelGrid>
								<p:inputText></p:inputText>
							</p:panelGrid>

						</p:panelGrid>
						<center>
							<p:dataTable id="SearchDetailsGrid" var="showDetails" rows="10"
								paginator="true"
								value="#{newPermitPrintingBackingBean.showDetailsForGrid}"
								selection="#{newPermitPrintingBackingBean.selectList}"
								rowKey="#{showDetails.applicationNo}" selectionMode="single"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" rowIndexVar="index" styleClass="myTable"
								emptyMessage="No Users are available">

								<p:column visible="false" style="width:16px;text-align:center" />
								<p:column headerText="Username" style="text-align:left;"
									visible="true">
									<h:outputText style="text-align:left;"
										value="#{showDetails.applicationNo}" visible="true" />
								</p:column>
								<p:column headerText="Responsibility" style="text-align:left;">
									<h:outputText style="text-align:left;"
										value="#{showDetails.serviceType}" />
								</p:column>
								<p:column headerText="Status" style="text-align:left;"
									width="80px">
									<h:outputText style="text-align:left;"
										value="#{showDetails.tenderRefNo}" />
								</p:column>
								<p:column headerText="Transaction Date" style="text-align:left;">
									<h:outputText style="text-align:left;"
										value="#{showDetails.busRegNo}" />
								</p:column>
								<p:column headerText="Remarks" style="text-align:left;"
									width="200px">
									<h:outputText style="text-align:left;"
										value="#{showDetails.vehicleOwner}" />
								</p:column>





							</p:dataTable>
						</center>


						<br></br>


						<center>

							<p:panelGrid>

								<p:row id="row1">

									<p:panelGrid columns="2">
										<p:column>
											<p:outputLabel value="Remarks" for="remarks"></p:outputLabel>

										</p:column>
										<p:column>
											<p:inputTextarea rows="5" cols="30" counter="display"
												maxlength="100" counterTemplate="{0} characters remaining."
												autoResize="false" id="remarks" style="width:500px" value=""
												onkeyup="this.value=this.value.toUpperCase();" />
											<h:outputText id="display" />

										</p:column>
									</p:panelGrid>


								</p:row>

							</p:panelGrid>
						</center>
						<br></br>
						<br></br>
						<center>
							<p:panelGrid>
								<p:row id="row2">

									<p:panelGrid columns="4">

										<p:column>
											<p:commandButton id="cmdbtnApprove" value="Approve"
												class="mainActionbtn" ajax="false"
												action="#{newPermitPrintingBackingBean.approveUser}"
												style="width:150px"></p:commandButton>
										</p:column>
										<p:column>
											<p:commandButton id="cmdbtnReject" value="Reject "
												class="mainActionbtn" ajax="false"
												action="#{newPermitPrintingBackingBean.rejectUser}"
												update="SearchDetailsGrid" style="width:150px"></p:commandButton>
										</p:column>

										<p:column>
											<p:commandButton id="cmdbtnDocmangment"
												value="Document Managment " class="mainActionbtn"
												ajax="false"
												action="#{newPermitPrintingBackingBean.rejectUser}"
												update="userTable" style="width:170px" disabled="false"></p:commandButton>
										</p:column>
										<p:column>
											<p:commandButton id="cmdbtnCancel" value="Cancel"
												class="mainActionbtn" ajax="false"
												actionListener="#{newPermitPrintingBackingBean.clearSearch}"
												update="rejectReason" style="width:150px"></p:commandButton>
										</p:column>
									</p:panelGrid>


								</p:row>


							</p:panelGrid>
						</center>
						<br></br>




					</p:panel>


				</h:form>




			</div>
		</div>
		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{permitRenewalsBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField" value="OK"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>
		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="Success" width="30%" severity="info" widgetVar="successSve"
			closeOnEscape="true" resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel value="#{permitRenewalsBackingBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton tabindex="1"
						style="margin-top:10px;margin-bottom:10px" process="frmsuccessSve"
						value="OK" onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>





	</ui:define>
</ui:composition>
