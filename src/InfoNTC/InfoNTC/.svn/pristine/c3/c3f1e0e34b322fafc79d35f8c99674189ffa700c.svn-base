package lk.informatics.ntc.model.exception;

public class PasswordLengthNotEnoughException extends Exception {

	private static final long serialVersionUID = -2351392372304857934L;

	public PasswordLengthNotEnoughException() {
	}

	public PasswordLengthNotEnoughException(String message) {
		super(message);
	}

	public PasswordLengthNotEnoughException(Throwable cause) {
		super(cause);
	}

	public PasswordLengthNotEnoughException(String message, Throwable cause) {
		super(message, cause);
	}

}
