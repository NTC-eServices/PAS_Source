								for="mrissuedate">
								<span style="color: red;">*</span>
							</p:outputLabel>


							<p:calendar id="mrissuedate" tabindex="31"
								disabled="#{driverConductorRegistrationBean.registrationDto.trainingType.code != 'ND'}"
								value="#{driverConductorRegistrationBean.registrationDto.mrissuedate}"
								style="width:220px;height:18px;" navigator="true" locale="en_US"
								showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
								yearRange="c-50:c+50" readonlyInput="true"></p:calendar>

							<p:outputLabel value="Expire Date  for Medical Report"
								for="mrexpiredate">
								<span style="color: red;">*</span>
							</p:outputLabel>


							<p:calendar id="mrexpiredate" tabindex="32"
								disabled="#{driverConductorRegistrationBean.registrationDto.trainingType.code != 'ND'}"
								value="#{driverConductorRegistrationBean.registrationDto.mrexpiredate}"
								style="width:220px;height:18px;" navigator="true" locale="en_US"
								showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
								yearRange="c-50:c+50" readonlyInput="true"></p:calendar>


							<p:outputLabel value="Registration No. of Current Service Bus"
								for="vehi">
							</p:outputLabel>

							<p:inputText id="vehi" style="width:95%;height:18px;"
								tabindex="33"
								value="#{driverConductorRegistrationBean.registrationDto.vehicleRegNo}"
								onkeyup="this.value=this.value.toUpperCase()" maxlength="20">
								<p:ajax
									listener="#{driverConductorRegistrationBean.onVehicleChange}"
									update="permit,route,txtfrom,txtto" />
							</p:inputText>
						</p:panelGrid>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">


							<p:outputLabel value="Permit No." for="permit">
							</p:outputLabel>

							<p:selectOneMenu id="permit" tabindex="34" style="width:84%; "
								value="#{driverConductorRegistrationBean.registrationDto.permitNo}"
								onkeyup="this.value=this.value.toUpperCase()" effect="fold"
								filter="true" filterMatchMode="contains">
								<p:ajax
									listener="#{driverConductorRegistrationBean.onPermitChange}"
									update="route,txtfrom,txtto" />

								<f:selectItem itemLabel="-- Select Permit No. --" itemValue="" />
								<f:selectItems
									value="#{driverConductorRegistrationBean.permitList}"
									var="permit" itemValue="#{permit.permitNo}"
									itemLabel="#{permit.permitNo}" />
							</p:selectOneMenu>

							<p:outputLabel value="Route No." for="route">
							</p:outputLabel>

							<p:inputText id="route" style="width:85%;height:18px;"
								tabindex="35" readonly="true"
								value="#{driverConductorRegistrationBean.registrationDto.routeNo}"
								maxlength="20">

							</p:inputText>

						</p:panelGrid>
						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">

							<p:outputLabel value="From (Origin)" for="txtfrom">
							</p:outputLabel>
							<p:inputText id="txtfrom" style="width:95%;height:18px;"
								tabindex="36" readonly="true"
								value="#{driverConductorRegistrationBean.registrationDto.from}">
							</p:inputText>


							<p:outputLabel value="To (Destination)" for="txtto">
							</p:outputLabel>
							<p:inputText id="txtto" style="width:95%;height:18px;"
								tabindex="37" readonly="true"
								value="#{driverConductorRegistrationBean.registrationDto.to}">
							</p:inputText>

							<p:outputLabel value="Remarks" for="txtRemarks">
							</p:outputLabel>
							<p:inputTextarea id="txtRemarks" style="width:95%;height:18px;"
								tabindex="38" maxlength="300" rows="3"
								value="#{driverConductorRegistrationBean.registrationDto.remarks}">
							</p:inputTextarea>

						</p:panelGrid>
						<p:panelGrid>
							<center>

								<p:commandButton styleClass="mainActionbtn" value="Update"
									rendered="#{driverConductorRegistrationBean.updateMode}"
									update="frmDriConReg" tabindex="39" process="frmDriConReg"
									action="#{driverConductorRegistrationBean.update}"></p:commandButton>
								<p:commandButton styleClass="mainActionbtn" value="Save"
									rendered="#{driverConductorRegistrationBean.saveMode}"
									update="frmDriConReg" tabindex="40" process="frmDriConReg"
									action="#{driverConductorRegistrationBean.save}"></p:commandButton>
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									update="frmDriConReg" tabindex="41" process="frmDriConReg"
									action="#{driverConductorRegistrationBean.clear}"></p:commandButton>
								<p:commandButton styleClass="mainActionbtn"
									value="Document Management" update="uploadDocumentsDialog"
									tabindex="42" process="frmDriConReg" style="width:18%"
									action="#{driverConductorRegistrationBean.documentManagement}"
									disabled="#{driverConductorRegistrationBean.disDocument}"></p:commandButton>
								<!--  -->


							</center>
						</p:panelGrid>

						<p:panelGrid></p:panelGrid>

					</p:panelGrid>


				</h:form>

			</div>
		</div>
		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{driverConductorRegistrationBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField" value="OK"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="Success" width="30%" severity="info" widgetVar="successSve"
			closeOnEscape="true" resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel
							value="#{driverConductorRegistrationBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton tabindex="1"
						style="margin-top:10px;margin-bottom:10px" process="frmsuccessSve"
						value="OK" onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<p:dialog widgetVar="dlgRejectConfirm" resizable="false" modal="true">
			<f:facet name="header">
				<h:outputText value="Warning" />
			</f:facet>
			<p:panelGrid columns="1">
				<h:panelGrid columns="3"
					columnClasses="msg-box-icon-col msg-box-msg-col">
					<p:graphicImage library="images" name="warning.png"></p:graphicImage>
					<p:spacer width="10"></p:spacer>

					<h:outputText
						value="Entered NIC No. already have record for selected Training Type. Do you want to continue?" />
				</h:panelGrid>

				<h:panelGroup style="display:block; text-align:center">
					<p:commandButton value="Yes" style="width:80px"
						onclick="PF('dlgRejectConfirm').hide() "
						actionListener="#{driverConductorRegistrationBean.proceedAction}"
						update="frmDriConReg" process="@all" />
					<p:commandButton value="No" style="width:80px;"
						onclick="PF('dlgRejectConfirm').hide();"
						actionListener="#{driverConductorRegistrationBean.deleteRecordAction}"
						update="frmDriConReg" process="@all" />
				</h:panelGroup>
			</p:panelGrid>
		</p:dialog>

		<p:dialog id="uploadDocumentsDialog" global="true" modal="true"
			header="Upload Documents" width="80%" height="400px" severity="info"
			widgetVar="uploadDocument" closeOnEscape="true" resizable="false"
			style="postion: absolute;">


			<h:form id="frmdoc">

				<p:tabView>
					<p:tab title="Upload Documents">

						<p:accordionPanel>
							<p:growl id="msgs" showDetail="false" />
							<p:tab title="Mandatory Documents">


								<h:panelGrid>
									<p:dataTable id="abc" tableStyle="width:auto"
										var="docMandatory"
										value="#{driverConductorRegistrationBean.mandatoryList}"
										rows="5" paginator="true" sortOrder="ascending"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,10,15">

										<p:column headerText="Code"
											style="width: 100px; text-align: left;">
											<h:outputText value="#{docMandatory.doc_Code}" />
										</p:column>
										<p:column headerText="Description"
											style="width: 300px;text-align: left;">
											<h:outputText value="#{docMandatory.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path"
											style="width: 300px;text-align: left;">

											<h:outputText value="#{docMandatory.uploadFilePath}" />
										</p:column>

										<p:column style="width: 150px !important;">
											<!-- "#{editPermitIssuanceBackingBean.strSelectedTitle}" -->

											<p:fileUpload
												fileUploadListener="#{documentManagementBean.saveDriverConductorMandatoryUploadDocuments}"
												mode="advanced" dragDropSupport="false" multiple="true"
												auto="true" showButtons="false" update="@form" fileLimit="1"
												accept=".pdf,.jpg,.jpeg,.png"
												allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
												

												<f:attribute name="uploadedObj" value="#{docMandatory}" />

											</p:fileUpload>

										</p:column>

									</p:dataTable>
								</h:panelGrid>

							</p:tab>
						</p:accordionPanel>

						<br></br>
						<br></br>

						<p:accordionPanel>
							<p:growl id="msgse" showDetail="false" />
							<p:tab title="Optional Documents">


								<h:panelGrid>
									<p:dataTable id="abcd" tableStyle="width:auto"
										var="docOptional"
										value="#{driverConductorRegistrationBean.optionalList}"
										rows="5" paginator="true" sortOrder="ascending"
										scrollable="true" scrollHeight="100%" liveResize="true"
										liveScroll="true"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,10,15">



										<p:column headerText="Code"
											style="width: 100px;text-align: left;">
											<h:outputText value="#{docOptional.doc_Code}" />
										</p:column>
										<p:column headerText="Description"
											style="width: 300px;text-align: left;">
											<h:outputText value="#{docOptional.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path"
											style="width: 300px;text-align: left;">
											<h:outputText value="#{docOptional.uploadOptionalFilePath}" />
										</p:column>

										<p:column style="width: 150px;">
											<!--  #{driverConductorRegistrationBean.strSelectedTitle} -->
											<p:fileUpload
												fileUploadListener="#{documentManagementBean.saveDriverConductorOptionalUploadDocuments}"
												mode="advanced" dragDropSupport="false" multiple="true"
												auto="true" showButtons="false" update="@form" fileLimit="1"
												accept=".pdf,.jpg,.jpeg,.png"
												allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640"
												fileLimitMessage="Please Upload Only One Document">
												

												<f:attribute name="uploadedOptionalObj"
													value="#{docOptional}" />



											</p:fileUpload>

										</p:column>
									</p:dataTable>
								</h:panelGrid>
							</p:tab>
						</p:accordionPanel>

					</p:tab>

					<p:tab title="Upload History">


						<p:accordionPanel multiple="true">
							<p:growl showDetail="false" />
							<p:tab title="Driver Conductor (Documents)" closable="true">


								<p:accordionPanel id="dkfj">
									<p:growl id="NP" showDetail="false" />
									<p:tab title="Mandatory Documents">


										<h:panelGrid>
											<p:dataTable id="mandatoryNP" tableStyle="width:auto"
												var="docMandatory"
												value="#{sessionBackingBean.sisuSariyaMandatoryDocumentList}"
												rows="5" paginator="true" sortOrder="ascending"
												paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
												rowsPerPageTemplate="5,10,15">

												<p:column headerText="Code"
													style="width: 100px;text-align: left;">
													<h:outputText value="#{docMandatory.doc_Code}" />
												</p:column>
												<p:column headerText="Description"
													style="width: 300px;text-align: left;">
													<h:outputText value="#{docMandatory.add_Doc_Description}" />
												</p:column>
												<p:column headerText="File Name"
													style="width: 200px;text-align: left;">

													<h:outputText value="#{docMandatory.documentName}" />
												</p:column>

												<p:column headerText="" style="width: 75px;">

													<p:commandLink
														actionListener="#{documentManagementBean.viewMandatoryUploadDocs}"
														oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
														<p:graphicImage library="images" name="view.png"
															style="width:20px;height:20px;" title="view"></p:graphicImage>
														<f:attribute name="documentMandatory"
															value="#{docMandatory}" />
													</p:commandLink>



												</p:column>


											</p:dataTable>
										</h:panelGrid>

									</p:tab>
								</p:accordionPanel>

								<br></br>
								<br></br>

								<p:accordionPanel>

									<p:growl showDetail="false" />

									<p:tab title="Optional Documents">

										<h:panelGrid>
											<p:dataTable id="optionalNP" tableStyle="width:auto"
												var="docOptional"
												value="#{sessionBackingBean.sisuSariyaOptionalDocumentList}"
												rows="5" paginator="true" sortOrder="ascending"
												scrollHeight="100%" liveResize="true" liveScroll="true"
												paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
												rowsPerPageTemplate="5,10,15">

												<p:column headerText="Code"
													style="width: 100px;text-align: left;">
													<h:outputText value="#{docOptional.doc_Code}" />
												</p:column>
												<p:column headerText="Description"
													style="width: 300px;text-align: left;">
													<h:outputText value="#{docOptional.add_Doc_Description}" />
												</p:column>
												<p:column headerText="File Name"
													style="width: 200px;text-align: left;">
													<h:outputText value="#{docOptional.documentName}" />
												</p:column>

												<p:column headerText="" style="width: 75px;">

													<p:commandLink
														actionListener="#{documentManagementBean.viewUploadedOptionalDocAction}"
														oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
														<p:graphicImage library="images" name="view.png"
															style="width:20px;height:20px;" title="view"></p:graphicImage>
														<f:attribute name="documentOptional"
															value="#{docOptional}" />
													</p:commandLink>

												</p:column>


											</p:dataTable>
										</h:panelGrid>
									</p:tab>

								</p:accordionPanel>

							</p:tab>
							<br></br>


						</p:accordionPanel>
					</p:tab>
				</p:tabView>







			</h:form>
		</p:dialog>



	</ui:define>
</ui:composition>
