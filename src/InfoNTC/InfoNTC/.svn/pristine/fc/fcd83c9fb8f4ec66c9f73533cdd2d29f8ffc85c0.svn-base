		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Driver/Conductor Registration</h1>

			<div style="width: 95%;" class="subDiv">


				<h:form id="frmDriConReg">
					<p:panelGrid columns="2"
						columnClasses="ui-grid-col-10,ui-grid-col-2" layout="grid">
						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<div style="border-style: solid;">
								<div>
									<p:outputLabel value="Total Points" style="font-weight:bold;"></p:outputLabel>
								</div>
								<div
									style="border-style: solid; background-color: yellow; Height: 35px; width: 82%;">
									<b><p:outputLabel id="points"
											style="font-size:20pt;text-align:center;"
											value="#{driverConductorRegistrationBean.totalPoints}">
										</p:outputLabel></b>
								</div>
							</div>

						</p:panelGrid>
					</p:panelGrid>

					<p:panelGrid columns="4"
						columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
						layout="grid">
						<p:outputLabel value="Training Language Medium"
							for="typeOfTraining">
						</p:outputLabel>

						<p:selectOneMenu id="language" tabindex="0" style="width:84%; "
							value="#{driverConductorRegistrationBean.selectedLanguage}"
							effect="fold" filter="true" filterMatchMode="contains">

							<f:selectItem itemLabel="Sinhala" itemValue="S" />
							<f:selectItem itemLabel="Tamil" itemValue="T" />
							<f:selectItem itemLabel="English" itemValue="E" />
						</p:selectOneMenu>
						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<p:outputLabel value="Type of Training" for="typeOfTraining">
							<span style="color: red;">*</span>
						</p:outputLabel>

						<p:selectOneMenu id="typeOfTraining" tabindex="1"
							style="width:84%; "
							disabled="#{driverConductorRegistrationBean.disableModeTrnType}"
							value="#{driverConductorRegistrationBean.registrationDto.trainingType.code}"
							effect="fold" filter="true" filterMatchMode="contains">
							<p:ajax
								listener="#{driverConductorRegistrationBean.onTrainingTypeChange}"
								update="points,typeOfTraining,license,dateOfIssueLicence,dateOfExpiryLicence,medicalcerNo,mrissuedate,mrexpiredate" />

							<f:selectItem itemLabel="-- Select Training Type --" itemValue="" />
							<f:selectItems
								value="#{driverConductorRegistrationBean.trainingTypeList}"
								var="type" itemValue="#{type.code}"
								itemLabel="#{type.description}" />
						</p:selectOneMenu>


						<p:outputLabel value="Application No." for="appNo"></p:outputLabel>


						<p:inputText
							style="width:85%;height:18px;color:red;font-weight:bold;"
							value="#{driverConductorRegistrationBean.registrationDto.appNo}"
							id="appNo" onkeyup="this.value=this.value.toUpperCase();"
							readonly="true">
						</p:inputText>

						<p:outputLabel value="ID Number" for="nic">
							<span style="color: red;">*</span>
						</p:outputLabel>

						<p:inputText style="width:95%;height:18px;" tabindex="2"
							value="#{driverConductorRegistrationBean.registrationDto.nic}"
							id="nic" onkeyup="this.value=this.value.toUpperCase();"
							disabled="#{driverConductorRegistrationBean.disableMode}"
							maxlength="12">
							<f:ajax event="keyup"
								listener="#{driverConductorRegistrationBean.validateNicFromUserinput}"
								execute="@this" />
						</p:inputText>


						<p:outputLabel value="Driver/Conductor ID" for="id"></p:outputLabel>


						<p:inputText
							style="width:85%;height:18px;color:green;font-weight:bold;"
							value="#{driverConductorRegistrationBean.registrationDto.driverConductorId}"
							id="id" onkeyup="this.value=this.value.toUpperCase();"
							readonly="true">
						</p:inputText>
					</p:panelGrid>
					<p:panelGrid columns="2"
						columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">
						<p:outputLabel value="Name with Initials" for="name">
							<span style="color: red;">*</span>
						</p:outputLabel>

						<p:inputText style="width:95%;height:18px;" tabindex="3"
							value="#{driverConductorRegistrationBean.registrationDto.nameWithInitials}"
							id="name" onkeyup="this.value=this.value.toUpperCase();"
							maxlength="100">
							<p:keyFilter regEx="/[a-z_\.\s\'\-]/i" />
						</p:inputText>

						<p:outputLabel value="Full Name" for="fullName">
							<span style="color: red;">*</span>
						</p:outputLabel>

						<p:inputText style="width:95%;height:18px;" tabindex="4"
							value="#{driverConductorRegistrationBean.registrationDto.fullNameEng}"
							id="fullName" onkeyup="this.value=this.value.toUpperCase();"
							maxlength="100">
							<p:keyFilter regEx="/[a-z_\.\s\'\-]/i" />
						</p:inputText>

						<p:outputLabel value="Full Name(Sinhala)" for="fullNameSin">

						</p:outputLabel>

						<p:inputText style="width:95%;height:18px;" tabindex="5"
							value="#{driverConductorRegistrationBean.registrationDto.fullNameSin}"
							id="fullNameSin" maxlength="100">

						</p:inputText>
						<p:outputLabel value="Full Name(Tamil)" for="fullNameTam">

						</p:outputLabel>

						<p:inputText style="width:95%;height:18px;" tabindex="6"
							value="#{driverConductorRegistrationBean.registrationDto.fullNameTam}"
							id="fullNameTam" maxlength="100">

						</p:inputText>

					</p:panelGrid>
					<p:panelGrid columns="4"
						columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
						layout="grid">
						<p:outputLabel value="Gender" for="gender">
							<span style="color: red;">*</span>
						</p:outputLabel>


						<p:selectOneMenu id="gender" tabindex="7" style="width:84%; "
							value="#{driverConductorRegistrationBean.registrationDto.gender.code}"
							effect="fold" filter="true" filterMatchMode="contains">

							<f:selectItem itemLabel="-- Select Gender --" itemValue="" />
							<f:selectItems
								value="#{driverConductorRegistrationBean.genderList}"
								var="gender" itemValue="#{gender.code}"
								itemLabel="#{gender.description}" />
						</p:selectOneMenu>

						<p:outputLabel value="Date of Birth" for="dob">
							<span style="color: red;">*</span>
						</p:outputLabel>


						<p:calendar id="dob" tabindex="8"
							value="#{driverConductorRegistrationBean.registrationDto.dob}"
							style="width:220px;height:18px;" navigator="true" locale="en_US"
							showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
							yearRange="c-100:c" readonlyInput="true"></p:calendar>

						<p:outputLabel value="District" for="district">
							<span style="color: red;">*</span>
						</p:outputLabel>


						<p:selectOneMenu id="district" tabindex="9" style="width:84%; "
							value="#{driverConductorRegistrationBean.registrationDto.district.code}"
							effect="fold" filter="true" filterMatchMode="contains">

							<f:selectItem itemLabel="-- Select District --" itemValue="" />
							<f:selectItems
								value="#{driverConductorRegistrationBean.districtList}"
								var="district" itemValue="#{district.code}"
								itemLabel="#{district.description}" />
						</p:selectOneMenu>

						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<p:panelGrid>
							<h3>Driver / Conductor ID</h3>
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>


						<p:outputLabel value="First Issue Date" for="firstDateOfIssue"></p:outputLabel>
						<p:calendar id="firstDateOfIssue" tabindex="10"
							value="#{driverConductorRegistrationBean.registrationDto.firstDateOfIssue}"
							style="width:220px;height:18px;" navigator="true" locale="en_US"
							showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
							yearRange="c-100:c" readonlyInput="true"></p:calendar>
						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>


						<p:outputLabel value="Valid Start Date" for="dateOfIssue">
						</p:outputLabel>

						<p:calendar id="dateOfIssue" tabindex="11"
							value="#{driverConductorRegistrationBean.registrationDto.dateOfIssue}"
							style="width:220px;height:18px;" navigator="true" locale="en_US"
							showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
							yearRange="c-100:c" readonlyInput="true"></p:calendar>

						<p:outputLabel value="Valid Expire Date" for="dateOfExpiry">
						</p:outputLabel>

						<p:calendar id="dateOfExpiry" tabindex="12"
							value="#{driverConductorRegistrationBean.registrationDto.dateOfExpiry}"
							style="width:220px;height:18px;" navigator="true" locale="en_US"
							showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
							yearRange="c-50:c+50" readonlyInput="true"></p:calendar>

					</p:panelGrid>

					<p:panel style="border-bottom:5px solid #404d6f;" />

					<p:panelGrid columns="1" layout="grid">
						<p:panelGrid>
							<h2 style="color: blue;">Permanent Address</h2>

						</p:panelGrid>

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">


							<p:panelGrid></p:panelGrid>
							<p:outputLabel value="English" style="font-weight:bold;"></p:outputLabel>
							<p:outputLabel value="Sinhala" style="font-weight:bold;"></p:outputLabel>
							<p:outputLabel value="Tamil" style="font-weight:bold;"></p:outputLabel>

							<p:outputLabel value="Address Line 1">
								<span style="color: red;">*</span>
							</p:outputLabel>

							<p:inputText style="width:95%;height:18px;" tabindex="13"
								value="#{driverConductorRegistrationBean.registrationDto.add1Eng}"
								onkeyup="this.value=this.value.toUpperCase()" id="add1Eng"
								maxlength="100">
							</p:inputText>
							<p:inputText style="width:95%;height:18px;" tabindex="14"
								value="#{driverConductorRegistrationBean.registrationDto.add1Sin}"
								onkeyup="this.value=this.value.toUpperCase()" id="add1Sin"
								maxlength="100">
							</p:inputText>
							<p:inputText style="width:85%;height:18px;" tabindex="15"
								value="#{driverConductorRegistrationBean.registrationDto.add1Tam}"
								onkeyup="this.value=this.value.toUpperCase()" id="add1Tam"
								maxlength="100">
							</p:inputText>

							<p:outputLabel value="Address Line 2">

							</p:outputLabel>

							<p:inputText style="width:95%;height:18px;" tabindex="16"
								value="#{driverConductorRegistrationBean.registrationDto.add2Eng}"
								onkeyup="this.value=this.value.toUpperCase()" id="add2Eng"
								maxlength="100">
							</p:inputText>
							<p:inputText style="width:95%;height:18px;" tabindex="17"
								value="#{driverConductorRegistrationBean.registrationDto.add2Sin}"
								onkeyup="this.value=this.value.toUpperCase()" id="add2Sin"
								maxlength="100">
							</p:inputText>
							<p:inputText style="width:85%;height:18px;" tabindex="18"
								value="#{driverConductorRegistrationBean.registrationDto.add2Tam}"
								onkeyup="this.value=this.value.toUpperCase()" id="add3Tam"
								maxlength="100">
							</p:inputText>

							<p:outputLabel value="City">
								<span style="color: red;">*</span>
							</p:outputLabel>

							<p:inputText style="width:95%;height:18px;" tabindex="19"
								value="#{driverConductorRegistrationBean.registrationDto.cityEng}"
								onkeyup="this.value=this.value.toUpperCase()" id="cityEng"
								maxlength="100">
							</p:inputText>
							<p:inputText style="width:95%;height:18px;" tabindex="20"
								value="#{driverConductorRegistrationBean.registrationDto.citySin}"
								onkeyup="this.value=this.value.toUpperCase()" id="citySin"
								maxlength="100">
							</p:inputText>
							<p:inputText style="width:85%;height:18px;" tabindex="21"
								value="#{driverConductorRegistrationBean.registrationDto.cityTam}"
								onkeyup="this.value=this.value.toUpperCase()" id="cityTam"
								maxlength="100">
							</p:inputText>

							<p:outputLabel value="Contact No." for="contact">
								<span style="color: red;">*</span>
							</p:outputLabel>

							<p:inputText style="width:95%;height:18px;" tabindex="22"
								value="#{driverConductorRegistrationBean.registrationDto.contactNo}"
								id="contact" maxlength="12">
								<p:keyFilter regEx="/[\d\-]/" />
							</p:inputText>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:outputLabel value="Driving License No." for="license">
								<span style="color: red;">*</span>
							</p:outputLabel>

							<p:inputText style="width:95%;height:18px;" tabindex="23"
								disabled="#{driverConductorRegistrationBean.registrationDto.trainingType.code != 'ND'}"
								value="#{driverConductorRegistrationBean.registrationDto.licenseNo}"
								onkeyup="this.value=this.value.toUpperCase()" id="license"
								maxlength="15">

							</p:inputText>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:outputLabel value="Driving Licence Issue Date"
								for="dateOfIssueLicence">
							</p:outputLabel>

							<p:calendar id="dateOfIssueLicence" tabindex="24"
								disabled="#{driverConductorRegistrationBean.registrationDto.trainingType.code != 'ND'}"
								value="#{driverConductorRegistrationBean.registrationDto.dateOfIssueLicence}"
								style="width:220px;height:18px;" navigator="true" locale="en_US"
								showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
								yearRange="c-100:c" readonlyInput="true"></p:calendar>

							<p:outputLabel value="Driving Licence Expiry Date"
								for="dateOfExpiryLicence">
							</p:outputLabel>

							<p:calendar id="dateOfExpiryLicence" tabindex="25"
								disabled="#{driverConductorRegistrationBean.registrationDto.trainingType.code != 'ND'}"
								value="#{driverConductorRegistrationBean.registrationDto.dateOfExpiryLicence}"
								style="width:220px;height:18px;" navigator="true" locale="en_US"
								showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
								yearRange="c-50:c+50" readonlyInput="true"></p:calendar>


						</p:panelGrid>

						<p:panelGrid columns="2"
							columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">

							<p:outputLabel value="Education Qualification" for="edu">
								<span style="color: red;">*</span>
							</p:outputLabel>

							<p:inputText style="width:95%;height:18px;" tabindex="26"
								value="#{driverConductorRegistrationBean.registrationDto.education}"
								onkeyup="this.value=this.value.toUpperCase()" id="edu"
								maxlength="250">

							</p:inputText>

							<p:outputLabel value="GS Certification No." for="gsCert">
								<span style="color: red;">*</span>
							</p:outputLabel>

							<p:inputText id="gsCert" style="width:95%;height:18px;"
								tabindex="27"
								value="#{driverConductorRegistrationBean.registrationDto.gsCertificateNo}"
								onkeyup="this.value=this.value.toUpperCase()" maxlength="100">

							</p:inputText>

							<p:outputLabel value="Issued Date for GS Certificate"
								for="gscissuedate">
								<span style="color: red;">*</span>
							</p:outputLabel>


							<p:calendar id="gscissuedate" tabindex="28"
								value="#{driverConductorRegistrationBean.registrationDto.gscissuedate}"
								style="width:220px;height:18px;" navigator="true" locale="en_US"
								showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
								yearRange="c-50:c+50" readonlyInput="true"></p:calendar>

							<p:outputLabel value="Police Clearance Station" for="police">
							</p:outputLabel>

							<p:inputText id="police" style="width:95%;height:18px;"
								tabindex="26"
								value="#{driverConductorRegistrationBean.registrationDto.policeClearanceStation}"
								onkeyup="this.value=this.value.toUpperCase()" maxlength="100">

							</p:inputText>

							<p:outputLabel value="Issued Date for Police Clearance"
								for="pcissuedate">
								<span style="color: red;">*</span>
							</p:outputLabel>


							<p:calendar id="pcissuedate" tabindex="29"
								value="#{driverConductorRegistrationBean.registrationDto.pcissuedate}"
								style="width:220px;height:18px;" navigator="true" locale="en_US"
								showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
								yearRange="c-50:c+50" readonlyInput="true"></p:calendar>

