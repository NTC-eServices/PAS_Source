<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
		<f:event type="preRenderView" listener="#{makeBean.onPageLoad}" />
	</ui:define>

	<ui:define name="content">
		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}
</style>

		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Inquiry for Admin</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmAssignModels">
					<p:panel>


						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
							layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Vehicle No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:inputText id="vehicleNo" readonly="false"
									value="#{inquiryAdmin.commonDTO.vehicleNo}"
									onkeyup="this.value=this.value.toUpperCase();"
									style="width:70%" maxlength="10" placeholder="-- Vehicle No.--">
											
									</p:inputText>
							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Application No."></p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="applicationNo" style="width:70%"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{inquiryAdmin.commonDTO.applicationNo}" tabindex="1"
									placeholder="-- Application No.--">
											
								</p:inputText>
							</p:panelGrid>



							<p:panelGrid>
								<p:outputLabel value="Permit No."></p:outputLabel>

							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="permitNo" style="width:70%" maxlength="10"
									onkeyup="this.value=this.value.toUpperCase();"
									value="#{inquiryAdmin.commonDTO.permitNo}" tabindex="1"
									placeholder="-- Permit No.--">
									
								</p:inputText>
							</p:panelGrid>

						</p:panelGrid>
					</p:panel>
					<br></br>
					<center>

						<p:commandButton value="Search" action="#{inquiryAdmin.search}"
							update="@form,frmError,modelDataTable,btnPanelOne"
							class="mainActionbtn"></p:commandButton>

						<p:commandButton value="Clear"
							update="@form,modelDataTable,btnPanelOne"
							action="#{inquiryAdmin.clear}" class="mainActionbtn"></p:commandButton>
					</center>
					<br></br>

					<p:panel>



						<p:dataTable id="modelDataTable" var="i" rows="20"
							paginator="true" value="#{inquiryAdmin.inquiryList}"
							selection="#{inquiryAdmin.selectDTO}" selectionMode="single"
							rowKey="#{i.applicationNo}"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="10,20" resizableColumns="false"
							liveResize="true" tableStyle="width:100%;font-size:12px;"
							rowIndexVar="index" styleClass="myTable"
							sortBy="#{i.applicationNo}" sortOrder="descending"
							emptyMessage="No data available">

							<p:column headerText="Vehicle No." style="text-align: left;">
								<h:outputText value="#{i.vehicleNo}" />
							</p:column>

							<p:column headerText="Permit No." style="text-align: left;">
								<h:outputText value="#{i.permitNo}" />
							</p:column>

							<p:column headerText="Application No." style="text-align: left;">
								<h:outputText value="#{i.applicationNo}" />
							</p:column>

							<p:column headerText="Tender No." style="text-align: left;">
								<h:outputText value="#{i.tenderNo}" />
							</p:column>


							<p:column headerText="Status" style="text-align: left;">
								<h:outputText value="#{i.status}" />
							</p:column>

							<p:column headerText="Owner Name" style="text-align: left;">
								<h:outputText value="#{i.ownerName}" />
							</p:column>

							<p:column headerText="Route No." style="text-align: left;">
								<h:outputText value="#{i.routeNo}" />
							</p:column>

							<p:column headerText="Expire Date" style="text-align: left;">
								<h:outputText value="#{i.expireDate}" />
							</p:column>

							<p:column headerText="Valid to Date" style="text-align: left;">
								<h:outputText value="#{i.validTodate}" />
							</p:column>

						</p:dataTable>
					</p:panel>


					<p:panel id="btnPanelOne">

						<center>
							<p:commandButton value="Inspection" class="mainActionbtn"
								style="width:16%; margin-left:1%"
								update="@form,frmError,modelDataTable,btnPanelOne,frmAlert"
								action="#{inquiryAdmin.viewInspection}"
								disabled="#{inquiryAdmin.disableButton}">

							</p:commandButton>
							<p:commandButton value="Issue New Permit" class="mainActionbtn"
								update="@form,frmError,modelDataTable,btnPanelOne,frmAlert"
								action="#{inquiryAdmin.viewIssueNewPermit}"
								style="width:16%; margin-left:1%"
								disabled="#{inquiryAdmin.disableButton}">
							</p:commandButton>
							<p:commandButton value="Permit Renewal" class="mainActionbtn"
								update="@form,frmError,modelDataTable,btnPanelOne,frmAlert"
								action="#{inquiryAdmin.viewPermitRenewal}"
								disabled="#{inquiryAdmin.disableButton}"
								style="width:16%; margin-left:1%">
							</p:commandButton>

							<p:commandButton value="Payments" class="mainActionbtn"
								update="@form,frmError,modelDataTable,btnPanelOne,frmAlert"
								action="#{inquiryAdmin.viewPayments}"
								disabled="#{inquiryAdmin.disableButton}"
								style="width:16%; margin-left:1%">
							</p:commandButton>

							<p:commandButton value="History" class="mainActionbtn"
								action="#{inquiryAdmin.viewHistory}"
								update="frmError,frmAlert,frmhistorydialog"
								disabled="#{inquiryAdmin.disableButton}"
								style="width:16%; margin-left:1%">
							</p:commandButton>
						</center>

					</p:panel>

				</h:form>
			</div>
		</div>

		<p:dialog id="historyDLG" global="true" modal="true"
			header="Inquiry History" width="70%" height="600px" severity="info"
			widgetVar="historyDialog" closeOnEscape="true" resizable="false">

			<h:form id="frmhistorydialog">
				<center>
					<p:panelGrid columns="1" styleClass="ui-noborder">

						<p:panel id="rolePanel">

							<p:dataTable id="historytable" var="e" styleClass="myTable-two"
								rows="20" value="#{inquiryAdmin.historyList}" paginator="true"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20,30" resizableColumns="false"
								liveResize="true" rowIndexVar="index"
								sortBy="#{e.createdDateString}" sortOrder="descending"
								emptyMessage="No records are available">

								<p:column headerText="Application No." style="text-align: left;">
									<h:outputText value="#{e.applicationNo}" />
								</p:column>

								<p:column headerText="Task Code" style="text-align: left;">
									<h:outputText value="#{e.taskCode}" />
								</p:column>


								<p:column headerText="Task Description"
									style="text-align: left;">
									<h:outputText value="#{e.taskCodeDes}" />
								</p:column>

								<p:column headerText="Status" style="text-align: left;">
									<h:outputText value="#{e.taskStatus}" />
								</p:column>

								<p:column headerText="Created By" style="text-align: left;">
									<h:outputText value="#{e.createdBy}" />
								</p:column>

								<p:column headerText="Created Date/Time"
									style="text-align: left;">
									<h:outputText value="#{e.createdDateString}" />
								</p:column>

							</p:dataTable>
						</p:panel>

					</p:panelGrid>
					<br></br>
					<p:commandButton value="OK" onclick="PF('historyDialog').hide()"
						class="btn btn-primary btn-lg btn-block"
						style="width:80px; margin-top:10px;height:30px;font-size: 12px !important; border: 1px solid #FFFFFF; 
						color: white;background:#6A9DED;text-shadow: 2px 2px 5px blue !important;letter-spacing: 1px !important;border-radius:7px;">
					</p:commandButton>
				</center>
			</h:form>

		</p:dialog>


		<p:dialog styleClass="" id="errorMSG" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="errorMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmError">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel value="#{inquiryAdmin.errorMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmError" onclick="PF('errorMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Info" width="30%" severity="info" widgetVar="successMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{inquiryAdmin.successMessage}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMSG"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="#{inquiryAdmin.alertMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmAlert" onclick="PF('alertMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

	</ui:define>
</ui:composition>