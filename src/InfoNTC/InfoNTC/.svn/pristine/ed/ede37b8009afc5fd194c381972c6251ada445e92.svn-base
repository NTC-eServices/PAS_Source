	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}
/* padding: 7px 25px 7px 25px !important; */
.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #404d6f !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
	align-self: center;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 79% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}

.ui-datatable-scrollable .ui-datatable-scrollable-header {
	min-height: 20px;
}

.ui-datatable-scrollable-body {
	height: 125px !important;
}

.genReqNumButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	text-decoration: none !important;
	color: white !important;
	border-radius: 5px !important;
}

.ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	text-shadow: none;
}

.ui-accordion .ui-accordion-header {
	cursor: pointer;
	position: relative;
	margin-top: 1px;
	min-height: 0;
	padding: .5em .5em .5em 2.2em;
	background-color: cornflowerblue;
}

.ui-autocomplete-itemtip {
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box;
}
</style>

		<div class="mainDiv">

			<!-- header -->

			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Create Voucher Test</h1>

			<div style="width: 95%;" class="subDiv">



				<p:panel id="pnl-sim-reg">


					<!-- Input Details part -->


					<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
						layout="grid">

						<!-- SIM reg Insertion -->

						<h:form id="simRegistrationForm">

							<p:panelGrid columns="4" style="margin-bottom: 15px;"
								columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
								layout="grid">

								<!-- SIM Registration Number -->

								<p:panelGrid>
									<p:outputLabel value="SIM Registration No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="simRegNoId" style="width:81%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											readonly="true"
											value="#{simRegistrationBackingBean.simRegistrationDTO.simRegNo}"
											required="true" />
									</span>
								</p:panelGrid>

								<!-- Service Category -->

								<p:panelGrid>
									<p:outputLabel value="Service Category "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:selectOneMenu id="serviceCategoryId" style="width:73%;"
										effect="fold" filter="true" filterMatchMode="contains"
										value="#{simRegistrationBackingBean.simRegistrationDTO.serviceCategory}">

										<p:ajax
											listener="#{simRegistrationBackingBean.onServiceCategoryChange}"
											update="simRegistrationForm"></p:ajax>

										<f:selectItem itemLabel=" -- Select Service Category -- "
											itemValue="" />
										<f:selectItem itemLabel="NTC" itemValue="ntc" />
										<f:selectItem itemLabel="Sisu Sariya" itemValue="sisu" />
										<f:selectItem itemLabel="Gami Sariya" itemValue="gami" />
										<f:selectItem itemLabel="Nisi Sariya" itemValue="nisi" />
										<f:selectItem itemLabel="Other" itemValue="other" />

									</p:selectOneMenu>
								</p:panelGrid>

								<!-- Permit No. -->

								<p:panelGrid>
									<p:outputLabel value="Permit No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="permitNoId" style="width:81%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="Permit Number"
											value="#{simRegistrationBackingBean.simRegistrationDTO.permitNo}"
											onkeyup="this.value=this.value.toUpperCase();"
											rendered="#{simRegistrationBackingBean.simRegistrationDTO.serviceCategory == 'other'}" />

										<p:autoComplete id="permitNoId2"
											value="#{simRegistrationBackingBean.simRegistrationDTO.permitNo}"
											completeMethod="#{simRegistrationBackingBean.completePermitNo}"
											size="29"
											rendered="#{simRegistrationBackingBean.simRegistrationDTO.serviceCategory != 'other'}"
											onkeyup="this.value=this.value.toUpperCase();"
											forceSelection="true" scrollHeight="500"
											placeholder="Permit Number">
											<p:ajax event="itemSelect"
												listener="#{simRegistrationBackingBean.onPermitNoSelect}"
												process="@all" update="simRegistrationForm" />
										</p:autoComplete>
									</span>
								</p:panelGrid>

								<!-- Bus No. -->

								<p:panelGrid>
									<p:outputLabel value="Bus No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="BusNoId"
											style="width:81%" filter="true" filterMatchMode="contains"
											tabindex="1" effect="fold" placeholder="Bus Number"
											value="#{simRegistrationBackingBean.simRegistrationDTO.busNo}"
											onkeyup="this.value=this.value.toUpperCase();"
											rendered="#{simRegistrationBackingBean.simRegistrationDTO.serviceCategory == 'other'}" />

										<p:autoComplete id="BusNoId2"
											value="#{simRegistrationBackingBean.simRegistrationDTO.busNo}"
											completeMethod="#{simRegistrationBackingBean.completeBusNo}"
											size="29"
											rendered="#{simRegistrationBackingBean.simRegistrationDTO.serviceCategory != 'other'}"
											onkeyup="this.value=this.value.toUpperCase();"
											forceSelection="true" scrollHeight="500"
											placeholder="Bus Number">
											<p:ajax event="itemSelect"
												listener="#{simRegistrationBackingBean.onBusNoSelect}"
												process="@all" update="simRegistrationForm" />
										</p:autoComplete>
									</span>
								</p:panelGrid>


								<!-- SIM No. -->

								<p:panelGrid>
									<p:outputLabel value="SIM No. (Mobile No.) "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="simNoId"
											style="width:81%" filter="true" filterMatchMode="contains"
											tabindex="1" maxlength="10" placeholder="SIM Number"
											value="#{simRegistrationBackingBean.simRegistrationDTO.simNo}">
											<p:keyFilter regEx="/[0-9-]/i" />
										</p:inputText>
									</span>
								</p:panelGrid>

							</p:panelGrid>


							<!-- Main Button Panel -->

							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
								layout="grid" style="margin-top:2%;margin-bottom:2%;">

								<!-- Save Button -->

								<p:commandButton id="saveBtn" styleClass="mainLinkButton"
									style="width:100%" value="Save" process="@all"
									update="simRegistrationForm"
									action="#{simRegistrationBackingBean.saveBtnAction}"
									rendered="#{simRegistrationBackingBean.saveBtnCheck}"
									disabled="true">

								</p:commandButton>

								<!-- Update Button -->

								<p:commandButton id="updateBtn" styleClass="mainLinkButton"
									style="width:100%" value="Update" process="@all"
									update="simRegistrationForm"
									action="#{simRegistrationBackingBean.updateBtnAction}"
									rendered="#{simRegistrationBackingBean.updateBtnCheck}">

								</p:commandButton>

								<!-- Create Voucher Button -->

								<p:commandButton id="createVouchertBtn"
									styleClass="mainLinkButton" style="width:100%"
									value="Create Voucher" process="@all"
									update="createPaymentVoucherID,simRegistrationForm"
									action="#{simRegistrationBackingBean.createVoucherBtnAction}">
								</p:commandButton>

								<!-- Document Management Button -->

								<p:commandButton id="documentManagementBtn"
									styleClass="mainLinkButton" style="width:100%"
									value="Document Management" process="@all"
									update="simRegistrationForm" disabled="true"
									action="#{simRegistrationBackingBean.documentManagementBtnAction}">


								</p:commandButton>

								<!-- Clear Button -->

								<p:commandButton id="clearBtn" styleClass="mainLinkButton"
									style="width:100%" value="Clear" process="@all"
									update="simRegistrationForm"
									action="#{simRegistrationBackingBean.clearBtnAction}">

								</p:commandButton>

							</p:panelGrid>


						</h:form>


					</p:panelGrid>

				</p:panel>



			</div>
		</div>


		<!-- Create Voucher Dialog -->

		<p:dialog id="createPaymentVoucherID" global="true" modal="true"
			showEffect="clip" hideEffect="clip" header=" Create Payment Voucher "
			width="80%" height="550px" severity="info"
			widgetVar="createVoucherDialog" closeOnEscape="true"
			resizable="false" style="postion: absolute;">

			<h:form id="frmCreateVoucher">

				<p:panel id="panelOne">

					<!-- 1st data set -->

					<p:panelGrid columns="4" style="margin-bottom:1%"
						columnClasses="ui-grid-col-4,ui-grid-col-2,ui-grid-col-3,ui-grid-col-3"
						layout="grid">

						<!-- SIM Registration No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="SIM Registration No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%" readonly="true"
								value="#{simRegistrationBackingBean.simRegistrationDTO.simRegNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- SIM No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="SIM No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%;" readonly="true"
								value="#{simRegistrationBackingBean.simRegistrationDTO.simNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Permit No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Permit No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%" readonly="true"
								value="#{simRegistrationBackingBean.simRegistrationDTO.permitNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Bus No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Bus No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%" readonly="true"
								value="#{simRegistrationBackingBean.simRegistrationDTO.busNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Voucher Number -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Voucher No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText
								style="width:90%;font-weight: bold;color: red;text-transform: uppercase; "
								readonly="true"
								value="#{simRegistrationBackingBean.voucherDTO.vouNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

					</p:panelGrid>


					<!-- 2nd data set -->

					<p:panel
						style="border: #404d6f solid 1px !important;border-radius: 25px;
						box-shadow: 3px 3px 3px grey; margin-bottom: 1px;">

						<p:panelGrid columns="6"
							columnClasses="ui-grid-col-2,ui-grid-col-2,ui-grid-col-2,ui-grid-col-2,ui-grid-col-1,ui-grid-col-3"
							layout="grid">

							<!-- Charge Type -->

							<p:panelGrid>
								<center>
									<p:outputLabel value="Charge Type"></p:outputLabel>
									<p:outputLabel value=" *" style="color:red;"></p:outputLabel>
								</center>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu style="width:90%" tabindex="1" filter="true"
									filterMatchMode="contains"
									value="#{simRegistrationBackingBean.voucherDTO.vouChargeType}"
									disabled="#{simRegistrationBackingBean.disableChargeType}">

									<f:selectItem itemLabel="--Select Charge Type--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems value="#{createPaymentVoucher.chargeTypeList}" />
									<p:ajax
										listener="#{simRegistrationBackingBean.ajaxFilterAcountNo}"
										process="@all" update="@form"></p:ajax>
								</p:selectOneMenu>
							</p:panelGrid>

							<!-- Account No. -->

							<p:panelGrid>
								<center>
									<p:outputLabel value="Account No.">
										<p:outputLabel value=" *" style="color:red;"></p:outputLabel>
									</p:outputLabel>
								</center>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="accountNoId" style="width:90%"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{simRegistrationBackingBean.voucherDTO.vouAccountNo}"
									disabled="#{simRegistrationBackingBean.disableAccountNo}">

									<f:selectItem itemLabel="--Select Account No.--" itemValue="" />
									<f:selectItems
										value="#{simRegistrationBackingBean.accountNoList}" />
								</p:selectOneMenu>
							</p:panelGrid>

							<!-- Amount -->

							<p:panelGrid>
								<center>
									<p:outputLabel value="Amount"></p:outputLabel>
									<p:outputLabel value=" *" style="color:red;"></p:outputLabel>
								</center>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputNumber placeholder="Enter Amount (Rs.)"
									value="#{simRegistrationBackingBean.voucherDTO.vouAmmount}"
									minValue="0" style="width:66%" inputStyle="text-align: right"
									maxlength="12"
									disabled="#{simRegistrationBackingBean.disableAmount}"></p:inputNumber>
							</p:panelGrid>

						</p:panelGrid>

						<br />
						<center>
							<!-- Add Button -->
							<p:commandButton value="Add" style="width:20%"
								update="dataTable,@form" process="@all"
								styleClass="otherLinkButton" id="vouAddBtn"
								actionListener="#{simRegistrationBackingBean.vouAddBtnAction}"
								rendered="#{simRegistrationBackingBean.renderVouAdd}" />

							<!-- Update Button -->
							<p:commandButton value="Update" style="width:20%"
								update="dataTable,@form" process="@all"
								styleClass="otherLinkButton" id="vouUpdateBtn"
								actionListener="#{simRegistrationBackingBean.vouUpdateBtnAction}"
								rendered="#{simRegistrationBackingBean.renderVouUpdate}" />

							<!-- Clear Button -->
							<p:commandButton value="Clear" style="width:20%"
								update="frmCreateVoucher" process="@all"
								styleClass="otherLinkButton" id="vouDetailsClearBtn"
								actionListener="#{simRegistrationBackingBean.vouDetailsClearBtnAction}"
								rendered="#{simRegistrationBackingBean.renderVouClear}" />

							<!-- Edit Clear Button -->
							<p:commandButton value="Clear Amount" style="width:20%"
								update="frmCreateVoucher" process="@all"
								styleClass="otherLinkButton" id="vouDetailsEditClearBtn"
								actionListener="#{simRegistrationBackingBean.vouEditClearBtnAction}"
								rendered="#{simRegistrationBackingBean.renderVouEditClear}" />
						</center>

					</p:panel>

				</p:panel>

				<p:panel id="panelTwo">

					<center>
						<p:dataTable id="dataTable" var="dto" sortBy="#{1}"
							resizableColumns="false" liveResize="true"
							tableStyle="width:100%" styleClass="myTable"
							emptyMessage="No records available." paginator="true" rows="5"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							value="#{simRegistrationBackingBean.voucherDTOList}">

							<!-- Charge Type -->

							<p:column headerText="Charge Type" style="text-align:left">
								<h:outputText style="float:left;" value="#{dto.vouChargeType}" />
							</p:column>

							<!-- Account No -->

							<p:column headerText="Account No." style="text-align:left">
								<h:outputText style="float:left;" value="#{dto.vouAccountNo}" />
							</p:column>

							<!-- Amount(Rs.) -->

							<p:column headerText="Amount (Rs.)"
								style="text-align:right;margin-right:10%">
								<h:outputText style="float:right;" value="#{dto.vouAmmount}">
									<f:convertNumber type="currency" currencySymbol="" />
								</h:outputText>
							</p:column>

							<!-- Edit -->

							<p:column width="9%">
								<center>
									<p:commandLink action="${simRegistrationBackingBean.vouEdit()}"
										disabled="${simRegistrationBackingBean.disableVouEdit}"
										update="@form,dataTable">

										<f:setPropertyActionListener value="#{dto}"
											target="${simRegistrationBackingBean.selectDTO}" />

										<p:graphicImage library="images" name="edit.png"
											style="width:24px;height:24px;" title="edit"></p:graphicImage>
									</p:commandLink>
								</center>

							</p:column>

							<!-- delete -->

							<p:column width="9%">
								<center>
									<p:commandLink
										action="${simRegistrationBackingBean.vouDelete()}"
										disabled="${simRegistrationBackingBean.disableVouDelete}"
										update="dataTable">

										<f:setPropertyActionListener value="#{dto}"
											target="${simRegistrationBackingBean.selectDTO}" />

										<p:graphicImage library="images" name="delete.png"
											style="width:24px;height:24px;" title="delete"></p:graphicImage>
									</p:commandLink>
								</center>

							</p:column>

							<p:columnGroup type="footer">
								<p:row>
									<p:column colspan="2" style="text-align:right"
										footerText="Total Fee (Rs.)" />

									<p:column style="text-align:right">
										<f:facet name="footer">
											<h:outputText style="float:right;"
												value="#{simRegistrationBackingBean.totalAmount}">
												<f:convertNumber type="currency" currencySymbol="" />
											</h:outputText>
										</f:facet>
									</p:column>

									<p:column></p:column>
									<p:column></p:column>

								</p:row>
							</p:columnGroup>

						</p:dataTable>
					</center>
				</p:panel>

				<!-- Main Button Panel -->

				<p:panel id="panelThree">
					<center>

						<!-- Generate Voucher -->

						<p:commandButton value="Generate Voucher" update="@form"
							process="@all" styleClass="mainActionbtn" id="generateVoucherBtn"
							style="width:15%"
							actionListener="#{simRegistrationBackingBean.generateVoucher}">
						</p:commandButton>

						<!-- Print -->

						<p:commandButton value="Print" update="@all" process="@all"
							styleClass="mainActionbtn" id="printBtn" style="width:15%">

						</p:commandButton>

						<!-- Re-Print -->

						<p:commandButton value="Re-Print"
							oncomplete="window.open('/InfoNTC/showUpload')"
							styleClass="mainActionbtn" id="rePrintBtn" style="width:15%">

						</p:commandButton>

						<!-- Clear -->

						<p:commandButton value="Clear" styleClass="mainActionbtn"
							style="width:15%" id="btnClear" update="@form"
							onclick="PF('voucherDialogForInves').hide()" />


					</center>

				</p:panel>

			</h:form>


		</p:dialog>


		<!-- End Create Voucher -->

	</ui:define>
</ui:composition>