<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">
		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.nopadd .ui-panelgrid-cell {
	padding: 4px 0px !important;
}

.sm-padding .ui-panelgrid-cell {
	padding-top: 4px !important;
	padding-bottom: 4px !important;
}

.panelsimple {
	border: 5px solid #bed6f8;
}

.panelsimple .ui-panel-titlebar {
	background: none !important;
	background-color: #bed6f8 !important;
	margin: -5px !important;
}

.dtright .ui-datatable .ui-datatable-header {
	text-align: left !important;
}
</style>


		<div class="mainDiv">

			<div style="margin-top: 0%;" class="common_form_header">
				<h1 style="color: #ffffff; text-align: center; font-weight: bold">School
					Awareness Program</h1>


			</div>

			<div style="width: 95%;" class="subDiv ui-grid ui-grid-responsive">
				<h:form id="frmCaptureAwareness"
					style="background-color :#f1f1f1; padding:10px"
					styleClass="dtright">

					<div class="rowpadding ui-grid-row">
						<div class="ui-grid-col-12">
							<p:commandButton value="New Project"
								icon="fa fa-plus-circle fa-lg" process="@this"
								action="#{awarenessManagementBackingBean.showNewProjectDialog}"
								update="newProjectDialog" style="width:125px; float:right"
								styleClass="mainActionbtn" tabindex="1" />
						</div>
					</div>

					<p:dataTable id="dtProject" widgetVar="projectTable" var="project"
						value="#{awarenessManagementBackingBean.projectList}"
						editable="true" paginator="true" rows="10"
						rowsPerPageTemplate="5,10,15" paginatorPosition="bottom"
						paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
						reflow="true" style="margin-bottom:20px"
						rowStyleClass="#{project.refNo eq awarenessManagementBackingBean.selectedProject.refNo ? 'highlight-row' : null}"
						filterMatchMode="contains" filterBy="#{project.projectName}"
						filteredValue="#{awarenessManagementBackingBean.filteredProjectList}">

						<p:ajax event="rowEdit"
							listener="#{awarenessManagementBackingBean.updateProject}"
							update="dtProject" />
						<p:ajax event="rowEditCancel" update="dtProject" />

						<p:column sortBy="#{project.refNo}" headerText="Ref. No."
							width="100" style="text-align:left">
							<h:outputText value="#{project.refNo}" />
						</p:column>

						<p:column sortBy="#{project.projectName}"
							headerText="Project Name" width="200" style="text-align:left">
							<p:cellEditor>
								<f:facet name="output">
									<h:outputText value="#{project.projectName}" />
								</f:facet>
								<f:facet name="input">
									<p:inputText value="#{project.projectName}"
										label="Project Name" style="width:calc(100% - 20px)"
										onkeyup="this.value=this.value.toUpperCase()" />
								</f:facet>
							</p:cellEditor>
						</p:column>

						<p:column sortBy="#{project.noOfProject}"
							headerText="No. of Program" width="70" style="text-align:center">
							<p:cellEditor>
								<f:facet name="output">
									<h:outputText value="#{project.noOfProject}" />
								</f:facet>
								<f:facet name="input">
									<p:spinner value="#{project.noOfProject}" min="0" size="7"
										style="padding-right: 14px;" />
								</f:facet>
							</p:cellEditor>
						</p:column>

						<p:column sortBy="#{project.status}" headerText="Status"
							width="80" style="text-align:center">
							<p:cellEditor>
								<f:facet name="output">
									<h:outputText
										value="#{project.status eq 'A' ?'Active' :(project.status eq 'I' ?'Inactive' :'Hold')}" />
								</f:facet>
								<f:facet name="input">
									<h:selectOneMenu value="#{project.status}"
										style="width:calc(100% - 18px)">
										<f:selectItem itemLabel="Active" itemValue="A"></f:selectItem>
										<f:selectItem itemLabel="Inactive" itemValue="I"></f:selectItem>
										<f:selectItem itemLabel="Hold" itemValue="H"></f:selectItem>
									</h:selectOneMenu>
								</f:facet>
							</p:cellEditor>
						</p:column>

						<p:column width="15" style="text-align:center">
							<p:rowEditor />
						</p:column>

						<p:column width="15" style="text-align:center">
							<p:commandLink onclick="PF('prjDeleteConfirm').show();"
								title="Delete Project" update="frmCaptureAwareness:dtProject">
								<i class="fa fa-trash fa-lg" style="color: #ff0000;" />
								<f:setPropertyActionListener value="#{project}"
									target="#{awarenessManagementBackingBean.selectedProject}" />
							</p:commandLink>
						</p:column>

						<p:column width="15" style="text-align:center">
							<p:commandLink
								action="#{awarenessManagementBackingBean.viewProjectDetails}"
								title="View Project Details"
								update="frmCaptureAwareness:idProjectDetailPanel, frmCaptureAwareness:dtProject">
								<i class="fa fa-th-list fa-lg" />
								<f:setPropertyActionListener value="#{project}"
									target="#{awarenessManagementBackingBean.selectedProject}" />
							</p:commandLink>
						</p:column>
					</p:dataTable>

					<h:panelGroup id="idProjectDetailPanel">

						<p:panel rendered="#{awarenessManagementBackingBean.showDetail}"
							styleClass="panelsimple">
							<f:facet name="header">
			            Details of the Programs
			        </f:facet>

							<div class="rowpadding ui-grid-row">
								<div class="ui-grid-col-12">
									<p:commandButton value="New Program"
										icon="fa fa-plus-circle fa-lg" process="@this"
										action="#{awarenessManagementBackingBean.showNewProgramDialog}"
										update="newProgramDialog" style="width:125px; float:right"
										styleClass="mainActionbtn" tabindex="1" />
								</div>
							</div>

							<p:dataTable id="dtProgram" widgetVar="programTable"
								var="program"
								value="#{awarenessManagementBackingBean.selectedProject.subProgramList}"
								editable="true" paginator="true" rows="10"
								rowsPerPageTemplate="5,10,15" paginatorPosition="bottom"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								reflow="true" style="margin-bottom:20px">

								<p:ajax event="rowEdit"
									listener="#{awarenessManagementBackingBean.updateProgram}"
									update="dtProgram" />
								<p:ajax event="rowEditCancel" update="dtProgram" />

								<p:column headerText="Sub Ref. No." sortBy="#{program.subRefNo}"
									width="90" style="text-align:left">
									<h:outputText value="#{program.subRefNo}" />
								</p:column>

								<p:column headerText="Province" sortBy="#{program.province}"
									width="110" style="text-align:left">
									<p:cellEditor>
										<f:facet name="output">
											<ui:repeat
												value="#{awarenessManagementBackingBean.provinceList}"
												var="p">
												<h:outputText value="#{p.description}"
													rendered="#{p.code eq program.province}" />
											</ui:repeat>
										</f:facet>
										<f:facet name="input">
											<p:selectOneMenu value="#{program.province}"
												style="width:calc(100% - 34px)">
												<f:ajax
													listener="#{awarenessManagementBackingBean.updateProvince('UPDATE',program)}"
													update="drpDistrictB" />
												<f:selectItem itemLabel="-- Select Province --" itemValue=""></f:selectItem>
												<f:selectItems
													value="#{awarenessManagementBackingBean.provinceList}"
													var="p" itemValue="#{p.code}" itemLabel="#{p.description}"></f:selectItems>
											</p:selectOneMenu>
										</f:facet>
									</p:cellEditor>
								</p:column>

								<p:column headerText="District" sortBy="#{program.district}"
									width="80" id="drpDistrictB" style="text-align:left">
									<p:cellEditor>
										<f:facet name="output">
											<ui:repeat
												value="#{awarenessManagementBackingBean.districtListAll}"
												var="d">
												<h:outputText value="#{d.description}"
													rendered="#{d.code eq program.district}" />
											</ui:repeat>
										</f:facet>
										<f:facet name="input">
											<p:selectOneMenu value="#{program.district}"
												style="width:calc(100% - 34px)">
												<f:selectItem itemLabel="-- Select District --" itemValue=""></f:selectItem>
												<f:selectItems
													value="#{awarenessManagementBackingBean.districtListAll}"
													var="d" itemValue="#{d.code}" itemLabel="#{d.description}"></f:selectItems>
											</p:selectOneMenu>
										</f:facet>
									</p:cellEditor>
								</p:column>

								<p:column headerText="Zone" sortBy="#{program.zone}" width="100"
									style="text-align:left">
									<p:cellEditor>
										<f:facet name="output">
											<ui:repeat
												value="#{awarenessManagementBackingBean.eduZoneList}"
												var="z">
												<h:outputText value="#{z.description}"
													rendered="#{z.code eq program.zone}" />
											</ui:repeat>
										</f:facet>
										<f:facet name="input">
											<p:selectOneMenu value="#{program.zone}"
												style="width:calc(100% - 34px)">
												<f:selectItem itemLabel="-- Select Zone --" itemValue=""></f:selectItem>
												<f:selectItems
													value="#{awarenessManagementBackingBean.eduZoneList}"
													var="z" itemValue="#{z.code}" itemLabel="#{z.description}"></f:selectItems>
											</p:selectOneMenu>
										</f:facet>
									</p:cellEditor>
								</p:column>

								<p:column headerText="Scheduled Date"
									sortBy="#{program.scheduleDate}" width="100"
									style="text-align:left">
									<p:cellEditor>
										<f:facet name="output">
											<h:outputText value="#{program.scheduleDate}" />
										</f:facet>
										<f:facet name="input">
											<p:calendar value="#{program.scheduleDate}"
												styleClass="calendarClass" navigator="true" locale="en_US"
												showOn="button" inputStyle="width:calc(100% - 43px)"
												pattern="dd/MM/yyyy" placeholder="dd/MM/yyyy"
												yearRange="c-100:c+100" readonlyInput="true">
											</p:calendar>
										</f:facet>
									</p:cellEditor>
								</p:column>

								<p:column headerText="Place" sortBy="#{program.place}"
									width="120" style="text-align:left">
									<p:cellEditor>
										<f:facet name="output">
											<h:outputText value="#{program.place}" />
										</f:facet>
										<f:facet name="input">
											<p:inputText value="#{program.place}"
												style="width:calc(100% - 15px)"
												onkeyup="this.value=this.value.toUpperCase()"></p:inputText>
										</f:facet>
									</p:cellEditor>
								</p:column>

								<p:column headerText="Status" sortBy="#{program.status}"
									width="70" style="text-align:center">
									<p:cellEditor>
										<f:facet name="output">
											<h:outputText
												value="#{program.status eq 'A' ?'Active' :(program.status eq 'I' ?'Inactive' :'Hold')}" />
										</f:facet>
										<f:facet name="input">
											<h:selectOneMenu value="#{program.status}"
												style="width:calc(100% - 18px)"
												disabled="#{awarenessManagementBackingBean.selectedProject.status != 'A'}">
												<f:selectItem itemLabel="Active" itemValue="A"></f:selectItem>
												<f:selectItem itemLabel="Inactive" itemValue="I"></f:selectItem>
												<f:selectItem itemLabel="Hold" itemValue="H"></f:selectItem>
											</h:selectOneMenu>
										</f:facet>
									</p:cellEditor>
								</p:column>

								<p:column width="15" style="text-align:center">
									<p:rowEditor />
								</p:column>

								<p:column width="15" style="text-align:center">
									<p:commandLink onclick="PF('prmDeleteConfirm').show();"
										title="Delete Program" style="color:#ff0000;">
										<i class="fa fa-trash fa-lg" />
										<f:setPropertyActionListener value="#{program}"
											target="#{awarenessManagementBackingBean.selectedProgram}" />
									</p:commandLink>
								</p:column>

								<p:column width="15" style="text-align:center">
									<p:commandLink
										action="#{awarenessManagementBackingBean.loadLetterManagement}"
										title="Letter Management" update="letterDialog">
										<i class="fa fa-envelope fa-lg" />
										<f:setPropertyActionListener value="#{program}"
											target="#{awarenessManagementBackingBean.selectedProgram}" />
									</p:commandLink>
								</p:column>

								<p:column width="15" style="text-align:center">
									<p:commandLink
										action="#{awarenessManagementBackingBean.loadEventManagement}"
										title="Event Management" update="eventDialog">
										<i class="fa fa-users fa-lg" />
										<f:setPropertyActionListener value="#{program}"
											target="#{awarenessManagementBackingBean.selectedProgram}" />
									</p:commandLink>
								</p:column>

								<p:column width="15" style="text-align:center">
									<p:commandLink
										action="#{awarenessManagementBackingBean.loadTransportManagement}"
										title="Transport Management" update="transportDialog">
										<i class="fa fa-taxi fa-lg" />
										<f:setPropertyActionListener value="#{program}"
											target="#{awarenessManagementBackingBean.selectedProgram}" />
									</p:commandLink>
								</p:column>
							</p:dataTable>


							<div class="gridspadding" align="center">
								<p:commandButton value="Letter Management Preview"
									styleClass="mainActionbtn"
									style="white-space:normal!important;width:171px;height:40px !important;"
									tabindex="1"
									actionListener="#{awarenessManagementBackingBean.previewLetterManagement}"
									alt="Letter Management Preview" update="lettersPreviewDialog"></p:commandButton>

								<p:spacer width="5px"></p:spacer>

								<p:commandButton value="Transport Management Preview"
									styleClass="mainActionbtn"
									style="white-space:normal!important;width:171px;height:40px !important;"
									tabindex="1"
									actionListener="#{awarenessManagementBackingBean.previewTransportManagement}"
									alt="Transport Management Preview" update="frmTransportPreview"></p:commandButton>
							</div>

						</p:panel>

					</h:panelGroup>

				</h:form>
			</div>


			<!-- ############################################################################################################################################
	###################################### ADD NEW PROJECT, ADD NEW PROGRAM #########################################################################
	############################################################################################################################################# -->

			<p:dialog id="newProjectDialog" widgetVar="newProjectDialog"
				global="true" modal="true" header=" Add New Project" width="30%"
				resizable="false">
				<h:form id="frmNewProject">

					<div class="ui-widget-content ui-grid ui-grid-responsive">
						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-1"></div>
							<div class="ui-grid-col-4">
								<p:outputLabel value="Reference No"></p:outputLabel>
								<p:outputLabel value=" *" style="color:#ff0000"></p:outputLabel>
							</div>
							<div class="ui-grid-col-5">
								<p:inputText
									value="#{awarenessManagementBackingBean.newProject.refNo}"
									style="text-transform:uppercase; width:100%" disabled="true"
									onkeyup="this.value=this.value.toUpperCase()"></p:inputText>
							</div>
						</div>

						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-1"></div>
							<div class="ui-grid-col-4">
								<p:outputLabel value="Project Name"></p:outputLabel>
								<p:outputLabel value=" *" style="color:#ff0000"></p:outputLabel>
							</div>
							<div class="ui-grid-col-5">
								<p:inputText
									value="#{awarenessManagementBackingBean.newProject.projectName}"
									style="width:100%"
									onkeyup="this.value=this.value.toUpperCase()"></p:inputText>
							</div>
						</div>

						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-1"></div>
							<div class="ui-grid-col-4">
								<p:outputLabel value="No. of Program"></p:outputLabel>
							</div>
							<p:spinner
								value="#{awarenessManagementBackingBean.newProject.noOfProject}"
								min="0" size="7" style="padding-right: 14px;" />
						</div>

						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-1"></div>
							<div class="ui-grid-col-4">
								<p:outputLabel value="Status"></p:outputLabel>
							</div>
							<div class="ui-grid-col-5">
								<p:selectOneMenu
									value="#{awarenessManagementBackingBean.newProject.status}"
									style="width:calc(100% - 18px)">
									<f:selectItem itemLabel="Active" itemValue="A"></f:selectItem>
									<f:selectItem itemLabel="Inactive" itemValue="I"></f:selectItem>
									<f:selectItem itemLabel="Hold" itemValue="H"></f:selectItem>
								</p:selectOneMenu>
							</div>
						</div>

						<div class="gridspadding" align="center">
							<p:commandButton value="Add" styleClass="mainActionbtn"
								tabindex="1"
								actionListener="#{awarenessManagementBackingBean.saveNewProject}"
								alt="Add" update="frmCaptureAwareness:dtProject"></p:commandButton>

							<p:spacer width="5px"></p:spacer>

							<p:commandButton value="Clear" styleClass="mainActionbtn"
								tabindex="1"
								actionListener="#{awarenessManagementBackingBean.clearNewProjectDialog}"
								alt="Clear" update="frmNewProject"></p:commandButton>
						</div>
					</div>
				</h:form>
			</p:dialog>

			<p:dialog id="newProgramDialog" widgetVar="newProgramDialog"
				global="true" modal="true" header=" Add New Program" width="30%"
				resizable="false">
				<h:form id="frmNewProgram">

					<div class="ui-widget-content ui-grid ui-grid-responsive">
						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-1"></div>
							<div class="ui-grid-col-4">
								<p:outputLabel value="Program Type"></p:outputLabel>
							</div>
							<div class="ui-grid-col-5">
								<p:selectOneRadio
									value="#{awarenessManagementBackingBean.newProgram.type}"
									disabled="#{awarenessManagementBackingBean.selectedProject.status != 'A'}">
									<p:ajax
										listener="#{awarenessManagementBackingBean.changeProgramType}"
										update="frmNewProgram"></p:ajax>
									<f:selectItem itemLabel="School Program" itemValue="S"></f:selectItem>
