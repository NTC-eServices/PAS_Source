									</f:facet>
									<f:facet name="input">
										<p:inputText value="#{var.idNo}"
											style="width:calc(100% - 30px)"
											onkeyup="this.value=this.value.toUpperCase()"></p:inputText>
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Officer Type" style="text-align:left">
								<p:cellEditor>
									<f:facet name="output">
										<ui:repeat var="officer"
											value="#{awarenessManagementBackingBean.officerTypeList}">
											<h:outputText value="#{officer.description}"
												rendered="#{var.officerType == officer.code}" />
										</ui:repeat>
									</f:facet>
									<f:facet name="input">
										<p:selectOneMenu value="#{var.officerType}"
											style="width:calc(100% - 30px)"
											rendered="#{var.typeOfParticipation == 'O'}">
											<f:selectItem itemLabel="-- Select Officer Type --"
												itemValue=""></f:selectItem>
											<f:selectItems
												value="#{awarenessManagementBackingBean.officerTypeList}"
												var="officer" itemLabel="#{officer.description}"
												itemValue="#{officer.code}"></f:selectItems>
										</p:selectOneMenu>
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column width="32" style="text-align:center">
								<p:rowEditor />
							</p:column>

							<p:column width="32" style="text-align:center">
								<p:commandLink title="Delete Letter"
									action="#{awarenessManagementBackingBean.removeParticipant}"
									style="color:#ff0000;" update="dtParticipant">
									<i class="fa fa-trash fa-lg" />
									<f:setPropertyActionListener value="#{var}"
										target="#{awarenessManagementBackingBean.selectedParticipant}" />
								</p:commandLink>
							</p:column>

						</p:dataTable>
					</p:panel>

				</h:form>
			</p:dialog>

			<p:dialog id="newParticipantDialog" widgetVar="newParticipantDialog"
				global="true" modal="true" header=" New Participant" width="30%"
				resizable="false">
				<h:form id="frmNewParticipant">

					<p:panelGrid columns="2" layout="grid"
						columnClasses="ui-grid-col-5, ui-grid-col-6">
						<p:outputPanel>
							<p:outputLabel value="Type of Participation"></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>
						<p:selectOneMenu
							value="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation}"
							style="width: calc(100% - 15px)">
							<p:ajax
								listener="#{awarenessManagementBackingBean.changeParticipationType('NEW')}"
								update="frmNewParticipant" />
							<f:selectItem itemValue="" itemLabel="-- Select Type --" />
							<f:selectItem itemValue="E" itemLabel="Employee" />
							<f:selectItem itemValue="O" itemLabel="Other" />
						</p:selectOneMenu>

						<p:outputPanel
							rendered="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'E'}">
							<p:outputLabel value="Employee No."></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>
						<p:autoComplete
							value="#{awarenessManagementBackingBean.newParticipant.empNo}"
							completeMethod="#{awarenessManagementBackingBean.completeEmpNo}"
							size="20" forceSelection="true"
							rendered="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'E'}"
							scrollHeight="500">
							<p:ajax event="itemSelect"
								listener="#{awarenessManagementBackingBean.changeEmpNo('NEW', awarenessManagementBackingBean.newParticipant)}"
								update="frmNewParticipant" />
						</p:autoComplete>

						<p:outputLabel value="EPF No."
							rendered="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'E'}"></p:outputLabel>
						<p:autoComplete
							value="#{awarenessManagementBackingBean.newParticipant.epfNo}"
							completeMethod="#{awarenessManagementBackingBean.completeEpfNo}"
							size="20" forceSelection="true"
							rendered="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'E'}"
							scrollHeight="500">
							<p:ajax event="itemSelect"
								listener="#{awarenessManagementBackingBean.changeEpfNo('NEW', awarenessManagementBackingBean.newParticipant)}"
								update="frmNewParticipant" />
						</p:autoComplete>

						<p:outputPanel>
							<p:outputLabel value="Name"></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>
						<p:inputText
							value="#{awarenessManagementBackingBean.newParticipant.participantName}"
							disabled="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'E'}"
							style="width:100%" onkeyup="this.value=this.value.toUpperCase()"></p:inputText>

						<p:outputLabel value="Id No"></p:outputLabel>
						<p:inputText
							value="#{awarenessManagementBackingBean.newParticipant.idNo}"
							disabled="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'E'}"
							style="width:100%" onkeyup="this.value=this.value.toUpperCase()"></p:inputText>

						<p:outputLabel value="Officer Type"
							rendered="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'O'}"></p:outputLabel>
						<p:selectOneMenu
							value="#{awarenessManagementBackingBean.newParticipant.officerType}"
							style="width: calc(100% - 15px)"
							rendered="#{awarenessManagementBackingBean.newParticipant.typeOfParticipation == 'O'}">
							<f:selectItem itemValue="" itemLabel="-- Select Type --" />
							<f:selectItems
								value="#{awarenessManagementBackingBean.officerTypeList}"
								var="v" itemValue="#{v.code}" itemLabel="#{v.description}" />
						</p:selectOneMenu>
					</p:panelGrid>

					<div class="gridspadding" align="center">
						<p:commandButton value="Add" styleClass="mainActionbtn"
							tabindex="1"
							actionListener="#{awarenessManagementBackingBean.saveNewParticipant}"
							alt="Add" update="eventDialog"></p:commandButton>

						<p:spacer width="5px"></p:spacer>

						<p:commandButton value="Clear" styleClass="mainActionbtn"
							tabindex="1"
							actionListener="#{awarenessManagementBackingBean.clearNewParticipantDialog}"
							alt="Clear" update="frmNewParticipant"></p:commandButton>
					</div>
				</h:form>
			</p:dialog>


			<!-- ################################# TRANSPORT MAINTENANCE ################################################################################## -->

			<p:dialog id="transportDialog" widgetVar="transportDialog"
				global="true" modal="true" header=" Transport Management"
				width="60%" resizable="false">
				<h:form id="frmTransportMgmnt">

					<p:panelGrid columns="4" layout="grid"
						columnClasses="ui-grid-col-3, ui-grid-col-4, ui-grid-col-3, ui-grid-col-4">
						<p:outputLabel value="Sub Reference No"
							style="font-weight:bold !important"></p:outputLabel>
						<p:outputLabel
							value="#{awarenessManagementBackingBean.selectedProgram.subRefNo}"></p:outputLabel>

						<p:outputLabel value="Province" style="font-weight:bold;"></p:outputLabel>
						<ui:repeat var="v"
							value="#{awarenessManagementBackingBean.provinceList}">
							<p:outputLabel value="#{v.description}"
								rendered="#{v.code == awarenessManagementBackingBean.selectedProgram.province}"></p:outputLabel>
						</ui:repeat>

						<p:panelGrid />
						<p:panelGrid />
						<p:panelGrid />
						<p:panelGrid />

						<p:outputLabel value="District"
							style="font-weight:bold; margin-bottom:5px"></p:outputLabel>
						<ui:repeat var="v"
							value="#{awarenessManagementBackingBean.districtListAll}">
							<p:outputLabel value="#{v.description}"
								rendered="#{v.code == awarenessManagementBackingBean.selectedProgram.district}"></p:outputLabel>
						</ui:repeat>

						<p:outputLabel value="Education Zone / Zone"
							style="font-weight:bold"></p:outputLabel>
						<ui:repeat var="v"
							value="#{awarenessManagementBackingBean.eduZoneList}">
