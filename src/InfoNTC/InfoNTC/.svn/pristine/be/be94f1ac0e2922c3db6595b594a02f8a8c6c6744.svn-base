								rendered="#{v.code == awarenessManagementBackingBean.selectedProgram.zone}"></p:outputLabel>
						</ui:repeat>

						<p:panelGrid />
						<p:panelGrid />
						<p:panelGrid />
						<p:panelGrid />

						<p:outputLabel value="Date" style="font-weight:bold"></p:outputLabel>
						<p:outputLabel
							value="#{awarenessManagementBackingBean.selectedProgram.scheduleDate}"></p:outputLabel>

						<p:panelGrid />
						<p:panelGrid />
					</p:panelGrid>


					<p:spacer height="10"></p:spacer>

					<p:panel style="border: 1px solid #bdd5f8;">
						<center>
							<h2 style="font-family: Palatino;">Vehicle Information</h2>
						</center>

						<div class="rowpadding ui-grid-row">
							<div class="ui-grid-col-12">
								<p:commandButton icon="fa fa-plus-circle fa-lg" process="@this"
									action="#{awarenessManagementBackingBean.showNewTransportDialog}"
									update="newTransportDialog" style="width:50px; float:right"
									styleClass="mainActionbtn" tabindex="1" />
							</div>
						</div>

						<p:dataTable id="dtTransport" widgetVar="transportTable" var="var"
							value="#{awarenessManagementBackingBean.transportList}"
							editable="true" paginator="true" rows="10"
							rowsPerPageTemplate="5,10,15" paginatorPosition="bottom"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							reflow="true">

							<p:ajax event="rowEdit"
								listener="#{awarenessManagementBackingBean.updateTransport}"
								update="dtTransport" />
							<p:ajax event="rowEditCancel" update="dtTransport" />

							<p:column headerText="Vehicle No." width="100"
								style="text-align:left">
								<p:cellEditor>
									<f:facet name="output">
										<h:outputText value="#{var.vehicleNo}" />
									</f:facet>
									<f:facet name="input">
										<p:inputText value="#{var.vehicleNo}"
											style="width: calc(100% - 23px)"
											onkeyup="this.value=this.value.toUpperCase()"></p:inputText>
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Driver Name" width="160"
								style="text-align:left">
								<p:cellEditor>
									<f:facet name="output">
										<h:outputText value="#{var.driverName}" />
									</f:facet>
									<f:facet name="input">
										<p:inputText value="#{var.driverName}"
											style="width: calc(100% - 23px)"
											onkeyup="this.value=this.value.toUpperCase()"></p:inputText>
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="KM" style="text-align:right" width="50">
								<p:cellEditor>
									<f:facet name="output">
										<h:outputText value="#{var.km}" />
									</f:facet>
									<f:facet name="input">
										<p:inputNumber value="#{var.km}" padControl="false"
											decimalPlaces="3" minValue="0"
											inputStyle="width: calc(100% - 23px)" styleClass="right"></p:inputNumber>
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column headerText="Transport Date" width="100"
								style="text-align:left">
								<p:cellEditor>
									<f:facet name="output">
										<h:outputText value="#{var.transportDate}" />
									</f:facet>
									<f:facet name="input">
										<p:calendar value="#{var.transportDate}"
											styleClass="calendarClass" navigator="true" locale="en_US"
											showOn="button" inputStyle="width:calc(100% - 43px)"
											pattern="dd/MM/yyyy" placeholder="dd/MM/yyyy"
											yearRange="c-100:c+100" readonlyInput="true">
										</p:calendar>
									</f:facet>
								</p:cellEditor>
							</p:column>

							<p:column width="15" style="text-align:center">
								<p:rowEditor />
							</p:column>

							<p:column width="15" style="text-align:center">
								<p:commandLink title="Delete Letter"
									action="#{awarenessManagementBackingBean.removeTransport}"
									style="color:#ff0000;" update="frmTransportMgmnt:dtTransport">
									<i class="fa fa-trash fa-lg" />
									<f:setPropertyActionListener value="#{var}"
										target="#{awarenessManagementBackingBean.selectedTransport}" />
								</p:commandLink>
							</p:column>

						</p:dataTable>
					</p:panel>
				</h:form>
			</p:dialog>

			<p:dialog id="newTransportDialog" widgetVar="newTransportDialog"
				global="true" modal="true" header=" New Transport" width="30%"
				resizable="false">
				<h:form id="frmNewTransport">

					<p:panelGrid columns="2" layout="grid"
						columnClasses="ui-grid-col-5, ui-grid-col-6">
						<p:outputPanel>
							<p:outputLabel value="Vehicle No."></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>

						<p:inputText
							value="#{awarenessManagementBackingBean.newTransport.vehicleNo}"
							tabindex="1"
							style="text-transform:uppercase; width: calc(100% - 10px);"
							onkeyup="this.value=this.value.toUpperCase()">

						</p:inputText>

						<p:outputPanel>
							<p:outputLabel value="Driver Name"></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>
						<p:inputText
							value="#{awarenessManagementBackingBean.newTransport.driverName}"
							style="width: calc(100% - 10px);"
							onkeyup="this.value=this.value.toUpperCase()"></p:inputText>

						<p:outputPanel>
							<p:outputLabel value="KM"></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>
						<p:inputNumber
							value="#{awarenessManagementBackingBean.newTransport.km}"
							padControl="false" decimalPlaces="3" minValue="0"
							inputStyle="width: calc(100% - 10px);" styleClass="right"></p:inputNumber>

						<p:outputPanel>
							<p:outputLabel value="Transport Date"></p:outputLabel>
							<p:outputLabel style="color:#ff0000" value=" *"></p:outputLabel>
						</p:outputPanel>
						<p:calendar
							value="#{awarenessManagementBackingBean.newTransport.transportDate}"
							styleClass="calendarClass" navigator="true" locale="en_US"
							showOn="button" inputStyle="width:calc(100% - 43px)"
							pattern="dd/MM/yyyy" placeholder="dd/MM/yyyy"
							yearRange="c-100:c+100" readonlyInput="true">
						</p:calendar>
					</p:panelGrid>

					<div class="gridspadding" align="center">
						<p:commandButton value="Add" styleClass="mainActionbtn"
							tabindex="1"
							actionListener="#{awarenessManagementBackingBean.saveNewTransport}"
							alt="Add" update="transportDialog"></p:commandButton>

						<p:spacer width="5px"></p:spacer>

						<p:commandButton value="Clear" styleClass="mainActionbtn"
							tabindex="1"
							actionListener="#{awarenessManagementBackingBean.clearNewTransportDialog}"
							alt="Clear" update="frmNewTransport"></p:commandButton>
					</div>
				</h:form>
			</p:dialog>

			<p:dialog id="transportPreviewDialog"
				widgetVar="transportPreviewDialog" global="true" modal="true"
				header=" Transport Management Preview" width="70%" resizable="false">
				<h:form id="frmTransportPreview">

					<p:panelGrid columns="4" layout="grid"
						columnClasses="ui-grid-col-3, ui-grid-col-4, ui-grid-col-3, ui-grid-col-4">
						<p:outputLabel value="Reference No :"
							style="font-weight:bold !important"></p:outputLabel>
						<p:outputLabel
							value="#{awarenessManagementBackingBean.selectedProject.refNo}"></p:outputLabel>

						<p:outputLabel value="Project Name :"
							style="font-weight:bold !important"></p:outputLabel>
						<p:outputLabel
							value="#{awarenessManagementBackingBean.selectedProject.projectName}"></p:outputLabel>

						<p:panelGrid />
						<p:panelGrid />
						<p:panelGrid />
						<p:panelGrid />


						<p:panelGrid />
						<p:panelGrid />
					</p:panelGrid>


					<p:spacer height="10"></p:spacer>


					<p:dataTable var="var"
						value="#{awarenessManagementBackingBean.transportList}"
						paginator="true" rows="10" rowsPerPageTemplate="5,10,15"
						paginatorPosition="bottom"
						paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
						reflow="true">
						<p:column headerText="Sub Ref No" sortBy="#{var.subRefNo}"
							style="text-align:left">
							<p:outputLabel value="#{var.subRefNo}"></p:outputLabel>
						</p:column>

						<p:column headerText="Place" sortBy="#{var.place}"
							style="text-align:left">
							<p:outputLabel value="#{var.place}"></p:outputLabel>
						</p:column>

						<p:column headerText="Event Date" sortBy="#{var.scheduleDate}"
							style="text-align:left">
							<p:outputLabel value="#{var.scheduleDate}"></p:outputLabel>
						</p:column>


						<p:column headerText="Vehicle No." sortBy="#{var.vehicleNo}"
							width="100" style="text-align:left">
							<p:outputLabel value="#{var.vehicleNo}" size="10"></p:outputLabel>
						</p:column>

						<p:column headerText="Driver Name" sortBy="#{var.driverName}"
							width="160" style="text-align:left">
							<p:outputLabel value="#{var.driverName}"></p:outputLabel>
						</p:column>

						<p:column headerText="KM" sortBy="1" style="text-align:right"
							width="50">
							<p:outputLabel value="#{var.km}"></p:outputLabel>
						</p:column>

						<p:column headerText="Transport Date"
							sortBy="#{var.transportDate}" width="100" style="text-align:left">
							<p:outputLabel value="#{var.transportDate}"></p:outputLabel>
						</p:column>
					</p:dataTable>

				</h:form>
			</p:dialog>


			<!-- ############################################################################################################################################
	###################################### CONFIRMATION DIALOGS #####################################################################################
	############################################################################################################################################# -->

			<p:dialog id="dialogDeleteConfirmPrj" global="true" modal="true"
				header="Confirmation" width="30%" widgetVar="prjDeleteConfirm"
				closeOnEscape="true" resizable="false">
				<h:form id="frmPrjDelete">
					<center>
						<p:outputLabel
							value="Are you sure you want delete Project including project details?"></p:outputLabel>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmPrjDelete" value="Yes"
							action="#{awarenessManagementBackingBean.deleteProject}"
							oncomplete="PF('prjDeleteConfirm').hide()"
							update="frmCaptureAwareness">
						</p:commandButton>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmPrjDelete" value="No"
							onclick="PF('prjDeleteConfirm').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>

			<p:dialog id="dialogDeleteConfirmPrm" global="true" modal="true"
				header="Confirmation" width="30%" widgetVar="prmDeleteConfirm"
				closeOnEscape="true" resizable="false">
				<h:form id="frmPrmDelete">
					<center>
						<p:outputLabel
							value="Are you sure you want delete selected Program and data?"></p:outputLabel>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmPrmDelete" value="Yes"
							action="#{awarenessManagementBackingBean.deleteProgram}"
							oncomplete="PF('prmDeleteConfirm').hide();"
							update="frmCaptureAwareness:dtProgram">
						</p:commandButton>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmPrmDelete" value="No"
							onclick="PF('prmDeleteConfirm').hide();">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>

			<p:dialog id="dialogError" global="true" modal="true" header="Error"
				width="30%" severity="info" widgetVar="errorDialog"
				closeOnEscape="true" resizable="false">
				<h:form id="frmError">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
							<p:outputLabel value="#{awarenessManagementBackingBean.errorMsg}"></p:outputLabel>
						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmError" value="OK" onclick="PF('errorDialog').hide()">
						</p:commandButton>

					</center>
				</h:form>
			</p:dialog>

			<p:dialog id="dialogSucces" global="true" modal="true"
				header="Success" width="30%" severity="info"
				widgetVar="successDialog" closeOnEscape="true" resizable="false"
				appendTo="@(body)">
				<h:form id="frmSuccess">
					<center>
						<p:panelGrid columns="2" styleClass="ui-noborder">
							<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
							<p:outputLabel
								value="#{awarenessManagementBackingBean.sucessMsg}"></p:outputLabel>
						</p:panelGrid>
						<br></br>
						<p:commandButton style="margin-top:10px;margin-bottom:10px"
							process="frmSuccess" value="OK"
							onclick="PF('successDialog').hide()">
						</p:commandButton>
					</center>
				</h:form>
			</p:dialog>

		</div>

	</ui:define>
</ui:composition>