<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>

	<ui:define name="content">

		<style>
.dateTimeField input {
	width: 100px;
}

.dateTimeField img {
	vertical-align: right;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.ui-input {
	margin-top: -20px;
}
</style>
		<!-- header -->

		<h:form id="formOne">
			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Approve Tender Advertisement</h1>

				<div style="width: 95%;" class="subDiv">

					<p:panel>
						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Tender Reference No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu style="width:200px" id="refNo"
									value="#{approveBackingBean.advertisementDTO.tenderNo}"
									filter="true" filterMatchMode="contains">
									<f:selectItem itemLabel="--Select No.--" itemValue="" />
									<f:selectItems value="#{approveBackingBean.dataList}"
										var="routeList" itemValue="#{routeList.tenderNo}"
										itemLabel="#{routeList.tenderNo}" />
									<p:ajax listener="#{approveBackingBean.onrefNoChange}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Tender Description    "></p:outputLabel>

							</p:panelGrid>

							<p:panelGrid>
								<p:inputText id="description" style="width:220px;"
									maxlength="50"
									value="#{approveBackingBean.advertisementDTO.description}"
									readonly="true">
								</p:inputText>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

						<center>
							<p:commandButton class="mainActionbtn" value="Search"
								action="#{approveBackingBean.search}" update="@form" disabled="">
							</p:commandButton>
							<p:commandButton value="Clear" class="mainActionbtn" ajax="true"
								action="#{approveBackingBean.clear}" update="@form">
							</p:commandButton>

							<h:commandButton class="mainActionbtn" ajax="true"
								value="Show PDF" onclick="this.form.target = '_blank'"
								actionListener="#{approveBackingBean.downloadPDF}"
								rendered="#{!approveBackingBean.pdfCheck}" />
						</center>

					</p:panel>

					<p:panel>


						<p:tabView>
							<p:tab title="English">
								<h:panelGrid width="100%">

									<p:dataTable id="dataTableTwo" var="x" rows="5"
										paginator="true"
										value="#{approveBackingBean.tenderDetailList}"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="10,20" resizableColumns="false"
										liveResize="true" tableStyle="width:100%;font-size:12px;"
										rowIndexVar="index" styleClass="myTable"
										emptyMessage="No tender details are available">


										<p:column headerText="SL No." width="7%" sortable="true"
											sortBy="#{x.slNumber}">
											<h:outputText value="#{x.slNumber}" />
										</p:column>


										<p:column headerText="Route No." width="7%">
											<h:outputText value="#{x.routeNo}" />
										</p:column>

										<p:column headerText="Departure" width="10%">
											<h:outputText value="#{x.departure}" />
										</p:column>

										<p:column headerText="Arrival" width="10%">
											<h:outputText value="#{x.arrival}" />
										</p:column>

										<p:column headerText="No. of Permits" width="7%">
											<h:outputText value="#{x.noOfPermits}" />
										</p:column>

										<p:column headerText="Via" width="14%">
											<h:outputText value="#{x.via}" />
										</p:column>

										<p:column headerText="Type of Service">
											<h:outputText value="#{x.typeOfServices}" />
										</p:column>

										<p:column headerText="One Way Fare (Rs.)"
											style="text-align:right">
											<h:outputText value="#{x.oneWayBusFare}">
												<f:convertNumber pattern="##,###.00" />
											</h:outputText>
										</p:column>

										<p:column headerText="No. of Turns Per Day Per Bus"
											width="13%">
											<h:outputText value="#{x.turnsPerDay}">

											</h:outputText>
										</p:column>


										<p:column headerText="Parallel Roads (Impact Prone)"
											width="13%">
											<h:outputText value="#{x.parallelRoads}" />
										</p:column>

										<p:column headerText="Effective" visible="false">
											<h:outputText value="#{x.effectiveRoutes}" />
										</p:column>

										<p:column headerText="teasure holder price" visible="false">
											<h:outputText value="#{x.treasureHolderPrice}" />
										</p:column>

									</p:dataTable>


								</h:panelGrid>
							</p:tab>
							<p:tab title="Sinhala">
								<h:panelGrid width="100%">

									<p:media value="/resources/demo/media/guide.pdf" width="100%"
										height="300px">
									</p:media>

								</h:panelGrid>
							</p:tab>
							<p:tab title="Tamil">
								<h:panelGrid width="100%">

									<p:media value="/resources/demo/media/guide.pdf" width="100%"
										height="300px">
									</p:media>

								</h:panelGrid>
							</p:tab>
						</p:tabView>
					</p:panel>





					<p:panel>
						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Tender Publish Date">
									<span style="color: red">*</span>
								</p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="cal"
									value="#{approveBackingBean.advertisementDTO.publishDate}"
									yearRange="c-100:c+100" navigator="true" locale="en_US"
									showOn="button" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY">
								</p:calendar>

								<p:commandButton value="Check Publication" class="mainActionbtn"
									ajax="true" action="#{approveBackingBean.checkPublication}"
									update="@form" style="width:50%;">
								</p:commandButton>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Reject Reason">
									<span style="color: red">*</span>
								</p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputTextarea rows="2" cols="33"
									value="#{approveBackingBean.rejectReason}">

									<p:ajax listener="#{approveBackingBean.onReasonChange}"
										update="@form" />
								</p:inputTextarea>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>
					</p:panel>

					<p:panel>
						<center>
							<p:commandButton value="Approve"
								action="#{approveBackingBean.approve}" class="mainActionbtn"
								ajax="true" update="@form"
								disabled="#{approveBackingBean.approvebutton}">
							</p:commandButton>

							<p:commandButton value="Reject" class="mainActionbtn" ajax="true"
								action="#{approveBackingBean.reject}" update="@form"
								disabled="#{approveBackingBean.rejectbutton}">
							</p:commandButton>

							<p:commandButton value="Cancel"
								action="#{approveBackingBean.clear}" class="mainActionbtn"
								ajax="true" update="@form">
							</p:commandButton>

							<p:commandButton value="Document Management"
								class="mainActionbtn"
								action="#{approveBackingBean.documentUpload}" ajax="true"
								style="width:200px;" update="@form,frmdocTender">
							</p:commandButton>
						</center>
					</p:panel>
				</div>
			</div>

		</h:form>


		<p:dialog styleClass="" id="fillfields" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="fill"
			closeOnEscape="true" resizable="false">

			<h:form id="xx">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel value="#{approveBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="xx" onclick="PF('fill').hide()">
					</p:commandButton>
				</center>
			</h:form>

		</p:dialog>

		<p:dialog styleClass="" id="successMSG" global="true" modal="true"
			header="Success" width="30%" severity="info"
			widgetVar="successMessage" closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{approveBackingBean.successMSG}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog header="Approved Advertisements" resizable="false"
			closeOnEscape="true" widgetVar="dlg3" width="600" height="200">
			<h:form id="lala">
				<p:dataTable id="abc" value="#{approveBackingBean.publishList}"
					var="data" tableStyle="100%"
					emptyMessage="No approved advertisements are available">


					<p:column headerText="Tender Reference No.">
						<h:outputText value="#{data.tenderNo}" />
					</p:column>

					<p:column headerText="Description">
						<h:outputText value="#{data.description}" />
					</p:column>

					<p:column headerText="Publish Date">
						<h:outputText value="#{data.datestring}" />
					</p:column>




				</p:dataTable>
			</h:form>
		</p:dialog>

		<p:dialog
			styleClass="#{editPermitIssuanceBackingBean.strSelectedTitle}"
			id="uploadTenderDocumentsDialog" global="true" modal="true"
			header="Upload Documents" width="80%" height="400px" severity="info"
			widgetVar="uploadTenderDocument" closeOnEscape="true"
			resizable="false" style="postion: absolute;">


			<h:form id="frmdocTender">

				<p:tabView>
					<p:tab title="Upload Documents">

						<p:accordionPanel>
							<p:growl id="mandatoryDocs" showDetail="false" />
							<p:tab title="Mandatory Documents">


								<h:panelGrid>
									<p:dataTable id="tenderMandatory" tableStyle="width:auto"
										var="docMandatory" value="#{approveBackingBean.mandatoryList}"
										rows="5" paginator="true" sortOrder="ascending"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,10,15">

										<p:column headerText="Code" style="width: 100px;">
											<h:outputText value="#{docMandatory.doc_Code}" />
										</p:column>
										<p:column headerText="Description" style="width: 300px;">
											<h:outputText value="#{docMandatory.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path" style="width: 300px;">

											<h:outputText value="#{docMandatory.uploadFilePath}" />
										</p:column>

										<p:column
											headerText="#{editPermitIssuanceBackingBean.strSelectedTitle}"
											style="width: 150px !important;">

											<p:fileUpload
												fileUploadListener="#{documentManagementBean.saveTenderMandatoryUploadDocuments}"
												mode="advanced" dragDropSupport="false" multiple="true"
												auto="true" showButtons="false" update="@form" fileLimit="1"
												accept=".pdf,.jpg,.jpeg,.png" allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
												

												<f:attribute name="uploadedObj" value="#{docMandatory}" />
												<f:attribute name="permitNo"
													value="#{committeeOrBoardApprovalBean.committeeApprovalDTO.committeTenderRefNo}" />
												<f:attribute name="applicationNo"
													value="#{committeeOrBoardApprovalBean.committeeApprovalDTO.committeeTenderApplicationNo}" />
												<f:attribute name="transactionCode"
													value="#{committeeOrBoardApprovalBean.typeTrans}" />
											</p:fileUpload>

										</p:column>

									</p:dataTable>
								</h:panelGrid>

							</p:tab>
						</p:accordionPanel>

						<br></br>
						<br></br>

						<p:accordionPanel>
							<p:growl id="optionalDocs" showDetail="false" />
							<p:tab title="Optional Documents">


								<h:panelGrid>
									<p:dataTable id="tenderOptional" tableStyle="width:auto"
										var="docOptional" value="#{approveBackingBean.optionalList}"
										rows="5" paginator="true" sortOrder="ascending"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,10,15">

										<p:column headerText="Code" style="width: 100px;">
											<h:outputText value="#{docOptional.doc_Code}" />
										</p:column>
										<p:column headerText="Description" style="width: 300px;">
											<h:outputText value="#{docOptional.add_Doc_Description}" />
										</p:column>
										<p:column headerText="File Path" style="width: 300px;">

											<h:outputText value="#{docOptional.uploadFilePath}" />
										</p:column>

										<p:column
											headerText="#{editPermitIssuanceBackingBean.strSelectedTitle}"
											style="width: 150px !important;">

											<p:fileUpload
												fileUploadListener="#{documentManagementBean.saveTenderOptionalUploadDocuments}"
												mode="advanced" dragDropSupport="false" multiple="true"
												auto="true" showButtons="false" update="@form" fileLimit="1"
												accept=".pdf,.jpg,.jpeg,.png" allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
												
												
												<f:attribute name="uploadedOptionalObj"
													value="#{docOptional}" />

											</p:fileUpload>

										</p:column>

									</p:dataTable>
								</h:panelGrid>

							</p:tab>
						</p:accordionPanel>



					</p:tab>

					<p:tab title="Upload History">


						<p:accordionPanel multiple="true">
							<p:growl showDetail="false" />
							<p:tab title="New Permit" closable="true">


								<p:accordionPanel id="dkfj">
									<p:growl id="NP" showDetail="false" />
									<p:tab title="Mandatory Documents">


										<h:panelGrid>
											<p:dataTable id="mandatoryNP" tableStyle="width:auto"
												var="docMandatory"
												value="#{sessionBackingBean.newPermitMandatoryDocumentList}"
												rows="5" paginator="true" sortOrder="ascending"
												paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
												rowsPerPageTemplate="5,10,15">

												<p:column headerText="Code" style="width: 100px;">
													<h:outputText value="#{docMandatory.doc_Code}" />
												</p:column>
												<p:column headerText="Description" style="width: 300px;">
													<h:outputText value="#{docMandatory.add_Doc_Description}" />
												</p:column>
												<p:column headerText="File Name" style="width: 200px;">

													<h:outputText value="#{docMandatory.documentName}" />
												</p:column>

												<p:column headerText="" style="width: 75px;">


													<p:commandLink
														actionListener="#{documentManagementBean.viewMandatoryUploadDocs}"
														oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
														<p:graphicImage library="images" name="view.png"
															style="width:20px;height:20px;" title="view"></p:graphicImage>
														<f:attribute name="documentMandatory"
															value="#{docMandatory}" />
													</p:commandLink>



												</p:column>


											</p:dataTable>
										</h:panelGrid>

									</p:tab>
								</p:accordionPanel>

								<br></br>
								<br></br>

								<p:accordionPanel>

									<p:growl showDetail="false" />

									<p:tab title="Optional Documents">

										<h:panelGrid>
											<p:dataTable id="optionalNP" tableStyle="width:auto"
												var="docOptional"
												value="#{sessionBackingBean.newPermitOptionalDocumentList}"
												rows="5" paginator="true" sortOrder="ascending"
												scrollHeight="100%" liveResize="true" liveScroll="true"
												paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
												rowsPerPageTemplate="5,10,15">

												<p:column headerText="Code" style="width: 100px;">
													<h:outputText value="#{docOptional.doc_Code}" />
												</p:column>
												<p:column headerText="Description" style="width: 300px;">
													<h:outputText value="#{docOptional.add_Doc_Description}" />
												</p:column>
												<p:column headerText="File Name" style="width: 200px;">
													<h:outputText value="#{docOptional.documentName}" />
												</p:column>

												<p:column headerText="" style="width: 75px;">


													<p:commandLink
														actionListener="#{documentManagementBean.viewUploadedOptionalDocAction}"
														oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
														<p:graphicImage library="images" name="view.png"
															style="width:20px;height:20px;" title="view"></p:graphicImage>
														<f:attribute name="documentOptional"
															value="#{docOptional}" />
													</p:commandLink>

												</p:column>


											</p:dataTable>
										</h:panelGrid>
									</p:tab>

								</p:accordionPanel>

							</p:tab>


