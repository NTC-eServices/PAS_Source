<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}
</style>

		<!-- Header -->

		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">#{msg['common.viewdocument.header']}</h1>
			<div style="width: 95%;" class="subDiv">
				<h:form id="frmdoc">

		


					<p:panel >
						<p:panelGrid id="searchPanelForPermitSearch" columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Permit No."></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu style="margin-left:-3%; width:90%;"
									id="searchPermit"
									value="#{documentManagementBeanNew.uploaddocumentManagementDTO.upload_Permit}"
									filter="true" filterMatchMode="contains">
									 <p:ajax update="@form"
										listener="#{documentManagementBeanNew.transactionTypeFilter}" /> 
										
									<f:selectItem
										itemLabel="--Select Permit No.--"
										itemValue="" noSelectionOption="true" />
									<f:selectItems value="#{documentManagementBeanNew.allPermitNoList}" />
								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>


						</p:panelGrid>
					</p:panel>
					
									<p:panel id="transactionType">
							<p:panelGrid id="searchPanel" columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
								layout="grid">
	
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Transaction Type"></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
	
								<p:panelGrid>
									<p:selectOneMenu style="margin-left:-3%; width:90%;"
										id="searchTrans" disabled="#{documentManagementBeanNew.disableUntilPermitSelect}"
										value="#{documentManagementBeanNew.uploaddocumentManagementDTO.upload_TransactionType}"
										filter="true" filterMatchMode="contains">
	
										<f:selectItem itemLabel="--Select Transaction Type--"
											itemValue="" noSelectionOption="true" />
										<f:selectItems
											value="#{documentManagementBeanNew.filterdTransacrtions}" var="t"
											itemLabel="#{t.transaction_Type}" itemValue="#{t.transaction_Type_Code}" />
										<p:ajax listener="#{documentManagementBeanNew.selection}"
											update="@form" />
									</p:selectOneMenu>
								</p:panelGrid>
	
	
								<p:panelGrid></p:panelGrid>
	
	
							</p:panelGrid>
						</p:panel>
	

					

					<p:panel >
						<p:panelGrid id="searchPanelForApplicationSearch" columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Application No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu style="margin-left:-3%; width:90%;"
									id="searchApplication"
									disabled="#{documentManagementBeanNew.disableUntilPermitAndTransSelect}"
									value="#{documentManagementBeanNew.uploaddocumentManagementDTO.upload_Application}"
									filter="true" filterMatchMode="contains">
									<p:ajax update="@form"
										listener="#{documentManagementBeanNew.generatePermitNo}" />
										
									<f:selectItem
										itemLabel="--Select Application No.--"
										itemValue="" noSelectionOption="true" />
									<f:selectItems
										value="#{documentManagementBeanNew.applicationNoList}" />
								</p:selectOneMenu>
							</p:panelGrid>


							<p:panelGrid>
							</p:panelGrid>


						</p:panelGrid>
					</p:panel>

				

					<p:panel >
						<center>
							<p:commandButton rendered="#{documentManagementBeanNew.btnPanel}"
								value="Search" update="@form"
								actionListener="#{documentManagementBeanNew.searchViewDoc}"
								styleClass="mainActionbtn" class="mainActionbtn"></p:commandButton>

							 <p:commandButton value="Clear" update="@form"
								styleClass="mainActionbtn" class="mainActionbtn"
								actionListener="#{documentManagementBeanNew.clearAction}"></p:commandButton> 

						</center>
					</p:panel>
		


					<br></br>
					<p:accordionPanel>
						<p:growl id="msgs" showDetail="false" />
						<p:tab title="Mandatory Documents">


							<h:panelGrid>
								<p:dataTable id="abc" tableStyle="width:auto" var="docMandatory"
									value="#{documentManagementBeanNew.mandatoryViewList}" rows="10"
									paginator="true" sortOrder="ascending"
									paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
									rowsPerPageTemplate="10,20,30">



									<p:column headerText="Code"
										style="width: 100px;text-align: left;">
										<h:outputText value="#{docMandatory.doc_Code}" />
									</p:column>
									<p:column headerText="Description"
										style="width: 300px;text-align: left;">
										<h:outputText value="#{docMandatory.add_Doc_Description}" />
									</p:column>
									<p:column headerText="File Path"
										style="width: 300px;text-align: left;">

										<h:outputText value="#{docMandatory.uploadFilePath}" />
									</p:column>

									<p:column headerText="" style="width: 75px;">

										<p:commandLink
											actionListener="#{documentManagementBeanNew.viewMandatoryUploadDocs}"
											oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
											<p:graphicImage library="images" name="view.png"
												style="width:20px;height:20px;" title="view"></p:graphicImage>
											<f:attribute name="documentMandatory" value="#{docMandatory}" />
										</p:commandLink>
									</p:column>

									<p:column headerText="" style="width: 75px;">
										<p:commandLink ajax="false"
											actionListener="#{documentManagementBeanNew.viewUploadHistory}">
											<p:graphicImage library="images" name="history.png"
												style="width:20px;height:20px;" title="Upload History"></p:graphicImage>
											<f:attribute name="documentHistory" value="#{docMandatory}" />

										</p:commandLink>


									</p:column>


								</p:dataTable>
							</h:panelGrid>










						</p:tab>
					</p:accordionPanel>

					<br></br>
					<br></br>

					<p:accordionPanel>
						<p:growl id="msgse" showDetail="false" />
						<p:tab title="Optional
         Documents">

							<p:commandLink value=""
								actionListener="#{documentManagementBeanNew.optionalViewList}"
								update="@form"></p:commandLink>

							<h:panelGrid>
								<p:dataTable id="abcd" tableStyle="width:auto" var="docOptional"
									value="#{documentManagementBeanNew.optionalViewList}" rows="10"
									paginator="true" sortOrder="ascending"
									paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
									rowsPerPageTemplate="10,20,30">



									<p:column headerText="Code"
										style="width: 100px;text-align: left;text-align: left;">
										<h:outputText value="#{docOptional.doc_Code}" />
									</p:column>
									<p:column headerText="Description"
										style="width: 300px;text-align: left;">
										<h:outputText value="#{docOptional.add_Doc_Description}" />
									</p:column>
									<p:column headerText="File Path"
										style="width: 300px;text-align: left;">
										<h:outputText value="#{docOptional.uploadOptionalFilePath}" />
									</p:column>


									<p:column headerText="" style="width: 75px;">

										<p:commandLink
											actionListener="#{documentManagementBeanNew.viewUploadedOptionalDocAction}"
											oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
											<p:graphicImage library="images" name="view.png"
												style="width:20px;height:20px;" title="view"></p:graphicImage>
											<f:attribute name="documentOptional" value="#{docOptional}" />
										</p:commandLink>

									</p:column>
									<p:column headerText="" style="width: 75px;">
										<p:commandLink ajax="false"
											actionListener="#{documentManagementBeanNew.viewUploadHistory}">
											<p:graphicImage library="images" name="history.png"
												style="width:20px;height:20px;" title="Upload History"></p:graphicImage>
											<f:attribute name="documentHistory" value="#{docOptional}" />

										</p:commandLink>


									</p:column>


								</p:dataTable>
							</h:panelGrid>



						</p:tab>
					</p:accordionPanel>

					<br></br>
					<br></br>
















				</h:form>

				<p:dialog styleClass="" id="AddNewDocument" global="true"
					modal="true" header="Add New Documents" width="40%" severity="info"
					widgetVar="viewAddDoc" closeOnEscape="true" resizable="false">
					<h:form id="newdoc">

						<p:panelGrid columns="4">
							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Document Code"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText value="#{documentManagementBeanNew.newDocCode}"></p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Document Description"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText value="#{documentManagementBeanNew.newDocDes}"></p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>


						</p:panelGrid>

						<br></br>
						<center>
							<p:commandButton value="Save" styleClass="mainActionbtn"
								class="mainActionbtn"
								actionListener="#{documentManagementBeanNew.addNewDocumentSave}"></p:commandButton>
							<p:commandButton value="Clear" styleClass="mainActionbtn"
								class="mainActionbtn"></p:commandButton>



						</center>
					</h:form>
				</p:dialog>


				<p:dialog styleClass="" id="successSve" global="true" modal="true"
					header="#{msg['um.employeeprofile.dlsuccess']}" width="30%"
					severity="info" widgetVar="successSve" closeOnEscape="true"
					resizable="false">
					<h:form id="frmsuccessSve">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
								<p:outputLabel value="Successfully Updated."></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								update="@all" process="frmsuccessSve"
								value="#{msg['um.employeeprofile.dlsuccessclose']}"
								onclick="PF('successSve').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>


				<p:dialog styleClass="" id="requiredField" global="true"
					modal="true" header="#{msg['um.employeeprofile.dlmissing']}"
					width="30%" severity="info" widgetVar="requiredField"
					closeOnEscape="true" resizable="false">
					<h:form id="frmrequiredField">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
								<p:outputLabel value="#{documentManagementBeanNew.errorMsg}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmrequiredField"
								value="#{msg['um.employeeprofile.dlmissingclose']}"
								onclick="PF('requiredField').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>

				<p:dialog styleClass="" id="successSveUploadDoc" global="true"
					modal="true" header="#{msg['um.employeeprofile.dlsuccess']}"
					width="30%" severity="info" widgetVar="successSveUploadDoc"
					closeOnEscape="true" resizable="false">
					<h:form id="frmsuccessSveUploadDoc">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
								<p:outputLabel value="#{documentManagementBeanNew.successMsg}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								update="frmdoc" process="frmsuccessSveUploadDoc"
								value="#{msg['um.employeeprofile.dlsuccessclose']}"
								onclick="PF('successSveUploadDoc').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>


				<p:dialog styleClass="" id="requiredFieldUploadDoc" global="true"
					modal="true" header="#{msg['um.employeeprofile.dlmissing']}"
					width="30%" severity="info" widgetVar="requiredFieldUploadDoc"
					closeOnEscape="true" resizable="false">
					<h:form id="frmrequiredFieldUploadDoc">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
								<p:outputLabel value="#{documentManagementBeanNew.errorMsg}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmrequiredFieldUploadDoc"
								value="#{msg['um.employeeprofile.dlmissingclose']}"
								onclick="PF('requiredFieldUploadDoc').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>



				<p:dialog styleClass="" id="historyView" global="true" modal="true"
					header="Upload History" width="50%" severity="info"
					widgetVar="historyView" closeOnEscape="true" resizable="false">
					<h:form id="frmhistoryView">

						<p:dataTable id="historyUpload" tableStyle="width:auto"
							var="docHistory"
							value="#{documentManagementBeanNew.historyViewList}" rows="5"
							paginator="true" sortOrder="ascending"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							rowsPerPageTemplate="5,10,15">


							<p:column headerText="Version"
								style="width:10%;text-align: left;">
								<h:outputText value="#{docHistory.versionNo}" />
							</p:column>
							<p:column headerText="Document Description"
								style="width:20%;text-align: left;">
								<h:outputText value="#{docHistory.add_Doc_Description}" />
							</p:column>
							<p:column headerText="File Path"
								style="width: 60%;text-align: left;">
								<h:outputText value="#{docHistory.uploadFilePath}" />
							</p:column>
							<p:column style="width: 10%;text-align: left;">

								<p:commandLink
									actionListener="#{documentManagementBeanNew.viewUploadedVersions}"
									oncomplete="window.open('/InfoNTC/showUpload?reportName=Upload Document')">
									<p:graphicImage library="images" name="view.png"
										style="width:20px;height:20px;" title="view"></p:graphicImage>
									<f:attribute name="documentVersion" value="#{docHistory}" />
								</p:commandLink>

							</p:column>

							<p:column headerText="" style="width: 75px;">

								<p:commandLink action="#{documentManagementBeanNew.removeAction}"
									update="@form" rendered="#{docHistory.checkdelete}">
									<p:graphicImage library="images" name="delete.png"
										style="width:20px;height:20px;" title="delete"></p:graphicImage>

									<f:setPropertyActionListener value="#{docHistory}"
										target="#{documentManagementBeanNew.selectedRemovePathRow}" />

								</p:commandLink>

							</p:column>



						</p:dataTable>

					</h:form>
				</p:dialog>

				<!-- No Records Found Message dialog Box  -->
				<p:dialog styleClass="" id="warningField" global="true" modal="true"
					header="#{msg['um.employeeprofile.dlwarning']}" width="30%"
					severity="info" widgetVar="warningField" closeOnEscape="true"
					resizable="false">
					<h:form id="frmwarningField">
						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="warning.png"></p:graphicImage>
								<p:outputLabel value="#{documentManagementBeanNew.warningMsg}"></p:outputLabel>

							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmwarningField"
								value="#{msg['um.employeeprofile.dlmissingclose']}"
								onclick="PF('warningField').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>

			</div>
		</div>


	</ui:define>
</ui:composition>