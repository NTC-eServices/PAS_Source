		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">View Amendments (Owner Change/Bus
				Change/Service Change)</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form>
					<!-- Search box start -->
					<p:panel>
						<p:panelGrid columns="3" style="text-align:center;"
							columnClasses="ui-grid-col-3,ui-grid-col-6,ui-grid-col-3"
							layout="grid">
							<!-- Amendment Type -->
							<p:outputLabel></p:outputLabel>
							<p:outputLabel
								style="width:100%;height:18px;text-align:center;color:#404d6f;font-weight: bold;"
								readonly="true"
								value="#{viewAmendmentBackingBean.strAmendmentType}">
							</p:outputLabel>

							<p:outputLabel></p:outputLabel>
						</p:panelGrid>
						<br></br>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
							layout="grid">
							<!-- Amendment Type -->

							<p:outputLabel>Application No.</p:outputLabel>
							<p:inputText
								style="width:100%;height:18px;color:red;font-weight: bold;"
								maxlength="50"
								value="#{viewAmendmentBackingBean.generatedApplicationNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>

							<!-- _Bus Registration No. -->
							<p:outputLabel>Vehicle No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="true" value="#{viewAmendmentBackingBean.strVehicleNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>



						</p:panelGrid>
						<!-- Search panel buttons -->
						<center>
							<p:panel>
								<!-- _Search -->
								<p:commandButton styleClass="mainActionbtn" value="Search"
									rendered="#{viewAmendmentBackingBean.renderSearch}"
									ajax="false" update="@form"
									action="#{viewAmendmentBackingBean.searchByApplicationNo}"
									disabled="{viewAmendmentBackingBean.callNext==true}"></p:commandButton>
								<!-- _Clear -->
								<p:commandButton styleClass="mainActionbtn" value="Cancel"
									rendered="#{viewAmendmentBackingBean.renderCancel}"
									ajax="false" update="@form"
									action="#{viewAmendmentBackingBean.clearAll}"></p:commandButton>


							</p:panel>



						</center>
					</p:panel>
					<!-- Search box end -->


				</h:form>
				<!-- Start tab view section -->

				<p:tabView id="tabviewId">


					<!-- ***********************************      Tab 01       ******************************************************************* -->
					<p:tab title="Particulars of Bus Owner"
						activeIndex="#{viewAmendmentBackingBean.activeTabIndex}"
						rendered="#{viewAmendmentBackingBean.oldOwner}">

