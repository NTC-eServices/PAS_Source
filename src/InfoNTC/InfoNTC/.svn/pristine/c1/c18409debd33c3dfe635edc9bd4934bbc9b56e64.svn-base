										layout="grid">

										<!-- Tab 01 > panel 01 : row 01 -->
										<p:outputLabel>
											Permit No.
										</p:outputLabel>

										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.permitNo}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>


										<!-- Tab 01 > panel 01 : row 02 -->
										<p:outputLabel>Application No.</p:outputLabel>

										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.applicationNo}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>



										<!-- Tab 01 > panel 01 : row 01-->
										<p:outputLabel>Service Type</p:outputLabel>

										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.serviceType}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>


										<!-- Tab 01 > panel 01 : row 04 -->
										<p:outputLabel>Existing Bus No.</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.busRegNo}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>


										<!-- Tab 01 > panel 01 : row 05 -->


