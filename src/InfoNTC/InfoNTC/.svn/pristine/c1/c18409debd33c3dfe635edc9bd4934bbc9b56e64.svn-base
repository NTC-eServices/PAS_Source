											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.expireDate}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>


										<p:outputLabel>Remarks</p:outputLabel>
										<p:inputTextarea rows="5" cols="87"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.finalRemark}"
											readonly="true" queryDelay="750" minQueryLength="4" />

									</p:panelGrid>
									<!-- Tab 01 > panel 01 : Buttons -->
								</p:panel>

								<!-- Tab 01 > panel 02 : Bus owner details -->
								<p:panel>
									<p:panelGrid columns="4"
										columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">

										<!-- Tab 01 > panel 02 : row 01 -->
