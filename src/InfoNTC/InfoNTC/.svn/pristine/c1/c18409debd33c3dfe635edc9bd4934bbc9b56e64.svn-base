											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.fullName}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>
									</p:panelGrid>
									<p:panelGrid columns="4"
										columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
										layout="grid">
										<p:outputLabel>Telephone No.</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.telephoneNo}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 01 > panel 02 : row 02 -->
										<p:outputLabel>Address 1</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.address1}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Mobile No.</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.mobileNo}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 01 > panel 02 : row 03 -->
										<p:outputLabel>Address 2</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.address2}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Province</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.province}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 01 > panel 02 : row 04 -->
										<p:outputLabel>City</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.city}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>District</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.district}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 01 > panel 02 : row 05 -->
										<!-- panel 02 : row 4 -->
										<p:outputLabel>NIC No.</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.nicNo}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Divisional Secretariat Division</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											value="#{viewAmendmentBackingBean.amendmentBusOwnerDTO.divSec}"
											readonly="true"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Back to Amendments Approval -->

										<p:commandButton styleClass="mainLinkButton"
											rendered="#{viewAmendmentBackingBean.renderBack}"
											value="Back"
											actionListener="#{viewAmendmentBackingBean.back}"
											update="@form"></p:commandButton>

										<p:commandButton styleClass="mainLinkButton"
											rendered="#{viewAmendmentBackingBean.showInquiryBack}"
											value="Back"
											actionListener="#{viewAmendmentBackingBean.backInquiry}"
											update="@form" id="btnBack"></p:commandButton>

									</p:panelGrid>
									<!-- Tab 01 > panel 02 : Buttons -->
								</p:panel>

							</p:panel>
						</h:form>
					</p:tab>

					<!-- ***********************************      Tab 02       ******************************************************************* -->
					<p:tab title="Particulars of Existing Bus"
						activeIndex="#{viewAmendmentBackingBean.activeTabIndex}"
						rendered="#{viewAmendmentBackingBean.oldBus}">

						<h:form id="frmParticularsOfBusOwner">

							<p:panel>

								<!-- Tab 02 > panel 02 : Bus owner details -->
								<p:panel>
									<p:panelGrid columns="4"
										columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
										layout="grid">

										<!-- Tab 02 > panel 02 : row 01 -->
										<p:outputLabel>Registration No. of the Bus</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.vehicleRegNo}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Permit No.</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.permitNo}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 02 > panel 02 : row 02 -->
										<p:outputLabel>Seating Capacity</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.seating}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Manufacture Year</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.manufactureDate}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 02 > panel 02 : row 03 -->
										<p:outputLabel>No. of Doors </p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.noofDoors}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Weight (Kg)</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.weight}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<!-- Tab 02 > panel 02 : row 04 -->
										<p:outputLabel>Service Type</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.serviceTypeDesc}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Route No.</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.routeNo}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>



										<!-- Tab 02 > panel 02 : row 05 -->

										<p:outputLabel>Service Origin</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.origin}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Service Destination</p:outputLabel>
										<p:inputText style="width:100%;height:18px;" maxlength="50"
											readonly="true"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.destination}"
											onkeyup="this.value=this.value.toUpperCase();">
										</p:inputText>

										<p:outputLabel>Date of Registration</p:outputLabel>
										<p:calendar styleClass="calendarClass"
											value="#{viewAmendmentBackingBean.amendmentOminiBusDTO.dateOfFirstReg}"
											maxdate="today" yearRange="c-100:c+100" navigator="true"
											locale="en_US" showOn="button" readonlyInput="true"
											disabled="true" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY">
										</p:calendar>



									</p:panelGrid>
									<!-- Tab 02 > panel 02 : row 06 -->
									<p:panelGrid columns="2"
										columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">
										<p:outputLabel rowspan="2">Remarks</p:outputLabel>
										<p:inputTextarea rows="5" cols="90" rowspan="2"
