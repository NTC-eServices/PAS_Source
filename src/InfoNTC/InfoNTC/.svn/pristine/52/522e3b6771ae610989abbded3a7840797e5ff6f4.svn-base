		<![CDATA[select distinct a.dcvd_account_no, a.dcvd_amount ,b.description,a.dcvd_voucher_no,a.dcvd_application_no,d.dcv_driver_conduc_id_no,
c.rec_receipt_no,d.dcv_training_type,e.description as transtype,f.dcr_full_name_eng
from public.nt_t_driver_conduc_payment_voucher a ,public.nt_r_charge_type b,
public.nt_t_driver_conductor_receipt c,public.nt_m_driver_conduc_payment_voucher d,
public.nt_r_training_type  e, public.nt_t_driver_conductor_registration f
where a.dcvd_charge_type=b.code
and  c.rec_voucher_no=a.dcvd_voucher_no
and a.dcvd_voucher_no = d.dcv_voucher_no
and e.code= d.dcv_training_type
and d.dcv_driver_conduc_id_no=f.dcr_driver_conductor_id 
and a.dcvd_voucher_no= $P{P_Voucher_No}]]>
	</queryString>
