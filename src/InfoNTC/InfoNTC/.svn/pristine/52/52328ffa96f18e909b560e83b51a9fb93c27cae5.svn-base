							<p:outputLabel
								style="width:100%;height:18px;text-align:center;color:#404d6f;font-weight: bold;"
								value="#{amendmentBackingBean.strAmendmentType}">
							</p:outputLabel>
							<p:outputLabel></p:outputLabel>

						</p:panelGrid>
						<br></br>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
							layout="grid">


							<!-- _Bus Registration No. -->
							<p:outputLabel>Vehicle No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="true" value="#{amendmentBackingBean.strVehicleNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<!-- _Queue No. -->
							<p:outputLabel>Queue No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								value="#{amendmentBackingBean.strQueueNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>


						</p:panelGrid>
