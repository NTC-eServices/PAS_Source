							<p:panelGrid>

								<p:outputLabel value="Select Counter No. :- "></p:outputLabel>

							</p:panelGrid>


							<p:panelGrid>
								<p:selectOneMenu style="width:171px" id="counter"
									value="#{amendmentBackingBean.commonDTO.counterId}">
									<p:ajax listener="#{amendmentBackingBean.onCounterSelect}"
										onClose="#{amendmentBackingBean.handleClose}">
									</p:ajax>
									<f:selectItem itemLabel="--Select Counter--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems value="#{amendmentBackingBean.counterList}"
										var="counterList" itemLabel="#{counterList.counter}"
										itemValue="#{counterList.counterId}" />

								</p:selectOneMenu>

							</p:panelGrid>

							<center>
								<p:panel>
									<p:commandButton id="Exit" value="Exit"
										action="#{amendmentBackingBean.handleClose}"
										class="mainActionbtn" ajax="true" update="@all">
									</p:commandButton>
								</p:panel>
							</center>

						</p:panelGrid>

					</p:dialog>
					<!-- Search box start -->
					<p:panel>
