	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 79% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}

.ui-datatable-scrollable .ui-datatable-scrollable-header {
	min-height: 20px;
}

.ui-datatable-scrollable-body {
	height: 125px !important;
}

.genReqNumButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	text-decoration: none !important;
	color: white !important;
	border-radius: 5px !important;
}

.ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	text-shadow: none;
}

.ui-accordion .ui-accordion-header {
	cursor: pointer;
	position: relative;
	margin-top: 1px;
	min-height: 0;
	padding: .5em .5em .5em 2.2em;
	background-color: cornflowerblue;
}

.ui-autocomplete-itemtip {
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box;
}
</style>

		<div class="mainDiv">

			<!-- header -->

			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">SIM Renewal</h1>

			<div style="width: 95%;" class="subDiv">

				<p:panel id="pnl-sim-reg">

					<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
						layout="grid">


						<h:form id="simRegistrationForm">

							<!-- Search area -->

							<p:panel
								style="border: #404d6f solid 1px !important;border-radius: 25px;background-color: #bdcafc;
								box-shadow: 3px 3px 3px grey; margin-bottom: 15px;">

								<!-- SIM REG No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="SIM Registration No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="simRegSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simRenewalBean.simSearchDTO.simRegNo}">

											<f:selectItem itemLabel=" -- Select SIM Reg No. -- "
												itemValue="" />
											<f:selectItems value="#{simRenewalBean.simDetailsList}"
												var="dto" itemValue="#{dto.simRegNo}"
												itemLabel="#{dto.simRegNo}" />

										</p:selectOneMenu>
									</div>
									<div class="ui-g-3"></div>
								</div>

								<!-- Permit No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="Permit No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="permitNoSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simRenewalBean.simSearchDTO.permitNo}">

											<f:selectItem itemLabel=" -- Select Permit No. -- "
												itemValue="" />
											<f:selectItems value="#{simRenewalBean.simDetailsList}"
												var="dto" itemValue="#{dto.permitNo}"
												itemLabel="#{dto.permitNo}" />

										</p:selectOneMenu>
									</div>

									<div class="ui-g-3"></div>
								</div>

								<!-- Bus No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="Bus No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="busNoSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simRenewalBean.simSearchDTO.busNo}">

											<f:selectItem itemLabel=" -- Select Bus No. -- " itemValue="" />
											<f:selectItems value="#{simRenewalBean.simDetailsList}"
												var="dto" itemValue="#{dto.busNo}" itemLabel="#{dto.busNo}" />

										</p:selectOneMenu>
									</div>

									<!-- Search Button -->
									<div class="ui-g-3">
										<p:commandButton id="searchBtn" styleClass="otherLinkButton"
											style="width:80%;" value="Search" process="@all"
											update="@form" action="#{simRenewalBean.searchAction}">
										</p:commandButton>
									</div>
								</div>

							</p:panel>

							<!-- Input Details part -->

							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4,ui-grid-col-3"
								layout="grid">

								<!-- SIM Registration Number -->
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="SIM Registration No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="simRegNoId"
											style="width:70%;color: green;font-weight: bold;"
											filter="true" placeholder="Search SIM Details"
											filterMatchMode="contains" tabindex="1" effect="fold"
											readonly="true"
											value="#{simRenewalBean.simRegistrationDTO.simRegNo}" />
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>


								<!-- SIM No. -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="SIM No. (Mobile No.) "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="simNoId"
											style="width:70%" filter="true" filterMatchMode="contains"
											tabindex="1" maxlength="10" readonly="true"
											placeholder="SIM Number"
											value="#{simRenewalBean.simRegistrationDTO.simNo}">
											<p:keyFilter regEx="/[0-9-]/i" />
										</p:inputText>
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>



								<!-- EMI Number -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="EMI Number "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="emiNoId" style="width:70%" filter="true"
										filterMatchMode="contains" tabindex="1" effect="fold"
										placeholder="EMI Number" maxlength="20"
										value="#{simRenewalBean.simRegistrationDTO.emiNo}"
										disabled="#{simRenewalBean.disableEmiNo}">
										<p:keyFilter regEx="/[0-9-]/i" />
									</p:inputText>

								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Issue Date -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Issue Date "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="issueDate" style="width:70%" filter="true"
										filterMatchMode="contains" tabindex="1" effect="fold"
										placeholder="Issue Date" readonly="true"
										value="#{simRenewalBean.simRegistrationDTO.issueDateString}">
									</p:inputText>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Last Valid Until -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Last Valid Until "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>

									<p:inputText id="lastValidUntilDate" style="width:70%"
										filter="true" filterMatchMode="contains" tabindex="1"
										effect="fold" placeholder="Valid Date" readonly="true"
										value="#{simRenewalBean.simRegistrationDTO.validUntilDateString}">
									</p:inputText>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Renewal Until -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Renewal Until "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:calendar id="renewalUntilDate" style="height:18px;"
										mindate="#{today}" navigator="true" showOn="button"
										locale="en_US" pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
										yearRange="c-100:c+100" readonlyInput="true"
										disabled="#{backlogTransaction.disableStartDateField}"
										value="#{simRenewalBean.simRegistrationDTO.renewalUntilDate}">
									</p:calendar>

								</p:panelGrid>
								<p:panelGrid></p:panelGrid>


								<!-- Payment Receipt Number -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Payment Receipt No."></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="paymentReceiptNoId" style="width:70%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="Payment Receipt Number" readonly="true"
											value="#{simRenewalBean.simRegistrationDTO.paymentReceiptNo}" />
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>


								<!-- Remarks -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Remarks"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputTextarea style="width:80%;height:60px;" id="remarkId"
										value="#{simRenewalBean.simRegistrationDTO.remarks}"
										placeholder="Enter Message" maxlength="300" rows="5"
										counter="display" counterTemplate="{0} characters remaining."
										autoResize="false">
									</p:inputTextarea>
									<br />
									<h:outputText id="display" />
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

							</p:panelGrid>



							<!-- Main Button Panel -->



							<p:panelGrid columns="3"
								columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
								layout="grid" style="margin-top:2%;margin-bottom:2%;">

								<!-- Create Voucher Button -->

								<p:commandButton id="createVouchertBtn"
									styleClass="mainLinkButton" style="width:80%"
									value="Create Voucher" process="@all"
									disabled="#{simRenewalBean.createVoucherCheck}"
									update="@form,createPaymentVoucherID"
									action="#{simRenewalBean.createVoucherBtnAction}">
								</p:commandButton>

								<!-- Save Button -->

								<p:commandButton id="saveBtn" styleClass="mainLinkButton"
									style="width:80%" value="Save" process="@all" update="@form"
									action="#{simRenewalBean.saveBtnAction}"
									rendered="#{simRenewalBean.saveBtnCheck}">

								</p:commandButton>

								<!-- Update Button -->

								<p:commandButton id="updateBtn" styleClass="mainLinkButton"
									style="width:80%" value="Update" process="@all" update="@form"
									action="#{simRenewalBean.updateBtnAction}"
									rendered="#{simRenewalBean.updateBtnCheck}">

								</p:commandButton>

								<!-- Clear Button -->

								<p:commandButton id="clearBtn" styleClass="mainLinkButton"
									style="width:80%" value="Clear" process="@all" update="@form"
									action="#{simRenewalBean.clearBtnAction}">

								</p:commandButton>

								<p:panelGrid></p:panelGrid>
								<p:commandButton id="cancelVoucherBtnId"
									styleClass="otherLinkButton" style="width:80%"
									value="Cancel Voucher" process="@all"
									update="@form,frmCreateVoucher"
									action="#{simRenewalBean.cancelVoucherBtnAction}"
									disabled="#{simRenewalBean.disableCancelVoucherBtn}">

								</p:commandButton>


							</p:panelGrid>


						</h:form>


					</p:panelGrid>

				</p:panel>



			</div>
		</div>


		<!-- Create Voucher Dialog -->

		<p:dialog id="createPaymentVoucherID" global="true" modal="true"
			showEffect="clip" hideEffect="clip" header=" Create Payment Voucher "
			width="80%" height="550px" severity="info"
			widgetVar="createVoucherDialog" closeOnEscape="true"
			resizable="false" style="postion: absolute;">

			<h:form id="frmCreateVoucher">

				<p:panel id="panelOne">

					<!-- 1st data set -->

					<p:panelGrid columns="4" style="margin-bottom:1%"
						columnClasses="ui-grid-col-4,ui-grid-col-2,ui-grid-col-3,ui-grid-col-3"
						layout="grid">

						<!-- SIM Registration No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="SIM Registration No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%" readonly="true"
								value="#{simRenewalBean.simRegistrationDTO.simRegNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- SIM No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="SIM No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%;" readonly="true"
								value="#{simRenewalBean.simRegistrationDTO.simNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Permit No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Permit No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%" readonly="true"
								value="#{simRenewalBean.simRegistrationDTO.permitNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Bus No. -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Bus No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText style="width:90%" readonly="true"
								value="#{simRenewalBean.simRegistrationDTO.busNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

						<!-- Voucher Number -->

						<p:panelGrid></p:panelGrid>
						<p:panelGrid>
							<p:outputLabel value="Voucher No."></p:outputLabel>
						</p:panelGrid>
						<p:panelGrid>
							<p:inputText
								style="width:90%;font-weight: bold;color: red;text-transform: uppercase; "
								readonly="true"
								value="#{simRenewalBean.simRegistrationDTO.vouNo}" />
						</p:panelGrid>
						<p:panelGrid></p:panelGrid>

					</p:panelGrid>


					<!-- 2nd data set -->

					<p:panel
						style="border: #404d6f solid 1px !important;border-radius: 25px;
						box-shadow: 3px 3px 3px grey; margin-bottom: 1px;">

						<p:panelGrid columns="6"
							columnClasses="ui-grid-col-2,ui-grid-col-2,ui-grid-col-2,ui-grid-col-2,ui-grid-col-1,ui-grid-col-3"
							layout="grid">

							<!-- Charge Type -->

							<p:panelGrid>
								<center>
									<p:outputLabel value="Charge Type"></p:outputLabel>
									<p:outputLabel value=" *" style="color:red;"></p:outputLabel>
								</center>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu style="width:90%" tabindex="1" filter="true"
									filterMatchMode="contains"
									value="#{simRenewalBean.voucherDTO.vouChargeType}"
									disabled="#{simRenewalBean.disableChargeType}">

									<f:selectItem itemLabel="--Select Charge Type--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems value="#{createPaymentVoucher.chargeTypeList}" />
									<p:ajax listener="#{simRenewalBean.ajaxFilterAcountNo}"
										process="@all" update="@form"></p:ajax>
								</p:selectOneMenu>
							</p:panelGrid>

							<!-- Account No. -->

							<p:panelGrid>
								<center>
									<p:outputLabel value="Account No.">
										<p:outputLabel value=" *" style="color:red;"></p:outputLabel>
									</p:outputLabel>
								</center>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="accountNoId" style="width:90%"
									filter="true" filterMatchMode="contains" tabindex="1"
									value="#{simRenewalBean.voucherDTO.vouAccountNo}"
									disabled="#{simRenewalBean.disableAccountNo}">

									<f:selectItem itemLabel="--Select Account No.--" itemValue="" />
									<f:selectItems value="#{simRenewalBean.accountNoList}" />
								</p:selectOneMenu>
							</p:panelGrid>

							<!-- Amount -->

							<p:panelGrid>
								<center>
									<p:outputLabel value="Amount"></p:outputLabel>
									<p:outputLabel value=" *" style="color:red;"></p:outputLabel>

								</center>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputNumber placeholder="Enter Amount (Rs.)"
									value="#{simRenewalBean.voucherDTO.vouAmmount}" minValue="0"
									style="width:66%" inputStyle="text-align: right" maxlength="12"
									disabled="#{simRenewalBean.disableAmount}"></p:inputNumber>
							</p:panelGrid>

						</p:panelGrid>

						<br />
						<center>
							<!-- Add Button -->
							<p:commandButton value="Add" style="width:20%"
								update="dataTable,@form" process="@all"
								styleClass="otherLinkButton" id="vouAddBtn"
								actionListener="#{simRenewalBean.vouAddBtnAction}"
								rendered="#{simRenewalBean.renderVouAdd}" />

							<!-- Update Button -->
							<p:commandButton value="Update" style="width:20%"
								update="dataTable,@form" process="@all"
								styleClass="otherLinkButton" id="vouUpdateBtn"
								actionListener="#{simRenewalBean.vouUpdateBtnAction}"
								rendered="#{simRenewalBean.renderVouUpdate}" />

							<!-- Clear Button -->
							<p:commandButton value="Clear" style="width:20%"
								update="frmCreateVoucher" process="@all"
								styleClass="otherLinkButton" id="vouDetailsClearBtn"
								actionListener="#{simRenewalBean.vouDetailsClearBtnAction}"
								rendered="#{simRenewalBean.renderVouClear}" />

							<!-- Edit Clear Button -->
							<p:commandButton value="Clear Amount" style="width:20%"
								update="frmCreateVoucher" process="@all"
								styleClass="otherLinkButton" id="vouDetailsEditClearBtn"
								actionListener="#{simRenewalBean.vouEditClearBtnAction}"
								rendered="#{simRenewalBean.renderVouEditClear}" />
						</center>

					</p:panel>

				</p:panel>

				<p:panel id="panelTwo">

					<center>
						<p:dataTable id="dataTable" var="dto" sortBy="#{1}"
							resizableColumns="false" liveResize="true"
							tableStyle="width:100%" styleClass="myTable"
							emptyMessage="No records available." paginator="true" rows="5"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							value="#{simRenewalBean.voucherDTOList}">

							<!-- Charge Type -->

							<p:column headerText="Charge Type" style="text-align:left">
								<h:outputText style="float:left;" value="#{dto.vouChargeType}" />
							</p:column>

							<!-- Account No -->

							<p:column headerText="Account No." style="text-align:left">
								<h:outputText style="float:left;" value="#{dto.vouAccountNo}" />
							</p:column>

							<!-- Amount(Rs.) -->

							<p:column headerText="Amount (Rs.)"
								style="text-align:right;margin-right:10%">
								<h:outputText style="float:right;" value="#{dto.vouAmmount}">
									<f:convertNumber type="currency" currencySymbol="" />
								</h:outputText>
							</p:column>

							<!-- Edit -->

							<p:column width="9%">
								<center>
									<p:commandLink action="${simRenewalBean.vouEdit()}"
										disabled="${simRenewalBean.disableVouEdit}"
										update="@form,dataTable">

										<f:setPropertyActionListener value="#{dto}"
											target="${simRenewalBean.selectDTO}" />

										<p:graphicImage library="images" name="edit.png"
											style="width:24px;height:24px;" title="edit"></p:graphicImage>
									</p:commandLink>
								</center>

							</p:column>

							<!-- delete -->

							<p:column width="9%">
								<center>
									<p:commandLink action="${simRenewalBean.vouDelete()}"
										disabled="${simRenewalBean.disableVouDelete}"
										update="dataTable">

										<f:setPropertyActionListener value="#{dto}"
											target="${simRenewalBean.selectDTO}" />

										<p:graphicImage library="images" name="delete.png"
											style="width:24px;height:24px;" title="delete"></p:graphicImage>
									</p:commandLink>
								</center>

							</p:column>

							<p:columnGroup type="footer">
								<p:row>
									<p:column colspan="2" style="text-align:right"
										footerText="Total Fee (Rs.)" />

									<p:column style="text-align:right">
										<f:facet name="footer">
											<h:outputText style="float:right;"
												value="#{simRenewalBean.totalAmount}">
												<f:convertNumber type="currency" currencySymbol="" />
											</h:outputText>
										</f:facet>
									</p:column>

									<p:column></p:column>
									<p:column></p:column>

								</p:row>
							</p:columnGroup>

						</p:dataTable>
					</center>
				</p:panel>

				<!-- Main Button Panel -->

				<p:panel id="panelThree">
					<center>

						<!-- Generate Voucher -->

						<p:commandButton value="Generate Voucher"
							update="@form,simRegistrationForm" process="@all"
							styleClass="mainActionbtn" id="generateVoucherBtn"
							style="width:15%"
							actionListener="#{simRenewalBean.generateVoucher}"
							rendered="#{simRenewalBean.renderGenerateVoucher}">
						</p:commandButton>

						<!-- Update generated Voucher -->

						<p:commandButton value="Update Voucher"
							update="@form,simRegistrationForm" process="@all"
							styleClass="mainActionbtn" id="updateVoucherBtn"
							style="width:15%"
							actionListener="#{simRenewalBean.updateVoucher}"
							rendered="#{simRenewalBean.renderUpdateVoucher}">
						</p:commandButton>

						<!-- Print -->

						<p:commandButton value="Print" update="@all" process="@all"
							styleClass="mainActionbtn" id="printBtn" style="width:15%"
							actionListener="#{simRenewalBean.printVoucher}"
							oncomplete="window.open('/InfoNTC/showUpload?reportName=Voucher')"
							disabled="#{simRenewalBean.disablePrint}">
							<f:attribute name="value" value="#{simRenewalBean.valueForPrint}"></f:attribute>
						</p:commandButton>


						<!-- Re-Print -->

						<p:commandButton value="Re-Print"
							oncomplete="window.open('/InfoNTC/showUpload')"
							styleClass="mainActionbtn" id="rePrintBtn" style="width:15%"
							rendered="false">

						</p:commandButton>

						<!-- Clear -->

						<p:commandButton value="Clear" styleClass="mainActionbtn"
							style="width:15%" id="btnClear" update="@form"
							onclick="PF('voucherDialogForInves').hide()" />


					</center>

				</p:panel>

			</h:form>


		</p:dialog>



		<!-- End Create Voucher -->

	</ui:define>
</ui:composition>