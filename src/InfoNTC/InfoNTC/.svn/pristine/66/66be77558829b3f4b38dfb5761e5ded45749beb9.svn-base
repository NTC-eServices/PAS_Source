							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Charge Reference No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="invesNo" style="width:60%" widgetVar="invesNo"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.invesNo}"
