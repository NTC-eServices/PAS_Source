								<p:inputText id="owner" style="width:60%"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.permitowner}"
									readonly="true">
								</p:inputText>
							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Route No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="route" style="width:60%"
