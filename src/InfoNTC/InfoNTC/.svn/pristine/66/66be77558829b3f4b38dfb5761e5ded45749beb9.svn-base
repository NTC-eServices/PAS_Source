										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,15,30" resizableColumns="false"
										sortBy="#{fundtTable.code}" sortOrder="ascending"
										liveResize="false" rowIndexVar="index" editable="true"
										editMode="cell">
										<p:ajax event="cellEdit"
											listener="#{flyingSquadChargeSheetBackingBeann.onCellEdit}" />
										<p:column headerText="Code" style="text-align:left;width:10%">
											<h:outputText value="#{fundtTable.code}" />
										</p:column>
										<p:column headerText="Description"
											style="text-align:left;width:25%">
											<h:outputText style="text-align:left;"
												value="#{fundtTable.description}" />
										</p:column>

										<p:column headerText="Remark"
											style="text-align:left;width:25%">
											<p:inputText style="text-align:left; width:100%"
												value="#{fundtTable.remarks}" />
										</p:column>
										<p:column headerText="" style="text-align:left;width:5%">
											<p:selectBooleanCheckbox value="#{fundtTable.ststus}" />
										</p:column>
									</p:dataTable>
								</center>
							</p:panel>

							<p:panel>
								<p:outputLabel value="Retain Documents"
									style="font-weight:bold;"></p:outputLabel>

								<p:growl id="msgs" showDetail="true" />
								<center>
									<p:dataTable var="fundtTablen" paginator="true" rows="5"
										id="fundtTablen" style="width:96%"
										value="#{flyingSquadChargeSheetBackingBeann.documentList}"
										paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
										rowsPerPageTemplate="5,15,30" resizableColumns="false"
										sortBy="#{fundtTablen.code}" sortOrder="ascending"
										liveResize="false" rowIndexVar="index" editable="true"
										editMode="cell">
										<p:ajax event="cellEdit"
											listener="#{flyingSquadChargeSheetBackingBeann.onCellEdit}" />

										<p:column headerText="Description"
											style="text-align:left;width:20%">
											<h:outputText style="text-align:left;"
												value="#{fundtTablen.description}" />
										</p:column>

										<p:column headerText="Details"
											style="text-align:left;width:25%">
											<p:inputText style="text-align:left; width:100%"
												value="#{fundtTablen.remark}" />
										</p:column>
										<p:column headerText="" style="text-align:left;width:5%">
											<p:selectBooleanCheckbox value="#{fundtTablen.violated}" />
										</p:column>
									</p:dataTable>
								</center>
							</p:panel>
						</p:panel>
					</h:form>
					<br></br>
					<center>
						<p:commandButton id="cmdbtnclose" value="Close"
							class="mainActionbtn"
							actionListener="#{flyingSquadChargeSheetBackingBeann.close}"></p:commandButton>
					</center>
				</p:dialog>

				<p:dialog id="errorDialog" global="true" modal="true" header="Error"
					width="30%" severity="info" widgetVar="errorDialog"
					closeOnEscape="true" resizable="false">
					<h:form id="frmError">

						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
								<p:outputLabel
									value="#{manageInvestigationChargesBean.errorMsg}"></p:outputLabel>
							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmError" value="OK" onclick="PF('errorDialog').hide()">
							</p:commandButton>

						</center>
					</h:form>
				</p:dialog>

				<p:dialog id="successDialog" global="true" modal="true"
					header="Success" width="30%" severity="info"
					widgetVar="successDialog" closeOnEscape="true" resizable="false">
					<h:form id="frmSuccess">

						<center>
							<p:panelGrid columns="2" styleClass="ui-noborder">
								<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
								<p:outputLabel
									value="#{manageInvestigationChargesBean.successMsg}"></p:outputLabel>
							</p:panelGrid>
							<br></br>
							<p:commandButton style="margin-top:10px;margin-bottom:10px"
								process="frmSuccess" value="OK"
								onclick="PF('successDialog').hide()">
							</p:commandButton>
						</center>

					</h:form>
				</p:dialog>

				<p:dialog id="dlgAction" widgetVar="dlgAction" global="true"
					modal="true" header="Action" width="70%" resizable="false">
