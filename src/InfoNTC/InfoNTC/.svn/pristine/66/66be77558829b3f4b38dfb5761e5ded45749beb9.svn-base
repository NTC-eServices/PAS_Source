		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Manage Investigation / Charges</h1>

			<div style="width: 95%;" class="subDiv">

				<h:form id="manageInvsFrm">

					<p:panel id="pnl-search">

						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Start Investigation Date"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:calendar id="invstStartDate"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.startInvestigationDate}"
									style="height:18px;" maxdate="today" navigator="true"
									locale="en_US" showOn="button" pattern="dd/MM/yyyy"
									placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
									readonlyInput="true"></p:calendar>
							</p:panelGrid>
							<p:panelGrid>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="End Investigation Date"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:calendar id="invstEndDate"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.endInvestigationDate}"
									style="height:18px;" maxdate="today" navigator="true"
									locale="en_US" showOn="button" pattern="dd/MM/yyyy"
									placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
									readonlyInput="true"></p:calendar>
							</p:panelGrid>
							<p:panelGrid>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Charge Reference No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="chargeRefNums" style="width:82%;"
									effect="fold" filter="true" filterMatchMode="contains"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.chargeRefCode}">
									<f:selectItem itemLabel="--Select Charge Ref No--" itemValue="" />
									<f:selectItems
										value="#{manageInvestigationChargesBean.chargeRefList}"
										var="refNoList" itemLabel="#{refNoList.chargeRefCode}"
										itemValue="#{refNoList.chargeRefCode}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Report No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:91%;" id="reportNo" onkeyup="this.value=this.value.toUpperCase();"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.reportNo}">
									<p:keyFilter regEx="/[0-9-a-zA-Z-]/i" />
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Vehicle No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:91%;" id="vehicleNo" maxlength="12" onkeyup="this.value=this.value.toUpperCase();"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.vehicleNo}">
									<p:keyFilter regEx="/[0-9-a-zA-Z-]/i" />
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Permit No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:91%;" id="permitNo" onkeyup="this.value=this.value.toUpperCase();"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.permitNo}">
									<p:keyFilter regEx="/[0-9-a-zA-Z-]/i" />
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Permit Owner Name"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText style="width:91%;" id="permitOwnerName" onkeyup="this.value=this.value.toUpperCase();"
									value="#{manageInvestigationChargesBean.manageInvestigationDTO.permitOwnerName}">
									<p:keyFilter regEx="/[a-zA-Z- ]/i" />
								</p:inputText>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

						<p:panelGrid columns="4" style="margin-top:2%;"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">

							<p:panelGrid></p:panelGrid>

							<p:commandButton id="searchBtn" styleClass="mainLinkButton"
								style="width: 100%" value="Search" update="@form"
								action="#{manageInvestigationChargesBean.searchButtonAction}">

							</p:commandButton>

							<p:commandButton id="clearBtn" styleClass="mainLinkButton"
								style="width: 100%" value="Clear" update="@form"
								action="#{manageInvestigationChargesBean.clearButtonAction}">
							</p:commandButton>

							<p:panelGrid></p:panelGrid>

						</p:panelGrid>

					</p:panel>

					<p:panel id="pnl-search-results">

						<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
							layout="grid">
							<h1
								style="margin-top: 3%; margin-bottom: 2%; text-align: left; color: #000000;">Pending
								/ Completed Investigation</h1>

							<p:dataTable id="pendCompltInvtDT" var="invDto"
								value="#{manageInvestigationChargesBean.investigationsList}"
								selectionMode="single"
								selection="#{manageInvestigationChargesBean.selectedInvestigation}"
								rowKey="#{invDto.chargeRefCode}" style="margin-bottom:0"
								rows="5" paginator="true"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="5,10,15">

								<p:ajax event="rowSelect"
									listener="#{manageInvestigationChargesBean.onRowSelect}"
									update=":manageInvsFrm" />
								<p:ajax event="rowUnselect"
									listener="#{manageInvestigationChargesBean.onRowUnselect}"
									update=":manageInvsFrm" />

								<p:column headerText="Charge Reference No.">
									<h:outputText value="#{invDto.chargeRefCode}" />
								</p:column>
								<p:column headerText="Permit No." width="80">
									<h:outputText value="#{invDto.permitNo}" />
								</p:column>
								<p:column headerText="Vehicle No." width="80">
									<h:outputText value="#{invDto.vehicleNo}" />
								</p:column>
								<p:column headerText="Permit Owner Name" width="180">
									<h:outputText value="#{invDto.permitOwnerName}" />
								</p:column>
								<p:column headerText="Date of Investigation" width="80">
									<h:outputText value="#{invDto.dateOfInvestigationStr}" />
								</p:column>
								<p:column headerText="Status">
									<h:outputText value="#{invDto.currentStatusDesc}" />
								</p:column>
								<p:column width="30">

									<p:commandLink
										action="#{manageInvestigationChargesBean.viewBtnAction}"
										update="frmview">

										<f:setPropertyActionListener value="#{p}"
											target="#{manageInvestigationChargesBean.selectedInvestigation}" />

										<p:graphicImage library="images" name="view1.png"
											style="width:24px;height:24px;" title="View"></p:graphicImage>
									</p:commandLink>

								</p:column>
							</p:dataTable>
						</p:panelGrid>
					</p:panel>

					<p:panel id="pnlComplaintDetails"
						rendered="#{manageInvestigationChargesBean.itemSelected}"
						style="margin-top:15px" styleClass="light-disabled">

						<h1
							style="margin-top: 2%; margin-bottom: 2%; text-align: left; color: #000000;">Permit,
							Driver and Conductor Details</h1>

						<p:panelGrid layout="grid" columns="2"
							columnClasses="ui-grid-col-8, ui-grid-col-4">

							<p:panelGrid layout="grid" columns="3"
								columnClasses="ui-grid-col-1, ui-grid-col-3, ui-grid-col-8">
								<p:spacer />
								<h:outputLabel value="Permit No."></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.selectedInvestigation.permitNo}"
									disabled="true" style="width:50%"></p:inputText>

								<p:spacer />
								<h:outputLabel value="Vehicle No."></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.selectedInvestigation.vehicleNo}"
									disabled="true" style="width:50%"></p:inputText>

								<p:spacer />
								<h:outputLabel value="Owner"></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.selectedInvestigation.permitOwnerName}"
									disabled="true" style="width:100%"></p:inputText>
							</p:panelGrid>
							<p:panelGrid layout="grid" columns="1">
								<p:spacer />
								<p:spacer />

								<h:panelGroup>
									<p:commandLink styleClass="round-iconbtn"
										title="Complaint History" 
										action="#{manageInvestigationChargesBean.loadComplaintHistory('O')}"
										update="frmComplaintHistory">
										<span class="fa-stack fa-2x"> <i
											class="fa fa-circle fa-stack-2x" style="color: #e8c166;"></i>
											<i class="fa fa-history fa-stack-1x fa-inverse"
											style="color: #000"></i>
										</span>
									</p:commandLink>


									<p:commandLink styleClass="round-iconbtn" title="Actions"
										disabled="#{!manageInvestigationChargesBean.enableActionPanel}"
										action="#{manageInvestigationChargesBean.loadActionDialog('O')}"
										update="dlgAction">
										<span class="fa-stack fa-2x"> <i
											class="fa fa-circle fa-stack-2x" style="color: #ec9f83;"></i>
											<i class="fa fa-gavel fa-stack-1x fa-inverse"
											style="color: #000"></i>
										</span>
									</p:commandLink>
								</h:panelGroup>
							</p:panelGrid>

							<p:spacer height="20" />
							<p:spacer height="8" />

							<p:panelGrid id="panelDriver" layout="grid" columns="3"
								columnClasses="ui-grid-col-1, ui-grid-col-3, ui-grid-col-8">
								<p:spacer
									rendered="#{!manageInvestigationChargesBean.newDriver}" />
								<h:outputLabel value="Driver ID"
									rendered="#{!manageInvestigationChargesBean.newDriver}"></h:outputLabel>
								<p:selectOneMenu
									value="#{manageInvestigationChargesBean.driverData.driverConductorId}"
									rendered="#{!manageInvestigationChargesBean.newDriver}"
									style="width:50%">
									<p:ajax
										listener="#{manageInvestigationChargesBean.loadDriverConductorData('D')}"
										update="panelDriver, actionPanelDriver, pointPnlDriver" />
									<f:selectItem itemLabel="-- Select Driver Id --" itemValue=""></f:selectItem>
									<f:selectItems
										value="#{manageInvestigationChargesBean.driverList}" var="v"
										itemLabel="#{v.code}" itemValue="#{v.code}"></f:selectItems>
								</p:selectOneMenu>

								<p:spacer rendered="#{manageInvestigationChargesBean.newDriver}" />
								<h:outputLabel value="Driver NIC No."
									rendered="#{manageInvestigationChargesBean.newDriver}"></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.driverData.nic}"
									rendered="#{manageInvestigationChargesBean.newDriver}"
									style="width:50%" maxlength="12"
									onkeyup="this.value=this.value.toUpperCase();">
									<p:ajax update="actionPanelDriver, pointPnlDriver" />
								</p:inputText>

								<p:spacer />
								<h:outputLabel value="Name"></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.driverData.fullNameEng}"
									disabled="#{!manageInvestigationChargesBean.newDriver}"
									style="width:100%"
									onkeyup="this.value=this.value.toUpperCase();"></p:inputText>

								<p:spacer />
								<h:outputLabel value="Address"></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.driverData.add1Eng}"
									disabled="#{!manageInvestigationChargesBean.newDriver}"
									style="width:100%"
									onkeyup="this.value=this.value.toUpperCase();"></p:inputText>

								<p:spacer />
								<h:outputLabel value=""></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.driverData.add2Eng}"
									disabled="#{!manageInvestigationChargesBean.newDriver}"
									style="width:100%"
									onkeyup="this.value=this.value.toUpperCase();"></p:inputText>

								<p:spacer />
								<h:outputLabel value=""></h:outputLabel>
								<p:inputText
									value="#{manageInvestigationChargesBean.driverData.cityEng}"
									disabled="#{!manageInvestigationChargesBean.newDriver}"
									style="width:100%"
									onkeyup="this.value=this.value.toUpperCase();"></p:inputText>
							</p:panelGrid>
							<p:panelGrid layout="grid" columns="1">
								<p:outputPanel style="margin-top:5px">
									<p:selectBooleanCheckbox
										value="#{manageInvestigationChargesBean.newDriver}"
										style="margin-right:10px">
										<p:ajax
											listener="#{manageInvestigationChargesBean.clearDriverData}"
											update="panelDriver, actionPanelDriver, pointPnlDriver"></p:ajax>
									</p:selectBooleanCheckbox>
									<p:outputLabel value="Is new Driver"></p:outputLabel>
								</p:outputPanel>
								<p:spacer />
								<p:outputPanel id="pointPnlDriver" style="color:#484442;"
									rendered="#{manageInvestigationChargesBean.driverData.driverConductorId != null or manageInvestigationChargesBean.driverData.nic != null}">

								</p:outputPanel>
								<p:spacer />
								<p:spacer />
								<h:panelGroup id="actionPanelDriver">
									<!-- <p:commandLink styleClass="round-iconbtn"
										title="Complaint History"
										disabled="#{manageInvestigationChargesBean.newDriver or manageInvestigationChargesBean.driverData.driverConductorId == null or manageInvestigationChargesBean.disableBtn}"
										action="#{manageInvestigationChargesBean.loadComplaintHistory('D')}"
										update="frmComplaintHistory">
										<span class="fa-stack fa-2x"> <i
											class="fa fa-circle fa-stack-2x" style="color: #e8c166;"></i>
											<i class="fa fa-history fa-stack-1x fa-inverse"
											style="color: #000"></i>
										</span>
									</p:commandLink> -->
									
									<p:commandLink styleClass="round-iconbtn"
										title="Complaint History"
										disabled="#{manageInvestigationChargesBean.disableBtn}"
										action="#{manageInvestigationChargesBean.loadComplaintHistory('D')}"
										update="frmComplaintHistory">
										<span class="fa-stack fa-2x"> <i
											class="fa fa-circle fa-stack-2x" style="color: #e8c166;"></i>
											<i class="fa fa-history fa-stack-1x fa-inverse"
											style="color: #000"></i>
										</span>
									</p:commandLink>
									<p:commandLink styleClass="round-iconbtn" title="Actions"
										disabled="#{!manageInvestigationChargesBean.enableActionPanel}"
										action="#{manageInvestigationChargesBean.loadActionDialog('D')}"
										update="dlgAction">
										<span class="fa-stack fa-2x"> <i
											class="fa fa-circle fa-stack-2x" style="color: #ec9f83;"></i>
											<i class="fa fa-gavel fa-stack-1x fa-inverse"
											style="color: #000"></i>
										</span>
									</p:commandLink>
								</h:panelGroup>
							</p:panelGrid>
							<p:spacer height="20" />
							<p:spacer height="8" />

							<p:panelGrid id="panelConductor" layout="grid" columns="3"
								columnClasses="ui-grid-col-1, ui-grid-col-3, ui-grid-col-8">
								<p:spacer
									rendered="#{!manageInvestigationChargesBean.newConductor}" />
								<h:outputLabel value="Conductor ID"
									rendered="#{!manageInvestigationChargesBean.newConductor}"></h:outputLabel>
								<p:selectOneMenu
									value="#{manageInvestigationChargesBean.conductorData.driverConductorId}"
									rendered="#{!manageInvestigationChargesBean.newConductor}"
									style="width:50%">
