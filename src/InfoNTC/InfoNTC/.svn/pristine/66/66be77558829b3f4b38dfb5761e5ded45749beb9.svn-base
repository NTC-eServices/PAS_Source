						<p:panelGrid layout="grid" columns="4"
							columnClasses="ui-grid-col-1, ui-grid-col-4, ui-grid-col-6, ui-grid-col-1">
							<p:spacer />
							<p:outputLabel value="Department Type"></p:outputLabel>
							<p:selectOneMenu
								value="#{manageInvestigationChargesBean.selectedInvestigation.departmentType}"
								style="width:calc(100% - 23px);">
								<f:selectItem itemLabel="-- Select Department --" itemValue=""></f:selectItem>
								<f:selectItems
									value="#{manageInvestigationChargesBean.deptList}" var="v"
									itemLabel="#{v.deptDesc}" itemValue="#{v.deptCode}"></f:selectItems>
							</p:selectOneMenu>
							<p:spacer />

							<p:spacer />
							<p:outputLabel value="Action Type"></p:outputLabel>
							<p:selectOneMenu
								value="#{manageInvestigationChargesBean.selectedInvestigation.actionCode}"
								style="width:calc(100% - 23px);">
								<f:selectItem itemLabel="-- Select Action Type --" itemValue=""></f:selectItem>
								<f:selectItems
									value="#{manageInvestigationChargesBean.actionList}" var="v"
									itemLabel="#{v.description}" itemValue="#{v.code}"></f:selectItems>
							</p:selectOneMenu>
							<p:spacer />

							<p:spacer />
							<p:outputLabel value="Action Description"></p:outputLabel>
							<p:inputTextarea
								value="#{manageInvestigationChargesBean.selectedInvestigation.actionDesc}"
								style="width:100%"
								onkeyup="this.value=this.value.toUpperCase();" rows="6"
								cols="60"></p:inputTextarea>
							<p:spacer />
						</p:panelGrid>
						<center>
							<br></br>
							<p:commandButton value="Save" title="Save"
								styleClass="mainActionbtn" tabindex="1"
								action="#{manageInvestigationChargesBean.actionBtnAction}"></p:commandButton>
							<p:commandButton value="Reset" title="Reset"
								styleClass="mainActionbtn" tabindex="1" update="frmAction"
								action="#{manageInvestigationChargesBean.resetAction}"></p:commandButton>
							<br></br> <br></br>
						</center>
					</h:form>
				</p:dialog>

				<p:dialog id="dlgComplaintHistory" global="true" modal="true"
					header="Complaint History" closable="true" width="60%"
					height="80vh" severity="info" widgetVar="dlgComplaintHistory"
					closeOnEscape="true" resizable="false">


					<h:form id="frmComplaintHistory">
						<p:panelGrid layout="grid" columns="4"
							columnClasses="ui-grid-col-1, ui-grid-col-4, ui-grid-col-6, ui-grid-col-1"
							styleClass="sm-padding">
							<p:spacer />
							<p:outputLabel
								value="#{manageInvestigationChargesBean.viewType == 'D' ?'Driver' :(manageInvestigationChargesBean.viewType == 'C' ?'Conductor' :'Owner')} Name"
								styleClass="bold"></p:outputLabel>
							<p:outputLabel
								value="#{manageInvestigationChargesBean.viewType == 'O' 
								?manageInvestigationChargesBean.selectedInvestigation.permitOwnerName :(manageInvestigationChargesBean.viewType == 'C' 
																				?manageInvestigationChargesBean.conductorData.fullNameEng :manageInvestigationChargesBean.driverData.fullNameEng)} "></p:outputLabel>
							<p:spacer />

							<p:spacer />
							<p:outputLabel value="ID No" styleClass="bold"></p:outputLabel>
							<p:outputLabel
								value="#{manageInvestigationChargesBean.viewType == 'O' 
								?manageInvestigationChargesBean.selectedInvestigation.permitOwnerNic :(manageInvestigationChargesBean.viewType == 'C' 
																				?manageInvestigationChargesBean.conductorData.driverConductorId :manageInvestigationChargesBean.driverData.driverConductorId)} "></p:outputLabel>
							<p:spacer />
						</p:panelGrid>

						<p:spacer height="5px" />

						<p:dataTable var="offence"
							value="#{manageInvestigationChargesBean.complaintHistoryList}"
							emptyMessage="No complaints." paginator="true" rows="5"
							rowsPerPageTemplate="5,10,15" paginatorPosition="bottom"
							paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
							reflow="true">
							<p:column headerText="Report No." width="20%"
								style="text-align:left">
								<h:outputText value="#{offence.complaintNo}" />
							</p:column>
							<p:column headerText="Date of Incident" width="20%"
								style="text-align:left">
								<h:outputText value="#{offence.eventDateTime}" />
							</p:column>
							<p:column headerText="Action" style="text-align:right">
								<h:outputText
									value="#{offence.actionCode} - #{offence.actionDesc}" />
							</p:column>
							<p:column headerText="Amount" width="15%"
								style="text-align:right">
								<h:outputText value="#{offence.totalCharge}" />
							</p:column>
							<p:column width="15" style="text-align:center">
								<p:commandLink
									action="#{manageInvestigationChargesBean.selectComplaintHistory}"
									title="View Complaint Details"
									update="frmComplaintHistory:historyData">
									<i class="fa fa-eye fa-lg" />
									<f:setPropertyActionListener value="#{offence}"
										target="#{manageInvestigationChargesBean.selectedHistory}" />
								</p:commandLink>
							</p:column>
						</p:dataTable>

						<p:panel id="historyData">
							<p:panel
								rendered="#{manageInvestigationChargesBean.historySelected}">
								<p:spacer height="15px" />
								<h3>Violations of Conditions</h3>
								<br></br>

								<p:dataTable var="com"
									value="#{manageInvestigationChargesBean.violationConditionList}"
									emptyMessage="No committed offences." paginator="true" rows="5"
									rowsPerPageTemplate="5,10,15" paginatorPosition="bottom"
									paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
									reflow="true">
									<p:column headerText="Code" width="20%">
										<h:outputText value="#{com.code}" />
									</p:column>
									<p:column headerText="Description">
										<h:outputText value="#{com.description}" />
									</p:column>
									<p:column headerText="Remark">
										<h:outputText value="#{com.remark}" />
									</p:column>
								</p:dataTable>

								<p:panelGrid layout="grid" columns="4"
									columnClasses="ui-grid-col-1, ui-grid-col-4, ui-grid-col-6, ui-grid-col-1"
									style="margin-top:5px">
									<p:spacer />
									<p:outputLabel value="Remarks" styleClass="bold"></p:outputLabel>
									<p:outputLabel
										value="#{manageInvestigationChargesBean.selectedHistory.otherOffence}"></p:outputLabel>
									<p:spacer />
								</p:panelGrid>
							</p:panel>
						</p:panel>

						<p:spacer height="5px" />
						<center>
							<p:commandButton value="OK" title="Reset"
								styleClass="mainActionbtn" tabindex="1"
								oncomplete="PF('dlgComplaintHistory').hide();"></p:commandButton>
						</center>
						<p:spacer height="10" />
					</h:form>
				</p:dialog>
			</div>
		</div>
	</ui:define>
</ui:composition>