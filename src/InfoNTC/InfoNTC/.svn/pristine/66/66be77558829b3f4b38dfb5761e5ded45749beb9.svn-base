								</p:inputText>
							</p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="To"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="to" style="width:60%"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.routeTo}"
									readonly="true">
								</p:inputText>
							</p:panelGrid>

							<p:panelGrid>
								<p:outputLabel value="Driver ID"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="driverID" style="width:55%"
									widgetVar="driverID"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.driverID}"
									effect="fold" filter="true" filterMatchMode="contains">
									<f:selectItem itemLabel="-- Select Driver ID --" itemValue=""
										noSelectionOption="true" />
									<f:selectItems
										value="#{flyingSquadChargeSheetBackingBeann.driverList}"
										var="driverList" itemValue="#{driverList.driverID}"
										itemLabel="#{driverList.driverID}" />

									<p:ajax
										listener="#{flyingSquadChargeSheetBackingBeann.getName()}"
										update="@form" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Name"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="DriverName" style="width:60%"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.driverName}">
								</p:inputText>
							</p:panelGrid>

							<p:panelGrid>
								<p:outputLabel value="Conductor ID"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="conductorID" style="width:55%"
									widgetVar="conductorID"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.conductorID}"
									effect="fold" filter="true" filterMatchMode="contains">
									<f:selectItem itemLabel="-- Select Conductor ID --"
										itemValue="" noSelectionOption="true" />
									<f:selectItems
										value="#{flyingSquadChargeSheetBackingBeann.conductorList}"
										var="conductorList" itemValue="#{conductorList.conductorID}"
										itemLabel="#{conductorList.conductorID}" />

									<p:ajax
										listener="#{flyingSquadChargeSheetBackingBeann.getConductor()}"
										update="@form,conductorName" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Name"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:inputText id="conductorName" style="width:60%"
									value="#{flyingSquadChargeSheetBackingBeann.flyingManageInvestigationLogDTO.conductName}">
								</p:inputText>
							</p:panelGrid>

						</p:panelGrid>

						<p:panel>

							<p:panel>
								<p:outputLabel value="Violation of Conditions"
