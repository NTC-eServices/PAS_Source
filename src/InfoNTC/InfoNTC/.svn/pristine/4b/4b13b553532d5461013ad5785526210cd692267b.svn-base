	<queryString>
		<![CDATA[select rec_receipt_no,rec_payment_mode,rec_total_fee ,rec_voucher_no from  public.nt_t_receipt
where rec_receipt_no=case when  $P{P_Receipt}   is null then rec_receipt_no else  $P{P_Receipt}  end
and to_char(rep_created_date,'yyyy-MM-dd')>=case when   $P{P_Start_date}  is null then to_char(rep_created_date,'yyyy-MM-dd') else  $P{P_Start_date}   end
and to_char(rep_created_date,'yyyy-MM-dd')<=case when  $P{P_end_date}     is null then to_char(rep_created_date,'yyyy-MM-dd') else    $P{P_end_date}   end
order by rec_receipt_no]]>
	</queryString>
