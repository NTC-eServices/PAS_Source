<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">
		<style>
.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

#frmviewRouteMaintenance:j_idt81 {
	background-color: blue !important;
	font-weight: bold;
}
</style>

		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Edit/View Normal Inspection</h1>

			<div style="width: 95%;" class="subDiv">
				<h:form id="frmAddVIAPM">

					<div style="width: 100%;">

						<p:panelGrid columns="4" style="margin-top: 1%;" id="gridOne"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
							layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Application No." for="applicationNoId"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="applicationNoId" style="width:70%"
									value="#{viewNormalVehicleInspectionBean.selectedApplicationNo}"
									filter="true" filterMatchMode="contains">
									<p:ajax
										listener="#{viewNormalVehicleInspectionBean.onApplicationNoChange}"
										update="vehicleNoId,frmInspectionPrintTable" />
									<f:selectItem itemLabel="-- Select Application No. --"
										itemValue="" noSelectionOption="true" />
									<f:selectItems
										value="#{viewNormalVehicleInspectionBean.applicationNoList}"
										var="applicationNoList"
										itemValue="#{applicationNoList.applicationNo}"
										itemLabel="#{applicationNoList.applicationNo}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Vehicle No." for="vehicleNoId"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="vehicleNoId" style="width:70%"
									value="#{viewNormalVehicleInspectionBean.selectedVehicleNo}"
									filter="true" filterMatchMode="contains">
									<p:ajax
										listener="#{viewNormalVehicleInspectionBean.onVehicleNoChange}"
										update="applicationNoId,frmInspectionPrintTable" />
									<f:selectItem itemLabel="-- Select Vehicle No. --" itemValue=""
										noSelectionOption="true" />
									<f:selectItems
										value="#{viewNormalVehicleInspectionBean.vehcileNoList}"
										var="vehcileNoList" itemValue="#{vehcileNoList.vehicleNo}"
										itemLabel="#{vehcileNoList.vehicleNo}" />
								</p:selectOneMenu>
							</p:panelGrid>

						</p:panelGrid>

					</div>
					<center>
						<p:panel id="panelOne">
							<p:commandButton id="cmdbtnSearch" value="Search"
								class="mainActionbtn" ajax="false"
								action="#{viewNormalVehicleInspectionBean.searchDetails}"
								update="dtFunActTable">
							</p:commandButton>
							<p:commandButton id="cmdbtnClearPage" value="Clear"
								class="mainActionbtn"
								update="frmInspectionPrintTable,frmAddVIAPM"
								action="#{viewNormalVehicleInspectionBean.clearFields}">
							</p:commandButton>

						</p:panel>
					</center>

					<br></br>
				</h:form>
				<h:form id="frmInspectionPrintTable">
					<br></br>
					<p:growl id="msgs" showDetail="true" />
					<p:dataTable var="funActTable" paginator="true" rows="10"
						id="dtFunActTable" style="width:100%"
						value="#{viewNormalVehicleInspectionBean.dataList}"
						paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
						rowsPerPageTemplate="10,20" resizableColumns="false"
						liveResize="false" rowIndexVar="index">
						<p:column headerText="Application No."
							style="text-align:left;width:10%">
							<h:outputText style="text-align:left;"
								value="#{funActTable.ownerApplicationNo}" />
						</p:column>
						<p:column headerText="Vehicle No."
							style="text-align:left;width:10%">
							<h:outputText style="text-align:left;"
								value="#{funActTable.ownerVehicleNo}" />
						</p:column>
						<p:column headerText="Permit No."
							style="text-align:left;width:10%">
							<h:outputText style="text-align:left;"
								value="#{funActTable.ownerPermitNo}" />
						</p:column>
						<p:column headerText="Owner" style="text-align:left;width:10%">
							<h:outputText style="text-align:left;"
								value="#{funActTable.owner}" />
						</p:column>

						<p:column headerText="" style="width:5%">

							<p:commandLink id="cmdbtnPrint"
								update="dtFunActTable,frmAddVIAPM" process="dtFunActTable"
								action="#{viewNormalVehicleInspectionBean.viewActButtonAction}">
								<f:setPropertyActionListener value="#{funActTable}"
									target="#{viewNormalVehicleInspectionBean.selectedViewRow}" />
								<p:graphicImage library="images" name="view1.png"
									style="width:24px;height:24px;" title="view"></p:graphicImage>
							</p:commandLink>
						</p:column>
						<p:column headerText="" style="width:5%">
							<p:commandLink id="cmdbtnEdit"
								disabled="#{viewNormalVehicleInspectionBean.disableEdit}"
								action="#{viewNormalVehicleInspectionBean.editAction}"
								update="dtFunActTable,frmAddVIAPM">
								<f:setPropertyActionListener value="#{funActTable}"
									target="#{viewNormalVehicleInspectionBean.selectedEditRow}" />
								<p:graphicImage library="images" name="edit.png"
									style="width:24px;height:24px;" title="edit"></p:graphicImage>
							</p:commandLink>
						</p:column>

					</p:dataTable>

					<br></br>
				</h:form>

			</div>
		</div>


		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel
							value="#{printInspectionReportBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField"
						value="#{msg['um.employeeprofile.dlmissingclose']}"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>
		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="#{msg['um.employeeprofile.dlsuccess']}" width="30%"
			severity="info" widgetVar="successSve" closeOnEscape="true"
			resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel value="#{paymentTypeMaintenanceBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmsuccessSve"
						value="#{msg['um.employeeprofile.dlsuccessclose']}"
						onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>




		<p:dialog styleClass="" id="deleteconfirmationsequence" global="true"
			modal="true" width="30%" severity="info"
			widgetVar="deleteconfirmationsequence" closeOnEscape="true"
			resizable="false" header="Information">
			<h:form id="frmdeleteconfirmationDept">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>
						<p:outputLabel value="Do you want to delete this record ?"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton id="yes" value="Yes"
						actionListener="#{paymentTypeMaintenanceBean.deleteRole()}"
						ajax="true" onclick="PF('deleteconfirmationsequence').hide()"></p:commandButton>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmdeleteconfirmationDept" value="No"
						onclick="PF('deleteconfirmationsequence').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>
	</ui:define>
</ui:composition>