package lk.informatics.ntc.model.dto;

import java.io.Serializable;

public class AssignDivSecDTO implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -4910385562156919864L;
	
	private String districtCode;
	private String districtDes;
	private String divSecCode;
	private String divSec_description_english;
	private String divSec_description_sinhala;
	private String divSec_description_tamil;
	private boolean editRecord;
	
	public String getDistrictCode() {
		return districtCode;
	}
	public void setDistrictCode(String districtCode) {
		this.districtCode = districtCode;
	}
	public String getDistrictDes() {
		return districtDes;
	}
	public void setDistrictDes(String districtDes) {
		this.districtDes = districtDes;
	}
	public String getDivSecCode() {
		return divSecCode;
	}
	public void setDivSecCode(String divSecCode) {
		this.divSecCode = divSecCode;
	}
	public String getDivSec_description_english() {
		return divSec_description_english;
	}
	public void setDivSec_description_english(String divSec_description_english) {
		this.divSec_description_english = divSec_description_english;
	}
	public String getDivSec_description_sinhala() {
		return divSec_description_sinhala;
	}
	public void setDivSec_description_sinhala(String divSec_description_sinhala) {
		this.divSec_description_sinhala = divSec_description_sinhala;
	}
	public String getDivSec_description_tamil() {
		return divSec_description_tamil;
	}
	public void setDivSec_description_tamil(String divSec_description_tamil) {
		this.divSec_description_tamil = divSec_description_tamil;
	}
	public boolean isEditRecord() {
		return editRecord;
	}
	public void setEditRecord(boolean editRecord) {
		this.editRecord = editRecord;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
}
