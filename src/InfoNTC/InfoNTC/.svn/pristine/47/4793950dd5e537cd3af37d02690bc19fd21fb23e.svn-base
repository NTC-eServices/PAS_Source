<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>

	<ui:define name="content">

		<style>
.dateTimeField input {
	width: 100px;
}

.dateTimeField img {
	vertical-align: right;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.ui-input {
	margin-top: -20px;
}
</style>



		<h:form id="form">
			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Activate Cancelled Permits</h1>
				<div style="width: 90%;" class="subDiv">

					<p:panel id="panelOne">

						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-3,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
							layout="grid">

							<p:panelGrid>
								<p:outputLabel value="Permit No."></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="permitNo" style="width:68%"
									value="#{activateCancelledPermitsBackingBean.selectedPermitNo}"
									filter="true" filterMatchMode="contains" tabindex="1"
									placeholder="--Select Permit No.--">
									<p:ajax
										listener="#{activateCancelledPermitsBackingBean.onPermitNoChange}"
										update="cancelledDate,registration" />
									<f:selectItem itemLabel="-- Select Permit No. --" itemValue="" />
									<f:selectItems
										value="#{activateCancelledPermitsBackingBean.permitNoList}"
										var="permitNoList" itemValue="#{permitNoList.permitNo}"
										itemLabel="#{permitNoList.permitNo}" />
								</p:selectOneMenu>

							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Cancelled Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="cancelledDate" style="width:68%"
									value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.cancelledDateObj}"
									showOn="button" yearRange="c-100:c+100" navigator="true"
									readonlyInput="true" pattern="dd/MM/yyyy"
									placeholder="DD/MM/YYYY" locale="en_US">
									<p:ajax event="dateSelect"
										listener="#{activateCancelledPermitsBackingBean.onCanceledDateChange}"
										update="permitNo,registration" />
								</p:calendar>

							</p:panelGrid>


							<p:panelGrid>
								<p:outputLabel value="Registration No. of the Bus"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:selectOneMenu id="registration" style="width:68%"
									value="#{activateCancelledPermitsBackingBean.selectedVehicleNo}"
									filter="true" filterMatchMode="contains" tabindex="1"
									placeholder="--Select Registration No.--">
									<p:ajax
										listener="#{activateCancelledPermitsBackingBean.onVehicleNoChange}"
										update="cancelledDate,permitNo" />
									<f:selectItem itemLabel="-- Select Registration No. --"
										itemValue="" />
									<f:selectItems
										value="#{activateCancelledPermitsBackingBean.vehicleNoList}"
										var="vehicleNoList" itemValue="#{vehicleNoList.regNoOfTheBus}"
										itemLabel="#{vehicleNoList.regNoOfTheBus}" />
								</p:selectOneMenu>
							</p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid></p:panelGrid>
							<br />

						</p:panelGrid>
						<center>
							<p:commandButton value="Search" styleClass="mainActionbtn"
								id="cmdBtnSearch"
								actionListener="#{activateCancelledPermitsBackingBean.searchAction}"
								update="panelOne,panelTwo,btnPanel" />
							<p:commandButton value="Clear" styleClass="mainActionbtn"
								actionListener="#{activateCancelledPermitsBackingBean.clearFields}"
								update="panelOne,panelTwo,specialRemarkId,btnPanel" />
						</center>
					</p:panel>

					<p:panel id="panelTwo">

						<center>


							<p:dataTable id="cancelledPermitsTable"
								var="cancelledPermitsTable" rows="5" paginator="true"
								value="#{activateCancelledPermitsBackingBean.dataList}"
								selection="#{activateCancelledPermitsBackingBean.selectDTO}"
								selectionMode="single"
								rowKey="#{cancelledPermitsTable.applicationNo}"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:auto" rowIndexVar="index"
								styleClass="myTable" emptyMessage="No records are available.">

								<p:ajax event="rowSelect"
									listener="#{activateCancelledPermitsBackingBean.selectRow}"
									update="@form" process="@form" />

								<p:column headerText="" visible="false">
									<h:outputText value="" />
								</p:column>
								
								<p:column headerText="Application No."
									style="text-align:left;width:8%">
									<h:outputText value="#{cancelledPermitsTable.applicationNo}" />
								</p:column>

								<p:column headerText="Permit No."
									style="text-align:left;width:8%">
									<h:outputText value="#{cancelledPermitsTable.permitNo}" />
								</p:column>



								<p:column headerText="Vehicle No."
									style="text-align:left;width:8%">
									<h:outputText value="#{cancelledPermitsTable.regNoOfTheBus}" />
								</p:column>


								<p:column headerText="Cancelled From"
									style="text-align:left;width:8%">
									<h:outputText value="#{cancelledPermitsTable.cancelFromDate}" />
								</p:column>
								<p:column headerText="Cancelled To"
									style="text-align:left;width:8%">
									<h:outputText value="#{cancelledPermitsTable.cancelToDate}" />
								</p:column>

								<p:column headerText="Cancelled Reason"
									style="text-align:left;width:8%">
									<h:outputText value="#{cancelledPermitsTable.cancelledReason}" />
								</p:column>

								<p:column headerText="Permit Owner"
									style="text-align:left;width:10%">
									<h:outputText value="#{cancelledPermitsTable.permitOwner}" />
								</p:column>


								<p:column headerText="" style="text-align:left;width:5%">
									<p:commandLink id="cmdbtnView"
										update="cancelledPermitsTable,frmRenewalPermitDetails"
										process="cancelledPermitsTable,frmRenewalPermitDetails"
										action="#{activateCancelledPermitsBackingBean.ViewActButtonAction}">
										<f:setPropertyActionListener value="#{cancelledPermitsTable}"
											target="#{activateCancelledPermitsBackingBean.selectedVieweRow}" />
										<p:graphicImage library="images" name="view.png"
											style="width:24px;height:24px;" title="View"></p:graphicImage>
									</p:commandLink>
								</p:column>


							</p:dataTable>
						</center>
					</p:panel>


					<p:panel id="specialRemarkId">

						<p:outputLabel style="margin-left:10%" value="Special Remarks"></p:outputLabel>
						<p:inputText style="width:50%; margin-left:5%"
							value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.specialRemarks}"
							id="reject">
						</p:inputText>

					</p:panel>




					<p:panel id="btnPanel">
						<center>

							<p:commandButton value="Active" styleClass="mainActionbtn"
								id="btnActive" update="btnPrint,btnUploadDocument"
								disabled="#{activateCancelledPermitsBackingBean.disabledActiveBtn}"
								actionListener="#{activateCancelledPermitsBackingBean.activeRecords}" />

							<p:commandButton value="Print Activation Letter"
								styleClass="mainActionbtn" style="width:20%" update="@form"
								id="btnPrint"
								disabled="#{activateCancelledPermitsBackingBean.disablePrintActivationLetterBtn}"
								actionListener="#{activateCancelledPermitsBackingBean.printActivationLetterRecords}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=ActivationLetter')"/>

							<p:commandButton value="Upload Documents" style="width:17%"
								rendered="false" styleClass="mainActionbtn"
								id="btnUploadDocument"
								disabled="#{activateCancelledPermitsBackingBean.disableUploadDocumentsBtn}"
								update="uploadDocumentsDialog"
								actionListener="#{activateCancelledPermitsBackingBean.uploadDocuments}" />

							<p:commandButton value="Print Permit" style="width:17%"
								styleClass="mainActionbtn" id="btnPermitPrint"
								disabled="#{activateCancelledPermitsBackingBean.disablePrintPermit}"
								update="@form"
								actionListener="#{activateCancelledPermitsBackingBean.printPermit}"
								oncomplete="window.open('/InfoNTC/showUpload?reportName=permit')"/>



						</center>
					</p:panel>
				</div>
			</div>

		</h:form>

		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="Error" width="30%" severity="info" widgetVar="requiredField"
			closeOnEscape="true" resizable="false">

			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>

						<p:outputLabel
							value="#{activateCancelledPermitsBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmrequiredField"
						onclick="PF('requiredField').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="Success" width="30%" severity="info" widgetVar="successSve"
			closeOnEscape="true" resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel
							value="#{activateCancelledPermitsBackingBean.sucessMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton tabindex="1"
						style="margin-top:10px;margin-bottom:10px" process="frmsuccessSve"
						value="OK" onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>


		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Alert" width="30%" severity="info" widgetVar="alertMessage"
			closeOnEscape="true" resizable="false">

			<h:form id="frmAlert">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>

						<p:outputLabel
							value="#{activateCancelledPermitsBackingBean.alertMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmAlert" onclick="PF('alertMSG').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>


		<p:dialog styleClass="" id="renewalPermitDetails" global="true"
			height="500px" modal="true" header="Renewal Permit Details"
			width="80%" severity="info" widgetVar="renewalPermitDetailsDB"
			closeOnEscape="true" resizable="false">

			<h:form id="frmRenewalPermitDetails">


				<div>
					<div style="width: 100%;" class="subDiv">

						<p:panel id="panelOne">

							<p:panelGrid columns="4" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-4,ui-grid-col-3,ui-grid-col-4"
								layout="grid">

								<p:panelGrid>
									<p:outputLabel value="Service Type"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="serviceType" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.serviceTypeDes}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Place of Origin of the Services"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="placeOfOrigin" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.routeOrigine}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Route No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="routeNo" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.routeNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Place of Destination"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="destination" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.routeServiceDestination}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Service Expiry Date"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="expiryDate" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.permitExpiryDate}"></p:inputText>
								</p:panelGrid>


								<p:panelGrid>
									<p:outputLabel value="Reminder Letter Status"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:inputText id="letterStatus" readonly="true" disabled="true"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Cancelled Date"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="cancelledDate" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.cancelledDate}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Expired Reason"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="expiredReason" style="height:40px;"
										readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.expiredReason}"></p:inputText>
								</p:panelGrid>
							</p:panelGrid>

							<p:panelGrid columns="2" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-11" layout="grid">


								<p:panelGrid>
									<p:outputLabel value="Cancelled Reason"></p:outputLabel>
								</p:panelGrid>


								<p:panelGrid>
									<p:inputText id="cancelledReason" readonly="true"
										style="height:40px; width:94%"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.cancelledReason}"></p:inputText>
								</p:panelGrid>
							</p:panelGrid>


							<p:panelGrid columns="6" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-1,ui-grid-col-2,ui-grid-col-3,ui-grid-col-2,ui-grid-col-3"
								layout="grid">


								<p:panelGrid>
									<p:outputLabel value="Temporary Cancelled"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:selectBooleanCheckbox id="checkId" disabled="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.tempStatusChecked}"></p:selectBooleanCheckbox>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Cancelled From"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="cancelledFromId" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.cancelFromDate}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Cancelled To"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="cancelledToId" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.cancelToDate}"></p:inputText>
								</p:panelGrid>


							</p:panelGrid>

							<p:panelGrid columns="2"
								style="margin-left:50px;border-bottom:5px solid #404d6f;"
								columnClasses="ui-grid-col-3,ui-grid-col-11" layout="grid">


								<p:panelGrid>
									<p:outputLabel value="Special Remark"></p:outputLabel>
								</p:panelGrid>


								<p:panelGrid>
									<p:inputText id="specialRemark" readonly="true"
										style="height:40px; width:94%"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.specialRemarks}"></p:inputText>
								</p:panelGrid>

								<br></br>
							</p:panelGrid>
						</p:panel>

						<p:panel>

							<p:panelGrid columns="4" style="margin-left:50px"
								columnClasses="ui-grid-col-3,ui-grid-col-4,ui-grid-col-3,ui-grid-col-4"
								layout="grid">


								<p:panelGrid>
									<p:outputLabel value="Owner/Organization Name"></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="ownerOrganizationName" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.permitOwner}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Telephone No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="telephoneNo" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.telNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="NIC/ Org Reg No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="nicorgNo" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.nicNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Mobile No."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="mobileNo" readonly="true"
										value="#{activateCancelledPermitsBackingBean.activateCancelledPermitsDTO.mobileNo}"></p:inputText>
								</p:panelGrid>

								<p:panelGrid>
									<p:outputLabel value="Address 1."></p:outputLabel>
								</p:panelGrid>

								<p:panelGrid>
									<p:inputText id="addOne" readonly="true"
