		<p:dialog styleClass="" id="uploadDocumentsDialog" global="true"
			closable="false" modal="true" header="Upload Documents" width="80%"
			severity="info" widgetVar="uploadDocument" closeOnEscape="true"
			resizable="false" draggable="false">
			<p:ajax event="close"
				listener="#{permitRenewalsBackingBean.clearCheckDocumentsTable}" />
			<h:form id="frmdoc">

				<div style="overflow: scroll">
					<p:accordionPanel>
						<p:growl id="msgs" showDetail="false" />
						<p:tab title="Mandatory Documents">


							<h:panelGrid>
								<p:dataTable id="abc" tableStyle="width:80%" var="docMandatory"
									value="#{activateCancelledPermitsBackingBean.mandatoryList}"
									rows="2" paginator="true" sortOrder="ascending"
									paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
									rowsPerPageTemplate="2,4,6,8,10">



									<p:column headerText="Code" style="width: 100px;">
										<h:outputText value="#{docMandatory.doc_Code}" />
									</p:column>
									<p:column headerText="Description" style="width: 300px;">
										<h:outputText value="#{docMandatory.add_Doc_Description}" />
									</p:column>
									<p:column headerText="File Path" style="width: 300px;">

										<h:outputText value="#{docMandatory.uploadFilePath}" />
									</p:column>

									<p:column headerText="" style="width: 150px !important;">

										<p:fileUpload
											fileUploadListener="#{documentManagementBean.saveMandatoryUploadDocuments}"
											mode="advanced" dragDropSupport="false" multiple="true"
											auto="true" showButtons="false" update="@form" fileLimit="1"
											accept=".pdf,.jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
											

											<f:attribute name="uploadedObj" value="#{docMandatory}" />
											<f:attribute name="permitNo"
												value="#{activateCancelledPermitsBackingBean.selectDTO.permitNo}" />
											<f:attribute name="applicationNo"
												value="#{activateCancelledPermitsBackingBean.selectDTO.applicationNo}" />
											<f:attribute name="transactionCode" value="CANCEL" />
										</p:fileUpload>





									</p:column>




								</p:dataTable>
							</h:panelGrid>





						</p:tab>
					</p:accordionPanel>

					<br></br> <br></br>

					<p:accordionPanel>
						<p:growl id="msgse" showDetail="false" />
						<p:tab title="Optional Documents">

							<p:commandLink value="Add New Document Types"
								actionListener="#{documentManagementBean.addNewDocumentOtherPages}"
								update="@form"></p:commandLink>

							<h:panelGrid>
								<p:dataTable id="abcd" tableStyle="width:80%" var="docOptional"
									value="#{sessionBackingBean.optionalList}" rows="2"
									paginator="true" sortOrder="ascending"
									paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
									rowsPerPageTemplate="2,4,6,8,10">



									<p:column headerText="Code" style="width: 100px;">
										<h:outputText value="#{docOptional.doc_Code}" />
									</p:column>
									<p:column headerText="Description" style="width: 300px;">
										<h:outputText value="#{docOptional.add_Doc_Description}" />
									</p:column>
									<p:column headerText="File Path" style="width: 300px;">

										<h:outputText value="#{docOptional.uploadOptionalFilePath}" />
									</p:column>

									<p:column headerText="" style="width: 150px !important;">

										<p:fileUpload
											fileUploadListener="#{documentManagementBean.saveOptionalUploadDocuments}"
											mode="advanced" dragDropSupport="false" multiple="true"
											auto="true" showButtons="false" update="@form" fileLimit="1"
											accept=".pdf,.jpg,.jpeg,.png"
											allowTypes="/(\.|\/)(jpe?g|png|pdf)$/" sizeLimit="15728640">
											

											<f:attribute name="uploadedOptionalObj"
												value="#{docOptional}" />
											<f:attribute name="permitNo"
												value="#{activateCancelledPermitsBackingBean.selectDTO.permitNo}" />
											<f:attribute name="applicationNo"
												value="#{activateCancelledPermitsBackingBean.selectDTO.applicationNo}" />
											<f:attribute name="transactionCode" value="CANCEL" />
										</p:fileUpload>





									</p:column>




								</p:dataTable>
							</h:panelGrid>

						</p:tab>
					</p:accordionPanel>
				</div>
				<br></br>
				<center>
					<p:commandButton id="cmdbtnBack" value="Back" class="mainActionbtn"
						onclick="PF('viewCreateChargeType').hide()"
						action="#{permitRenewalsBackingBean.goBack}">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>
