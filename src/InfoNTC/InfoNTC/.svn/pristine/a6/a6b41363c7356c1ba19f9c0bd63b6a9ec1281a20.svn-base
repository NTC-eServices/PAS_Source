<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:p="http://primefaces.org/ui"
	xmlns:f="http://java.sun.com/jsf/core"
	template="/WEB-INF/template/template.xhtml">
	<ui:define name="metadata">
	</ui:define>
	<ui:define name="content">

		<style>
.ui-datatable-scrollable-theadclone {
	visibility: collapse;
}

.ui-dialog .ui-dialog-titlebar {
	padding: .4em .4em .4em 1em;
	position: relative;
	background: #07295b !important;
	padding: 5px 5px 5px 5px;
	document margin: 0px 0px 0px 0px;
	color: #ffffff;
}

.ui-panelgrid .ui-panelgrid-cell {
	background-color: #f1f1f1;
}

.ui-icon.ui-confirm-dialog-severity {
	background-position: 0 0 !important;
	background-image: none !important;
}

.colorHighlight {
	background: yellow !important;
}

.ui-dialog .ui-widget .ui-widget-content .ui-corner-all .ui-shadow .ui-hidden-container .ui-draggable
	{
	width: 50%;
}

.firstColumn {
	width: 100%;
}

.secondColumn {
	width: 100%;
}

.dashboardNRResendDocRecord {
	background-color: #16e5d7 !important;
}

.ui-state-default a, .ui-state-default a:link, .ui-state-default a:visited
	{
	color: #07295b;
}

.mainDiv {
	width: 99%;
	height: auto;
	border-left: solid;
	border-right: solid;
	border-bottom: solid;
	border-top: solid;
	border-color: #07295b;
	border-width: 1px;
	margin-left: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	/*background-image: linear-gradient(to right bottom, #d9ddfa, #ccd4fb, #bdcafc, #aec2fe,
		#9cb9ff);*/
	background-image: linear-gradient(to right bottom, #f1f1f1, #dfdfe4, #f1f1f1, #e9eaeb,
		#bbb5b5);
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	color: #07295b;
}

.mainLinkButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.mainLinkButton:hover {
	background: #fba026 !important;
}

.otherLinkButton {
	font-weight: bold;
	background: #296807 !important;
	text-align: center;
	padding: 7px 25px 7px 25px !important;
	height: 35px !important;
	text-decoration: none !important;
	color: white !important;
	white-space: nowrap !important;
	border-radius: 5px !important;
}

.otherLinkButton:hover {
	background: #fba026 !important;
}

.mainActionbtn {
	font-weight: bold;
	background: #2571fa !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 65% !important;
}
</style>

		<!-- Main form -->
		<div class="mainDiv">
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Issue New Permit</h1>
			<div style="width: 95%;" class="subDiv">
				<!-- search panel start -->
				<h:form id="searchForm">

					<!-- Select counter start -->


					<p:dialog closable="false" header="Select Counter"
						visible="#{newPermitIssuanceBackingBean.localcheckcounter}"
						widgetVar="dlg2" modal="true" height="120" width="420"
						resizable="false" update="@all">

						<br> </br>

						<p:panelGrid columns="2" id="dialoggrid"
							columnClasses="ui-grid-col-5,ui-grid-col-5">

							<p:panelGrid>

								<p:outputLabel value="Select Counter No. :- "></p:outputLabel>

							</p:panelGrid>


							<p:panelGrid>
								<p:selectOneMenu style="width:171px" id="counter"
									value="#{newPermitIssuanceBackingBean.commonDTO.counterId}">
									<p:ajax
										listener="#{newPermitIssuanceBackingBean.onCounterSelect}"
										onClose="#{newPermitIssuanceBackingBean.handleClose}">
									</p:ajax>
									<f:selectItem itemLabel="--Select Counter--" itemValue=""
										noSelectionOption="true" />
									<f:selectItems
										value="#{newPermitIssuanceBackingBean.counterList}"
										var="counterList" itemLabel="#{counterList.counter}"
										itemValue="#{counterList.counterId}" />

								</p:selectOneMenu>

							</p:panelGrid>

							<center>
								<p:panel>
									<p:commandButton id="Exit" value="Exit"
										action="#{newPermitIssuanceBackingBean.handleClose}"
										class="mainActionbtn" ajax="true" update="@all">
									</p:commandButton>
								</p:panel>
							</center>

						</p:panelGrid>

					</p:dialog>



					<!-- Select counter end -->
					<!-- Search box start -->
					<p:panel>
						<p:panelGrid columns="4"
							columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
							layout="grid">
							<!-- _Tender Reference No -->
							<p:outputLabel>Tender Reference No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="true"
								value="#{newPermitIssuanceBackingBean.strTenderRefNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<!-- _Date -->
							<p:outputLabel>Date</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="true"
								value="#{newPermitIssuanceBackingBean.strCurrentDate}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<!-- _Application No. -->
							<p:outputLabel>Application No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								value="#{newPermitIssuanceBackingBean.strApplicationNo}"
								readonly="true" onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<!-- _Queue No. -->
							<p:outputLabel>Queue No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="#{newPermitIssuanceBackingBean.busOwnerDTO.seq != 0 || newPermitIssuanceBackingBean.callNext==true}"
								value="#{newPermitIssuanceBackingBean.strQueueNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<!-- _Service Type -->
							<p:outputLabel>Service Type</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="true"
								value="#{newPermitIssuanceBackingBean.strServiceType}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
							<!-- _Bus Registration No. -->
							<p:outputLabel>Bus Registration No.</p:outputLabel>
							<p:inputText style="width:100%;height:18px;" maxlength="50"
								readonly="true"
								value="#{newPermitIssuanceBackingBean.strBusRegNo}"
								onkeyup="this.value=this.value.toUpperCase();">
							</p:inputText>
						</p:panelGrid>
						<!-- Search panel buttons -->
						<center>
							<p:panel>
								<!-- _Search -->
								<p:commandButton styleClass="mainActionbtn" value="Search"
									ajax="false" update="@all"
									action="#{newPermitIssuanceBackingBean.searchApplication}"
									disabled="#{newPermitIssuanceBackingBean.busOwnerDTO.seq != 0 || newPermitIssuanceBackingBean.callNext==true}"></p:commandButton>
								<!-- _Call Next -->
								<p:commandButton styleClass="mainActionbtn" value="Call Next"
									ajax="false" update="@all"
									action="#{newPermitIssuanceBackingBean.callNext}"
									disabled="#{newPermitIssuanceBackingBean.busOwnerDTO.seq != 0 || newPermitIssuanceBackingBean.callNext==true}"></p:commandButton>
								<!-- _Skip -->
								<p:commandButton styleClass="mainActionbtn" value="Skip"
									ajax="false" update="@all"
									action="#{newPermitIssuanceBackingBean.skip}"
									disabled="#{newPermitIssuanceBackingBean.busOwnerDTO.seq != 0 || newPermitIssuanceBackingBean.skip==true || newPermitIssuanceBackingBean.next==true}"></p:commandButton>
								<!-- _Clear -->
								<p:commandButton styleClass="mainActionbtn" value="Clear"
									ajax="false" update="@all"
									action="#{newPermitIssuanceBackingBean.clearAll}"
									disabled="#{newPermitIssuanceBackingBean.busOwnerDTO.seq != 0}"></p:commandButton>
							</p:panel>
						</center>
					</p:panel>
					<!-- Search box end -->
				</h:form>
				<!-- search panel end -->

				<!-- tab view start-->
				<p:tabView id="tabviewId"
					activeIndex="#{newPermitIssuanceBackingBean.activeTabIndex}"
					rendered="#{newPermitIssuanceBackingBean.applicationFound}">

					<!-- Tab 01 start -->
					<p:tab title="Particulars of Bus Owner">
						<h:form id="frmBusOwnerInfo">
							<!-- panel 01 start -->
							<p:panel>
								<p:panelGrid columns="3"
									columnClasses="ui-grid-col-2,ui-grid-col-3,ui-grid-col-7"
									layout="grid">
									<!-- _Application No -->
									<p:panelGrid
										rendered="#{newPermitIssuanceBackingBean.busOwnerDTO.seq == 0}">
										<p:outputLabel value="Application No."></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:inputText style="width:90%;height:18px;" id="txtNic"
										rendered="#{newPermitIssuanceBackingBean.busOwnerDTO.seq == 0}"
										maxlength="12" readonly="true"
										value="#{newPermitIssuanceBackingBean.strApplicationNo}"
										onkeyup="this.value=this.value.toUpperCase();">

									</p:inputText>

									<p:panelGrid>
										<!-- _Next button -->
										<p:commandButton styleClass="mainActionbtn" value="Proceed"
											ajax="false" update="@form"
											rendered="#{newPermitIssuanceBackingBean.busOwnerDTO.seq == 0}"
											action="#{newPermitIssuanceBackingBean.getOwnerDetails}"
											disabled="#{newPermitIssuanceBackingBean.next}"></p:commandButton>
										<!-- _Clear button -->
										<p:commandButton styleClass="mainActionbtn" value="Clear"
											rendered="#{newPermitIssuanceBackingBean.busOwnerDTO.seq == 0}"
											ajax="false"
											action="#{newPermitIssuanceBackingBean.clearOwnerDetails}"></p:commandButton>

									</p:panelGrid>

								</p:panelGrid>

							</p:panel>
							<!-- panel 01 end -->
							<!-- panel 02 start -->
							<p:panel rendered="#{newPermitIssuanceBackingBean.next}">
								<!-- T01_panel grid 01 start -->
								<p:panelGrid columns="4"
									columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-3,ui-grid-col-3"
									layout="grid">
									<!-- T01r01_prefered language -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.prelanguage']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:selectOneMenu effect="fold" style="width:59%;"
										value="#{newPermitIssuanceBackingBean.strSelectedLanguage}">
										<f:selectItem itemLabel="ENGLISH" itemValue="E" />
										<f:selectItem itemLabel="SINHALA" itemValue="S" />
										<f:selectItem itemLabel="TAMIL" itemValue="T" />
									</p:selectOneMenu>
									<!-- T01r01_permit no -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbpermitinfo.permitno']}"></p:outputLabel>
									</p:panelGrid>

									<p:inputText
										style="width:90%;height:18px;color:red;font-weight: bold;"
										maxlength="12" readonly="#{newPermitIssuanceBackingBean.next}"
										value="#{newPermitIssuanceBackingBean.strPermitNo}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

									<!-- T01r02_title -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.title']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:selectOneMenu id="txtTitle" style="width:59%;" effect="fold"
										value="#{newPermitIssuanceBackingBean.strSelectedTitle}">
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems
											value="#{newPermitIssuanceBackingBean.titleList}"
											var="titleList" itemValue="#{titleList.code}"
											itemLabel="#{titleList.description}" />
									</p:selectOneMenu>
									<!-- T01r02_gender -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.gender']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:selectOneMenu id="txtGender" style="width:80%;"
										effect="fold"
										value="#{newPermitIssuanceBackingBean.strSelectedGender}">
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems
											value="#{newPermitIssuanceBackingBean.genderList}"
											var="genderList" itemValue="#{genderList.code}"
											itemLabel="#{genderList.description}" />
									</p:selectOneMenu>
									<!-- T01r03_date of birth -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.dob']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:panelGrid>
										<div style="width: 84%">
											<p:calendar id="Dob" navigator="true" locale="en_US"
												styleClass="calendarClass"
												value="#{newPermitIssuanceBackingBean.busOwnerDTO.dob}"
												maxdate="today" showOn="button" pattern="dd/MM/yyyy"
												placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
												readonlyInput="true"></p:calendar>
										</div>
									</p:panelGrid>
									<!--T01r03_nic no -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.nic']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:inputText style="width:90%;height:18px;" maxlength="12"
										readonly="true"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.nicNo}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

								</p:panelGrid>
								<!-- T01_panel grid 01 end -->
								<!-- T01_panel grid 02 start -->
								<p:panelGrid columns="2"
									columnClasses="ui-grid-col-2,ui-grid-col-10" layout="grid">

									<!--T01r04_name in full -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.name']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:inputText style="width:97%;height:18px;" id="txtFullName"
										maxlength="200" readonly="true"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.fullName}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<!--T01r05_name in full sinhala -->
									<p:panelGrid>
										<p:outputLabel value="Name in Full"></p:outputLabel>
										<div>
											<b>(Sinhala)</b>
										</div>
									</p:panelGrid>

									<p:inputText style="width:97%;height:18px;"
										id="txtFullNameSinhala" maxlength="200"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.fullNameSinhala}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<!--T01r06_name in full tamil -->
									<p:panelGrid>
										<p:outputLabel value="Name in Full"></p:outputLabel>
										<div>
											<b>(Tamil)</b>
										</div>
									</p:panelGrid>

									<p:inputText style="width:97%;height:18px;"
										id="txtFullNameTamil" maxlength="200"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.fullNameTamil}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<!--T01r07_name with initials -->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.namewithinitials']}"></p:outputLabel>
									</p:panelGrid>

									<p:inputText style="width:97%;height:18px;"
										id="txtNamewithInitials" maxlength="200"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.nameWithInitials}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
								</p:panelGrid>
								<!-- T01_panel grid 02 end -->
								<!-- T01_panel grid 03 start -->
								<p:panelGrid columns="4" style="margin-top: 1%;margin-bottom:1%"
									columnClasses="ui-grid-col-2,ui-grid-col-3,ui-grid-col-4,ui-grid-col-3"
									layout="grid">
									<!--T01r08_address in -->
									<p:outputLabel value="Address in" style="font-weight: bold;"></p:outputLabel>
									<p:outputLabel value="Address Line 1"
										style="font-weight: bold;"></p:outputLabel>
									<p:outputLabel value="Address Line 2"
										style="font-weight: bold;"></p:outputLabel>
									<p:outputLabel value="City" style="font-weight: bold;"></p:outputLabel>
									<!--T01r09_engish address-->
									<p:panelGrid>
										<p:outputLabel value="English"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:inputText style="width:90%;height:18px;" id="txtAddress1"
										maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.address1}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<p:inputText style="width:90%;height:18px;" id="txtAddress2"
										maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.address2}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<p:inputText style="width:90%;height:18px;" id="txtCity"
										maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.city}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<!--T01r10_sinhala address-->
									<p:outputLabel value="Sinhala"></p:outputLabel>

									<p:inputText style="width:90%;height:18px;"
										id="txtAddress1Sinhala" maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.address1Sinhala}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<p:inputText style="width:90%;height:18px;"
										id="txtAddress2Sinhala" maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.address2Sinhala}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<p:inputText style="width:90%;height:18px;" id="txtCitySinhala"
										maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.citySinhala}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<!--T01r11_tamil address-->
									<p:outputLabel value="Tamil"></p:outputLabel>

									<p:inputText style="width:90%;height:18px;"
										id="txtAddress1Tamil" maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.address1Tamil}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<p:inputText style="width:90%;height:18px;"
										id="txtAddress2Tamil" maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.address2Tamil}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
									<p:inputText style="width:90%;height:18px;" id="txtCityTamil"
										maxlength="50"
										value="#{newPermitIssuanceBackingBean.busOwnerDTO.cityTamil}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

								</p:panelGrid>
								<!-- T01_panel grid 03 end -->
								<!-- T01_panel grid 04 start -->
								<p:panelGrid columns="4"
									columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-3,ui-grid-col-3"
									layout="grid">

									<!-- T01r12_marital status-->
									<p:panelGrid>
										<p:outputLabel
											value="#{msg['common.backlog.tbbusownerinfo.marital']}"></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
