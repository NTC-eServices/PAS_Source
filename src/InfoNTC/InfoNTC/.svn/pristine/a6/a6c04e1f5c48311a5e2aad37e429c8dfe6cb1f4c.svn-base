	<queryString>
		<![CDATA[SELECT DISTINCT
    nri_service_agreement_no AS agreementno,
    nri_operator_name,
    nt_r_service_types.description,
    nri_origin,
    nri_destination,
    nri_bus_no,
    nri_province,
    nri_district,
    CASE WHEN nri_bus_no LIKE '%SLTB%' THEN 1 ELSE 2 END AS bus_group
FROM
    nt_m_nri_requester_info
    INNER JOIN nt_r_service_types ON nt_m_nri_requester_info.nri_service_type=nt_r_service_types.code
WHERE
    nri_service_agreement_no IS NOT null
    and nri_province =  case when $P{P_Province} is null then nri_province else $P{P_Province} end
	and nri_district = case when $P{P_District} is null then nri_district else $P{P_District} end
ORDER BY
    bus_group, 
    nri_operator_name,
    nri_service_agreement_no]]>
	</queryString>
