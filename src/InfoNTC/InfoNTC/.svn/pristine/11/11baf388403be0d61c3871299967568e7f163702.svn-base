		<script type="text/javascript">
			//<![CDATA[
			function exportChart() {
				//export image
				$('#output').empty().append(PF('chart').exportAsImage());

				//show the dialog
				PF('dlg').show();
			}
			//]]>
		</script>

		<p:chart type="line"
			model="#{analyzeSurveyDataBean.graphAnalyzedData}"
			style="width:800px;height:500px" widgetVar="chart" rendered="false" />

		<br />


		<p:dialog widgetVar="dlg" showEffect="fade" modal="true"
			header="Chart as an Image" resizable="false">
			<p:outputPanel id="output" layout="block"
				style="width:500px;height:300px" />
		</p:dialog>


		<div class="mainDiv">
			<!-- header -->
			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">Analyze Survey Data</h1>
			<div style="width: 95%;" class="subDiv">

				<p:tabView id="tabviewId"
					activeIndex="#{analyzeSurveyDataBean.activeTabIndex}">

					<!-- ***********************************      Tab 01       ******************************************************************* -->
					<p:tab title="Survey Analyze">
						<h:form id="frm_AnalyzeSurveyData">

							<!-- Panel 01 -->
							<p:panel id="pnl_Search">
								<p:panelGrid columns="4"
									columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
									layout="grid">

									<p:panelGrid>
										<p:outputLabel value="Form Id."></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<p:selectOneMenu id="drpd_FormId" style="width:80%;"
										filter="true" filterMatchMode="contains" effect="fold"
										value="#{analyzeSurveyDataBean.formDTO.formId}">
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems value="#{analyzeSurveyDataBean.formIdList}"
											var="formIdList" itemValue="#{formIdList}"
											itemLabel="#{formIdList}" />
										<p:ajax listener="#{analyzeSurveyDataBean.onFormIdChange}"
											process="@form" update="@form" />
									</p:selectOneMenu>

									<p:panelGrid></p:panelGrid>
									<p:panelGrid></p:panelGrid>

								</p:panelGrid>

								<p:panelGrid columns="2"
									columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">

									<p:panelGrid>
										<p:outputLabel value="Form Descrption"></p:outputLabel>
									</p:panelGrid>

									<p:inputText id="txt_FormDescrption" readonly="true"
										style="width:96.5%;height:18px;" maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.formDescription}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>
								</p:panelGrid>



								<p:panelGrid columns="4"
									columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
									layout="grid">

									<p:panelGrid>
										<p:outputLabel value="Survey No."></p:outputLabel>
									</p:panelGrid>
									<p:inputText id="txt_SurveyNo" readonly="true"
										style="width:89%;height:18px;" maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.surveyNo}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

									<p:panelGrid>
										<p:outputLabel value="Survey Type."></p:outputLabel>
									</p:panelGrid>
									<p:inputText id="txt_SurveyType" readonly="true"
										style="width:89%;height:18px;" maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.surveyTypeDes}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

									<p:panelGrid>
										<p:outputLabel value="Survey Method."></p:outputLabel>
									</p:panelGrid>
									<p:inputText id="txt_SurveyMethod" readonly="true"
										style="width:89%;height:18px;" maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.surveyMethodDes}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

									<p:panelGrid>
										<p:outputLabel value="Survey Form No."></p:outputLabel>
									</p:panelGrid>
									<p:inputText id="txt_SurveyFormNo" readonly="true"
										disabled="true" style="width:89%;height:18px;" maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.surveyFormNo}"
										onkeyup="this.value=this.value.toUpperCase();">
									</p:inputText>

								</p:panelGrid>
								<br></br>
								<p:panelGrid columns="4"
									columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
									layout="grid">

									<p:panelGrid>
										<p:outputLabel value="Copy Form Id."></p:outputLabel>

									</p:panelGrid>

									<p:selectOneMenu id="drpd_CopyFormulaId" style="width:80%;"
										effect="fold"
										value="#{analyzeSurveyDataBean.formDTO.formulaTemplateId}">
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems value="#{analyzeSurveyDataBean.formulaIdList}"
											var="formulaIdList" itemValue="#{formulaIdList}"
											itemLabel="#{formulaIdList}" />
										<p:ajax listener="#{analyzeSurveyDataBean.onFormIdChange}"
											process="@form" update="@form" />
									</p:selectOneMenu>
									<p:panelGrid></p:panelGrid>
									<p:panelGrid></p:panelGrid>

									<p:panelGrid>
										<p:outputLabel value="Formula Name."></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>

									<!-- Formula Name -->
									<p:selectOneMenu id="drpd_FormulaNameId" style="width:80%;"
										effect="fold"
										value="#{analyzeSurveyDataBean.formDTO.formulaNameCode}">
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems
											value="#{analyzeSurveyDataBean.formulaNameList}"
											var="formIdList" itemValue="#{formIdList.formulaNameCode}"
											itemLabel="#{formIdList.formulaNameDescription}" />
										<p:ajax
											listener="#{analyzeSurveyDataBean.onFormulaNameChange}"
											process="@form"
											update="drpd_Operator,txt_Formula,drpd_FieldName,txt_Value,btnAddFormula" />
									</p:selectOneMenu>

									<!-- Field Name -->
									<p:panelGrid>
										<p:outputLabel value="Field Name."></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>
									<p:selectOneMenu id="drpd_FieldName" style="width:80%;"
										disabled="#{analyzeSurveyDataBean.disableFieldName}"
										effect="fold"
										value="#{analyzeSurveyDataBean.formDTO.fieldName}">
										<p:ajax listener="#{analyzeSurveyDataBean.onFieldNameChange}"
											process="@form"
											update="txt_Formula,drpd_FieldName,txt_Value,drpd_Operator,btnAddFormula" />
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems value="#{analyzeSurveyDataBean.fieldNameList}"
											var="fieldNameList" itemValue="#{fieldNameList.fieldNameEn}"
											itemLabel="#{fieldNameList.fieldNameEn}" />
									</p:selectOneMenu>

									<!-- Operator -->
									<p:panelGrid>
										<p:outputLabel value="Operator."></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>
									<p:selectOneMenu id="drpd_Operator" style="width:80%;"
										disabled="#{analyzeSurveyDataBean.disableOperetor}"
										effect="fold"
										value="#{analyzeSurveyDataBean.formDTO.operator}">
										<f:selectItem itemLabel="-- Select --" itemValue="" />
										<f:selectItems value="#{analyzeSurveyDataBean.operatorList}"
											var="operatorList" itemValue="#{operatorList}"
											itemLabel="#{operatorList}" />
										<p:ajax listener="#{analyzeSurveyDataBean.onOperaterChange}"
											process="@form"
											update="drpd_Operator,txt_Formula,drpd_FieldName,txt_Value,btnAddFormula" />
									</p:selectOneMenu>
									<!-- value -->
									<p:panelGrid>
										<p:outputLabel value="Value."></p:outputLabel>
									</p:panelGrid>
									<p:inputNumber id="txt_Value" styleClass="numField"
										disabled="#{analyzeSurveyDataBean.disableValue}"
										style="width:89%;height:18px;" maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.value}"
										onkeyup="this.value=this.value.toUpperCase();">
										<p:ajax listener="#{analyzeSurveyDataBean.onValueChange}"
											process="@form"
											update="drpd_Operator,txt_Formula,drpd_FieldName,txt_Value" />
									</p:inputNumber>

								</p:panelGrid>

								<p:panelGrid columns="2"
									columnClasses="ui-grid-col-3,ui-grid-col-9" layout="grid">

									<p:panelGrid>
										<p:outputLabel value="Formula."></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>
									<p:inputText id="txt_Formula" style="width:96.5%;height:18px;"
										maxlength="50"
										value="#{analyzeSurveyDataBean.formDTO.displayFormulaName}"
										onkeyup="this.value=this.value.toUpperCase();">
										<p:ajax listener="#{analyzeSurveyDataBean.onFormulaChange}"
											process="@form" update="drpd_Operator" />
											
									</p:inputText>

								</p:panelGrid>

								<br></br>
								<p:panelGrid columns="4"
									columnClasses="ui-grid-col-3,ui-grid-col-3,ui-grid-col-3,ui-grid-col-3"
									layout="grid">
									<p:outputLabel></p:outputLabel>

									<!-- Add formula Button -->
									<p:commandButton id="btnAddFormula" styleClass="mainLinkButton"
										style="width:100%"
										disabled="#{analyzeSurveyDataBean.disableAdd}" value="Add"
										ajax="true" update="tbl_Formula,,successMessage,alertMessage"
										action="#{analyzeSurveyDataBean.addFormulaToGrid}"></p:commandButton>
									<!-- Clear 03 Button -->
									<p:commandButton styleClass="mainLinkButton" style="width:100%"
										value="Clear" ajax="true"
										update="drpd_CopyFormulaId,drpd_FormulaNameId,drpd_FieldName,drpd_Operator,txt_Value,txt_Formula"
										action="#{analyzeSurveyDataBean.clearFormula}"></p:commandButton>
									<p:outputLabel></p:outputLabel>
									<p:outputLabel></p:outputLabel>

								</p:panelGrid>

								<p:panelGrid columns="3"
									columnClasses="ui-grid-col-3,ui-grid-col-6,ui-grid-col-3"
									layout="grid">
									<p:outputLabel></p:outputLabel>

									<!-- Add Survey Completed Button -->
									<p:commandButton id="btnSurveyCompleted"
										styleClass="mainLinkButton" style="width:100%"
										disabled="#{analyzeSurveyDataBean.disableSurveyCompleted}"
										value="Survey Completed" ajax="false" update="@form"
										action="#{analyzeSurveyDataBean.btn_CompleteSurvey}"></p:commandButton>

								</p:panelGrid>

								<br></br>
								<!-- Table -->
								<center>
									<p:dataTable id="tbl_Formula"
										style="width:100%;margin-bottom:20px"
										value="#{analyzeSurveyDataBean.tbl_FormulaList}"
										var="formulaList" resizableColumns="false" liveResize="true"
										rowIndexVar="index" emptyMessage="No Records Available"
										paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {Exporters}"
										paginator="true" rows="10">

										<p:column headerText="Formula Name."
											style="text-align:left;width:30%">
											<h:outputText value="#{formulaList.formulaName}"></h:outputText>
										</p:column>
										<p:column headerText="Formula"
											style="text-align:left;width:80%">
											<h:outputText value="#{formulaList.formula}"></h:outputText>
										</p:column>
										<p:column headerText="" style="text-align:center;width:10%">

											<center>
												<p:commandLink
													action="${analyzeSurveyDataBean.removeSelectedFormula(formulaList.formulaNameCode)}"
													process="@all" ajax="true"
													update="tbl_Formula,successMessage">

													<p:graphicImage library="images" name="delete.png"
														style="text-align:center;width:24px;height:24px;"
														title="Remove"></p:graphicImage>
												</p:commandLink>
											</center>

										</p:column>
									</p:dataTable>
								</center>
								<br></br>


								<p:panelGrid columns="3"
									columnClasses="ui-grid-col-3,ui-grid-col-6,ui-grid-col-3"
									layout="grid">
									<p:outputLabel></p:outputLabel>

									<!-- Analyzed data Button  -->
									<p:commandButton styleClass="mainLinkButton" style="width:100%"
										disabled="#{analyzeSurveyDataBean.disableAnalyzedData}"
										id="btn-AnalyzeData" value="Analyze Data"
										action="#{analyzeSurveyDataBean.analyzeData}" process="@all"
										update="@all" ajax="true"></p:commandButton>

								</p:panelGrid>


							</p:panel>

						</h:form>
					</p:tab>
					<p:tab title="Analyze Data" id="tab-AnalyzeData"
						disabled="#{analyzeSurveyDataBean.disableTabAnalyadzedData}">
						<h:form id="frm_AnalyzedData">
							<div style="overflow-x: auto;">

								<table id="tbl-AnalyzedDat">
									<tr>
										<c:forEach begin="0"
											end="#{analyzeSurveyDataBean.columnCount}"
											items="#{analyzeSurveyDataBean.formulaList}"
											var="AnalyzeSurveryDataDTO">

											<th><h:outputLabel
													style="margin-right:40px;margin-bottom:15px;"
													value="#{AnalyzeSurveryDataDTO.formulaName}" /></th>
										</c:forEach>
									</tr>

									<!-- main list -->
									<c:forEach
										items="#{analyzeSurveyDataBean.analyzeSurveryDataList}"
										var="innerList">
										<tr>
											<!-- index list -->
											<c:forEach items="${innerList}" begin="0"
												end="#{analyzeSurveyDataBean.columnCount}" var="dto">
												<td><p:inputText value="#{dto.answer}"
														style="margin-right:40px;" readonly="true" /></td>

											</c:forEach>

											<c:forEach begin="0" end="0" items="${innerList}" var="dto">
												<td><p:inputText value="#{dto.timeband}"
														style="margin-right:40px;" readonly="true" /></td>

											</c:forEach>

										</tr>
									</c:forEach>

								</table>


							</div>
						</h:form>

					</p:tab>



					<p:tab title="Display Graph" id="tab_03"
						disabled="#{analyzeSurveyDataBean.disableTabDisplayGraph}">
						<h:form id="frm_DisplayGraph">

							<center>
								<p:chart type="line"
									model="#{analyzeSurveyDataBean.graphAnalyzedData}"
									style="width:800px;height:500px" widgetVar="chart" />
							</center>



							<p:panelGrid columns="3"
								columnClasses="ui-grid-col-3,ui-grid-col-6,ui-grid-col-3"
								layout="grid">

								<p:panelGrid>
								</p:panelGrid>

								<p:commandButton type="button" value="Save Graph As Image"
									styleClass="mainLinkButton" style="width:100%"
									onclick="exportChart()" />
								<p:panelGrid>
								</p:panelGrid>

							</p:panelGrid>

						</h:form>
					</p:tab>




				</p:tabView>



			</div>
		</div>


		<!-- Display required field field error massage -->
		<p:dialog styleClass="" id="dlg_Required" global="true" modal="true"
			header="#{msg['um.employeeprofile.dlmissing']}" width="30%"
			severity="info" widgetVar="requiredField" closeOnEscape="true"
			resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{analyzeSurveyDataBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField"
						value="#{msg['um.employeeprofile.dlmissingclose']}"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>
		<!-- Display alert massage -->
		<p:dialog styleClass="" id="alertMessage" global="true" modal="true"
			header="Information" width="30%" severity="info"
			widgetVar="alertMessage" closeOnEscape="true" resizable="false">
			<h:form id="frmAlert">
				<p:panelGrid columns="3" style="margin-top:3%"
					columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-9"
					layout="grid">
					<p:panelGrid></p:panelGrid>
					<p:panelGrid>
						<div></div>
						<div></div>
						<p:panelGrid></p:panelGrid>
						<p:panelGrid></p:panelGrid>
						<p:graphicImage library="images" name="warning.png"></p:graphicImage>
					</p:panelGrid>
					<p:panelGrid>
						<p:outputLabel style="margin-left:15%"
							value="#{analyzeSurveyDataBean.alertMsg}"></p:outputLabel>

						<p:panelGrid columns="2" style="margin-top:3%"
							columnClasses="ui-grid-col-2,ui-grid-col-10" layout="grid">
							<center>
								<p:commandButton style="margin-top:10px;margin-bottom:10px"
									process="frmAlert"
									value="#{msg['um.employeeprofile.dlmissingclose']}"
									onclick="PF('alertMSG').hide()">
								</p:commandButton>
							</center>
							<p:panelGrid></p:panelGrid>
						</p:panelGrid>
					</p:panelGrid>
				</p:panelGrid>
			</h:form>
		</p:dialog>
		<!-- Display success massage -->
		<p:dialog styleClass="" id="successMessage" global="true" modal="true"
			header="Success" width="30%" severity="info"
			widgetVar="successMessage" closeOnEscape="true" resizable="false">

			<h:form id="frmsuccess">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>

						<p:outputLabel value="#{analyzeSurveyDataBean.successMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						value="OK" process="frmsuccess"
						onclick="PF('successMessage').hide()">
					</p:commandButton>
				</center>
			</h:form>
		</p:dialog>


	</ui:define>
</ui:composition>