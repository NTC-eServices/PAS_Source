	transition: background-color 0.3s ease-out;
}

.mainActionbtn:hover {
	background: #fba026 !important;
}

.OthActionbtn {
	font-weight: bold;
	background: #296807 !important;
	height: 30px !important;
	border-radius: 5px !important;
	color: white !important;
	text-align: center;
	width: 80px;
	white-space: nowrap !important;
	-webkit-transition: background-color 0.3s ease-out;
	-moz-transition: background-color 0.3s ease-out;
	-o-transition: background-color 0.3s ease-out;
	transition: background-color 0.3s ease-out;
	margin-bottom: 5px;
}

.OthActionbtn:hover {
	background: #fba026 !important;
}

.subDiv {
	margin: 0 auto;
	background: rgb(254, 255, 255); /* Old browsers */
	background: -moz-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(-45deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(135deg, rgba(254, 255, 255, 1) 0%,
		rgba(210, 235, 249, 1) 100%);
	/* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff',
		endColorstr='#d2ebf9', GradientType=1);
	/* IE6-9 fallback on horizontal gradient */
}

.internalPanel .ui-panelgrid-cell {
	padding: 5px 0px;
}

.internalInput .ui-panelgrid-cell {
	padding: 0px;
}

#Dob {
	width: 80% !important;
}

.calendarClass input {
	width: 79% !important;
}

.numField .ui-inputfield {
	width: 89% !important;
	font-weight: bold;
	text-align: right;
}

.footer {
	height: 20px;
	background-color: #07295b;
	/* padding: .7em 1.5em 0em 1.5em; */
	text-align: center;
	colour: #84063A;
	color: #777373;
}

.ui-datatable-scrollable .ui-datatable-scrollable-header {
	min-height: 20px;
}

.ui-datatable-scrollable-body {
	height: 125px !important;
}

.genReqNumButton {
	font-weight: bold;
	background: #2571fa !important;
	text-align: center;
	text-decoration: none !important;
	color: white !important;
	border-radius: 5px !important;
}

.ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	text-shadow: none;
}

.ui-accordion .ui-accordion-header {
	cursor: pointer;
	position: relative;
	margin-top: 1px;
	min-height: 0;
	padding: .5em .5em .5em 2.2em;
	background-color: cornflowerblue;
}

.ui-autocomplete-itemtip {
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box;
}
</style>

		<div class="mainDiv">

			<!-- header -->

			<h1
				style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
				class="common_form_header">View/Edit SIM Details</h1>

			<div style="width: 95%;" class="subDiv">

				<p:panel id="pnl-sim-reg">

					<p:panelGrid columns="1" columnClasses="ui-grid-col-12"
						layout="grid">


						<h:form id="simRegistrationForm">

							<!-- Search area -->

							<p:panel
								style="border: #404d6f solid 1px !important;border-radius: 25px;background-color: #bdcafc;
								box-shadow: 3px 3px 3px grey; margin-bottom: 15px;">

								<!-- SIM REG No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="SIM Registration No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="simRegSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simEditBean.simSearchDTO.simRegNo}">

											<f:selectItem itemLabel=" -- Select SIM Reg No. -- "
												itemValue="" />
											<f:selectItems value="#{simEditBean.simDetailsList}"
												var="dto" itemValue="#{dto.simRegNo}"
												itemLabel="#{dto.simRegNo}" />


										</p:selectOneMenu>
									</div>
									<div class="ui-g-3"></div>
								</div>

								<!-- Permit No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="Permit No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="permitNoSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simEditBean.simSearchDTO.permitNo}">

											<f:selectItem itemLabel=" -- Select Permit No. -- "
												itemValue="" />
											<f:selectItems value="#{simEditBean.simDetailsList}"
												var="dto" itemValue="#{dto.permitNo}"
												itemLabel="#{dto.permitNo}" />

										</p:selectOneMenu>
									</div>
									<div class="ui-g-3"></div>
								</div>

								<!-- Bus No. -->
								<div class="ui-g">
									<div class="ui-g-2"></div>
									<div class="ui-g-3" style="text-align: left;">
										<p:outputLabel value="Bus No."></p:outputLabel>
									</div>
									<div class="ui-g-4">
										<p:selectOneMenu id="busNoSearchID" style="width:73%;"
											effect="fold" filter="true" filterMatchMode="contains"
											value="#{simEditBean.simSearchDTO.busNo}">

											<f:selectItem itemLabel=" -- Select Bus No. -- " itemValue="" />
											<f:selectItems value="#{simEditBean.simDetailsList}"
												var="dto" itemValue="#{dto.busNo}" itemLabel="#{dto.busNo}" />

										</p:selectOneMenu>
									</div>

									<!-- Search Button -->
									<div class="ui-g-3">
										<p:commandButton id="searchBtn" styleClass="otherLinkButton"
											style="width:80%;" value="Search" process="@all"
											update="@form" action="#{simEditBean.searchAction}">
										</p:commandButton>
									</div>
								</div>

							</p:panel>

							<!-- Input Details part -->

							<p:panelGrid columns="4" style="margin-bottom: 15px;"
								columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
								layout="grid">

								<!-- SIM Registration Number -->

								<p:panelGrid>
									<p:outputLabel value="SIM Registration No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="simRegNoId" style="width:81%" filter="true"
											placeholder="Search SIM Details" filterMatchMode="contains"
											tabindex="1" effect="fold" readonly="true"
											value="#{simEditBean.simRegistrationDTO.simRegNo}" />
									</span>
								</p:panelGrid>

								<!-- Service Category -->

								<p:panelGrid>
									<p:outputLabel value="Service Category "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:selectOneMenu id="serviceCategoryId" style="width:73%;"
										effect="fold" filter="true" filterMatchMode="contains"
										value="#{simEditBean.simRegistrationDTO.serviceCategory}">

										<p:ajax listener="#{simEditBean.onServiceCategoryChange}"
											update="@form"></p:ajax>

										<f:selectItem itemLabel=" -- Select Service Category -- "
											itemValue="" />
										<f:selectItem itemLabel="NTC" itemValue="ntc" />
										<f:selectItem itemLabel="Sisu Sariya" itemValue="sisu" />
										<f:selectItem itemLabel="Gami Sariya" itemValue="gami" />
										<f:selectItem itemLabel="Nisi Sariya" itemValue="nisi" />
										<f:selectItem itemLabel="Other" itemValue="other" />

									</p:selectOneMenu>
								</p:panelGrid>

								<!-- Permit No. -->

								<p:panelGrid>
									<p:outputLabel value="Permit No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="permitNoId" style="width:81%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="Permit Number"
											value="#{simEditBean.simRegistrationDTO.permitNo}"
											onkeyup="this.value=this.value.toUpperCase();"
											rendered="#{simEditBean.simRegistrationDTO.serviceCategory == 'other'}" />

										<p:autoComplete id="permitNoId2"
											value="#{simEditBean.simRegistrationDTO.permitNo}"
											completeMethod="#{simEditBean.completePermitNo}" size="29"
											rendered="#{simEditBean.simRegistrationDTO.serviceCategory != 'other'}"
											onkeyup="this.value=this.value.toUpperCase();"
											forceSelection="true" scrollHeight="500"
											placeholder="Permit Number">
											<p:ajax event="itemSelect"
												listener="#{simEditBean.onPermitNoSelect}" process="@all"
												update="@form" />
										</p:autoComplete>
									</span>
								</p:panelGrid>

								<!-- Bus No. -->

								<p:panelGrid>
									<p:outputLabel value="Bus No. "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="BusNoId"
											style="width:81%" filter="true" filterMatchMode="contains"
											tabindex="1" effect="fold" placeholder="Bus Number"
											value="#{simEditBean.simRegistrationDTO.busNo}"
											onkeyup="this.value=this.value.toUpperCase();"
											rendered="#{simEditBean.simRegistrationDTO.serviceCategory == 'other'}" />

										<p:autoComplete id="BusNoId2"
											value="#{simEditBean.simRegistrationDTO.busNo}"
											completeMethod="#{simEditBean.completeBusNo}" size="29"
											rendered="#{simEditBean.simRegistrationDTO.serviceCategory != 'other'}"
											onkeyup="this.value=this.value.toUpperCase();"
											forceSelection="true" scrollHeight="500"
											placeholder="Bus Number">
											<p:ajax event="itemSelect"
												listener="#{simEditBean.onBusNoSelect}" process="@all"
												update="@form" />
										</p:autoComplete>
									</span>
								</p:panelGrid>


								<!-- SIM No. -->

								<p:panelGrid>
									<p:outputLabel value="SIM No. (Mobile No.) "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="simNoId"
											style="width:81%" filter="true" filterMatchMode="contains"
											tabindex="1" maxlength="10" placeholder="SIM Number"
											value="#{simEditBean.simRegistrationDTO.simNo}">
											<p:keyFilter regEx="/[0-9-]/i" />
										</p:inputText>
									</span>
								</p:panelGrid>

							</p:panelGrid>

							<!-- SIM Transfer -->
							<p:panelGrid columns="4" style="margin-bottom: 15px;"
								columnClasses="ui-grid-col-4,ui-grid-col-4,ui-grid-col-4"
								layout="grid">
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<center>
										<p:commandButton id="simTransferBtnId" rendered="false"
											styleClass="otherLinkButton" style="width:80%;"
											value="SIM Transfer" process="@all" update="@form"
											disabled="#{simEditBean.disableButton}"
											action="#{simEditBean.simTransferBtnAction}">
										</p:commandButton>
									</center>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>
							</p:panelGrid>

							<!-- EMI Number Panel -->


							<p:panel
								style="border: #404d6f solid 1px !important;border-radius: 25px;background-color: #e4eeff;
						box-shadow: 3px 3px 3px grey; margin-bottom: 15px;">

								<p:panelGrid columns="4" style="margin-bottom: 15px;"
									columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-2,ui-grid-col-4"
									layout="grid">

									<!-- EMI Number -->

									<p:panelGrid>
										<p:outputLabel value="EMI Number "></p:outputLabel>
										<p:outputLabel value="*" style="color:red;"></p:outputLabel>
									</p:panelGrid>
									<p:panelGrid>
										<p:inputText id="emiNoId" style="width:81%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="EMI Number" maxlength="20"
											value="#{simEditBean.simRegistrationDTO.emiNo}"
											disabled="#{simEditBean.disableEmiNo}">
											<p:keyFilter regEx="/[0-9-]/i" />
										</p:inputText>

									</p:panelGrid>

								</p:panelGrid>


								<p:panelGrid columns="7"
									columnClasses="ui-grid-col-1,ui-grid-col-2,ui-grid-col-1,ui-grid-col-2,ui-grid-col-2,ui-grid-col-2,ui-grid-col-2"
									layout="grid">

									<!-- Bus Number -->

									<p:panelGrid>
										<center>
											<p:outputLabel value="Bus No."></p:outputLabel>
											<p:outputLabel value="*" style="color:red;"></p:outputLabel>
										</center>
									</p:panelGrid>
									<p:panelGrid>
										<span class="ui-float-label"> <p:inputText
												id="emiBusNoId" style="width:100%" filter="true"
												filterMatchMode="contains" tabindex="1" effect="fold"
												placeholder="Bus Number"
												onkeyup="this.value=this.value.toUpperCase();"
												value="#{simEditBean.emiDTO.emiBusNo}" />
										</span>
									</p:panelGrid>

									<!-- Status -->

									<p:panelGrid>
										<center>
											<p:outputLabel value="Status"></p:outputLabel>
										</center>
									</p:panelGrid>
									<p:panelGrid>
										<span class="ui-float-label"> <p:selectOneMenu
												id="emiStatusId" value="#{simEditBean.emiDTO.emiStatus}"
												style="width:70%;" filterMatchMode="contains" tabindex="1"
												effect="fold">
												<f:selectItem itemLabel="Active" itemValue="Active" />
												<f:selectItem itemLabel="In-active" itemValue="Inactive" />
											</p:selectOneMenu>
										</span>
									</p:panelGrid>

									<!-- Transaction Date -->

									<p:panelGrid>
										<center>
											<p:outputLabel value="Transaction Date"></p:outputLabel>
										</center>
									</p:panelGrid>
									<p:panelGrid>
										<p:calendar id="reIssueDate" style="height:18px;"
											maxdate="today" inputStyle="width:50%;" navigator="true"
											showOn="button" locale="en_US" pattern="dd/MM/yyyy"
											placeholder="DD/MM/YYYY" yearRange="c-100:c+100"
											readonlyInput="true"
											disabled="#{backlogTransaction.disableStartDateField}"
											value="#{simEditBean.emiDTO.emiReIssueDate}">
										</p:calendar>
									</p:panelGrid>

									<!-- Add Button -->

									<p:commandButton id="addBtn" styleClass="otherLinkButton"
										style="width:100%;" value="Add" process="@all" update="@form"
										disabled="#{simEditBean.addBtnCheck}"
										action="#{simEditBean.addEmiBtnAction}">

									</p:commandButton>

								</p:panelGrid>

								<!-- EMI Data Grid -->

								<p:dataTable id="emiDT" styleClass="myTable-two" var="emiDto"
									style="margin-top:2%; margin-bottom:3%;" rowIndexVar="index"
									rowsPerPageTemplate="2,3" resizableColumns="false"
									liveResize="true" emptyMessage="No records are available"
									value="#{simEditBean.emiDTOList}" sortBy="#{emiDto.emiStatus}">

									<!-- Index -->
									<p:column width="8%" headerText="Index"
										style="text-align:left;">
										<h:outputText value="#{index + 1}" />
									</p:column>

									<!-- Bus No. -->
									<p:column headerText="Bus No." style="text-align:left;">
										<h:outputText value="#{emiDto.emiBusNo}" />
									</p:column>

									<!-- Status -->
									<p:column headerText="Status" style="text-align:left;">
										<h:outputText value="#{emiDto.emiStatus}" />
									</p:column>

									<!-- Re Issue Date -->
									<p:column headerText="Transaction Date"
										style="text-align:left;">
										<h:outputText value="#{emiDto.emiReIssueDateString}">
										</h:outputText>
									</p:column>

									<!-- delete -->

									<p:column width="9%">
										<center>
											<p:commandLink action="${simEditBean.emiDelete()}"
												disabled="${simEditBean.disableEmiDelete}" process="@all"
												update="emiDT">

												<f:setPropertyActionListener value="#{emiDto}"
													target="${simEditBean.selectEmiDTO}" />

												<p:graphicImage library="images" name="delete.png"
													style="width:24px;height:24px;" title="delete"></p:graphicImage>
											</p:commandLink>
										</center>

									</p:column>

								</p:dataTable>

							</p:panel>

							<!-- Data -->

							<p:panelGrid columns="4"
								columnClasses="ui-grid-col-3,ui-grid-col-2,ui-grid-col-4,ui-grid-col-3"
								layout="grid">

								<!-- Receivers Name -->
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Receivers Name "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText
											id="receiversNameId" style="width:62%" filter="true"
											filterMatchMode="contains" tabindex="1" effect="fold"
											placeholder="Receivers Name"
											value="#{simEditBean.simRegistrationDTO.receiversName}" />
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- NIC Number -->
								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="NIC Number "></p:outputLabel>
									<p:outputLabel value="*" style="color:red;"></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<span class="ui-float-label"> <p:inputText id="nicNoId"
											style="width:62%" filter="true" filterMatchMode="contains"
											tabindex="1" effect="fold" placeholder="NIC Number"
											maxlength="15" onkeyup="this.value=this.value.toUpperCase();"
											value="#{simEditBean.simRegistrationDTO.nicNo}">
											<p:keyFilter regEx="/[a-z0-9-]/i" />
										</p:inputText>
									</span>
								</p:panelGrid>
								<p:panelGrid></p:panelGrid>

								<!-- Issue Date -->

								<p:panelGrid></p:panelGrid>
								<p:panelGrid>
									<p:outputLabel value="Issue Date "></p:outputLabel>
								</p:panelGrid>
								<p:panelGrid>
									<p:calendar id="issueDate" style="height:18px;" maxdate="today"
										navigator="true" showOn="button" locale="en_US"
										pattern="dd/MM/yyyy" placeholder="DD/MM/YYYY"
										yearRange="c-100:c+100" readonlyInput="true" disabled="true"
										value="#{simEditBean.simRegistrationDTO.issueDate}">
										<p:ajax event="dateSelect" update="validUntilDate"
											process="@all" />
									</p:calendar>
