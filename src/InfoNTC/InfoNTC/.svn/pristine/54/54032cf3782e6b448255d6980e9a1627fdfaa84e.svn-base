	<parameter name="P_Date" class="java.lang.String"/>
	<queryString>
		<![CDATA[select que_number,nt_r_transaction_type.description,que_application_no, que_vehicle_no,
case when nt_m_queue_master.que_task_code is null then '-' else ( select que_task_code || ' - ' ||nt_r_task.description from public.nt_r_task where nt_r_task.code = que_task_code ) end as task,
case when que_task_status='C' Then 'Complete' when que_task_status='O' Then 'Ongoing' else '' end
from public.nt_m_queue_master inner join public.nt_r_transaction_type on
public.nt_m_queue_master.que_trn_type_code = public.nt_r_transaction_type.code
where public.nt_m_queue_master.que_date LIKE '%$P!{P_Date}%'
order by  nt_r_transaction_type.description]]>
