		<h:form id="form">
			<div class="mainDiv">
				<h1
					style="margin-top: 0%; text-align: center; color: #ffffff; background-color: #404d6f !important;"
					class="common_form_header">Fare Table Approval</h1>
				<div style="width: 90%;" class="subDiv">


					<p:panel id="panelOne">





						<p:panelGrid columns="4" style="margin-left:50px"
							columnClasses="ui-grid-col-2,ui-grid-col-4,ui-grid-col-4,ui-grid-col-2"
							layout="grid">

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Start Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="fareStartDateId"
									value="#{fareTableApprovalBackingBean.busFareDTO.startDateObj}"
									style="width:60%;height:18px;" yearRange="c-100:c+100"
									navigator="true" locale="en_US" showOn="button" maxdate="today"
									readonlyInput="true" pattern="dd/MM/yyyy"
									placeholder="DD/MM/YYYY">
									<p:ajax event="dateSelect"
										listener="#{fareTableApprovalBackingBean.onStartDateChange}"
										update="fareStartDateId,fareEndDateId" />
								</p:calendar>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="End Date"></p:outputLabel>
							</p:panelGrid>

							<p:panelGrid>
								<p:calendar id="fareEndDateId"
									value="#{fareTableApprovalBackingBean.busFareDTO.endDateObj}"
									style="width:60%;height:18px;" yearRange="c-100:c+100"
									navigator="true" locale="en_US" showOn="button"
									readonlyInput="true" pattern="dd/MM/yyyy" mindate="today"
									placeholder="DD/MM/YYYY"
									disabled="#{fareTableApprovalBackingBean.disabledFareEndDate}">
									<p:ajax event="dateSelect"
										listener="#{fareTableApprovalBackingBean.onEndDateChange}"
										update="fareEndDateId,fareRefNo" />
								</p:calendar>

							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Status"></p:outputLabel>
								<p:outputLabel value="*" style="color:red;"></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="statusId" style="width:68%"
									value="#{fareTableApprovalBackingBean.selectedStatus}"
									filter="true" filterMatchMode="contains" tabindex="1"
									placeholder="-- All --">
									<p:ajax
										listener="#{fareTableApprovalBackingBean.onStatusChange}"
										update="@form" />
									<f:selectItem itemLabel="-- All --" itemValue="" />
									<f:selectItems
										value="#{fareTableApprovalBackingBean.statusList}" var="e"
										itemLabel="#{e.statusDes}" itemValue="#{e.status}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>

							<p:panelGrid></p:panelGrid>
							<p:panelGrid>
								<p:outputLabel value="Fare Reference No."></p:outputLabel>
							</p:panelGrid>
							<p:panelGrid>
								<p:selectOneMenu id="fareRefNo" style="width:68%"
									value="#{fareTableApprovalBackingBean.selectedFareRefNo}"
									filter="true" filterMatchMode="contains" tabindex="1"
									placeholder="-- All --">
									<p:ajax
										listener="#{fareTableApprovalBackingBean.onFareNoChange}"
										update="@form" />
									<f:selectItem itemLabel="-- All --" itemValue="" />
									<f:selectItems
										value="#{fareTableApprovalBackingBean.fareRefNoList}" var="e"
										itemLabel="#{e.fareReferenceNo}"
										itemValue="#{e.fareReferenceNo}" />
								</p:selectOneMenu>
							</p:panelGrid>
							<p:panelGrid></p:panelGrid>



						</p:panelGrid>
						<br></br>
						<center>
							<p:commandButton styleClass="mainActionbtn" value="Search"
								ajax="false" update="@form"
								action="#{fareTableApprovalBackingBean.searchAction}"></p:commandButton>

							<p:commandButton styleClass="mainActionbtn" value="Clear"
								ajax="false" update="@form"
								action="#{fareTableApprovalBackingBean.clearAction}"></p:commandButton>

						</center>
					</p:panel>


					<!--Data Table -->
					<p:panel id="panelTwo">
						<center>
							<p:dataTable id="dataTable" rows="5" paginator="true"
								var="roleEditTable"
								selection="#{fareTableApprovalBackingBean.selectDTO}"
								selectionMode="single" rowKey="#{roleEditTable.fareReferenceNo}"
								value="#{fareTableApprovalBackingBean.dataList}"
								paginatorTemplate="{CurrentPageReport} {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
								rowsPerPageTemplate="10,20" resizableColumns="false"
								liveResize="true" tableStyle="width:100%" rowIndexVar="index"
								styleClass="myTable" emptyMessage="No data available">


								<p:ajax event="rowSelect"
									listener="#{fareTableApprovalBackingBean.selectRow}"
									process="@form" update="@form" />

								<p:column headerText="" visible="false">
									<h:outputText value="" />
								</p:column>



								<p:column headerText="Reference No."
									style="text-align:left;width:15%">
									<h:outputText style="text-align:left;"
										value="#{roleEditTable.fareReferenceNo}" />
								</p:column>

								<p:column headerText="Date" style="text-align:left;width:15%">
									<h:outputText style="text-align:left;"
										value="#{roleEditTable.ammendmentsDateVal}" />
								</p:column>

								<p:column headerText="Status" style="text-align:left;width:15%">
									<h:outputText style="text-align:left;"
										value="#{roleEditTable.statusDes}" />
								</p:column>

								<p:column headerText="Cost Percentage %"
									style="text-align:left;width:16%">
									<h:outputText style="text-align:left;"
										value="#{roleEditTable.currentCostPresentage}" />
								</p:column>
								<p:column tyle="text-align:left;width:15%">
									<p:commandLink
										action="#{fareTableApprovalBackingBean.viewRate()}"
										update="@form,frmfarerate">

										<f:setPropertyActionListener value="#{roleEditTable}"
											target="#{fareTableApprovalBackingBean.viewSelect}" />

										<p:graphicImage library="images" name="view1.png"
											style="width:24px;height:24px;" title="view"></p:graphicImage>
									</p:commandLink>
								</p:column>

							</p:dataTable>
						</center>
					</p:panel>

					<p:panel id="panelThree">
						<center>

							<p:commandButton styleClass="mainActionbtn" value="Checked"
								ajax="false" update="@form" style="width:15%"
								action="#{fareTableApprovalBackingBean.checkedAction}"
								disabled="#{fareTableApprovalBackingBean.disabledCheckedBtn}"></p:commandButton>

							<p:commandButton styleClass="mainActionbtn" value="Recommended"
								ajax="false" update="@form" style="width:15%"
								action="#{fareTableApprovalBackingBean.recommendedAction}"
								disabled="#{fareTableApprovalBackingBean.disabledRecommendedBtn}"></p:commandButton>

							<p:commandButton styleClass="mainActionbtn" value="Approve"
								ajax="false" update="@form" style="width:15%"
								action="#{fareTableApprovalBackingBean.approveAction}"
								disabled="#{fareTableApprovalBackingBean.disabledApproveBtn}"></p:commandButton>

							<p:commandButton styleClass="mainActionbtn" value="Reject"
								ajax="false" update="@form" style="width:15%"
								action="#{fareTableApprovalBackingBean.rejectAction}"
								disabled="#{fareTableApprovalBackingBean.disabledApproveBtn}"></p:commandButton>

						</center>

					</p:panel>

					<!--Data Table -->

				</div>
			</div>

		</h:form>

		<p:dialog styleClass="" id="successSve" global="true" modal="true"
			header="#{msg['um.employeeprofile.dlsuccess']}" width="30%"
			severity="info" widgetVar="successSve" closeOnEscape="true"
			resizable="false">
			<h:form id="frmsuccessSve">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="success-24.png"></p:graphicImage>
						<p:outputLabel value="#{fareTableApprovalBackingBean.successMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmsuccessSve"
						value="#{msg['um.employeeprofile.dlsuccessclose']}"
						onclick="PF('successSve').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<p:dialog styleClass="" id="requiredField" global="true" modal="true"
			header="#{msg['um.employeeprofile.dlmissing']}" width="30%"
			severity="info" widgetVar="requiredField" closeOnEscape="true"
			resizable="false">
			<h:form id="frmrequiredField">
				<center>
					<p:panelGrid columns="2" styleClass="ui-noborder">
						<p:graphicImage library="images" name="error-24.png"></p:graphicImage>
						<p:outputLabel value="#{fareTableApprovalBackingBean.errorMsg}"></p:outputLabel>

					</p:panelGrid>
					<br></br>
					<p:commandButton style="margin-top:10px;margin-bottom:10px"
						process="frmrequiredField"
						value="#{msg['um.employeeprofile.dlmissingclose']}"
						onclick="PF('requiredField').hide()">
					</p:commandButton>

				</center>
			</h:form>
		</p:dialog>

		<p:dialog id="viewFareDB" global="true" modal="true" closable="flase"
			header="Fare Rate Details" width="98%" height="auto" severity="info"
			widgetVar="viewFareRate" closeOnEscape="true" resizable="false">

