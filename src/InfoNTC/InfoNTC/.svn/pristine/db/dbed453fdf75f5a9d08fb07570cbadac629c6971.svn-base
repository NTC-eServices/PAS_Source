										<p:column colspan="3" headerText="Sisu Sariya Quarter"
											visible="#{fareTableApprovalBackingBean.sisuSariyaQuarterAdded}" />

									</p:row>
									<p:row>
										<p:column headerText="Current"
											visible="#{fareTableApprovalBackingBean.normalFareAdd}" />
										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.normalFareAdd}" />
										<p:column headerText="Rounded"
											visible="#{fareTableApprovalBackingBean.normalFareAdd}" />

										<p:column headerText="Current"
											visible="#{fareTableApprovalBackingBean.semiLuxuaryFareAdded}" />
										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.semiLuxuaryFareAdded}" />
										<p:column headerText="Rounded"
											visible="#{fareTableApprovalBackingBean.semiLuxuaryFareAdded}" />

										<p:column headerText="Current"
											visible="#{fareTableApprovalBackingBean.luxuaryFareAdded}" />
										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.luxuaryFareAdded}" />
										<p:column headerText="Rounded"
											visible="#{fareTableApprovalBackingBean.luxuaryFareAdded}" />

										<p:column headerText="Current"
											visible="#{fareTableApprovalBackingBean.superLuxuaryFareAdded}" />
										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.superLuxuaryFareAdded}" />
										<p:column headerText="Rounded"
											visible="#{fareTableApprovalBackingBean.superLuxuaryFareAdded}" />

										<p:column headerText="Current"
											visible="#{fareTableApprovalBackingBean.highWayFareAdded}" />
										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.highWayFareAdded}" />
										<p:column headerText="Rounded"
											visible="#{fareTableApprovalBackingBean.highWayFareAdded}" />

										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.sisuSariyaHalfAdded}" />
										<p:column headerText="Adjuested"
											visible="#{fareTableApprovalBackingBean.sisuSariyaHalfAdded}" />
										<p:column headerText="Diffrent"
											visible="#{fareTableApprovalBackingBean.sisuSariyaHalfAdded}" />

										<p:column headerText="New"
											visible="#{fareTableApprovalBackingBean.sisuSariyaQuarterAdded}" />
										<p:column headerText="Adjuested"
											visible="#{fareTableApprovalBackingBean.sisuSariyaQuarterAdded}" />
										<p:column headerText="Diffrent"
											visible="#{fareTableApprovalBackingBean.sisuSariyaQuarterAdded}" />
									</p:row>
								</p:columnGroup>

								<p:column styleClass="colStage">
									<h:outputText value="#{e.stageNo}" />
								</p:column>
								<p:column
									visible="#{fareTableApprovalBackingBean.normalFareAdd}"
									styleClass="colOne">
									<h:outputText value="#{e.normalCurrentFee}" />
								</p:column>
								<p:column
									visible="#{fareTableApprovalBackingBean.normalFareAdd}"
									styleClass="colOne">
									<h:outputText value="#{e.normalNewFee}" />
								</p:column>
								<p:column
									visible="#{fareTableApprovalBackingBean.normalFareAdd}"
									styleClass="colOne">
									<h:outputText value="#{e.normalRoundFee}" />

								</p:column>

								<p:column
									visible="#{fareTableApprovalBackingBean.semiLuxuaryFareAdded}"
									styleClass="colTwo">
									<h:outputText value="#{e.semiLuxuryCurrentFee}" />
								</p:column>
								<p:column
									visible="#{fareTableApprovalBackingBean.semiLuxuaryFareAdded}"
									styleClass="colTwo">
									<h:outputText value="#{e.semiLuxuryNewFee}" />
								</p:column>
								<p:column
									visible="#{fareTableApprovalBackingBean.semiLuxuaryFareAdded}"
									styleClass="colTwo">
									<h:outputText value="#{e.semiLuxuryRoundFee}" />
								</p:column>


								<p:column
									visible="#{fareTableApprovalBackingBean.luxuaryFareAdded}"
