	<queryString>
		<![CDATA[SELECT DISTINCT
    lss_service_no AS aggrementNO,
    lss_month,
    public.nt_m_sisu_requestor_route_info.srr_origin,
    public.nt_m_sisu_requestor_route_info.srr_destination,
    public.nt_m_sisu_requestor_route_info.srr_no_of_passengers,
    public.nt_m_sisu_permit_hol_school_info.ssc_request_no,
    CASE WHEN lss_created_by IS NOT NULL THEN '0' ELSE '0' END AS zeroVal,
    (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 1 AND $P{P_end_month_value} >= 1 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'JANUARY'
    ) AS JAN,
    (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 2 AND $P{P_end_month_value} >= 2 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'FEBRUARY'
    ) AS FEB,
        (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 3 AND $P{P_end_month_value} >= 3 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'MARCH'
    ) AS MAR,
        (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 4 AND $P{P_end_month_value} >= 4 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'APRIL'
    ) AS APR,
            (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 5 AND $P{P_end_month_value} >= 5 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'MAY'
    ) AS MAY,
                (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 6 AND $P{P_end_month_value} >= 6 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'JUNE'
    ) AS JUN,
                    (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 7 AND $P{P_end_month_value} >= 7 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'JULY'
    ) AS JUL,
                        (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 8 AND $P{P_end_month_value} >= 8 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'AUGUST'
    ) AS AUG,
                            (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 9 AND $P{P_end_month_value} >= 9 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'SEPTEMBER'
    ) AS SEP,
                                (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 10 AND $P{P_end_month_value} >= 10 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'OCTOBER'
    ) AS OCT,
                                    (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 11 AND $P{P_end_month_value} >= 11 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'NOVEMBER'
    ) AS NOV,
                                        (
        SELECT
            CASE
                WHEN $P{P_start_month_value} <= 12 AND $P{P_end_month_value} >= 12 THEN
                SUM(CAST(srr_no_of_passengers AS INTEGER))
                ELSE 0
            END
        FROM public.nt_m_sisu_requestor_route_info
        WHERE public.nt_m_sisu_permit_hol_school_info.ssc_request_no = srr_request_no
            AND lss_year = $P{P_selected_year}
            AND lss_month = 'DECEMBER'
    ) AS DEC
FROM public.nt_m_log_sheet_summary
INNER JOIN public.nt_m_log_sheets ON public.nt_m_log_sheets.los_seq = lss_log_sheet_master_seq
INNER JOIN public.nt_m_sisu_permit_hol_school_info  ON public.nt_m_sisu_permit_hol_school_info.ssc_service_ref_no = public.nt_m_log_sheets.los_std_service_ref_no
INNER JOIN public.nt_m_sisu_permit_hol_service_info  ON public.nt_m_sisu_permit_hol_service_info.sps_service_ref_no = public.nt_m_log_sheets.los_std_service_ref_no
INNER JOIN public.nt_m_sisu_requestor_route_info ON public.nt_m_sisu_requestor_route_info.srr_request_no = public.nt_m_sisu_permit_hol_school_info.ssc_request_no
WHERE public.nt_m_log_sheets.los_subsidy_service_type_code = 'S01'
    AND lss_month IS NOT NULL
    AND lss_service_no IS NOT NULL
    AND nt_m_sisu_permit_hol_school_info.ssc_status IS NOT NULL
    AND public.nt_m_sisu_permit_hol_service_info.sps_status NOT IN ('I')
    AND public.nt_m_sisu_permit_hol_service_info.is_sltb = CASE WHEN $P{P_is_sltb} IS NULL THEN public.nt_m_sisu_permit_hol_service_info.is_sltb ELSE $P{P_is_sltb} end
    and public.nt_m_sisu_permit_hol_service_info.sps_province =  case when  $P{P_Province}   is null then public.nt_m_sisu_permit_hol_service_info.sps_province else $P{P_Province}   end
and public.nt_m_sisu_permit_hol_service_info.sps_district_code = case when  $P{P_District}   is null then public.nt_m_sisu_permit_hol_service_info.sps_district_code else   $P{P_District}  end
    AND lss_year = $P{P_selected_year}
    AND EXTRACT(MONTH FROM TO_DATE(lss_month, 'Month')) BETWEEN $P{P_start_month_value} AND $P{P_end_month_value}
ORDER BY lss_service_no, lss_month, 
public.nt_m_sisu_requestor_route_info.srr_origin,
public.nt_m_sisu_requestor_route_info.srr_destination,
public.nt_m_sisu_requestor_route_info.srr_no_of_passengers;]]>
	</queryString>
