	<queryString>
		<![CDATA[select distinct a.chr_offence_code,a.chr_offence_desc,a.chr_charge_amount,b.string_value,c.permit_no,c.vehicle_no,c.pmo_full_name,c.rec_receipt_no
from 
(select  chr_offence_code,chr_offence_desc,chr_charge_amount
from public.nt_t_offence_charge_sheet
where chr_complaint_no=$P{P_complain_no})a,
(select string_value
from public.nt_r_parameters where param_name='CHARGE_SHEET_VOUCHER_ACCOUNT_NUMBER')b,
(SELECT permit_no,vehicle_no, y.pmo_full_name,z.rec_receipt_no FROM public.nt_m_complain_request x
inner join public.nt_t_pm_vehi_owner y on y.pmo_permit_no= x.permit_no
inner join public.nt_t_grievance_chargesheet_receipt z on z.rec_complaint_no=x.complain_no
where complain_no=$P{P_complain_no} )c]]>
	</queryString>
