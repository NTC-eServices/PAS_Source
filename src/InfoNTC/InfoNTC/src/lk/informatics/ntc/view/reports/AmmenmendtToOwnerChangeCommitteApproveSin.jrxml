	<parameter name="P_LOGO" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.amd_application_no,a.amd_existing_busno,a.amd_relationship_with_transferor,d.description,
b.pmo_full_name,b.pmo_full_name_sinhala,
c.pm_permit_expire_date,c.pm_old_application_no
from public.nt_m_amendments a
left outer join public.nt_t_pm_vehi_owner b on b.pmo_application_no=a.amd_application_no
left outer join public.nt_t_pm_application c on c.pm_application_no=a.amd_application_no
inner join public.nt_r_relationships d on d.code=a.amd_relationship_with_transferor
where a.amd_status ='C'
and a.amd_trn_type ='14']]>
