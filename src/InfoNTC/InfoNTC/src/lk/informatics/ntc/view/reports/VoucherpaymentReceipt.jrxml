		<![CDATA[select a.vod_account_no , a.vod_amount ,b.description,a.vod_voucher_no,a.vod_permitno,c.rec_receipt_no,d.pav_tran_type,e.description as transtype
from nt_t_voucher_details a ,public.nt_r_charge_type b,nt_t_receipt c,nt_m_payment_voucher d, nt_r_transaction_type e
where a.vod_charge_code=b.code
and  c.rec_voucher_no=a.vod_voucher_no
and a.vod_voucher_no = d.pav_voucher_no
and e.code = d.pav_tran_type
and a.vod_voucher_no= $P{P_Voucher_No}]]>
