		<![CDATA[select  distinct pm_application_no,pm_permit_no,pm_vehicle_regno,pm_permit_issue_date,pm_valid_to,pm_permit_expire_date,pm_route_flag,
nt_t_pm_vehi_owner.pmo_full_name,nt_t_pm_vehi_owner.pmo_address1,nt_t_pm_vehi_owner.pmo_address2,nt_t_pm_vehi_owner.pmo_address3,nt_t_pm_vehi_owner.pmo_city ,
