	<queryString>
		<![CDATA[select distinct nt_m_log_sheet_summary.lss_service_no as AgreementNo, nt_m_sisu_permit_hol_service_info.sps_operator_name as Owner,
nt_m_sisu_permit_hol_service_info.sps_origin as Origin, nt_m_sisu_permit_hol_service_info.sps_destination as Destination,nt_m_sisu_permit_hol_service_info.sps_service_ref_no,
nt_m_sisu_permit_hol_school_info.ssc_school_name,case when lss_created_by is not null then '0.00' else '0.00' end  as zeroVal, 
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 1 AND $P{P_end_month_value} >= 1 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'JANUARY'))) as JAN,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 2 AND $P{P_end_month_value} >= 2 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'FEBRUARY'))) as FEB,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 3 AND $P{P_end_month_value} >= 3 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'MARCH'))) as MAR,
 (SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 4 AND $P{P_end_month_value} >= 4 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'APRIL'))) as APR,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 5 AND $P{P_end_month_value} >= 5 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'MAY'))) as MAY,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 6 AND $P{P_end_month_value} >= 6 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'JUNE'))) as JUN,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 7 AND $P{P_end_month_value} >= 7 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'JULY'))) as JUL,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 8 AND $P{P_end_month_value} >= 8 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'AUGUST'))) as AUG,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 9 AND $P{P_end_month_value} >= 9 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'SEPTEMBER'))) as SEP,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 10 AND $P{P_end_month_value} >= 10 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'OCTOBER'))) as OCT,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 11 AND $P{P_end_month_value} >= 11 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'NOVEMBER'))) as NOV,
(SELECT 
            CASE 
                WHEN $P{P_start_month_value} <= 12 AND $P{P_end_month_value} >= 12 THEN SUM(lss_payment_amt)
                ELSE 0
            END from public.nt_m_log_sheet_summary
where ((lss_service_no =sps_service_no) and (lss_year =$P{P_selected_year} ) and
  (lss_month = 'DECEMBER'))) as DEC
from nt_m_log_sheet_summary
inner join nt_m_log_sheets 
on nt_m_log_sheets.los_seq = nt_m_log_sheet_summary.lss_log_sheet_master_seq 
inner join nt_m_sisu_permit_hol_service_info
on nt_m_sisu_permit_hol_service_info.sps_service_ref_no = nt_m_log_sheets.los_std_service_ref_no 
inner join  public.nt_m_sisu_permit_hol_school_info on public.nt_m_sisu_permit_hol_school_info.ssc_service_ref_no=nt_m_sisu_permit_hol_service_info.sps_service_ref_no
where sps_service_no is not null
and sps_status = 'A'
and lss_year = $P{P_selected_year}
and EXTRACT(MONTH FROM TO_DATE(lss_month, 'Month')) BETWEEN $P{P_start_month_value} AND $P{P_end_month_value}
and is_sltb =case when   $P{P_is_sltb} is null then is_sltb else  $P{P_is_sltb} end
and lss_service_no is not null
and lss_payment_amt is not null
 and sps_province =  case when $P{P_province_code}  is null then sps_province else   $P{P_province_code} end
and sps_district_code = case when  $P{P_district_code}  is null then sps_district_code else  $P{P_district_code} end
and nt_m_log_sheets.los_subsidy_service_type_code= $P{P_subsidy_type} 
order by nt_m_sisu_permit_hol_service_info.sps_operator_name]]>
	</queryString>
