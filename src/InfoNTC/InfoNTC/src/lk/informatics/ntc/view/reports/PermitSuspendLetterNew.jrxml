		<parameter name="P_ref_NO" class="java.lang.String"/>
		<queryString>
			<![CDATA[select  distinct x.suspend_permit,x.suspend_vehicle,c.vio_condition_cd,d.ofm_offence_desc_sinhala
from public.nt_t_suspend_master  x
inner join public.nt_t_flying_vio_conditions c on c.vio_vehicleno=x.suspend_vehicle
inner join public.nt_m_offence_management d on d.ofm_offence_code=c.vio_condition_cd
where suspend_ref_no= $P{P_ref_NO} 
and c.vio_violated_status='true']]>
		</queryString>
		<field name="suspend_permit" class="java.lang.String"/>
		<field name="suspend_vehicle" class="java.lang.String"/>
		<field name="vio_condition_cd" class="java.lang.String"/>
		<field name="ofm_offence_desc_sinhala" class="java.lang.String"/>
	</subDataset>
	<parameter name="P_Suspend_Ref_No" class="java.lang.String"/>
	<parameter name="P_driver_name" class="java.lang.String"/>
	<parameter name="P_conductor_name" class="java.lang.String"/>
	<queryString>
		<![CDATA[select x.suspend_permit,x.suspend_vehicle,x.suspend_start_date,x.suspend_end_date,to_char(x.created_date,'dd/MM/yyyy') as createDate,x.suspend_permit_owner,
a.pm_route_no,b.rou_service_origine_sin,b.rou_service_destination_sin
--c.vio_condition_cd,d.ofm_offence_desc_sinhala,
,e.pmo_full_name_sinhala
from public.nt_t_suspend_master  x
inner join public.nt_t_pm_application a on a.pm_permit_no=x.suspend_permit
inner join public.nt_r_route b on b.rou_number= a.pm_route_no
--inner join public.nt_t_flying_vio_conditions c on c.vio_vehicleno=x.suspend_vehicle
--inner join public.nt_m_offence_management d on d.ofm_offence_code=c.vio_condition_cd
inner join public.nt_t_pm_vehi_owner e on e.pmo_permit_no=x.suspend_permit
where suspend_ref_no=$P{P_Suspend_Ref_No}
--and c.vio_violated_status='true']]>
	</queryString>
