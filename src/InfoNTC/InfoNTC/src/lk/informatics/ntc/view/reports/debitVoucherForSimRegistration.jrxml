from public.nt_m_sim_payment_voucher
inner join public.nt_t_sim_voucher_details a on a.vod_payment_vou_seq=public.nt_m_sim_payment_voucher.spv_seq_no
inner join public.nt_r_charge_type b on b.code=a.vod_charge_type
inner join public.nt_m_sim_registration c on c.sim_reg_no =public.nt_m_sim_payment_voucher.spv_sim_no 
