	<queryString>
		<![CDATA[SELECT DISTINCT
    sps_service_no AS agreementno,
    sps_operator_name,
    nt_r_service_types.description,
    sps_origin,
    sps_destination,
    sps_bus_no,
    sps_province,
    sps_district_code,
    CASE WHEN sps_bus_no LIKE '%SLTB%' THEN 1 ELSE 2 END AS bus_group
FROM
    nt_m_sisu_permit_hol_service_info
    INNER JOIN nt_r_service_types ON nt_m_sisu_permit_hol_service_info.sps_service_type_code = nt_r_service_types.code
WHERE
    sps_service_no IS NOT NULL
    and sps_province =  case when $P{P_Province} is null then sps_province else $P{P_Province} end
	and sps_district_code = case when $P{P_District} is null then sps_district_code else $P{P_District} end
ORDER BY
    bus_group,
    sps_operator_name,
    sps_service_no]]>
	</queryString>
