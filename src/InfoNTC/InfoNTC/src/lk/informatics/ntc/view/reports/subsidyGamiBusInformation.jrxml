	<queryString>
		<![CDATA[SELECT DISTINCT
    gps_service_no AS agreementno,
    gps_operator_name,
    nt_r_service_types.description,
    gps_origin,
    gps_destination,
    gps_bus_no,
    gps_province,
    gps_district_code,
    CASE WHEN gps_bus_no LIKE '%SLTB%' THEN 1 ELSE 2 END AS bus_group
FROM
    nt_m_gami_permit_hol_service_info
    INNER JOIN nt_r_service_types ON nt_m_gami_permit_hol_service_info.gps_service_type_code=nt_r_service_types.code
WHERE
    gps_service_no IS NOT null
    and gps_province =  case when $P{P_Province} is null then gps_province else $P{P_Province} end
	and gps_district_code = case when $P{P_District} is null then gps_district_code else $P{P_District} end
ORDER BY
    bus_group, 
    gps_operator_name,
    gps_service_no]]>
	</queryString>
