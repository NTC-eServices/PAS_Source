	<queryString>
		<![CDATA[select a.pm_is_backlog_app, a.pm_valid_to, a.pm_application_no,a.pm_permit_no, a.pm_vehicle_regno,TO_CHAR(a.pm_created_date,'dd/MM/yyyy') as date, a.pm_created_by,
pm_permit_expire_date,nt_t_pm_vehi_owner.pmo_full_name
FROM public.nt_t_pm_application a
left outer join  public.nt_t_pm_vehi_owner on a.pm_vehicle_regno = public.nt_t_pm_vehi_owner.pmo_vehicle_regno
where  a.pm_is_backlog_app='Y' and pm_application_no not like '%PAP%'and
a.pm_created_by=case when $P{P_User_Id}  is null then a.pm_created_by else $P{P_User_Id} end
and a.pm_permit_no= case when  $P{P_Permit_No}  is null then a.pm_permit_no else  $P{P_Permit_No}  end 
and a.pm_application_no= case  when  $P{P_APP_no}  is null then a.pm_application_no else $P{P_APP_no}   end 
and a.pm_vehicle_regno  =case when  $P{P_Vehi_No}  is null then a.pm_vehicle_regno else  $P{P_Vehi_No}  end
and to_char(A.pm_created_date,'yyyy-MM-dd')>=case when  $P{P_Start_Date}   is null then to_char(A.pm_created_date,'yyyy-MM-dd') else $P{P_Start_Date}  end
and to_char(A.pm_created_date,'yyyy-MM-dd')<=case when   $P{P_End_Date}   is null then to_char(A.pm_created_date,'yyyy-MM-dd') else   $P{P_End_Date}  end
order by a.pm_application_no]]>
	</queryString>
